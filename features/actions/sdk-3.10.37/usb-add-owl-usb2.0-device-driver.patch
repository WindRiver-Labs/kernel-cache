+	.get_frame = aotg_udc_get_frame,
+	.wakeup = aotg_udc_wakeup,
+	.pullup = aotg_udc_pullup,
+	.vbus_session = aotg_udc_vbus_session,
+	.vbus_draw = aotg_udc_vbus_draw,
+	.udc_start = aotg_udc_start,
