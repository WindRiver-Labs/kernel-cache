+static int aotg_udc_stop(struct usb_gadget *g)
+{
+	struct aotg_udc *udc = container_of(g, struct aotg_udc, gadget);
+	udc->driver = NULL;
+	extern_irq_disable(udc);
+	udc_disable(udc);
+	if (udc->transceiver)
+		return otg_set_peripheral(udc->transceiver->otg, NULL);
+	return 0;
+}
