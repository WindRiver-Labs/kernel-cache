+	UDC_DEBUG("<EP FREE REQ>%s, %p\n", _ep->name, _req);
+	req = container_of(_req, struct aotg_request, req);
+	if (!list_empty(&req->queue))
+		UDC_DEBUG("<EP FREE REQ>ep's queue is not empty\n");
+	kfree(req);
+
