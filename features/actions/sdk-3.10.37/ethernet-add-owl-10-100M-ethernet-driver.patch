+static void gen_frame_pattern(char *buf, int len, int pattern)
+{
+	//printk("buf: %p, len: %d, pattern: %d\n", buf, len, pattern);
+	switch (pattern) {
+	case 0:
+		memset(buf, 0, len);
+		break;
+	case 1:
+		memset(buf, 0xff, len);
+		break;
+	case 2:
+		get_random_bytes(buf, len);
+		break;
+	default:
+		printk(KERN_WARNING"not supported pattern: %d\n", pattern);
+		break;
+	}
+}
