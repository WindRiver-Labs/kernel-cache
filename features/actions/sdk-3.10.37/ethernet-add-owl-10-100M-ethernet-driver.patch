+                    ecp->rx_bd_base, ecp->rx_bd_paddr);
+
+        return -ENOMEM;
+    }
+   #if 0
+    if (ecp->phy_model == ETH_PHY_MODEL_ATC2605) {
+        /*For gl5302 INT Pending unusual */
+        ethernet_clock_enable();
+        ecp->phy_ops->phy_hw_init(ecp);
