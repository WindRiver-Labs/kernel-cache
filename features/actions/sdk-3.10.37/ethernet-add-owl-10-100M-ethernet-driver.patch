+	/*clear phy INT_STAT*/
+	atc260x_reg_write(ecp->atc260x, atc2603_PHY_INT_STAT, 0xff);
+	//printk("1f.atc2603_PHY_INT_STAT:0x%x\n", atc260x_reg_read(atc260x, gl5302_PHY_INT_STAT));
