+static int asoc_ethernet_resume(struct platform_device *pdev)
+{
+    dev_info(&pdev->dev, "asoc_ethernet_resume()\n");
+
+    queue_delayed_work(resume_queue, &resume_work, msecs_to_jiffies(PHY_RESUME_DELAY));
+    return 0;
+}
