+static void detect_power_save_timer_func(unsigned long data)
+{
+    ec_priv_t *ecp = (ec_priv_t *)data;
+    unsigned long flags;
+
+    printk(KERN_INFO "ecp->enable: %u\n", ecp->enable);
+    if (!ecp->opened) {
+        printk(KERN_INFO "not opened yet\n");
+        return;
+    }
+
+    spin_lock_irqsave(&ecp->lock, flags);
+    if (ecp->linked) {
