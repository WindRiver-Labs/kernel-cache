+static void ec_netdev_uninit(struct net_device *dev)
+{
+    ec_priv_t *ecp = netdev_priv(dev);
+
+    printk(KERN_DEBUG"%s %d\n",__func__,__LINE__);
+#if 0
+    if (ecp->tx_wq) {
