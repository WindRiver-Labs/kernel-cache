+		gen_frame_pattern(skb->data, skb->len, pat);
+
+		ec_netdev_start_xmit(skb, ndev);
+		if (continuity_interval > 0)
+			udelay(continuity_interval);
+	}
+	return 0;
+err:
+	return ret;
+}
