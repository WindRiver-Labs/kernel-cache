+/* NOTE: it has side effect for dest parameter */
+#define COPY_MAC_ADDR(dest, mac)  do {\
+    *(unsigned short *)(dest) = *(unsigned short *)(mac); \
+    *(unsigned short *)((dest) + 4) = *(unsigned short *)((mac) + 2); \
+    *(unsigned short *)((dest) + 8) = *(unsigned short *)((mac) + 4); \
+    (dest) += 12; \
+}while (0)
