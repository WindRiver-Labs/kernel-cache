+/* suitable for less than 7 chars of string */
+static inline int string_to_hex(char *str, int len)
+{
+    int     val;
+    int     i;
+    char    ch;
+
+    val = 0;
+    for (i = 0; i < len; i++) {
+        ch = str[i];
+        if ('0' <= ch && ch <= '9') {
+            val = (val << 4) + ch - '0';
+        } else if ('a' <= ch && ch <= 'f') {
+            val = (val << 4) + 10 + ch - 'a';
+        } else if ('A' <= ch && ch <= 'F') {
+            val = (val << 4) + 10 + ch - 'A';
+        } else {
+            return (-1);
+        }
+    }
+
+    return (val);
+}
