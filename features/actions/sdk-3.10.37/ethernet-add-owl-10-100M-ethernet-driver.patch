+
+    if (ecp->tx_bd_base)
+        dma_free_coherent(NULL, sizeof(ec_bd_t) * TX_RING_SIZE,
+                ecp->tx_bd_base, ecp->tx_bd_paddr);
+    if (ecp->rx_bd_base)
+        dma_free_coherent(NULL, sizeof(ec_bd_t) * RX_RING_SIZE,
+                ecp->rx_bd_base, ecp->rx_bd_paddr);
+}
