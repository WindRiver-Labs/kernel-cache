+    printk(KERN_DEBUG"virt_to_phys(ecp->tx_bd_base):%p\n", (void *)virt_to_phys(ecp->tx_bd_base));
+
+    if (ecp->tx_bd_base == NULL || ecp->rx_bd_base == NULL) {
