+static inline void set_mode_all_multicast(ec_priv_t * ecp, bool supported)
+{
+    volatile ethregs_t *hw_regs = ecp->hwrp;
+
+    ecp->all_multicast = supported;
+    if (supported)
+        hw_regs->er_opmode |= EC_OPMODE_PM;
+    else
+        hw_regs->er_opmode &= ~EC_OPMODE_PM;
+
+    return;
+}
