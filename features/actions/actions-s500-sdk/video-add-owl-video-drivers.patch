+static bool dsihw_check_boot_dsi_inited(struct platform_device *pdev) {
+
+	boot_dsi_inited = dsihw_read_reg(pdev,DSI_VIDEO_CFG)&0x01;	
+	
+    return boot_dsi_inited;
