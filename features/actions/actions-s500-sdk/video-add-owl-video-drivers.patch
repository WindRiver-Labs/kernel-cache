+
+	vce_info("width:%d  height:%d freq:%d\n",
+	freq_MHz->width, freq_MHz->height, (int) freq_MHz->freq);
+
+	if (new_rate > 500) {
+		vce_err("cannot set vce freq to : %ld MHz\n",new_rate);
