+static void __exit cvbs_panel_remove(struct owl_dss_device *dssdev)
+{
+	struct panel_drv_data *drv_data = dev_get_drvdata(&dssdev->dev);
+
+	kfree(drv_data);
+
+	dev_set_drvdata(&dssdev->dev, NULL);
