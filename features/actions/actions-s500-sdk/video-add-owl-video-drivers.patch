+static u32 de_irq_to_mask(enum de_irq_type irq)
+{
+	switch (irq) {
+	case DE_IRQ_CVBS_PRE:
+		return (1 << 0);
+		break;
+	case DE_IRQ_DSI_PRE:
+		return (1 << 1);
+		break;
+	case DE_IRQ_HDMI_PRE:
+		return (1 << 2);
+		break;
+	case DE_IRQ_LCD_PRE:
+		return (1 << 3);
+		break;
+	case DE_IRQ_LCD2_PRE:
+		return (1 << 4);
+		break;
+	default:
+		return 0;
+		break;
+	}
+}
+
+static u32 de_irq_mask_to_enable(u32 mask)
+{
+	/* TODO */
+	u32 enable = 0;
+
+	if (mask & (1 << 0)) {
+		/* CVBS */
+		enable |= (1 << 3);
+	}
+	if (mask & (1 << 1)) {
+		/* DSI */
+		enable |= (1 << 2);
+	}
+	if (mask & (1 << 2)) {
+		/* HDMI */
+		enable |= (1 << 4);
+	}
+	if (mask & (1 << 3)) {
+		/* LCD */
+		enable |= (1 << 0);
+	}
+
+	return enable;
+}
