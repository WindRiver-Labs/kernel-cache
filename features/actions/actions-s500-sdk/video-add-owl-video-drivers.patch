+/* convert INT8 number to little endien number */
+int c2ln14(unsigned char *num, char *a) {
+    int i;
+    int n = 14;
+    for (i = 0; i < 11; i++) {
+        num[i] = 0;
+    }
+
+    for (i = 0; i < n; i++) {
+        if (i % 2) {
+            if (a[n - i - 1] >= '0' && a[n - i - 1] <= '9') {
+                num[i / 2] |= (a[n - i - 1] - '0') << 4;
+            } else if (a[n - i - 1] >= 'a' && a[n - i - 1] <= 'f') {
+                num[i / 2] |= (a[n - i - 1] - 'a' + 10) << 4;
+            } else if (a[n - i - 1] >= 'A' && a[n - i - 1] <= 'F') {
+                num[i / 2] |= (a[n - i - 1] - 'A' + 10) << 4;
+            }
+        } else {
+            if (a[n - i - 1] >= '0' && a[n - i - 1] <= '9') {
+                num[i / 2] |= (a[n - i - 1] - '0');
+            } else if (a[n - i - 1] >= 'a' && a[n - i - 1] <= 'f') {
+                num[i / 2] |= (a[n - i - 1] - 'a' + 10);
+            } else if (a[n - i - 1] >= 'A' && a[n - i - 1] <= 'F') {
+                num[i / 2] |= (a[n - i - 1] - 'A' + 10);
+            }
+        }
+    }
+    return 0;
+}
