+int edid_checksum(u8 block,u8 *buf)
+{
+    int i = 0, CheckSum = 0;
+	u8 *pbuf = buf + 128*block;
+	
+    for( i = 0, CheckSum = 0 ; i < 128 ; i++ )
+	{
+        CheckSum += pbuf[i] ; 
+        CheckSum &= 0xFF ;
+    }
+
+	if( CheckSum != 0 )
+	{
+		DEBUG_ERR("EDID block %d checksum error\n",block);
+		return -1 ;
+	}
+	return 0;
+}
