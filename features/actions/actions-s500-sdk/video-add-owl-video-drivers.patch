+static int vde_query_interval(unsigned int reg4)
+{
+    unsigned int mb_w = (reg4 >> 23)&0x1ff;
+    unsigned int mb_h = (reg4 >> 15)&0xff;
+    unsigned int r = (mb_w *mb_h * 300)/(1000 * vde_pre_freq);
+    if(r < 5) {
+	r = 5;
+    }
+    return r;
+}
