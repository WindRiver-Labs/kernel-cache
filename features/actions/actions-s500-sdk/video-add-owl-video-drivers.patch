+static int vde_suspend (struct platform_device * dev, pm_message_t state)
+{    
+	printk("vde : vde_suspend: In , vde_clk_isEnable=%d, RST0=0x%x\n", vde_clk_isEnable, readl_relaxed(VDE_CMU_DEVRST0));		    
+    if(vde_clk_isEnable != 0) {
