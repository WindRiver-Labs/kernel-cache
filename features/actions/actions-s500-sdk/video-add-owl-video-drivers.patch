+static u32 string_to_data_fmt(const char *name)
+{
+	int i;
+	DEBUG_CVBS("ARRAY_SIZE=%d\n",ARRAY_SIZE(date_fmts));
+	for (i = 0; i < ARRAY_SIZE(date_fmts); i++) {
+		if (!strcmp(date_fmts[i].name, name))
+			return date_fmts[i].data_fmt;
+	}
+
+	return -1;
