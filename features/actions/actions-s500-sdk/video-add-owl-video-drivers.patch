+{
+	hdmi.ip_data.ops->write_reg(&hdmi.ip_data, idx, val);
+}
+EXPORT_SYMBOL(hdmihw_write_reg); 
+
+int hdmihw_read_reg(const u16 idx)
+{
+	return hdmi.ip_data.ops->read_reg(&hdmi.ip_data, idx);
+}
+EXPORT_SYMBOL(hdmihw_read_reg); 
