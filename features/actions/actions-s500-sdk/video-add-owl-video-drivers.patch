+{	
+	u32 val;
+		
+	val = edphw_read_reg(edpdev,EDP_RGB_CTL);
+	
+	val = REG_SET_VAL(val, single, 1, 1);
+		
+	edphw_write_reg(edpdev,EDP_RGB_CTL, val);
