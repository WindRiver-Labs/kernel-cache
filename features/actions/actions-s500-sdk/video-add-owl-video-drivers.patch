+static unsigned calc_offset_dma(const struct fb_var_screeninfo *var,
+		const struct fb_fix_screeninfo *fix)
+{
+	unsigned offset;
+
+	offset = var->yoffset * fix->line_length +
+		var->xoffset * (var->bits_per_pixel >> 3);
+
+	return offset;
+}
