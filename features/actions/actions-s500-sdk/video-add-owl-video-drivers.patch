+static int wait_lanes_stop(void)
+{
+	u32 tmp;
+	int cnt = 1000;
+	
+	struct platform_device *pdev = dsi_pdev_map[0] ;
+	do {
+		tmp = dsihw_read_reg(pdev, DSI_LANE_STA);
+		if ((tmp & (1 << 12)) || (tmp & (1 << 5)))
+			break;
+
+		udelay(2);
+	} while (--cnt);
+
+	if (cnt <= 0) {
+		printk("lanes cannot STOP\n");
+		return -1;
+	}
+
+	return 0;
+}
