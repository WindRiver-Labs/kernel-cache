+    /* get Ri */
+    ip_data->hdcp.Ri = (owl_hdmi_read(HDCP_LIR) >> 16) & 0xffff;
+    HDCP_DEBUG("Ri:0x%x\n", ip_data->hdcp.Ri);
+
+    return 0;
+}
