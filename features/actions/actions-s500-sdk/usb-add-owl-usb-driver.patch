+int mon_sysfs_exit(void)
+{
+	sysfs_remove_group(my_mon->mon_port, &mon_port_status);
+	sysfs_remove_group(my_mon->mon_port, &mon_port_config);
+	kobject_del(my_mon->mon_port);
+	kobject_del(my_mon->mon_obj);
+	return 0;
+}
