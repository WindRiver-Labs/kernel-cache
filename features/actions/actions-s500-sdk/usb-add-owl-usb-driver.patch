+static void adfu_sync_bdev(const char *path)
+{
+	struct file *filp;
+
+	pr_err("[adfuserver]sync .. bdev %s\n", path);
+
+	filp = filp_open(path, O_RDWR, 0);
+	if (IS_ERR(filp)) {
+		pr_err("can not open %s\n", path);
+		return;
+	}
+
+	vfs_fsync(filp, 1);
+	filp_close(filp, 0);
+}
