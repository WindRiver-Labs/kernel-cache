+static int ep0_queue(struct fsg_dev *fsg)
+{
+	int rc;
+
+	rc = usb_ep_queue(fsg->ep0, fsg->ep0req, GFP_ATOMIC);
+	if (rc != 0 && rc != -ESHUTDOWN) {
+
+		/* We can't do much more than wait for a reset */
+		WARNING(fsg, "error in submission: %s --> %d\n",
+			fsg->ep0->name, rc);
+	}
+	return rc;
+}
