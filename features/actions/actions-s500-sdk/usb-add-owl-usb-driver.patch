+int mon_sysfs_init(void)
+{
+	int ret = 0;
+
+	my_mon->mon_obj = kobject_create_and_add("monitor", NULL);
+	if (!my_mon->mon_obj) {
+		MONITOR_ERR("unable to create monitor kobject\n");
