+/* \BB\F1\D4\CB\D0\BD\CF\C2\B4\EC\B2\E2\B5\C4\BC\E4\BC\E4\B8\F4\A3\AC\B7\B5\BB\D8\D2\C1\C3\EB\B5\A5\A1\A3 */
+unsigned int umonitor_get_timer_step_interval(void)
+{
+	umonitor_dev_status_t *pStatus;
+
+	pStatus = umonitor_status;
+
+	if ((pStatus->port_config->detect_type == UMONITOR_DISABLE)
+		|| (pStatus->detect_valid == 0)) {
+		return 0x70000000;	/* be longer enough that it would not run again. */
+	}
+
+	if (pStatus->timer_steps == 0) {
+		//pStatus->timer_interval = USB_MONITOR_DEF_INTERVAL;
+		pStatus->timer_interval = 30;	/*\D6\D8\D0\F8\C8\EBstep 0 \BC\EC\B2\E9 */
+		goto out;
+	}
+
+	if (pStatus->det_phase == 0) {
+		switch (pStatus->timer_steps) {
+			/* 
+			 * 1\A3\AD3\B2\BD\A3\AC\BC\EC\B2\E2\BC\E4\BC\E4\B8\F4500 ms\A3\A8\BF\F7\D5\FB\A3\A9\A3\AC\B5\A9\B6\DA\D3\C2\ED\C9\F7\B5\BD\B5\DA\CB\BD\A3\AC
+			 * \BD\F8\C8\EBdebounce\BA\CDconfirm\A1\A3
+			 */
+		case 1:
+		case 2:
+		case 3:
+			pStatus->timer_interval = USB_MONITOR_DEF_INTERVAL;
+			break;
+
+		case 4:
+			switch (pStatus->device_confirm) {
+			case 0:	/* \BD\F8\C8\EB\B5\DA4\B2\BD\A3\AC\CF\C2\B2\BD\CA\EC\B2\E2\B6\DAvbus\D3\D0\CE\A1\A3 */
+				pStatus->timer_interval =
+				    USB_MONITOR_DEF_INTERVAL;
+				break;
+
+			case 1:	/* \D2\AD\BC\EC\B2\B6\DAvbus\D3\D0\E8\CF\C2\B2\BD\D4\D9\C8\CF\B4\A3 */
+				pStatus->timer_interval = 10;	/* 10 ms, 1 tick. */
+				break;
+
+				/*
+				 * device_confirm == 2, \D2\AD\C8\CFvbus\D3\B2\A2enable\C9\CF\C0\AD500k\A3\AC
+				 * disable\CF\C2\C0\AD15k\A3\AC\CF\C2\B2\BD\BC\EC\B2\E2dp\A1\A2dm\A1\A3 
+				 */
+			case 2:
+				pStatus->timer_interval = 300;
+				break;
+
+				/* \B5\DA\B4\F1dp\A1\A2dm\A3\AC\D0\E8\CF\C2\B4\CE\D4\D9\C8\CF\B4\A3 */
+			case 3:
+				pStatus->timer_interval = 30;
+				break;
+
+				/* 
+				 * \BD\F8\C8\EB\D5\E2\B2\BD\A3\AC\D0\E8\B6\E0\B4\CE\C5\CFpc\CA\F1\D3\D0sof\BB\F2reset\D0\F8\BB\FA\A3\AC\D5\E2\C0\EF20\BA\C1\C3\EB\B2\E9sof\D6\CF\B4\AC
+				 * \C0\FD\BB\E1\B2\E9 MAX_DETECT_SOF_CNT \B4\AC\B5\FD\BC\E4\BF\C9\C4\E1\D3\D08\C3\EB\D2\D4\C9\A3 
+				 * \D2\F2\B4\D3\BB\FAdp\C9\CF\C0\AD\B5\BDpc\B7\A2sof\A3\AC\D6\E4\BF\C9\C4\DC\D1\D3\B3\A4\B4\EF8\C3\EB\D2\D4\C9\A3
+				 */
+				/* wait sof again time interval, the whole detect sof time is (20 * sof_check_times) msecond. */
+			case 4:
+				pStatus->timer_interval = 20;
+				break;
+
+			default:
+				USB_ERR_PLACE;
+				pStatus->timer_interval =
+				    USB_MONITOR_DEF_INTERVAL;
+				break;
+			}
+			break;
+
+		default:
+			USB_ERR_PLACE;
+			pStatus->timer_interval = USB_MONITOR_DEF_INTERVAL;
+			break;
+		}
+	} else {
+		switch (pStatus->timer_steps) {
+		case 1:	/* \B4\D3step 0\B5\C4idle\C7\BB\B5\BDvbus\B6\D4\CD\B5\E7\B5\C4\BC\E4\BC\E4\B8\F4\A1\A3 */
+			pStatus->timer_interval = 30;
+			break;
+
+		case 2:	/* vbus\B6\D4\CD\B5\E7\BA\BD\BF\AA\B5\DA2\B4\EC\B2\C5\CFdp\CA\F1\C9\CF\C0\AD\B5\C4\BC */
+			pStatus->timer_interval = 600;
+			break;
+
+		case 3:	/* \B5\DA2\B4\EC\B2\B5\DA3\B4\EC\B2\E2\BC\C5\CFdp\CA\F1\C9\CF\C0\AD\B5\C4\BC */
+			pStatus->timer_interval = 600;
+			break;
+
+		case 4:
+			switch (pStatus->host_confirm) {
+			case 0:
+				pStatus->timer_interval =
+				    USB_MONITOR_DEF_INTERVAL;
+				break;
+
+			case 1:	/* debounce time. */
+				pStatus->timer_interval = 10;	/* 10 ms, 1 tick. */
+				break;
+
+			default:
+				USB_ERR_PLACE;
+				pStatus->timer_interval =
+				    USB_MONITOR_DEF_INTERVAL;
+				break;
+			}
+			break;
+
+		default:
+			USB_ERR_PLACE;
+			pStatus->timer_interval = USB_MONITOR_DEF_INTERVAL;
+			break;
+		}
+	}
+
+out:
+	return pStatus->timer_interval;
+}
