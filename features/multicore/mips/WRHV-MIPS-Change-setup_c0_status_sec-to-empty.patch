From 2d8e6423be158e69062d29ffce9fee248205511e Mon Sep 17 00:00:00 2001
From: Yang Shi <yang.shi@windriver.com>
Date: Wed, 23 Mar 2011 12:57:59 +0800
Subject: [PATCH 1/7] WRHV: MIPS: Change setup_c0_status_sec to empty

GOS doesn't change CP0 registers, so change setup_c0_status_sec
macro to empty.

Signed-off-by: Yang Shi <yang.shi@windriver.com>
---
 arch/mips/kernel/wrhv_head.S |    5 -----
 1 files changed, 0 insertions(+), 5 deletions(-)

diff --git a/arch/mips/kernel/wrhv_head.S b/arch/mips/kernel/wrhv_head.S
index 047fecb..c4b4d8f 100644
--- a/arch/mips/kernel/wrhv_head.S
+++ b/arch/mips/kernel/wrhv_head.S
@@ -112,11 +112,6 @@
 	.endm
 
 	.macro	setup_c0_status_sec
-#ifdef CONFIG_64BIT
-	setup_c0_status ST0_KX ST0_BEV
-#else
-	setup_c0_status 0 ST0_BEV
-#endif
 	.endm
 
 #ifndef CONFIG_NO_EXCEPT_FILL
