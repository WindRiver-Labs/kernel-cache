From 6f9bd0656de3c2b3993e5e2c82deeba8f3e49161 Mon Sep 17 00:00:00 2001
From: Joe MacDonald <joe.macdonald@windriver.com>
Date: Tue, 17 Jul 2012 11:52:36 -0400
Subject: [PATCH 3/6] grsec: Avoid collision with lttng

Both lttng2 and grsecurity define a fs.h in include/trace, doing different
things.  Make the two live happily together.

Signed-off-by: Joe MacDonald <joe.macdonald@windriver.com>
---
 .../instrumentation/events/lttng-module/irq.h      |    4 ++--
 include/trace/events/fs.h                          |    6 +++---
 2 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/drivers/staging/lttng2/instrumentation/events/lttng-module/irq.h b/drivers/staging/lttng2/instrumentation/events/lttng-module/irq.h
index 344015d..30f5925 100644
--- a/drivers/staging/lttng2/instrumentation/events/lttng-module/irq.h
+++ b/drivers/staging/lttng2/instrumentation/events/lttng-module/irq.h
@@ -41,7 +41,7 @@ struct softirq_action;
  */
 TRACE_EVENT(irq_handler_entry,
 
-	TP_PROTO(int irq, struct irqaction *action),
+	TP_PROTO(int irq, const struct irqaction *action),
 
 	TP_ARGS(irq, action),
 
@@ -71,7 +71,7 @@ TRACE_EVENT(irq_handler_entry,
  */
 TRACE_EVENT(irq_handler_exit,
 
-	TP_PROTO(int irq, struct irqaction *action, int ret),
+	TP_PROTO(int irq, const struct irqaction *action, int ret),
 
 	TP_ARGS(irq, action, ret),
 
diff --git a/include/trace/events/fs.h b/include/trace/events/fs.h
index 2efe49d..124d7c6 100644
--- a/include/trace/events/fs.h
+++ b/include/trace/events/fs.h
@@ -1,8 +1,8 @@
 #undef TRACE_SYSTEM
 #define TRACE_SYSTEM fs
 
-#if !defined(_TRACE_FS_H) || defined(TRACE_HEADER_MULTI_READ)
-#define _TRACE_FS_H
+#if !defined(_TRACE_FS_EV_H) || defined(TRACE_HEADER_MULTI_READ)
+#define _TRACE_FS_EV_H
 
 #include <linux/fs.h>
 #include <linux/tracepoint.h>
@@ -47,7 +47,7 @@ TRACE_EVENT(open_exec,
 		  __get_str(filename))
 );
 
-#endif /* _TRACE_FS_H */
+#endif /* _TRACE_FS_EV_H */
 
 /* This part must be outside protection */
 #include <trace/define_trace.h>
-- 
