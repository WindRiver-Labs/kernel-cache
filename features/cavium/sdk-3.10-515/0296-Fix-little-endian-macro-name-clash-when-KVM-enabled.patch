From a41d2ef066be3c0d0c2a6531c2f2868fcc7ed2f1 Mon Sep 17 00:00:00 2001
From: Peter Swain <pswain@cavium.com>
Date: Tue, 15 Oct 2013 14:32:24 -0700
Subject: [PATCH 296/382] Fix little-endian macro name clash when KVM enabled

Based on SDK octeon3_3.10.

Commit fe15c398 spelled CP0_CVMCTL[_REG] wrong in little-endian case
Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 .../asm/mach-cavium-octeon/kernel-entry-init.h     |    4 ++--
 1 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/mips/include/asm/mach-cavium-octeon/kernel-entry-init.h b/arch/mips/include/asm/mach-cavium-octeon/kernel-entry-init.h
index 4f01f24..563540a 100644
--- a/arch/mips/include/asm/mach-cavium-octeon/kernel-entry-init.h
+++ b/arch/mips/include/asm/mach-cavium-octeon/kernel-entry-init.h
@@ -42,11 +42,11 @@ FEXPORT(octeon_hotplug_entry)
 	.set push
 	.set noreorder
 	/* Hotpplugged CPUs enter in Big-Endian mode, switch here to LE */
-	dmfc0   v0, CP0_CVMCTL
+	dmfc0   v0, CP0_CVMCTL_REG
 	nop
 	ori     v0, v0, 2
 	nop
-	dmtc0   v0, CP0_CVMCTL	/* little-endian */
+	dmtc0   v0, CP0_CVMCTL_REG	/* little-endian */
 	nop
 	synci	0($0)
 	.set pop
-- 
1.7.0.4

