From 78fd8e3dff41e641f40690974988fdf0c39f5ca9 Mon Sep 17 00:00:00 2001
From: Peter Swain <peter.swain@cavium.com>
Date: Sun, 10 Aug 2014 09:04:35 -0700
Subject: [PATCH 121/202] net: phy: qca833x does not need MDIO_BUS_MUX

Removed this obsolete requirement, left from previous codebase.

Signed-off-by: Peter Swain <peter.swain@cavium.com>
[Original patch taken from Cavium SDK 3.1.1-544]
Signed-off-by: Bin Jiang <bin.jiang@windriver.com>
---
 drivers/net/phy/Kconfig | 4 +---
 1 file changed, 1 insertion(+), 3 deletions(-)

diff --git a/drivers/net/phy/Kconfig b/drivers/net/phy/Kconfig
index 83b1216..f193e99 100644
--- a/drivers/net/phy/Kconfig
+++ b/drivers/net/phy/Kconfig
@@ -126,7 +126,7 @@ config MICREL_PHY
 
 config QCA833X_PHY
 	tristate "Drivers for Qualcomm/Atheros QCA833X PHYs"
-	depends on CAVIUM_OCTEON_SOC && MDIO_BUS_MUX
+	depends on CAVIUM_OCTEON_SOC
 	---help---
 	  Presents a Qualcomm/Atheros QCA8334/5/6/7 multi-port PHY as a
 	  single SGMII device. Does not enable advanced features, such as
@@ -134,8 +134,6 @@ config QCA833X_PHY
 	  or NAT.
 	  Link-state transitions require PSC cycling to undo hang,
 	  which is only implemented for CAVIUM_OCTEON_SOC.
-	  Non-standard MII_C45-like addressing is handled by creating virtual
-	  MDIO bus with MDIO_BUS_MUX.
 
 config FIXED_PHY
 	bool "Driver for MDIO Bus/PHY emulation with fixed speed/link PHYs"
-- 
1.8.2.1

