From 80effff3976e7a7178a299ab171b40abc273ffd7 Mon Sep 17 00:00:00 2001
From: David Daney <david.daney@cavium.com>
Date: Fri, 13 Feb 2015 13:57:22 +0530
Subject: [PATCH 035/132] usb/Kconfig: Add host driver for cn3XXX and cn5XXX
 SoCs

Commit c17b46b936ef2d7cfc7ad0c7e3900de58bd3435c from
git://git.yoctoproject.org/linux-yocto-3.14

Signed-off-by: David Daney <david.daney@cavium.com>
Signed-off-by: Abhishek Paliwal <abhishek.paliwal@aricent.com>
[Original patch taken from OCTEON-SDK 3.1.1-544.]
Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 drivers/usb/host/Kconfig | 8 ++++++++
 1 file changed, 8 insertions(+)

diff --git a/drivers/usb/host/Kconfig b/drivers/usb/host/Kconfig
index 55e7923..ec7fb2e 100644
--- a/drivers/usb/host/Kconfig
+++ b/drivers/usb/host/Kconfig
@@ -296,6 +296,14 @@ config USB_EHCI_HCD_PLATFORM
 
 	  If unsure, say N.
 
+config USB_OCTEON_HCD
+	tristate "Octeon usb host driver"
+	depends on CAVIUM_OCTEON_SOC
+	default n
+	 help
+	  The Octeon DWC_OTG USB host controller.  All CN3XXX and
+	  CN5XXX based chips with USB are supported.
+
 config USB_OCTEON_EHCI
 	bool "Octeon on-chip EHCI support (DEPRECATED)"
 	depends on CAVIUM_OCTEON_SOC
-- 
