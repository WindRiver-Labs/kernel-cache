+static inline void *cvmx_sso_work_request_grp_sync_nocheck(unsigned int lgrp,
+	cvmx_pow_wait_t wait)
+{
+	cvmx_pow_load_addr_t ptr;
+	cvmx_pow_tag_load_resp_t result;
+	unsigned int node = cvmx_get_node_num() & 3;
