+		reg_addr = CVMX_SSO_PPX_SX_GRPMSKX(core, 1, grp),
+		grp_msk.u64 = 0;
+		if (mask_set & 2)
+			grp_msk.s.grp_msk = xgrp_mask[grp];
+		cvmx_write_csr_node(node, reg_addr, grp_msk.u64);
+	}
+}
