From ef1407bcae6873006a9add6e034413e0234377de Mon Sep 17 00:00:00 2001
From: Chandrakala Chavva <cchavva@caviumnetworks.com>
Date: Fri, 14 May 2010 14:59:15 -0700
Subject: [PATCH 138/337] MIPS: Make Cavium as the default configuration.

Based On SDK 3.0.0-482

Signed-off-by: Chandrakala Chavva <cchavva@caviumnetworks.com>
Signed-off-by: Corey Minyard <cminyard@mvista.com>
Signed-off-by: Jack Tan <jack.tan@windriver.com>
---
 arch/mips/Kconfig  |    2 +-
 arch/mips/Makefile |    2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/mips/Kconfig b/arch/mips/Kconfig
index 31a3de2..66b0c47 100644
--- a/arch/mips/Kconfig
+++ b/arch/mips/Kconfig
@@ -38,7 +38,7 @@ config ZONE_DMA
 
 choice
 	prompt "System type"
-	default SGI_IP22
+	default CAVIUM_OCTEON_REFERENCE_BOARD
 
 config MIPS_ALCHEMY
 	bool "Alchemy processor based machines"
diff --git a/arch/mips/Makefile b/arch/mips/Makefile
index d9b6ba9..34822aa 100644
--- a/arch/mips/Makefile
+++ b/arch/mips/Makefile
@@ -12,7 +12,7 @@
 # for "archclean" cleaning up for this architecture.
 #
 
-KBUILD_DEFCONFIG := ip22_defconfig
+KBUILD_DEFCONFIG := cavium-octeon_defconfig
 
 #
 # Select the object file format to substitute into the linker script.
-- 
1.7.5.4

