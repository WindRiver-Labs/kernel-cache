From b36f3a0d758f54e2c237c4785b1699b4c39f2f75 Mon Sep 17 00:00:00 2001
From: Chandrakala Chavva <cchavva@caviumnetworks.com>
Date: Fri, 3 Jun 2011 11:01:58 -0700
Subject: [PATCH 222/337] MIPS: Octeon: Define NR_CPUS_DEFAULT_32 for Cavium
 Octeon.

Based On SDK 3.0.0-482

Signed-off-by: Chandrakala Chavva <cchavva@caviumnetworks.com>
Signed-off-by: Corey Minyard <cminyard@mvista.com>
Signed-off-by: Jack Tan <jack.tan@windriver.com>
---
 arch/mips/Kconfig |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/arch/mips/Kconfig b/arch/mips/Kconfig
index 7d06635..9ed3ae6 100644
--- a/arch/mips/Kconfig
+++ b/arch/mips/Kconfig
@@ -1448,7 +1448,7 @@ config CPU_CAVIUM_OCTEON
 	select CPU_HAS_PREFETCH
 	select CPU_SUPPORTS_64BIT_KERNEL
 	select SYS_SUPPORTS_SMP
-	select NR_CPUS_DEFAULT_16
+	select NR_CPUS_DEFAULT_32
 	select WEAK_ORDERING
 	select CPU_SUPPORTS_HIGHMEM
 	select CPU_SUPPORTS_HUGEPAGES
