+++ b/arch/arm/boot/dts/imx7d-12x12-lpddr3-arm2.dts
@@ -43,7 +43,7 @@
 
 	pxp_v4l2_out {
 		compatible = "fsl,imx7d-pxp-v4l2", "fsl,imx6sx-pxp-v4l2", "fsl,imx6sl-pxp-v4l2";
-		status = "disabled";
