Subject: [PATCH 0400/1532] Added section for net resource allocation

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp9xx-linux.dts | 4 ++++
 1 file changed, 4 insertions(+)

diff --git a/arch/mips/boot/dts/xlp9xx-linux.dts b/arch/mips/boot/dts/xlp9xx-linux.dts
index 655c7ab..76ebe6a 100644
--- a/arch/mips/boot/dts/xlp9xx-linux.dts
+++ b/arch/mips/boot/dts/xlp9xx-linux.dts
@@ -154,6 +154,8 @@
                                         vfbid-sw-entries = <0 0 0 0>;
                                         ucore-mask = <0x0 0 0 0>;
                                         sram-off = <0 0 0 0>;
+					max-l3rules = <0 0 0 0>;
+					max-l4rules = <0 0 0 0>;
                                 };
                                 net@1 { 
                                         port-mask = <0x3 0 0 0>;
@@ -163,6 +165,8 @@
                                         vfbid-sw-entries = <0 0 0 0>;
                                         ucore-mask = <0xc 0 0 0>;
                                         sram-off = <0 0 0 0>;
+					max-l3rules = <1 0 0 0>;
+					max-l4rules = <2 0 0 0>;
                                 };
                         };
 			app-param{
