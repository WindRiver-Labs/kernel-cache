+++ b/arch/mips/netlogic/lib/netlib/include/xlp2xx_netsoc.h
@@ -34,10 +34,10 @@
 
 #define XLP2XX_MAX_PORTS                8
 #define XLP2XX_MAX_FREEIN_LIFOS         8
