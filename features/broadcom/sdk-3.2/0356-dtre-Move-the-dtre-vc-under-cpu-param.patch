From 69fe92830b4e7f7a577147ca87b9a2583ba4c764 Mon Sep 17 00:00:00 2001
From: Sandip Matte <sandipm@broadcom.com>
Date: Wed, 20 Mar 2013 17:22:35 +0530
Subject: [PATCH 0356/1532] dtre: Move the dtre vc under cpu param.

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp8xx-helinux.dts | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/mips/boot/dts/xlp8xx-helinux.dts b/arch/mips/boot/dts/xlp8xx-helinux.dts
index edf6254..08de7bb 100644
--- a/arch/mips/boot/dts/xlp8xx-helinux.dts
+++ b/arch/mips/boot/dts/xlp8xx-helinux.dts
@@ -88,6 +88,7 @@
 			cpu {
 				onlinemask = <0xffff>;
                                 nae-rx-vc = <0>;
+				dtre-rx-vc = <2>;
                                 nae-fb-vc = <3>;
 				sae-rx-vc = <0>; /* sharing with nae */
 				sae-rx-sync-vc = <0>;  /* sharing with nae */
@@ -108,7 +109,6 @@
 				enable-nae-hw-parser = <0>;
 				enable-nae-jumbo = <0>;
 				enable-nae-p2p = <0>;
-				dtre_rx_vc = <2>;
 			};
 			nae{
 			};
