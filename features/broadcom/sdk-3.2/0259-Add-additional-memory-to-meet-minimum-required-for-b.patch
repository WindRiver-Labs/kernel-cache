From 17f64573c907a2f641acf40350bfa454bc76175b Mon Sep 17 00:00:00 2001
From: Lakshan Dias <ldias@netlogicmicro.com>
Date: Mon, 3 Oct 2011 20:09:50 -0700
Subject: [PATCH 0259/1532] Add additional memory to meet minimum required for
 boot.

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/multi-os.dts | 5 +++--
 1 file changed, 3 insertions(+), 2 deletions(-)

diff --git a/arch/mips/boot/dts/multi-os.dts b/arch/mips/boot/dts/multi-os.dts
index 8749f51..35cd340 100644
--- a/arch/mips/boot/dts/multi-os.dts
+++ b/arch/mips/boot/dts/multi-os.dts
@@ -38,7 +38,8 @@
 			};
 			memory {
 				reg = <0x01000000 0x0F000000	// 240M at 16M
-				       0x20000000 0x10000000>;  // 256M at 512M
+				       0x20000000 0x10000000    // 256M at 512M
+					   0x80000000 0x10000000>;  // 256M at 2G
 			};
 			pic {
 			};
@@ -117,7 +118,7 @@
 				sharedcfg = <0x1f000000>;
 			};
 			memory {
-				reg = <0x60000000 0x30000000>;  // 1536M -> 2304M (768M)
+				reg = <0x60000000 0x20000000>;  // 1536M -> 2048M (512M)
 			};
 			nae {
 				owner = <0>;
-- 
1.9.1

