diff --git a/arch/mips/boot/dts/xlp9xx-guest.dts b/arch/mips/boot/dts/xlp9xx-guest.dts
index 3c51fe4..10ac35b 100644
--- a/arch/mips/boot/dts/xlp9xx-guest.dts
+++ b/arch/mips/boot/dts/xlp9xx-guest.dts
@@ -12,7 +12,7 @@
 
 	memory {
 		device_type = "memory";
