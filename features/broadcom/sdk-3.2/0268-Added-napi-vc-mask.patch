From 34b397bfd2fefe00c1c949ab78621bed0a53cf26 Mon Sep 17 00:00:00 2001
From: Mehul <vmehul@netlogicmicro.com>
Date: Thu, 17 Nov 2011 16:58:42 +0530
Subject: [PATCH 0268/1532] Added napi-vc-mask

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/linux.dts       | 1 +
 arch/mips/boot/dts/storm-linux.dts | 1 +
 2 files changed, 2 insertions(+)

diff --git a/arch/mips/boot/dts/linux.dts b/arch/mips/boot/dts/linux.dts
index 457a158..1070835 100644
--- a/arch/mips/boot/dts/linux.dts
+++ b/arch/mips/boot/dts/linux.dts
@@ -32,6 +32,7 @@
                                 nae-fb-vc = <1>;
 				sae-rx-vc = <0>; /* sharing with nae */
 				sae-rx-sync-vc  = <3>; /* should be exclusive */
+				napi-vc-mask = <0x3>;
 			};
 			uart {
 				id = <0>;
diff --git a/arch/mips/boot/dts/storm-linux.dts b/arch/mips/boot/dts/storm-linux.dts
index 2212ba2..8516568 100644
--- a/arch/mips/boot/dts/storm-linux.dts
+++ b/arch/mips/boot/dts/storm-linux.dts
@@ -30,6 +30,7 @@
 				onlinemask = <0xffff>;
                                 nae-rx-vc = <0>;
                                 nae-fb-vc = <1>;
+				napi-vc-mask = <0x3>;
 			};
 			uart {
 				id = <0>;
-- 
1.9.1

