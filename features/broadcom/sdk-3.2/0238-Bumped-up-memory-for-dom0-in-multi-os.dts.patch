From e4815be085a6c2b4f15ef4b0a7053e89dc355efd Mon Sep 17 00:00:00 2001
From: Siva Pochiraju <sivap@netlogicmicro.com>
Date: Tue, 5 Apr 2011 14:37:46 -0700
Subject: [PATCH 0238/1532] Bumped up memory for dom0 in multi-os.dts

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/multi-os.dts | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/mips/boot/dts/multi-os.dts b/arch/mips/boot/dts/multi-os.dts
index 93c95c4..a4dc9c7 100644
--- a/arch/mips/boot/dts/multi-os.dts
+++ b/arch/mips/boot/dts/multi-os.dts
@@ -37,8 +37,8 @@
 				sharedcfg = <0x1f000000>;
 			};
 			memory {
-				reg = <0x04000000 0x0C000000	// 192M at 64M
-					0x20000000 0x10000000>;  // 256M at 512M
+				reg = <0x00100000 0x0F000000	// 240M at 16M
+				       0x20000000 0x10000000>;  // 256M at 512M
 			};
 			pic {
 			};
