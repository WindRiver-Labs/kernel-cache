From 111f9e794e6c14a31a33c37294775ce1a3126f02 Mon Sep 17 00:00:00 2001
From: "P. Sadik" <psadik@broadcom.com>
Date: Tue, 8 Apr 2014 16:28:38 +0530
Subject: [PATCH 0509/1532] mn9xx: Add frequency section.

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/mnxlp9xx-linux.dts | 23 +++++++++++++++--------
 1 file changed, 15 insertions(+), 8 deletions(-)

diff --git a/arch/mips/boot/dts/mnxlp9xx-linux.dts b/arch/mips/boot/dts/mnxlp9xx-linux.dts
index 3b062f0..23326bc 100644
--- a/arch/mips/boot/dts/mnxlp9xx-linux.dts
+++ b/arch/mips/boot/dts/mnxlp9xx-linux.dts
@@ -148,7 +148,7 @@
                                         ucore-mask = <0x1 0 0 0>;
                                         sram-off = <0 0 0 0>;
                                 };
-				net@1 { 
+				net@1 {
                                         port-mask = <0x4 0 0 0>;
                                         frinlifo-mask = <0xffff 0 0 0>;
                                         frinlifo-shared-mode = <0 0 0 0>;
@@ -162,8 +162,8 @@
 		dom@1 {
 			device_type = "domain";
 
-			cpu {   
-                                onlinemask = <0xffff>; 
+			cpu {
+                                onlinemask = <0xffff>;
                                 nae-rx-vc = <2>;
 			};
 
@@ -191,16 +191,16 @@
                                 onlinemask = <0xffff>;
 			};
 		};
-                dom@2 { 
+                dom@2 {
                         device_type = "domain";
 
-                        cpu {   
+                        cpu {
                                 onlinemask = <0xffff0000 0x0 0x0>;
                                 nae-rx-vc = <2>;
                         };
 
                         resource-allocation {
-                                net@0 { 
+                                net@0 {
                                         port-mask = <0x0 0 0 0>;
                                         frinlifo-mask = <0x00 0 0 0>;
                                         frinlifo-shared-mode = <0 0 0 0>;
@@ -211,7 +211,7 @@
 					max-l3rules = <0 0 0 0>;
 					max-l4rules = <0 0 0 0>;
                                 };
-                                net@1 { 
+                                net@1 {
                                         port-mask = <0x3 0 0 0>;
                                         frinlifo-mask = <0xff 0 0 0>;
                                         frinlifo-shared-mode = <0xff 0 0 0>;
@@ -240,7 +240,7 @@
 		 * 15 core(core 5 disabled): netlogic,cpumask=<0xffffffff 0xff0fffff>;
 		 */
 		netlogic,cpumask = <0x0 0x0 0xffffffff 0xffffffff
-		                    0x0 0x0 0xffffffff 0xffffffff>;					
+		                    0x0 0x0 0xffffffff 0xffffffff>;
 		bootargs = " console=ttyS0,115200  rhash_entries=1048576";
 		/*linux,initrd-start=<0x00c00000>;
 		linux,initrd-end=<0x02c00000>;*/
@@ -268,5 +268,12 @@
 		};
 
 	};
+        frequency-config {
+                nae = <500>;
+                sae = <767>;
+                rsa = <767>;
+                dtre = <250>;
+                cde = <250>;
+        };
 };
 
-- 
