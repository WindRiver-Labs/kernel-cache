From 0ce31a7c58c84e99a46f2e54bd3fd73d0cb0b579 Mon Sep 17 00:00:00 2001
From: Pavani Reddy <pavani@broadcom.com>
Date: Mon, 18 Aug 2014 12:47:48 +0530
Subject: [PATCH 0573/1532] Hybrid_nae_app : Increase number of descriptors and
 remove naeperf/soc-net-hybrid-nae.dts

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp9xx-hybrid-nae-helinux.dts | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/mips/boot/dts/xlp9xx-hybrid-nae-helinux.dts b/arch/mips/boot/dts/xlp9xx-hybrid-nae-helinux.dts
index f5b65a5..e8db107 100644
--- a/arch/mips/boot/dts/xlp9xx-hybrid-nae-helinux.dts
+++ b/arch/mips/boot/dts/xlp9xx-hybrid-nae-helinux.dts
@@ -158,7 +158,7 @@
 				/* < node paddr-H32bits paddr-L32bits size-H32bits size-L32bits */
 				freein-fifo-replenish-addr-info = <0x0 0x0 0x88000000 0x0 0x10000000>; /* 256M @ 2176M*/
 				/* < node fifomask descsize headroom ndescs > */
-				freein-fifo-replenish-desc-info = <0 0x00ff 2048 0 150>;
+				freein-fifo-replenish-desc-info = <0 0x00ff 2048 0 750>;
 			};
 			nae-1 {
 				freein-fifo-mask = <0xfeff 0 0 0>;
@@ -173,7 +173,7 @@
 				/* < node paddr-H32bits paddr-L32bits size-H32bits size-L32bits */
 				freein-fifo-replenish-addr-info = <0x0 0x0 0x98000000 0x0 0x10000000>; /* 256M @ 2432M*/
 				/* < node fifomask descsize headroom ndescs > */
-				freein-fifo-replenish-desc-info = <0 0x00ff 2048 0 150>;
+				freein-fifo-replenish-desc-info = <0 0x00ff 2048 0 750>;
 			};
 			resource-allocation {
 				net@0 {
-- 
