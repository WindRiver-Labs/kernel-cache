From fcf67888d7234a0051c8c2a7205d8739aecac85f Mon Sep 17 00:00:00 2001
From: Sandip Matte <sandipm@broadcom.com>
Date: Fri, 8 Mar 2013 20:29:34 +0530
Subject: [PATCH 0355/1532] dtre: Change dtre freq to 500MHz. Use vc 2 for
 dtre.

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp8xx-helinux.dts | 4 +++-
 1 file changed, 3 insertions(+), 1 deletion(-)

diff --git a/arch/mips/boot/dts/xlp8xx-helinux.dts b/arch/mips/boot/dts/xlp8xx-helinux.dts
index 4ae0bf8..edf6254 100644
--- a/arch/mips/boot/dts/xlp8xx-helinux.dts
+++ b/arch/mips/boot/dts/xlp8xx-helinux.dts
@@ -108,6 +108,7 @@
 				enable-nae-hw-parser = <0>;
 				enable-nae-jumbo = <0>;
 				enable-nae-p2p = <0>;
+				dtre_rx_vc = <2>;
 			};
 			nae{
 			};
@@ -116,6 +117,7 @@
 				sae = <0x1>;
 				rsa = <0x1>;
 				fmn = <0x1>;
+				dtr = <0x1>;
 			};
 
                         fmn {
@@ -254,7 +256,7 @@
 		nae = <500>;
 		sae = <500>;
 		rsa = <500>;
-		dtre = <250>;
+		dtre = <500>;
 		cde = <250>;
 	};
 };
