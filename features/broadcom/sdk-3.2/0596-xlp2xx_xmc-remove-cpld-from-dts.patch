From 7510a560b2f0b6190194b20c544739832584f9e4 Mon Sep 17 00:00:00 2001
From: Subhendu Sekhar Behera <sbehera@broadcom.com>
Date: Wed, 3 Dec 2014 11:29:43 +0530
Subject: [PATCH 0596/1532] xlp2xx_xmc: remove cpld from dts

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp2xx-xmc-linux-kernel.dts | 9 ---------
 1 file changed, 9 deletions(-)

diff --git a/arch/mips/boot/dts/xlp2xx-xmc-linux-kernel.dts b/arch/mips/boot/dts/xlp2xx-xmc-linux-kernel.dts
index cef6ac6..4f4d483 100644
--- a/arch/mips/boot/dts/xlp2xx-xmc-linux-kernel.dts
+++ b/arch/mips/boot/dts/xlp2xx-xmc-linux-kernel.dts
@@ -75,15 +75,6 @@
 			reg = <0 0x4000 0x200>;
 		};
 
-		cpld@0 {
-			compatible = "netlogic,xlp-cpld";
-			reg = <1 0x1000000 0x100000>;
-			reg-io-width = <2>;
-			version-reg = <0>;
-			reset-reg = <1>;
-			little-endian;
-		};
-
 		watchdog@0 {
 			compatible = "netlogic,nlm-wdt";
 		};
-- 
