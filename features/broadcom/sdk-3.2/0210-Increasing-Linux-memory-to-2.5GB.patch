From 0df4259cb5cb798394beadb504b3d01c851d68b1 Mon Sep 17 00:00:00 2001
From: Venu Vadapalli <vvadapalli@netlogicmicro.com>
Date: Wed, 27 Oct 2010 20:06:02 -0700
Subject: [PATCH 0210/1532] Increasing Linux memory to ~2.5GB

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/linux.dts | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/mips/boot/dts/linux.dts b/arch/mips/boot/dts/linux.dts
index 407d625..7d4b7b3 100644
--- a/arch/mips/boot/dts/linux.dts
+++ b/arch/mips/boot/dts/linux.dts
@@ -39,7 +39,7 @@
 			};
 			memory {
 				reg = <0x01000000 0x0B000000	// 176M at 16M
-					0x20000000 0x40000000>;  // 1024M at 512M
+					0x20000000 0xa0000000>;  // 2586M at 512M
 			};
 			pic {
 			};
