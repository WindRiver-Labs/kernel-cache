From 9727118480e173e8c300ebdfb305c1f823bb1ad2 Mon Sep 17 00:00:00 2001
From: Hareesh R <hareeshr@broadcom.com>
Date: Fri, 6 Sep 2013 09:59:11 +0530
Subject: [PATCH 0407/1532] nae : netsoc nae shared mode changes

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp9xx-helinux.dts | 2 +-
 arch/mips/boot/dts/xlp9xx-linux.dts   | 4 ++--
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/mips/boot/dts/xlp9xx-helinux.dts b/arch/mips/boot/dts/xlp9xx-helinux.dts
index 5f0e3de..c47cd5a 100644
--- a/arch/mips/boot/dts/xlp9xx-helinux.dts
+++ b/arch/mips/boot/dts/xlp9xx-helinux.dts
@@ -42,7 +42,7 @@
 			#size-cells = <1>;
 
 			cpu {
-				onlinemask = <0x1 0x0 0x0>;
+				onlinemask = <0x1>;
                                 nae-rx-vc = <0>;
                                 nae-fb-vc = <1>;
 				sae-rx-vc = <0>; /* sharing with nae */
diff --git a/arch/mips/boot/dts/xlp9xx-linux.dts b/arch/mips/boot/dts/xlp9xx-linux.dts
index f4bc90d..416d235 100644
--- a/arch/mips/boot/dts/xlp9xx-linux.dts
+++ b/arch/mips/boot/dts/xlp9xx-linux.dts
@@ -45,7 +45,7 @@
 			#cpumask-cells=<3>;
 
 			cpu {
-				onlinemask = <0x1 0x0 0x0>; /* <0xffffffff 0xffffffff 0xffff> */
+				onlinemask = <0x1>; /* */
                                 nae-rx-vc = <0>;
                                 nae-fb-vc = <1>;
 				napi-vc-mask = <0xb>;
@@ -123,7 +123,7 @@
 			device_type = "domain";
 
 			cpu {   
-                                onlinemask = <0xffff 0x0 0x0>; 
+                                onlinemask = <0xffff>; 
                                 nae-rx-vc = <2>;
 			};
 
-- 
1.9.1

