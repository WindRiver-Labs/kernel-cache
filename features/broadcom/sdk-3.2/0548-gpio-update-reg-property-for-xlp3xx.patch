From f376e1ce9a0bb9359e8db91e09d1d949c82d6f23 Mon Sep 17 00:00:00 2001
From: Kamlakant Patel <kamlakant.patel@broadcom.com>
Date: Thu, 5 Jun 2014 15:23:08 +0530
Subject: [PATCH 0548/1532] gpio: update reg property for xlp3xx

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp3xx-linux-tmpl.dts | 3 ++-
 1 file changed, 2 insertions(+), 1 deletion(-)

diff --git a/arch/mips/boot/dts/xlp3xx-linux-tmpl.dts b/arch/mips/boot/dts/xlp3xx-linux-tmpl.dts
index 7ff26f3..b3509b4 100644
--- a/arch/mips/boot/dts/xlp3xx-linux-tmpl.dts
+++ b/arch/mips/boot/dts/xlp3xx-linux-tmpl.dts
@@ -225,7 +225,8 @@
 
 		gpio: xlp_gpio@34000 {
 			compatible = "netlogic,xlp-gpio";
-			reg = <0 0x34100 0x1000>;
+			reg = <0 0x34100 0x1000
+			       0 0x35100 0x1000>;
 			#gpio-cells = <2>;
 			gpio-controller;
 
