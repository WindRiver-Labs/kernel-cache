From 635112ceece61195c1f92fefd3530322df553db1 Mon Sep 17 00:00:00 2001
From: Saswat Kumar Dash <saswat.dash@broadcom.com>
Date: Mon, 26 May 2014 06:41:26 -0700
Subject: [PATCH 0533/1532] xlp5xx_guest: modified app onlinemask

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp5xx-linux.dts | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/mips/boot/dts/xlp5xx-linux.dts b/arch/mips/boot/dts/xlp5xx-linux.dts
index c24b715..d750759 100644
--- a/arch/mips/boot/dts/xlp5xx-linux.dts
+++ b/arch/mips/boot/dts/xlp5xx-linux.dts
@@ -45,7 +45,7 @@
 			#cpumask-cells=<3>;
 
 			cpu {
-				onlinemask = <0xffffffff>; /* */
+				onlinemask = <0xffff>; /* */
                                 nae-rx-vc = <0>;
                                 nae-fb-vc = <1>;
 				napi-vc-mask = <0xb>;
@@ -104,7 +104,7 @@
 			device_type = "domain";
 
 			cpu {
-                                onlinemask = <0xffffffff>;
+                                onlinemask = <0xf>;
                                 nae-rx-vc = <2>;
 			};
 
@@ -120,7 +120,7 @@
 				};
 			};
 			app-param{
-                                onlinemask = <0xffffffff>;
+                                onlinemask = <0xffff>;
 			};
 		};
                 dom@2 {
