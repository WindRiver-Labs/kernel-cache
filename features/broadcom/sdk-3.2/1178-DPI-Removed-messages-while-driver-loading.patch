From 03f6fc6f1d7be31d4768eae8ee881419f561566b Mon Sep 17 00:00:00 2001
From: Lakshman Garlapati <kumarga@broadcom.com>
Date: Fri, 30 Aug 2013 12:42:22 +0530
Subject: [PATCH 1178/1532] DPI: Removed messages while driver loading.

Modified demo file.
[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 drivers/misc/netlogic/netl7driver/netl7driver.c | 14 +++++++-------
 1 file changed, 7 insertions(+), 7 deletions(-)

diff --git a/drivers/misc/netlogic/netl7driver/netl7driver.c b/drivers/misc/netlogic/netl7driver/netl7driver.c
index 47727c0..dc07ff3 100755
--- a/drivers/misc/netlogic/netl7driver/netl7driver.c
+++ b/drivers/misc/netlogic/netl7driver/netl7driver.c
@@ -281,7 +281,7 @@ nlm_drv_module_init (void)
 {
   int status;
 
-  NLM_INFO (": driver load process started.\n");
+  NLM_VERB (": driver load process started.\n");
   status = pci_register_driver (&pci_driver_funcs);
   if (status < 0)
     {
@@ -314,15 +314,15 @@ nlm_drv_module_init (void)
     int i;
     for (i = 0; i < n_devices; i++)
       {
-        NLM_INFO (": === Probed device no %d (%p) (%s) === \n",
+        NLM_VERB (": === Probed device no %d (%p) (%s) === \n",
                   i, &device_list[i], nlm_device_type_name[device_list[i].type]);
-        NLM_INFO (":     proc_entry /proc/NetL7/%s\n",
+        NLM_VERB (":     proc_entry /proc/NetL7/%s\n",
                   device_list[i].proc_entry->name);
-        NLM_INFO (":     register_map base = 0x%llx size = 0x%x\n",
+        NLM_VERB (":     register_map base = 0x%llx size = 0x%x\n",
                   device_list[i].regmap_base, device_list[i].regmap_size);
-        NLM_INFO (":     system memory base = 0x%llx size = 0x%x\n",
+        NLM_VERB (":     system memory base = 0x%llx size = 0x%x\n",
                   device_list[i].sysmem_base, device_list[i].sysmem_size);
-        NLM_INFO (":     packet memory base = 0x%llx size = 0x%x\n",
+        NLM_VERB (":     packet memory base = 0x%llx size = 0x%x\n",
                   device_list[i].packetmem_base, device_list[i].packetmem_size);
       }
   }
@@ -1203,7 +1203,7 @@ dynamic_memory_init (void)
     }
   else if ((chunklist->npages * PAGE_SIZE) > size)
     {
-      NLM_INFO (": Found bigger chunk than requested of size %ld at 0x%lx, %ld\n", 
+      NLM_VERB (": Found bigger chunk than requested of size %ld at 0x%lx, %ld\n", 
                 size, (ulong) virt_to_phys (chunklist), chunklist->npages * PAGE_SIZE);
       chunk = (struct mem_desc *) (((ulong) chunklist) + size);
       chunk->next = chunklist->next;
