From 0729876336e954e8d94685bdbf2524888b163f9b Mon Sep 17 00:00:00 2001
From: Saswat Kumar Dash <saswat.dash@broadcom.com>
Date: Fri, 1 Aug 2014 04:08:55 -0700
Subject: [PATCH 0572/1532] hybrid_nat: fixed call trace while guest OS bring
 up

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp5xx-hybrid-nat-helinux-guest.dts | 4 ++--
 arch/mips/boot/dts/xlp9xx-hybrid-nat-helinux-guest.dts | 6 +++---
 2 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/arch/mips/boot/dts/xlp5xx-hybrid-nat-helinux-guest.dts b/arch/mips/boot/dts/xlp5xx-hybrid-nat-helinux-guest.dts
index 334956d..a6750fc 100644
--- a/arch/mips/boot/dts/xlp5xx-hybrid-nat-helinux-guest.dts
+++ b/arch/mips/boot/dts/xlp5xx-hybrid-nat-helinux-guest.dts
@@ -11,7 +11,7 @@
 
 	memory {
 		device_type = "memory";
-		reg =  <0x0 0x1000000  0x0 0xb000000     /* 16M - 16+176M */
+		reg =  <0x0 0x0100000  0x0 0xb000000     /*  1M - 1+176M */
 			0x1 0x0        0x0 0xc0000000>;  /* 4096M - 4096M+3072M */
 
 	};
@@ -21,7 +21,7 @@
 		device_type = "pktmem";
 
 		/* format: node number, 64-bit addr, 64-bit size */
-		reg = <0 0x0 0xc0000000 0x0 0x10000000
+		reg = <0 0x0 0xc0000000 0x0 0x3000000
 			1 0x0 0x0 0x0 0x0
 			2 0x0 0x0 0x0 0x0
 			3 0x0 0x0 0x0 0x0>;
diff --git a/arch/mips/boot/dts/xlp9xx-hybrid-nat-helinux-guest.dts b/arch/mips/boot/dts/xlp9xx-hybrid-nat-helinux-guest.dts
index f04216b..5689977 100644
--- a/arch/mips/boot/dts/xlp9xx-hybrid-nat-helinux-guest.dts
+++ b/arch/mips/boot/dts/xlp9xx-hybrid-nat-helinux-guest.dts
@@ -11,8 +11,8 @@
 
 	memory {
 		device_type = "memory";
-		reg =  <0x0 0x1000000  0x0 0xb000000     /* 16M - 16+176M */
-			0x1 0x0        0x0 0xc0000000>;  /*   4096M - 4096M + 3072M */
+		reg =  <0x0 0x0100000  0x0 0xb000000      /* 1M - 1+177M */
+			0x1 0x0        0x0 0xc0000000>;  /* 4096M - 4096M+3072M */
 
 	};
 	/* memory range for HEv2 pkt mem. Only one contig memory per node supported */
@@ -21,7 +21,7 @@
 		device_type = "pktmem";
 
 		/* format: node number, 64-bit addr, 64-bit size */
-		reg = <0 0x0 0xc0000000 0x0 0x20000000
+		reg = <0 0x0 0xc0000000 0x0 0x3000000
 			1 0x0 0x0 0x0 0x0
 			2 0x0 0x0 0x0 0x0
 			3 0x0 0x0 0x0 0x0>;
-- 
1.9.1

