Subject: [PATCH 0529/1532] 5xx: Add timer support

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp5xx-helinux.dts | 1 +
 1 file changed, 1 insertion(+)

diff --git a/arch/mips/boot/dts/xlp5xx-helinux.dts b/arch/mips/boot/dts/xlp5xx-helinux.dts
index 00535dd..0119d2e 100644
--- a/arch/mips/boot/dts/xlp5xx-helinux.dts
+++ b/arch/mips/boot/dts/xlp5xx-helinux.dts
@@ -124,6 +124,7 @@
 				enable-popq-mode = <0>; /* If set to 1, popq mode is enabled else disabled */
 				enable-popq-priority = <0>; /* If set to 1, popq priority mode is enabled else disabled */
 				launch-mode = "smp";	/* smp or amp */
+				timer-service-vc = <2>;
 			};
 			nae{
 				freein-fifo-mask = <0xffff 0 0 0>;
