From 857e8553fb5f0e05159989475240197279379b63 Mon Sep 17 00:00:00 2001
From: Subhendu Sekhar Behera <sbehera@broadcom.com>
Date: Thu, 11 Dec 2014 18:28:44 +0530
Subject: [PATCH 0614/1532] mnxlp9xx: add support for node-1 devices & disable
 cpufreq scaling.

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp9xx-linux-mn-kernel.dts | 90 ++++++++++++++++++++++++---
 1 file changed, 82 insertions(+), 8 deletions(-)

diff --git a/arch/mips/boot/dts/xlp9xx-linux-mn-kernel.dts b/arch/mips/boot/dts/xlp9xx-linux-mn-kernel.dts
index 6db78b3..c1263e3 100644
--- a/arch/mips/boot/dts/xlp9xx-linux-mn-kernel.dts
+++ b/arch/mips/boot/dts/xlp9xx-linux-mn-kernel.dts
@@ -23,10 +23,11 @@
 			reg-shift = <2>;
 			reg-io-width = <4>;
 			clock-frequency = <125000000>;
-			interrupt-parent = <&pic>;
+			interrupt-parent = <&pic0>;
 			interrupts = <17>;
 		};
-		pic: pic@11000 {
+		pic0: pic0@11000 {
+			compatible = "netlogic,xlp-pic";
 			interrupt-controller;
 			#address-cells = <0>;
 			#interrupt-cells = <1>;
@@ -51,19 +52,23 @@
 			reg = <0 0x12a000 0x80>;
 		};
 
-		i2c0: i2c@113100 {
+		i2c00: i2c@113100 {
 			compatible = "netlogic,xlp9xx-i2c";
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0 0x113100 0x100>;
 			clock-frequency = <133000000>;
+			interrupts = <30>;
+			interrupt-parent = <&pic0>;
 		};
-		i2c1: i2c@113300 {
+		i2c01: i2c@113300 {
 			compatible = "netlogic,xlp9xx-i2c";
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0 0x113300 0x100>;
 			clock-frequency = <133000000>;
+			interrupts = <31>;
+			interrupt-parent = <&pic0>;
 
 			rtc@68 {
 				compatible = "dallas,ds1374";
@@ -80,20 +85,29 @@
                                 reg = <0x57>;
                         };
 
+			dtt@2c {
+				compatible = "max6653";
+				reg = <0x2c>;
+			};
+
 		};
-		i2c2: i2c@113500 {
+		i2c02: i2c@113500 {
 			compatible = "netlogic,xlp9xx-i2c";
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0 0x113500 0x100>;
 			clock-frequency = <133000000>;
+			interrupts = <32>;
+			interrupt-parent = <&pic0>;
 		};
-		i2c3: i2c@113700 {
+		i2c03: i2c@113700 {
 			compatible = "netlogic,xlp9xx-i2c";
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0 0x113700 0x100>;
 			clock-frequency = <133000000>;
+			interrupts = <33>;
+			interrupt-parent = <&pic0>;
 		};
 
 		spi: xlp_spi@13a000 {
@@ -103,7 +117,7 @@
 			reg = <0 0x13a000 0x100>;
 
 			interrupts = <34>;
-			interrupt-parent = <&pic>;
+			interrupt-parent = <&pic0>;
 
 			spi_nor@1 {
 				compatible = "spansion,s25sl12801";
@@ -226,11 +240,71 @@
 			gpio-controller;
 
 			#interrupt-cells = <1>;
-			interrupt-parent = <&pic>;
+			interrupt-parent = <&pic0>;
 			interrupts = <39>;
 			interrupt-controller;
 		};
 
+		pic1: pic1@610000 {
+			compatible = "netlogic,xlp-pic";
+			interrupt-controller;
+			#address-cells = <0>;
+			#interrupt-cells = <1>;
+			reg = <0 0x610000 0x200>;
+		};
+
+		i2c10: i2c@613100 {
+			compatible = "netlogic,xlp9xx-i2c";
+			#address-cells = <1>;
+			#size-cells = <0>;
+			reg = <0 0x613100 0x100>;
+			clock-frequency = <133000000>;
+			interrupts = <30>;
+			interrupt-parent = <&pic1>;
+		};
+		i2c11: i2c@613300 {
+			compatible = "netlogic,xlp9xx-i2c";
+			#address-cells = <1>;
+			#size-cells = <0>;
+			reg = <0 0x613300 0x100>;
+			clock-frequency = <133000000>;
+			interrupts = <31>;
+			interrupt-parent = <&pic1>;
+
+			rtc@68 {
+				compatible = "dallas,ds1374";
+				reg = <0x68>;
+			};
+
+			eeprom@57 {
+                                compatible = "xlpge_eeprom";
+                                reg = <0x57>;
+                        };
+			dtt@2c {
+				compatible = "max6653";
+				reg = <0x2c>;
+			};
+
+		};
+		i2c12: i2c@613500 {
+			compatible = "netlogic,xlp9xx-i2c";
+			#address-cells = <1>;
+			#size-cells = <0>;
+			reg = <0 0x613500 0x100>;
+			clock-frequency = <133000000>;
+			interrupts = <32>;
+			interrupt-parent = <&pic1>;
+		};
+		i2c13: i2c@613700 {
+			compatible = "netlogic,xlp9xx-i2c";
+			#address-cells = <1>;
+			#size-cells = <0>;
+			reg = <0 0x613700 0x100>;
+			clock-frequency = <133000000>;
+			interrupts = <33>;
+			interrupt-parent = <&pic1>;
+		};
+
 	};
 
 	chosen {
-- 
1.9.1

