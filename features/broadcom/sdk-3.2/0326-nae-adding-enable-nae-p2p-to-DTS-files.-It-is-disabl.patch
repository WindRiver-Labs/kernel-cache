From 55ee9d31ad1ba41f23bbd13f42b6304e8ba6c3ba Mon Sep 17 00:00:00 2001
From: Sreenidhi BR <sreenira@broadcom.com>
Date: Thu, 16 Aug 2012 17:32:33 +0530
Subject: [PATCH 0326/1532] nae: adding <enable-nae-p2p> to DTS files. It is
 disabled by default.

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp8xx-helinux.dts | 2 ++
 arch/mips/boot/dts/xlp8xx-netos.dts   | 1 +
 2 files changed, 3 insertions(+)

diff --git a/arch/mips/boot/dts/xlp8xx-helinux.dts b/arch/mips/boot/dts/xlp8xx-helinux.dts
index 5f267a4..c2eb702 100644
--- a/arch/mips/boot/dts/xlp8xx-helinux.dts
+++ b/arch/mips/boot/dts/xlp8xx-helinux.dts
@@ -99,6 +99,8 @@
 			app-param{
 				onlinemask = <0xffff>;
 				enable-nae-hw-parser = <0>;
+				enable-nae-jumbo = <0>;
+				enable-nae-p2p = <0>;
 			};
 			nae{
 			};
diff --git a/arch/mips/boot/dts/xlp8xx-netos.dts b/arch/mips/boot/dts/xlp8xx-netos.dts
index 4dac487..5d46797 100644
--- a/arch/mips/boot/dts/xlp8xx-netos.dts
+++ b/arch/mips/boot/dts/xlp8xx-netos.dts
@@ -65,6 +65,7 @@
                                 onlinemask = <0xffffffff>;
 				enable-nae-jumbo = <0>;
 				enable-nae-hw-parser = <0>;
+				enable-nae-p2p = <0>;
                         };
 			nae{
 			};
