From 2f58b27527df39706a4b9e2f2eb4fdad82e1049c Mon Sep 17 00:00:00 2001
From: Rahul Gupta <guptar@broadcom.com>
Date: Mon, 11 Nov 2013 17:12:18 +0530
Subject: [PATCH 0443/1532] 2xx pcie: added dts file and build-dtb.sh

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp2xx-helinux-pcienet.dts | 137 ++++++++++++++++++++++++++
 1 file changed, 137 insertions(+)
 create mode 100644 arch/mips/boot/dts/xlp2xx-helinux-pcienet.dts

diff --git a/arch/mips/boot/dts/xlp2xx-helinux-pcienet.dts b/arch/mips/boot/dts/xlp2xx-helinux-pcienet.dts
new file mode 100644
index 0000000..eb83be5
--- /dev/null
+++ b/arch/mips/boot/dts/xlp2xx-helinux-pcienet.dts
@@ -0,0 +1,137 @@
+/* XLP8XX Device Tree Source 
+ *
+ */
+
+/dts-v1/;
+/ {
+
+	model = "netlogic,XLP-EVP";
+        compatible = "netlogic,xlp";
+        #address-cells = <2>;
+        #size-cells = <2>;
+		/*memory range for linux. This will not appear in dom0*/
+        memory {
+                device_type = "memory";
+                reg =  <0 0x01000000 0 0x0B000000   //16MB 176MB
+                        0 0x20000000 0 0x26000000>; //512M 608M
+        };
+
+        pktmem {
+            device_type = "pktmem";
+             /* format: node number, 64-bit addr, 64-bit size */
+             reg = <0 0x0 0x50000000 0x0 0x20000000 // 512M @ 1.25GB
+                    1 0x0 0x0 0x0 0x0
+                    2 0x0 0x0 0x0 0x0
+                    3 0x0 0x0 0x0 0x0>;
+        };
+                
+	hypervisor {
+		hypervisor-name = "Xen";
+		alloc_dom0_memory = <1>;
+		bootargs = "dom0_loadaddr=0x72000000 dom0_cpumask=0x0f -- ";
+		domain_heap = <0x50000000 0x20000000>;
+	};
+
+	doms {
+		#address-cells = <1>;
+		#size-cells = <1>;
+		dom@0 {
+			device_type = "domain";
+			os = "linux";
+
+			#address-cells = <1>;
+			#size-cells = <1>;
+
+			cpu {
+				onlinemask = <0xff>;
+           		napi-vc-mask = <0x8>;
+			};
+			uart {
+				id = <0>;
+				owner = <1>;
+				sharedcfg = <0x1f000000>;
+			};
+			fmn {
+	           		node_0_vc_mask = <0x88888888 0x88888888 0x88888888 0x88888888>;
+	           		node_1_vc_mask = <0x00000000 0x00000000 0x00000000 0x00000000>;
+        	   		node_2_vc_mask = <0x00000000 0x00000000 0x00000000 0x00000000>;
+           			node_3_vc_mask = <0x00000000 0x00000000 0x00000000 0x00000000>;
+			};
+			owner-config {
+				nae = <0x0>;   // currently for node 0
+				sae = <0x0>;
+				rsa = <0x0>;
+				fmn = <0x1>;
+			};
+		};
+
+		dom@1 {
+			device_type = "domain";
+			os = "netos";
+
+			heap-size = <0x0 0x2000000>; // 32M
+			shared-mem = <0x0 0x50000000 0x0 0x20000000>; // 512M @ 1.25GB
+
+			#address-cells = <1>;
+			#size-cells = <1>;
+
+			app-bootcmd = "smpload";
+			app-loadaddr = "0xf0000000"; // 3.75GB
+
+			kernel {
+				/* Kernel Memory allocated from xen domain-heap */
+				size = <0x02000000>;  // 32M
+
+				/* Location in Dom0 ramdisk */
+				location = "/tmp/xen/netos.elf";
+
+				/* Domain destroy currently doesn't work with hyperexec */
+				dom_destroy_hack = <0>;
+			};
+
+			cpu {
+				onlinemask = <0xff>;
+                nae-rx-vc = <2>;
+                nae-fb-vc = <3>;
+			};
+			uart {
+				id = <0>;
+			};
+			memory {
+				reg = <0x60000000 0x60000000>;  // 1.5GB @ 1.5GB
+			};
+			app-param{
+				onlinemask = <0xff>;
+				ipoverpcie = <0x3>;
+				max_xaui_rx_context = <0x1>;
+				max_xaui_tx_context = <0x1>;
+				max_xaui_ports = <0x2>;
+				nae_egrees_context_base = <0x1b0>;
+				nae_ingrees_context_base = <0x1f0>;
+				nae_mgmt_ingrees_context = <0x8>;
+			};
+			owner-config {
+				nae = <0x1>;
+				fmn = <0x0>;
+				sae = <0x0>;
+				rsa = <0x0>;
+			};
+		};
+	};
+
+	chosen {
+        netlogic,cpumask = <0xff>;
+		/*"autoboot_pciedrv";*/
+	    bootargs = " xlp_noi2c console=ttyS0,115200 console=pcie_co,115200 autoboot_pciedrv console_over_pcie";
+        /*bootargs = " xlp_noi2c console=ttyS0,115200 ";*/
+	};
+
+	soc {
+		nodes {
+			num-nodes = <1>;
+		};
+	};
+	frequency-config {
+		nae = <250>;
+	};
+};
-- 
