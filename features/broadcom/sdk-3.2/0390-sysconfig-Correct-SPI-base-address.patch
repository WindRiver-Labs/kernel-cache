From 4c21e26796d46fcb0a0ff5aa3d42dbd6b6784c67 Mon Sep 17 00:00:00 2001
From: "P. Sadik" <psadik@broadcom.com>
Date: Thu, 25 Jul 2013 09:30:15 +0530
Subject: [PATCH 0390/1532] sysconfig: Correct SPI base address.

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp2xx-linux-tmpl.dts | 4 ++--
 arch/mips/boot/dts/xlp3xx-linux-tmpl.dts | 4 ++--
 arch/mips/boot/dts/xlp8xx-linux-tmpl.dts | 4 ++--
 3 files changed, 6 insertions(+), 6 deletions(-)

diff --git a/arch/mips/boot/dts/xlp2xx-linux-tmpl.dts b/arch/mips/boot/dts/xlp2xx-linux-tmpl.dts
index b8770f9..d310996 100644
--- a/arch/mips/boot/dts/xlp2xx-linux-tmpl.dts
+++ b/arch/mips/boot/dts/xlp2xx-linux-tmpl.dts
@@ -73,7 +73,7 @@
 				compatible = "national,lm90";
 				reg = <0x4c>;
 			};
-			
+
 			eeprom@57 {
                                 compatible = "xlpge_eeprom";
                                 reg = <0x57>;
@@ -147,7 +147,7 @@
 			compatible = "netlogic,xlp-spi";
 			#address-cells = <1>;
 			#size-cells = <0>;
-			reg = <0 0x13a000 0x100>;
+			reg = <0 0x3a000 0x100>;
 
 			spi_nor@1 {
 				compatible = "spansion,s25sl12801";
diff --git a/arch/mips/boot/dts/xlp3xx-linux-tmpl.dts b/arch/mips/boot/dts/xlp3xx-linux-tmpl.dts
index fc671c2..d5ced30 100644
--- a/arch/mips/boot/dts/xlp3xx-linux-tmpl.dts
+++ b/arch/mips/boot/dts/xlp3xx-linux-tmpl.dts
@@ -74,7 +74,7 @@
 				compatible = "national,lm90";
 				reg = <0x4c>;
 			};
-			
+
 			eeprom@57 {
                                 compatible = "xlpge_eeprom";
                                 reg = <0x57>;
@@ -148,7 +148,7 @@
 			compatible = "netlogic,xlp-spi";
 			#address-cells = <1>;
 			#size-cells = <0>;
-			reg = <0 0x13a000 0x100>;
+			reg = <0 0x3a000 0x100>;
 
 			spi_nor@1 {
 				compatible = "spansion,s25sl12801";
diff --git a/arch/mips/boot/dts/xlp8xx-linux-tmpl.dts b/arch/mips/boot/dts/xlp8xx-linux-tmpl.dts
index 4660dfb..00c0dcc 100644
--- a/arch/mips/boot/dts/xlp8xx-linux-tmpl.dts
+++ b/arch/mips/boot/dts/xlp8xx-linux-tmpl.dts
@@ -74,7 +74,7 @@
 				compatible = "national,lm90";
 				reg = <0x4c>;
 			};
-			
+
  			eeprom@57 {
                                 compatible = "xlpge_eeprom";
                                 reg = <0x57>;
@@ -148,7 +148,7 @@
 			compatible = "netlogic,xlp-spi";
 			#address-cells = <1>;
 			#size-cells = <0>;
-			reg = <0 0x13a000 0x100>;
+			reg = <0 0x3a000 0x100>;
 
 			spi_nor@1 {
 				compatible = "spansion,s25sl12801";
-- 
