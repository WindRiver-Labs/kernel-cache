From 5639a85fb3bc535efd74fc53b543729c3a0a028d Mon Sep 17 00:00:00 2001
From: Subhendu Sekhar Behera <sbehera@broadcom.com>
Date: Wed, 10 Dec 2014 19:53:23 +0530
Subject: [PATCH 0612/1532] i2c: change name convention for xlp9xx

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp5xx-linux-kernel.dts | 8 ++++----
 arch/mips/boot/dts/xlp9xx-linux-kernel.dts | 8 ++++----
 2 files changed, 8 insertions(+), 8 deletions(-)

diff --git a/arch/mips/boot/dts/xlp5xx-linux-kernel.dts b/arch/mips/boot/dts/xlp5xx-linux-kernel.dts
index 57c18c3..90bbf37 100644
--- a/arch/mips/boot/dts/xlp5xx-linux-kernel.dts
+++ b/arch/mips/boot/dts/xlp5xx-linux-kernel.dts
@@ -109,7 +109,7 @@
 		};
 
 		i2c0: i2c@113100 {
-			compatible = "netlogic,xlp-i2c";
+			compatible = "netlogic,xlp9xx-i2c";
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0 0x113100 0x100>;
@@ -118,7 +118,7 @@
 			interrupt-parent = <&pic>;
 		};
 		i2c1: i2c@113300 {
-			compatible = "netlogic,xlp-i2c";
+			compatible = "netlogic,xlp9xx-i2c";
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0 0x113300 0x100>;
@@ -137,7 +137,7 @@
                         };
 		};
 		i2c2: i2c@113500 {
-			compatible = "netlogic,xlp-i2c";
+			compatible = "netlogic,xlp9xx-i2c";
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0 0x113500 0x100>;
@@ -151,7 +151,7 @@
 			};
 		};
 		i2c3: i2c@113700 {
-			compatible = "netlogic,xlp-i2c";
+			compatible = "netlogic,xlp9xx-i2c";
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0 0x113700 0x100>;
diff --git a/arch/mips/boot/dts/xlp9xx-linux-kernel.dts b/arch/mips/boot/dts/xlp9xx-linux-kernel.dts
index c529b92..3e91bb5 100644
--- a/arch/mips/boot/dts/xlp9xx-linux-kernel.dts
+++ b/arch/mips/boot/dts/xlp9xx-linux-kernel.dts
@@ -110,7 +110,7 @@
 		};
 
 		i2c0: i2c@113100 {
-			compatible = "netlogic,xlp-i2c";
+			compatible = "netlogic,xlp9xx-i2c";
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0 0x113100 0x100>;
@@ -119,7 +119,7 @@
 			interrupt-parent = <&pic>;
 		};
 		i2c1: i2c@113300 {
-			compatible = "netlogic,xlp-i2c";
+			compatible = "netlogic,xlp9xx-i2c";
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0 0x113300 0x100>;
@@ -144,7 +144,7 @@
 
 		};
 		i2c2: i2c@113500 {
-			compatible = "netlogic,xlp-i2c";
+			compatible = "netlogic,xlp9xx-i2c";
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0 0x113500 0x100>;
@@ -153,7 +153,7 @@
 			interrupt-parent = <&pic>;
 		};
 		i2c3: i2c@113700 {
-			compatible = "netlogic,xlp-i2c";
+			compatible = "netlogic,xlp9xx-i2c";
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0 0x113700 0x100>;
