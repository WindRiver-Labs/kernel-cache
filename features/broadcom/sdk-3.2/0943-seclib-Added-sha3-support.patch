From dac99f77c7bdf5267655bec1b82b9e80c1f001a8 Mon Sep 17 00:00:00 2001
From: reshmic <reshmic@broadcom.com>
Date: Wed, 21 May 2014 17:51:08 +0530
Subject: [PATCH 0943/1532] seclib: Added sha3 support

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/netlogic/lib/seclib/saesoc_defs.h          | 4 ++++
 arch/mips/netlogic/lib/seclib/saesoc_gen2_hal_defs.h | 8 ++++++++
 arch/mips/netlogic/lib/seclib/saesoc_lib.h           | 4 ++++
 3 files changed, 16 insertions(+)

diff --git a/arch/mips/netlogic/lib/seclib/saesoc_defs.h b/arch/mips/netlogic/lib/seclib/saesoc_defs.h
index 6a733e0..7871791 100644
--- a/arch/mips/netlogic/lib/seclib/saesoc_defs.h
+++ b/arch/mips/netlogic/lib/seclib/saesoc_defs.h
@@ -377,6 +377,10 @@ enum saesoc_hash_type {
 	SAESOC_HASH_TYPE_SHA_512 = 0x15,
 	SAESOC_HASH_TYPE_SHA_512_224 = 0x16,
 	SAESOC_HASH_TYPE_SHA_512_256 = 0x17,
+	SAESOC_HASH_TYPE_SHA3_224 = 0x18,
+	SAESOC_HASH_TYPE_SHA3_256 = 0x19,
+	SAESOC_HASH_TYPE_SHA3_384 = 0x1a,
+	SAESOC_HASH_TYPE_SHA3_512 = 0x1b,
 	SAESOC_HASH_TYPE_MAX
 };
 
diff --git a/arch/mips/netlogic/lib/seclib/saesoc_gen2_hal_defs.h b/arch/mips/netlogic/lib/seclib/saesoc_gen2_hal_defs.h
index ba0173c..85a6db9 100644
--- a/arch/mips/netlogic/lib/seclib/saesoc_gen2_hal_defs.h
+++ b/arch/mips/netlogic/lib/seclib/saesoc_gen2_hal_defs.h
@@ -285,6 +285,10 @@ static char  saesoc_gen2_hash_type_name[][32] = {
 	"SHA_512",
 	"SHA_512/224",
 	"SHA_512/256"
+	"SHA3_224",
+	"SHA3_256",
+	"SHA3_384",
+	"SHA3_512",
 };
 
 static char  saesoc_gen2_hash_mode_name[][32] = {
@@ -345,6 +349,10 @@ enum saesoc_gen2_hash_type {
 	SAESOC_GEN2_HASH_TYPE_SHA_512 = 0x15,
 	SAESOC_GEN2_HASH_TYPE_SHA_512_224 = 0x16,
 	SAESOC_GEN2_HASH_TYPE_SHA_512_256 = 0x17,
+	SAESOC_GEN2_HASH_TYPE_SHA3_224 = 0x18,
+	SAESOC_GEN2_HASH_TYPE_SHA3_256 = 0x19,
+	SAESOC_GEN2_HASH_TYPE_SHA3_384 = 0x1a,
+	SAESOC_GEN2_HASH_TYPE_SHA3_512 = 0x1b,
 	SAESOC_GEN2_HASH_TYPE_MAX
 };
 
diff --git a/arch/mips/netlogic/lib/seclib/saesoc_lib.h b/arch/mips/netlogic/lib/seclib/saesoc_lib.h
index a5e7568..64900f9 100644
--- a/arch/mips/netlogic/lib/seclib/saesoc_lib.h
+++ b/arch/mips/netlogic/lib/seclib/saesoc_lib.h
@@ -186,6 +186,10 @@ static int saesoc_gen1_hash_type_map[SAESOC_HASH_TYPE_MAX +1] = {
 	[SAESOC_HASH_TYPE_SHA_512] = 0x42,
 	[SAESOC_HASH_TYPE_SHA_512_224] = 0x00,
 	[SAESOC_HASH_TYPE_SHA_512_256] = 0x00,
+	[SAESOC_HASH_TYPE_SHA3_224] = 0x00,
+	[SAESOC_HASH_TYPE_SHA3_256] = 0x00,
+	[SAESOC_HASH_TYPE_SHA3_384] = 0x00,
+	[SAESOC_HASH_TYPE_SHA3_512] = 0x00,
 	[SAESOC_HASH_TYPE_MAX] = 0xff,
 };
 
