From 2fbd2f76eff540d65425fc23ecad22b079cac609 Mon Sep 17 00:00:00 2001
From: Vikas Gupta <vikasg@netlogicmicro.com>
Date: Thu, 1 Dec 2011 12:43:11 +0530
Subject: [PATCH 0271/1532] Fix phy addresses for SGMII daughter card(Marvell).

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/nae.dts | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/mips/boot/dts/nae.dts b/arch/mips/boot/dts/nae.dts
index 0a73045..fad842f 100644
--- a/arch/mips/boot/dts/nae.dts
+++ b/arch/mips/boot/dts/nae.dts
@@ -232,7 +232,7 @@
                 // Max ingress fifo size 256 units (size of one unit is 64 byte)
                 iface_fifo_sizes = <13 13 13 13>;
 
-                ext_phy_addr = <7 6 5 4>;
+                ext_phy_addr = <5 6 7 4>;
                 ext_phy_bus = <1 1 1 1>;
 
                 // Max parser sequence fifo size 1024 packets
@@ -260,7 +260,7 @@
                 // Max ingress fifo size 256 units (size of one unit is 64 byte)
                 iface_fifo_sizes = <13 13 13 13>;
 
-                ext_phy_addr = <11 10 9 8>;
+                ext_phy_addr = <9 10 11 8>;
                 ext_phy_bus = <1 1 1 1>;
 
                 // Max parser sequence fifo size 1024 packets
@@ -288,7 +288,7 @@
                 // Max ingress fifo size 256 units (size of one unit is 64 byte)
                 iface_fifo_sizes = <13 13 13 13>;
 
-                ext_phy_addr = <3 2 1 0>;
+                ext_phy_addr = <1 2 3 0>;
                 ext_phy_bus = <1 1 1 1>;
 
                 // Max parser sequence fifo size 1024 packets
@@ -316,7 +316,7 @@
                 // Max ingress fifo size 256 units (size of one unit is 64 byte)
                 iface_fifo_sizes = <13 13 13 13>;
 
-                ext_phy_addr = <15 14 13 12>;
+                ext_phy_addr = <13 14 15 12>;
                 ext_phy_bus = <1 1 1 1>;
 
                 // Max parser sequence fifo size 1024 packets
