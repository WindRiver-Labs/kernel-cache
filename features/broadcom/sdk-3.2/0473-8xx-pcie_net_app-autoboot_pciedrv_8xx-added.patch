From 2a47c24d46cca91b2caa61dd0c20ef1fc690dcea Mon Sep 17 00:00:00 2001
From: Rahul Gupta <guptar@broadcom.com>
Date: Tue, 17 Dec 2013 23:02:56 +0530
Subject: [PATCH 0473/1532] 8xx pcie_net_app: autoboot_pciedrv_8xx added.

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp8xx-helinux-pcienet.dts | 10 ++++++----
 1 file changed, 6 insertions(+), 4 deletions(-)

diff --git a/arch/mips/boot/dts/xlp8xx-helinux-pcienet.dts b/arch/mips/boot/dts/xlp8xx-helinux-pcienet.dts
index db64ee5..7611dba 100644
--- a/arch/mips/boot/dts/xlp8xx-helinux-pcienet.dts
+++ b/arch/mips/boot/dts/xlp8xx-helinux-pcienet.dts
@@ -12,8 +12,8 @@
 	/*memory range for linux. This will not appear in dom0*/
         memory {
                 device_type = "memory";
-                reg =  <0 0x01000000 0 0x0B000000   
-                        0 0x20000000 0 0x26000000>;
+                reg =  <0 0x01000000 0 0x0B000000   /*16M to 176M */
+                        0 0x20000000 0 0x26000000>; /*512M to 1120M */
         };
 
         pktmem {
@@ -96,7 +96,8 @@
 				id = <0>;
 			};
 			memory {
-				reg = <0x60000000 0x60000000>;  // 1.5GB @ 1.5GB
+				//reg = <0x60000000 0x60000000>;  // 1.5GB @ 1.5GB
+				reg = <0x75000000 0x2bc00000>;  // 1.5GB @ 1.5GB
 			};
 			app-param{
 				onlinemask = <0xffff>;
@@ -112,7 +113,8 @@
 
 	chosen {
         netlogic,cpumask = <0xffff>;
-		bootargs = " xlp_noi2c console=ttyS0,115200 autoboot_pciedrv";
+		bootargs = " xlp_noi2c console=ttyS0,115200 autoboot_pciedrv_8xx";
+        /*bootargs = " xlp_noi2c console=ttyS0,115200 autoboot_pciedrv_8xx console_over_pcie";*/
 	};
 
 	soc {
