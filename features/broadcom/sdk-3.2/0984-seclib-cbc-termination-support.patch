From edafffe03c60d469c2d4cd921c04088d9abb296a Mon Sep 17 00:00:00 2001
From: Hareesh R <hareeshr@broadcom.com>
Date: Thu, 29 May 2014 12:26:39 +0530
Subject: [PATCH 0984/1532] seclib:cbc-termination support

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/netlogic/lib/seclib/saesoc_defs.h          | 2 ++
 arch/mips/netlogic/lib/seclib/saesoc_gen2_hal_defs.h | 5 +++--
 2 files changed, 5 insertions(+), 2 deletions(-)

diff --git a/arch/mips/netlogic/lib/seclib/saesoc_defs.h b/arch/mips/netlogic/lib/seclib/saesoc_defs.h
index 7871791..d244c4f 100644
--- a/arch/mips/netlogic/lib/seclib/saesoc_defs.h
+++ b/arch/mips/netlogic/lib/seclib/saesoc_defs.h
@@ -312,6 +312,7 @@ enum saesoc_cipher_type {
 * @SAESOC_CIPHER_MODE_XTS: Cipher mode is XTS
 * @SAESOC_CIPHER_MODE_CCM: Cipher mode is CCM
 * @SAESOC_CIPHER_MODE_GCM: Cipher mode is GCM
+* @SAESOC_CIPHER_MODE_CBC_TERM: Cipher mode is CBC termination
 * @SAESOC_CIPHER_MODE_MAX: End of cipher mode
 */
 
@@ -326,6 +327,7 @@ enum saesoc_cipher_mode {
 	SAESOC_CIPHER_MODE_XTS = 0x07,
 	SAESOC_CIPHER_MODE_CCM = 0x08,
 	SAESOC_CIPHER_MODE_GCM = 0x09,
+	SAESOC_CIPHER_MODE_CBC_TERM = 0x0a,
 	SAESOC_CIPHER_MODE_MAX
 };
 
diff --git a/arch/mips/netlogic/lib/seclib/saesoc_gen2_hal_defs.h b/arch/mips/netlogic/lib/seclib/saesoc_gen2_hal_defs.h
index 85a6db9..339f1db 100644
--- a/arch/mips/netlogic/lib/seclib/saesoc_gen2_hal_defs.h
+++ b/arch/mips/netlogic/lib/seclib/saesoc_gen2_hal_defs.h
@@ -258,7 +258,8 @@ static char  saesoc_gen2_cipher_mode_name[][32] = {
 	"LRW",
 	"XTS",
 	"CCM",
-	"GCM"
+	"GCM",
+	"CBC-TERM"
 };
 static char  saesoc_gen2_hash_type_name[][32] = {
 	"AES_128",
@@ -284,7 +285,7 @@ static char  saesoc_gen2_hash_type_name[][32] = {
 	"SHA_384",
 	"SHA_512",
 	"SHA_512/224",
-	"SHA_512/256"
+	"SHA_512/256",
 	"SHA3_224",
 	"SHA3_256",
 	"SHA3_384",
-- 
