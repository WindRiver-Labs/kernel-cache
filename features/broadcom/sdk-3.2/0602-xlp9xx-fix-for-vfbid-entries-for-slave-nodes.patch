From d39b922b776038a70a60ce973c803fa5a8f2127c Mon Sep 17 00:00:00 2001
From: Vikas Gupta <vikas.gupta@broadcom.com>
Date: Fri, 5 Dec 2014 19:57:55 +0530
Subject: [PATCH 0602/1532] xlp9xx: fix for vfbid entries for slave nodes.

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/mips/boot/dts/xlp9xx-config.dts | 20 ++++++++++----------
 1 file changed, 10 insertions(+), 10 deletions(-)

diff --git a/arch/mips/boot/dts/xlp9xx-config.dts b/arch/mips/boot/dts/xlp9xx-config.dts
index 4399aa4..8998a80 100644
--- a/arch/mips/boot/dts/xlp9xx-config.dts
+++ b/arch/mips/boot/dts/xlp9xx-config.dts
@@ -42,8 +42,8 @@
 				freein-fifo-mask = <0xffff 0xffff 0 0>; /* Node-0  Node-1 Node-2 Node-3 */
 				/* node offsets and size */
 				vfbtbl-sw-offset = <0 0 0 0>;
-				vfbtbl-sw-nentries = <80 32 32 80>;
-				vfbtbl-hw-offset = <80 32 32 80>;
+				vfbtbl-sw-nentries = <80 80 80 80>;
+				vfbtbl-hw-offset = <80 80 80 80>;
 				vfbtbl-hw-nentries = <20 20 20 20>;
 			};
 
@@ -133,8 +133,8 @@
 				sh-domains = <0>;
 				/* node offsets and size */
 				vfbtbl-sw-offset = <0 0 0 0>;
-				vfbtbl-sw-nentries = <80 32 32 80>;
-				vfbtbl-hw-offset = <80 32 32 80>;
+				vfbtbl-sw-nentries = <80 80 80 80>;
+				vfbtbl-hw-offset = <80 80 80 80>;
 				vfbtbl-hw-nentries = <20 20 20 20>;
 				/* replenish info syntax */
 				freein-fifo-replenish-by-owner = <1>;
@@ -151,8 +151,8 @@
 				sh-domains = <0>;
 				/* node offsets and size */
 				vfbtbl-sw-offset = <0 0 0 0>;
-				vfbtbl-sw-nentries = <80 32 32 80>;
-				vfbtbl-hw-offset = <80 32 32 80>;
+				vfbtbl-sw-nentries = <80 80 80 80>;
+				vfbtbl-hw-offset = <80 80 80 80>;
 				vfbtbl-hw-nentries = <20 20 20 20>;
 				/* replenish info syntax */
 				freein-fifo-replenish-by-owner = <1>;
@@ -228,8 +228,8 @@
 				freein-fifo-mask = <0xffff 0 0 0>;
 				/* node offsets and size */
 				vfbtbl-sw-offset = <0 0 0 0>;
-				vfbtbl-sw-nentries = <80 32 32 80>;
-				vfbtbl-hw-offset = <80 32 32 80>;
+				vfbtbl-sw-nentries = <80 80 80 80>;
+				vfbtbl-hw-offset = <80 80 80 80>;
 				vfbtbl-hw-nentries = <20 20 20 20>;
 				/* replenish info syntax */
 				freein-fifo-replenish-by-owner = <1>;
@@ -245,8 +245,8 @@
 				freein-fifo-mask = <0xffff 0 0 0>;
 				/* node offsets and size */
 				vfbtbl-sw-offset = <0 0 0 0>;
-				vfbtbl-sw-nentries = <80 32 32 80>;
-				vfbtbl-hw-offset = <80 32 32 80>;
+				vfbtbl-sw-nentries = <80 80 80 80>;
+				vfbtbl-hw-offset = <80 80 80 80>;
 				vfbtbl-hw-nentries = <20 20 20 20>;
 				/* replenish info syntax */
 				freein-fifo-replenish-by-owner = <1>;
-- 
