From 1381ab2aedf8048d46e5f896c4054ed4037dc25f Mon Sep 17 00:00:00 2001
From: Gregory Haskins <ghaskins@novell.com>
Date: Fri, 2 Oct 2009 17:52:54 -0400
Subject: [PATCH 061/119] vbus: synchronize with cleanup branch

Signed-off-by: Gregory Haskins <ghaskins@novell.com>
---
 kernel/vbus/Kconfig             |    1 -
 kernel/vbus/Makefile            |    2 --
 kernel/vbus/connectors/Kconfig  |   18 +++++++++---------
 kernel/vbus/connectors/Makefile |    5 ++---
 4 files changed, 11 insertions(+), 15 deletions(-)

diff --git a/kernel/vbus/Kconfig b/kernel/vbus/Kconfig
index 8461528..bf54bbb 100644
--- a/kernel/vbus/Kconfig
+++ b/kernel/vbus/Kconfig
@@ -29,4 +29,3 @@ config VBUS_DEVICES
 
 source "kernel/vbus/devices/Kconfig"
 source "kernel/vbus/connectors/Kconfig"
-
diff --git a/kernel/vbus/Makefile b/kernel/vbus/Makefile
index 8a68303..72294c0 100644
--- a/kernel/vbus/Makefile
+++ b/kernel/vbus/Makefile
@@ -3,5 +3,3 @@ obj-$(CONFIG_VBUS) += shm-ioq.o
 
 obj-y += devices/
 obj-y += connectors/
-
-
diff --git a/kernel/vbus/connectors/Kconfig b/kernel/vbus/connectors/Kconfig
index e8e3a91..35e6226 100644
--- a/kernel/vbus/connectors/Kconfig
+++ b/kernel/vbus/connectors/Kconfig
@@ -1,4 +1,13 @@
 
+config VBUS_NULL
+       tristate "Virtual-Bus NULL connector"
+       depends on VBUS
+       default n
+       help
+         Provides a example connector
+
+	 If unsure, say N
+
 config VBUS_KVM
        tristate "Virtual-Bus connector for KVM"
        depends on VBUS
@@ -10,12 +19,3 @@ config VBUS_KVM
 
 	 If unsure, say N
 
-config VBUS_NULL
-       tristate "Virtual-Bus NULL connector"
-       depends on VBUS
-       default n
-       help
-         Provides a example connector
-
-	 If unsure, say N
-
diff --git a/kernel/vbus/connectors/Makefile b/kernel/vbus/connectors/Makefile
index 64faa91..d485bca 100644
--- a/kernel/vbus/connectors/Makefile
+++ b/kernel/vbus/connectors/Makefile
@@ -1,7 +1,6 @@
 
-vbus-kvm-objs += kvm.o
-obj-$(CONFIG_VBUS_KVM) += vbus-kvm.o
-
 vbus-null-objs += null.o
 obj-$(CONFIG_VBUS_NULL) += vbus-null.o
 
+vbus-kvm-objs += kvm.o
+obj-$(CONFIG_VBUS_KVM) += vbus-kvm.o
