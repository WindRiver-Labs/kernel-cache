 			      pipe_config->dpll_hw_state.ebb4,
@@ -12028,6 +12028,7 @@ static void intel_dump_pipe_config(struct intel_crtc *crtc,
 			      pipe_config->dpll_hw_state.pll6,
 			      pipe_config->dpll_hw_state.pll8,
