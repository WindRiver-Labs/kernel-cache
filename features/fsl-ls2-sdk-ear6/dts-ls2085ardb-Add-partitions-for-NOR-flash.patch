From 860a10a969a41935e8fbd5b6a1981a4e01e310d3 Mon Sep 17 00:00:00 2001
From: Jiang Lu <lu.jiang@windriver.com>
Date: Mon, 9 Nov 2015 17:32:53 +0800
Subject: [PATCH 451/452] dts:ls2085ardb:Add partitions for NOR flash

Add partitions for NOR flash of fsl-ls2085ardb.

Signed-off-by: Jiang Lu <lu.jiang@windriver.com>
Signed-off-by: Xulin Sun <xulin.sun@windriver.com>
---
 arch/arm64/boot/dts/fsl-ls2085a-rdb.dts |   20 ++++++++++++++++++++
 1 files changed, 20 insertions(+), 0 deletions(-)

diff --git a/arch/arm64/boot/dts/fsl-ls2085a-rdb.dts b/arch/arm64/boot/dts/fsl-ls2085a-rdb.dts
index 147c21a..ecfbc49 100644
--- a/arch/arm64/boot/dts/fsl-ls2085a-rdb.dts
+++ b/arch/arm64/boot/dts/fsl-ls2085a-rdb.dts
@@ -38,6 +38,26 @@
 	     reg = <0x0 0x0 0x8000000>;
 	     bank-width = <2>;
 	     device-width = <1>;
+
+		partition@0 {
+			label = "firmware";
+			reg = <0x0 0x1100000>;
+			read-only;
+		};
+		partition@1100000 {
+			label = "writable0";
+			reg = <0x1100000 0x2F00000>;
+		};
+		partition@4000000 {
+			label = "Alt-firmware";
+			reg = <0x4000000 0x1100000>;
+			read-only;
+		};
+		partition@5100000 {
+			label = "writable1";
+			reg = <0x5100000 0x2F00000>;
+		};
+
 	};
 
 	nand@2,0 {
