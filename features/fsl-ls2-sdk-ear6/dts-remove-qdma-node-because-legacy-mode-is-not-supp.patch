From c1299ca55eaa142267e0739bd5692c816b91fc3e Mon Sep 17 00:00:00 2001
From: Stuart Yoder <stuart.yoder@nxp.com>
Date: Tue, 26 Jan 2016 14:00:01 -0600
Subject: [PATCH 441/452] dts: remove qdma node because legacy mode is not
 supported

Signed-off-by: Stuart Yoder <stuart.yoder@nxp.com>
[Xulin: Original patch taken from FSL LS2085 SDK EAR6.0,
LS2085A-SDK-SOURCE-20160304-yocto.iso]
Signed-off-by: Xulin Sun <xulin.sun@windriver.com>
---
 arch/arm64/boot/dts/freescale/fsl-ls2080a.dtsi |    9 ---------
 arch/arm64/boot/dts/freescale/fsl-ls2085a.dtsi |    9 ---------
 2 files changed, 0 insertions(+), 18 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-ls2080a.dtsi b/arch/arm64/boot/dts/freescale/fsl-ls2080a.dtsi
index 802f63b..7906357 100644
--- a/arch/arm64/boot/dts/freescale/fsl-ls2080a.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-ls2080a.dtsi
@@ -381,15 +381,6 @@
 		clock-names = "qspi_en", "qspi";
 	};
 
-	qdma: qdma@8380000 {
-		compatible = "fsl,ls-qdma";
-		reg = <0x0 0x8380000 0x0 0x20000>;
-		interrupts = <0 42 0x4>,
-				<0 43 0x4>;
-		interrupt-names = "qdma-tx", "qdma-err";
-		channels = <1>;
-	};
-
 	pcie@3400000 {
 		compatible = "fsl,ls2080a-pcie", "snps,dw-pcie";
 		reg = <0x00 0x03400000 0x0 0x00100000   /* controller registers */
diff --git a/arch/arm64/boot/dts/freescale/fsl-ls2085a.dtsi b/arch/arm64/boot/dts/freescale/fsl-ls2085a.dtsi
index dc99400..c3a46ad 100644
--- a/arch/arm64/boot/dts/freescale/fsl-ls2085a.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-ls2085a.dtsi
@@ -380,15 +380,6 @@
 		clock-names = "qspi_en", "qspi";
 	};
 
-	qdma: qdma@8380000 {
-		compatible = "fsl,ls-qdma";
-		reg = <0x0 0x8380000 0x0 0x20000>;
-		interrupts = <0 42 0x4>,
-				<0 43 0x4>;
-		interrupt-names = "qdma-tx", "qdma-err";
-		channels = <1>;
-	};
-
 	pcie@3400000 {
 		compatible = "fsl,ls2085a-pcie", "snps,dw-pcie";
 		reg = <0x00 0x03400000 0x0 0x00100000   /* controller registers */
-- 
1.7.5.4

