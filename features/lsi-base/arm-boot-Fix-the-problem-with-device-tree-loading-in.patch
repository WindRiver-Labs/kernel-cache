From 22f6259268e48b1df2445aef641cb10372434c69 Mon Sep 17 00:00:00 2001
From: John Jacques <john.jacques@lsi.com>
Date: Thu, 28 Mar 2013 13:00:20 -0500
Subject: [PATCH 015/159] arm/boot: Fix the problem with device tree loading
 in emulation

git.yoctoproject.org/git/linux-yocto-3.10
commit 6ec44fcdff419f7b6b31d6d1330125eaf91368aa standard/axxia/base.

The "pack" script was getting the wrong arguments.

Signed-off-by: John Jacques <john.jacques@lsi.com>
Signed-off-by: Paul Butler <paul.butler@windriver.com>
---
 arch/arm/boot/emuboot/Makefile |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/arch/arm/boot/emuboot/Makefile b/arch/arm/boot/emuboot/Makefile
index 1d8e491..eb907c5 100644
--- a/arch/arm/boot/emuboot/Makefile
+++ b/arch/arm/boot/emuboot/Makefile
@@ -19,7 +19,7 @@ linux.img: emuboot.o emuboot.lds zImage.emu
 	rm -f linux.tmp
 
 zImage.emu: $(ZIMAGE) axm55xx.dtb
-	python pack.py $(ZIMAGE) $< > $@
+	python pack.py $(ZIMAGE) axm55xx.dtb > $@
 
 axm55xx.dtb: $(DTS)
 	$(DTC) -O dtb -o $@ $<
-- 
