From 3a98865a87983fe2e00ffe674e95bde101e51da7 Mon Sep 17 00:00:00 2001
From: Yongli He <yongli.he@windriver.com>
Date: Mon, 25 Aug 2008 11:27:58 +0800
Subject: [PATCH] revoke wire up i386 system call V7 From: Pekka Enberg <penberg@cs.helsinki.fi>

Make revokeat system call available to user-space on i386.

[akpm@linux-foundation.org: fix 32-bit userspace]
Cc: Alan Cox <alan@redhat.com>
Cc: Al Viro <viro@zeniv.linux.org.uk>
Cc: Christoph Hellwig <hch@infradead.org>
Cc: Peter Zijlstra <peterz@infradead.org>
Signed-off-by: Pekka Enberg <penberg@cs.helsinki.fi>
Integrated-by: Yongli he   <yongli.he@windriver.com>
---
 arch/x86/ia32/ia32entry.S          |    1 +
 arch/x86/include/asm/unistd_32.h   |    3 ++-
 arch/x86/kernel/syscall_table_32.S |    1 +
 3 files changed, 4 insertions(+), 1 deletions(-)

diff --git a/arch/x86/ia32/ia32entry.S b/arch/x86/ia32/ia32entry.S
index 349547f..c0ae489 100644
--- a/arch/x86/ia32/ia32entry.S
+++ b/arch/x86/ia32/ia32entry.S
@@ -931,4 +931,5 @@ ia32_sys_call_table:
 	.quad sys_sched_setparam_ex
 	.quad sys_sched_getparam_ex		/* 340 */
 	.quad sys_sched_wait_interval
+	.quad sys_revokeat		
 ia32_syscall_end:
diff --git a/arch/x86/include/asm/unistd_32.h b/arch/x86/include/asm/unistd_32.h
index b969e56..930345d 100644
--- a/arch/x86/include/asm/unistd_32.h
+++ b/arch/x86/include/asm/unistd_32.h
@@ -350,6 +350,7 @@
 #define __NR_sched_setparam_ex		342
 #define __NR_sched_getparam_ex		343
 #define __NR_sched_wait_interval	344
+#define __NR_revokeat		345
 
 #define VBI_MEM_READ		0x0010
 #define VBI_MEM_WRITE		0x0100
@@ -359,7 +360,7 @@
 
 #ifdef __KERNEL__
 
-#define NR_syscalls 345
+#define NR_syscalls 346
 
 #define __ARCH_WANT_IPC_PARSE_VERSION
 #define __ARCH_WANT_OLD_READDIR
diff --git a/arch/x86/kernel/syscall_table_32.S b/arch/x86/kernel/syscall_table_32.S
index f25bdb5..757ce69 100644
--- a/arch/x86/kernel/syscall_table_32.S
+++ b/arch/x86/kernel/syscall_table_32.S
@@ -345,3 +345,4 @@ ENTRY(sys_call_table)
 	.long sys_sched_setparam_ex
 	.long sys_sched_getparam_ex	/* 343 */
 	.long sys_sched_wait_interval	/* 344 */
+	.long sys_revokeat
-- 
1.7.0.4

