+++ b/tools/perf/util/annotate.c
@@ -70,11 +70,40 @@ static struct ins instructions[] = {
 	{ .name = "call",  .ops  = &call_ops, },
 	{ .name = "callq", .ops  = &call_ops, },
 	{ .name = "ja",	   .ops  = &jump_ops, },
