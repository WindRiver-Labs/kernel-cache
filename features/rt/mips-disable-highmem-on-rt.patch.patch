 arch/mips/Kconfig | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/mips/Kconfig b/arch/mips/Kconfig
index 7d36f6e..62090aa 100644
--- a/arch/mips/Kconfig
+++ b/arch/mips/Kconfig
@@ -2082,7 +2082,7 @@ config CPU_R4400_WORKAROUNDS
 #
 config HIGHMEM
 	bool "High Memory Support"
