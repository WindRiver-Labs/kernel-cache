+++ b/arch/powerpc/boot/dts/p4080ds.dts
@@ -319,14 +319,14 @@
 				reg = <0x3f80000 0xc0000>;
 				read-only;
 			};
-			partition@4020000 {
+			partition@4040000 {
 				label = "alt-rcw";
-				reg = <0x4020000 0x20000>;
+				reg = <0x4040000 0x20000>;
 				read-only;
 			};
-			partition@4040000 {
+			partition@4060000 {
 				label = "fs2";
-				reg = <0x4040000  0x3ea0000>;
+				reg = <0x4060000  0x3ea0000>;
 			};
 			partition@7f00000 {
 				label = "fman-firmware";
