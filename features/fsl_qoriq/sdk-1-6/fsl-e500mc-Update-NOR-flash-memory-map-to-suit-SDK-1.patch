 			};
-			partition@4040000 {
+			partition@4060000 {
 				label = "fs2";
-				reg = <0x4040000  0x3ea0000>;
+				reg = <0x4060000  0x3ea0000>;
 			};
 			partition@7f00000 {
 				label = "fman-firmware";
