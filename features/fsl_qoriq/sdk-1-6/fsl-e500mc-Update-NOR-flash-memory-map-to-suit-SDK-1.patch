From: Xulin Sun <xulin.sun@windriver.com>
Date: Mon, 4 Aug 2014 15:18:32 +0800
Subject: [PATCH] fsl-e500mc: Update NOR flash memory map to suit SDK 1.6.

Signed-off-by: Xulin Sun <xulin.sun@windriver.com>
---
 arch/powerpc/boot/dts/p2041rdb.dts |    8 ++++----
 arch/powerpc/boot/dts/p3041ds.dts  |    8 ++++----
 arch/powerpc/boot/dts/p4080ds.dts  |    8 ++++----
 3 files changed, 12 insertions(+), 12 deletions(-)

diff --git a/arch/powerpc/boot/dts/p2041rdb.dts b/arch/powerpc/boot/dts/p2041rdb.dts
index 651f8a6..cf8fe14 100644
--- a/arch/powerpc/boot/dts/p2041rdb.dts
+++ b/arch/powerpc/boot/dts/p2041rdb.dts
@@ -309,14 +309,14 @@
 				reg = <0x3f80000 0xc0000>;
 				read-only;
 			};
-			partition@4020000 {
+			partition@4040000 {
 				label = "alt-rcw";
-				reg = <0x4020000 0x20000>;
+				reg = <0x4040000 0x20000>;
 				read-only;
 			};
-			partition@4040000 {
+			partition@4060000 {
 				label = "fs2";
-				reg = <0x4040000  0x3ea0000>;
+				reg = <0x4060000  0x3ea0000>;
 			};
 			partition@7f00000 {
 				label = "fman-firmware";
diff --git a/arch/powerpc/boot/dts/p3041ds.dts b/arch/powerpc/boot/dts/p3041ds.dts
index a299353e..7b56cc9 100644
--- a/arch/powerpc/boot/dts/p3041ds.dts
+++ b/arch/powerpc/boot/dts/p3041ds.dts
@@ -296,14 +296,14 @@
