From 230e3690403c273c8407a746ff46bdb98c1a14b0 Mon Sep 17 00:00:00 2001
From: Xulin Sun <xulin.sun@windriver.com>
Date: Tue, 3 Mar 2015 14:09:38 +0800
Subject: [PATCH] fsl-t4xxx/dts: add NOR flash partition information

Signed-off-by: Xulin Sun <xulin.sun@windriver.com>
---
 arch/powerpc/boot/dts/t4240qds.dts |   64 ++++++++++++++++++++++++++++++++++++
 1 files changed, 64 insertions(+), 0 deletions(-)

diff --git a/arch/powerpc/boot/dts/t4240qds.dts b/arch/powerpc/boot/dts/t4240qds.dts
index 8949f65..78304e8 100644
--- a/arch/powerpc/boot/dts/t4240qds.dts
+++ b/arch/powerpc/boot/dts/t4240qds.dts
@@ -93,6 +93,70 @@
 
 			bank-width = <2>;
 			device-width = <1>;
+			partition@0 {
+				label = "rcw";
+				reg = <0x0 0x20000>;
+				read-only;
+			};
+			partition@20000 {
+				label = "kernel1";
+				reg = <0x20000 0x7e0000>;
+			};
+			partition@800000 {
+				label = "dtb1";
+				reg = <0x800000 0x100000>;
+			};
+			partition@900000 {
+				label = "fs1";
+				reg = <0x900000 0x3600000>;
+			};
+			partition@3f00000 {
+				label = "alt-fman-firmware";
+				reg = <0x3f00000 0x20000>;
+				read-only;
+			};
+			partition@3f20000 {
+				label = "alt-uboot-env";
+				reg = <0x3f20000 0x20000>;
+				read-only;
+			};
+			partition@3f40000 {
+				label = "alt-u-boot";
+				reg = <0x3f40000 0xc0000>;
+				read-only;
+			};
+			partition@4000000 {
+				label = "alt-rcw";
+				reg = <0x4000000 0x20000>;
+				read-only;
+			};
+			partition@4020000 {
+				label = "kernel2";
+				reg = <0x4020000 0x7e0000>;
+			};
+			partition@4800000 {
+				label = "dtb2";
+				reg = <0x4800000 0x100000>;
+			};
+			partition@4900000 {
+				label = "fs2";
+				reg = <0x4900000 0x3600000>;
+			};
+			partition@7f00000 {
+				label = "fman-firmware";
+				reg = <0x7f00000 0x20000>;
+				read-only;
+			};
+			partition@7f20000 {
+				label = "u-boot env";
+				reg = <0x7f20000  0x20000>;
+				read-only;
+			};
+			partition@7f40000 {
+				label = "u-boot";
+				reg = <0x7f40000 0xc0000>;
+				read-only;
+			};
 		};
 
 		nand@2,0 {
-- 
