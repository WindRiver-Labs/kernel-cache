From 602083d979814157036fa130d7f38fbf8b385745 Mon Sep 17 00:00:00 2001
From: Yanjiang Jin <yanjiang.jin@windriver.com>
Date: Thu, 8 Jan 2015 05:34:39 +0000
Subject: [PATCH 2/2] fsl-p50xx: add SATA compatible node

Add SATA compatible node for fsl-p50xx.

Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 arch/powerpc/boot/dts/fsl/p5020si-post.dtsi |    2 ++
 1 files changed, 2 insertions(+), 0 deletions(-)

diff --git a/arch/powerpc/boot/dts/fsl/p5020si-post.dtsi b/arch/powerpc/boot/dts/fsl/p5020si-post.dtsi
index ea5db06..f66f2f6 100644
--- a/arch/powerpc/boot/dts/fsl/p5020si-post.dtsi
+++ b/arch/powerpc/boot/dts/fsl/p5020si-post.dtsi
@@ -454,11 +454,13 @@
 	};
 /include/ "qoriq-sata2-0.dtsi"
 	sata@220000 {
+		compatible = "fsl,p5020-sata", "fsl,pq-sata-v2";
 		fsl,iommu-parent = <&pamu1>;
 		fsl,liodn-reg = <&guts 0x550>; /* SATA1LIODNR */
 	};
 /include/ "qoriq-sata2-1.dtsi"
 	sata@221000 {
+		compatible = "fsl,p5020-sata", "fsl,pq-sata-v2";
 		fsl,iommu-parent = <&pamu1>;
 		fsl,liodn-reg = <&guts 0x554>; /* SATA2LIODNR */
 	};
-- 
