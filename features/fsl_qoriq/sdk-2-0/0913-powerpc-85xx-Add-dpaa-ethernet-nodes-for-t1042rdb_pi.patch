From 02a1ec6001b399dbf0f776600ccb3fe0b0abf196 Mon Sep 17 00:00:00 2001
From: Priyanka Jain <Priyanka.Jain@freescale.com>
Date: Fri, 5 Feb 2016 16:13:57 +0530
Subject: [PATCH 0913/1383] powerpc/85xx: Add dpaa ethernet nodes for
 t1042rdb_pi

Signed-off-by: Priyanka Jain <Priyanka.Jain@freescale.com>
[Original patch taken from QorIQ-SDK-V2.0-20160527-yocto]
Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 arch/powerpc/boot/dts/fsl/t1042rdb_pi.dts | 61 +++++++++++++++++++++++++++++++
 1 file changed, 61 insertions(+)

diff --git a/arch/powerpc/boot/dts/fsl/t1042rdb_pi.dts b/arch/powerpc/boot/dts/fsl/t1042rdb_pi.dts
index 98c0010..863f2b0 100644
--- a/arch/powerpc/boot/dts/fsl/t1042rdb_pi.dts
+++ b/arch/powerpc/boot/dts/fsl/t1042rdb_pi.dts
@@ -38,6 +38,11 @@
 / {
 	model = "fsl,T1042RDB_PI";
 	compatible = "fsl,T1042RDB_PI";
+	aliases {
+		phy_rgmii_0 = &phy_rgmii_0;
+		phy_rgmii_1 = &phy_rgmii_1;
+	};
+
 	ifc: localbus@ffe124000 {
 		cpld@3,0 {
 			compatible = "fsl,t1042rdb_pi-cpld";
@@ -51,7 +56,63 @@
 				interrupts = <0x2 0x1 0 0>;
 			};
 		};
+		fman0: fman@400000 {
+
+			enet0: ethernet@e0000 {
+				status = "disabled";
+			};
+
+			enet1: ethernet@e2000 {
+				status = "disabled";
+			};
+
+			enet2: ethernet@e4000 {
+				status = "disabled";
+			};
+
+			enet3: ethernet@e6000 {
+				phy-handle = <&phy_rgmii_0>;
+				phy-connection-type = "rgmii";
+			};
+
+			enet4: ethernet@e8000 {
+				phy-handle = <&phy_rgmii_1>;
+				phy-connection-type = "rgmii";
+			};
+
+			mdio0: mdio@fc000 {
+				phy_rgmii_0: ethernet-phy@01 {
+					reg = <0x01>;
+				};
+				phy_rgmii_1: ethernet-phy@02 {
+					reg = <0x02>;
+				};
+			};
+		};
+	};
+	fsl,dpaa {
+		compatible = "fsl,t1042-dpaa", "fsl,dpaa";
+		ethernet@0 {
+			status = "disabled";
+		};
+		ethernet@1 {
+			status = "disabled";
+		};
+		ethernet@2 {
+			status = "disabled";
+		};
+		ethernet@3 {
+			compatible = "fsl,t1042-dpa-ethernet",
+					"fsl,dpa-ethernet";
+			fsl,fman-mac = <&enet3>;
+		};
+		ethernet@4 {
+			compatible = "fsl,t1042-dpa-ethernet",
+					"fsl,dpa-ethernet";
+			fsl,fman-mac = <&enet4>;
+		};
 	};
 };
 
 #include "t1042si-post.dtsi"
+/include/ "qoriq-dpaa-res3.dtsi"
-- 
