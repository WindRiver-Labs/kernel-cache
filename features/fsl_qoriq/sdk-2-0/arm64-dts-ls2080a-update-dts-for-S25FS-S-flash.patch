From 98aad2a80aa6c1e67e216cc3ac3d6ceac5887bf6 Mon Sep 17 00:00:00 2001
From: Yuan Yao <yao.yuan@nxp.com>
Date: Tue, 2 Feb 2016 14:30:41 +0800
Subject: [PATCH 0620/1429] arm64: dts: ls2080a: update dts for S25FS-S flash

fast-read mode is supported for S25FS-S family flash on ls2080aqds.

Signed-off-by: Yuan Yao <yao.yuan@nxp.com>
[Original patch taken from QorIQ-SDK-V2.0-20160527-yocto]
Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 arch/arm64/boot/dts/freescale/fsl-ls2080a-qds.dts |   12 +++++++++++-
 1 files changed, 11 insertions(+), 1 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-ls2080a-qds.dts b/arch/arm64/boot/dts/freescale/fsl-ls2080a-qds.dts
index fe5067e..9e2d91a 100644
--- a/arch/arm64/boot/dts/freescale/fsl-ls2080a-qds.dts
+++ b/arch/arm64/boot/dts/freescale/fsl-ls2080a-qds.dts
@@ -142,13 +142,23 @@
 
 &qspi {
 	status = "okay";
-	qflash0: s25fl008k {
+	qflash0: s25fs256s1@0 {
 		#address-cells = <1>;
 		#size-cells = <1>;
 		compatible = "st,m25p80";
 		spi-max-frequency = <20000000>;
+		m25p,fast-read;
 		reg = <0>;
 	};
+
+	qflash2: s25fs256s1@2 {
+		#address-cells = <1>;
+		#size-cells = <1>;
+		compatible = "st,m25p80";
+		spi-max-frequency = <20000000>;
+		m25p,fast-read;
+		reg = <2>;
+	};
 };
 
 &sata0 {
