From a28dd87b16d531b1ed118b618054181f6babcb51 Mon Sep 17 00:00:00 2001
From: Kevin Hao <kexin.hao@windriver.com>
Date: Sun, 17 Feb 2013 17:02:51 +0800
Subject: [PATCH 497/547] dts/t4240qds: amend the EEPROM device nodes

* Add two missing EEPROM devices.
    0x50 -- Atmel AT24C64A, store RCW and PBLOADER data.
    0x57 -- Atmel AT24C02A, store board specific data.
* fix the compatible typo for 0x51 ~ 0x56, these EEPROMs are Atmel
  AT24C02 in memory DIMMs.

Signed-off-by: Kevin Hao <kexin.hao@windriver.com>
Signed-off-by: Bin Jiang <bin.jiang@windriver.com>
---
 arch/powerpc/boot/dts/t4240qds.dts | 20 ++++++++++++++------
 1 file changed, 14 insertions(+), 6 deletions(-)

diff --git a/arch/powerpc/boot/dts/t4240qds.dts b/arch/powerpc/boot/dts/t4240qds.dts
index 01c44b5..be186e0 100644
--- a/arch/powerpc/boot/dts/t4240qds.dts
+++ b/arch/powerpc/boot/dts/t4240qds.dts
@@ -372,30 +372,38 @@
 				compatible = "philips,pca9547";
 				reg = <0x77>;
 			};
+			eeprom@50 {
+				compatible = "at24,24c64";
+				reg = <0x50>;
+			};
 			eeprom@51 {
-				compatible = "at24,24c256";
+				compatible = "at24,spd";
 				reg = <0x51>;
 			};
 			eeprom@52 {
-				compatible = "at24,24c256";
+				compatible = "at24,spd";
 				reg = <0x52>;
 			};
 			eeprom@53 {
-				compatible = "at24,24c256";
+				compatible = "at24,spd";
 				reg = <0x53>;
 			};
 			eeprom@54 {
-				compatible = "at24,24c256";
+				compatible = "at24,spd";
 				reg = <0x54>;
 			};
 			eeprom@55 {
-				compatible = "at24,24c256";
+				compatible = "at24,spd";
 				reg = <0x55>;
 			};
 			eeprom@56 {
-				compatible = "at24,24c256";
+				compatible = "at24,spd";
 				reg = <0x56>;
 			};
+			eeprom@57 {
+				compatible = "at24,24c02";
+				reg = <0x57>;
+			};
 			rtc@68 {
 				compatible = "dallas,ds3232";
 				reg = <0x68>;
-- 
