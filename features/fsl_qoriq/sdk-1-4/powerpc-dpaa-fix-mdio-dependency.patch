From 180b15a1ab0a244bb145cc781a51096bfaf98cb7 Mon Sep 17 00:00:00 2001
From: Shaohui Xie <Shaohui.Xie@freescale.com>
Date: Thu, 25 Apr 2013 15:13:49 +0800
Subject: [PATCH 058/547] powerpc/dpaa: fix mdio dependency

The mdio dependency should be changed from DPA_ETH to FSL_DPAA_ETH.

Signed-off-by: Shaohui Xie <Shaohui.Xie@freescale.com>
Change-Id: I97697d33dbd1f8da2b504bb45b8d45aa489a0a96
Reviewed-on: http://git.am.freescale.net:8181/2081
Reviewed-by: Bucur Madalin-Cristian-B32716 <madalin.bucur@freescale.com>
Reviewed-by: Hamciuc Bogdan-BHAMCIU1 <bogdan.hamciuc@freescale.com>
Reviewed-by: Zang Tiefei-R61911 <tie-fei.zang@freescale.com>
Reviewed-by: Fleming Andrew-AFLEMING <AFLEMING@freescale.com>
Tested-by: Fleming Andrew-AFLEMING <AFLEMING@freescale.com>
[Original patch taken from QorIQ-SDK-V1.4-SOURCE-20130625-yocto.iso]
Signed-off-by: Bin Jiang <bin.jiang@windriver.com>
---
 arch/powerpc/platforms/85xx/Kconfig | 22 +++++++++++-----------
 1 file changed, 11 insertions(+), 11 deletions(-)

diff --git a/arch/powerpc/platforms/85xx/Kconfig b/arch/powerpc/platforms/85xx/Kconfig
index 30247a8..ee428a2 100644
--- a/arch/powerpc/platforms/85xx/Kconfig
+++ b/arch/powerpc/platforms/85xx/Kconfig
@@ -244,8 +244,8 @@ config P3041_DS
 	select HAS_RAPIDIO
 	select PPC_EPAPR_HV_PIC
         select HAS_FSL_QBMAN
-	select MDIO_BUS_MUX if DPA_ETH
-	select MDIO_BUS_MUX_MMIOREG if DPA_ETH
+	select MDIO_BUS_MUX if FSL_DPAA_ETH
+	select MDIO_BUS_MUX_MMIOREG if FSL_DPAA_ETH
 	help
 	  This option enables support for the P3041 DS board
 
@@ -260,7 +260,7 @@ config P4080_DS
 	select HAS_RAPIDIO
 	select PPC_EPAPR_HV_PIC
         select HAS_FSL_QBMAN
-	select MDIO_BUS_MUX if DPA_ETH
+	select MDIO_BUS_MUX if FSL_DPAA_ETH
 	select MDIO_BUS_MUX_GPIO
 	help
 	  This option enables support for the P4080 DS board
@@ -287,8 +287,8 @@ config P5020_DS
 	select HAS_RAPIDIO
 	select PPC_EPAPR_HV_PIC
         select HAS_FSL_QBMAN
-	select MDIO_BUS_MUX if DPA_ETH
-	select MDIO_BUS_MUX_MMIOREG if DPA_ETH
+	select MDIO_BUS_MUX if FSL_DPAA_ETH
+	select MDIO_BUS_MUX_MMIOREG if FSL_DPAA_ETH
 	help
 	  This option enables support for the P5020 DS board
 
@@ -308,8 +308,8 @@ config P5040_DS
 	select HAS_RAPIDIO
 	select PPC_EPAPR_HV_PIC
         select HAS_FSL_QBMAN
-	select MDIO_BUS_MUX if DPA_ETH
-	select MDIO_BUS_MUX_MMIOREG if DPA_ETH
+	select MDIO_BUS_MUX if FSL_DPAA_ETH
+	select MDIO_BUS_MUX_MMIOREG if FSL_DPAA_ETH
 	help
 	  This option enables support for the P5040 DS board
 
@@ -342,8 +342,8 @@ config T4240_QDS
 	select HAS_RAPIDIO
 	select PPC_EPAPR_HV_PIC
 	select HAS_FSL_QBMAN
-	select MDIO_BUS_MUX if DPA_ETH
-	select MDIO_BUS_MUX_MMIOREG if DPA_ETH
+	select MDIO_BUS_MUX if FSL_DPAA_ETH
+	select MDIO_BUS_MUX_MMIOREG if FSL_DPAA_ETH
 	help
 	  This option enables support for the T4240 QDS board
 
@@ -359,8 +359,8 @@ config B4_QDS
 	select HAS_RAPIDIO
 	select PPC_EPAPR_HV_PIC
 	select HAS_FSL_QBMAN
-	select MDIO_BUS_MUX if DPA_ETH
-	select MDIO_BUS_MUX_MMIOREG if DPA_ETH
+	select MDIO_BUS_MUX if FSL_DPAA_ETH
+	select MDIO_BUS_MUX_MMIOREG if FSL_DPAA_ETH
 	help
 	  This option enables support for the B4 QDS board
 	  The B4 application development system B4 QDS is a complete
