+/include/ "p2020rdb-pc_32b.dts"
+
+/ {
+	model = "fsl,P2020RDB";
+	compatible = "fsl,P2020RDB-PC", "fsl,MPC85XXRDB-CAMP";
+
+	cpus {
+		PowerPC,P2020@1 {
+			status = "disabled";
+		};
+	};
+
+	lbc: localbus@ffe05000 {
+		status = "disabled";
+	};
+
+	soc: soc@ffe00000 {
+		serial1: serial@4600 {
+			status = "disabled";
+		};
+
+		dma@c300 {
+			status = "disabled";
+		};
+
+		enet0: ethernet@24000 {
+			status = "disabled";
+		};
+
+		mpic: pic@40000 {
+			protected-sources = <
+			42 76 77 78 79 /* serial1 , dma2 */
+			29 30 34 26 /* enet0, pci1 */
+			0xe0 0xe1 0xe2 0xe3 /* msi */
+			0xe4 0xe5 0xe6 0xe7
+			>;
+		};
+
+		msi@41600 {
+			status = "disabled";
+		};
+	};
+
+	rio: rapidio@ffec0000 {
+		status = "disabled";
+	};
+
+	pci2: pcie@ffe08000 {
+		status = "disabled";
+	};
+
+	pci0: pcie@ffe0a000 {
+		status = "disabled";
+	};
+};
