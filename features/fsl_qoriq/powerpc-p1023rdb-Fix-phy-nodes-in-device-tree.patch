From bfbbbe3b4a93033c8805f1067f0cb1a9b04bf907 Mon Sep 17 00:00:00 2001
From: Wei Yang <Wei.Yang@windriver.com>
Date: Mon, 25 Mar 2013 13:32:30 +0800
Subject: [PATCH 18/21] powerpc/p1023rdb: Fix phy nodes in device tree

Signed-off-by: John Rigby <jcrigby@silicontkx.com>
[Extracted from SDK 1.2.1 for p1023, minor patch context modifications.]
Signed-off-by: Wei Yang <Wei.Yang@windriver.com>
---
 arch/powerpc/boot/dts/p1023rdb.dts |   10 +++++-----
 1 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/arch/powerpc/boot/dts/p1023rdb.dts b/arch/powerpc/boot/dts/p1023rdb.dts
index 423d41c..e0581ad 100644
--- a/arch/powerpc/boot/dts/p1023rdb.dts
+++ b/arch/powerpc/boot/dts/p1023rdb.dts
@@ -106,17 +106,17 @@
 				phy-connection-type = "rgmii";
 			};
 			enet1: ethernet@e2000 {
-				phy-handle = <&phy1>;
-				phy-connection-type = "rgmii";
 				/* Port 1 is connected directly to mv88e6165 switch */
 				fixed-link = <3 1 1000 0 0>;
+				phy-handle = <&phy1>;
+				phy-connection-type = "rgmii";
 			};
 			mdio0: mdio@e1120 {
-				phy0: ethernet-phy@2 {
+				phy0: ethernet-phy@1 {
 					reg = <0x01>;
 				};
-				phy1: ethernet-phy@7 {
-					reg = <0x03>;
+				phy1: ethernet-phy@2 {
+					reg = <0x02>;
 				};
 			};
 		};
-- 
