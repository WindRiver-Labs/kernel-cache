From 37b6414c5769f2acdf2ec9451251ce9ab5438240 Mon Sep 17 00:00:00 2001
From: Bogdan Hamciuc <bogdan.hamciuc@freescale.com>
Date: Thu, 4 Oct 2012 17:23:02 +0300
Subject: [PATCH 051/162] dpaa_eth: Remove FCoE support in the presence of
 DPAA-Eth

Due to a temporary hack we have made in the netdevice's list of
capabilities (linux/netdevice.h), disabling FCoE to avoid any related
problems.

Signed-off-by: Bogdan Hamciuc <bogdan.hamciuc@freescale.com>
[Kevin: Original patch taken from FSL
QorIQ-SDK-V1.3-SOURCE-20121114-yocto.iso tarball.]
Signed-off-by: Kevin Hao <kexin.hao@windriver.com>
---
 drivers/net/ethernet/freescale/Kconfig |    2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/drivers/net/ethernet/freescale/Kconfig b/drivers/net/ethernet/freescale/Kconfig
index 8494841..f821951 100644
--- a/drivers/net/ethernet/freescale/Kconfig
+++ b/drivers/net/ethernet/freescale/Kconfig
@@ -116,7 +116,7 @@ config GIANFAR_L2SRAM
 
 config DPA
 	bool "Freescale Data Path Frame Manager Ethernet"
-	depends on FSL_SOC && FSL_BMAN && FSL_QMAN && FSL_FMAN
+	depends on FSL_SOC && FSL_BMAN && FSL_QMAN && FSL_FMAN && LIBFCOE=n
 	select PHYLIB
 
 config DPA_OFFLINE_PORTS
-- 
1.7.9.7

