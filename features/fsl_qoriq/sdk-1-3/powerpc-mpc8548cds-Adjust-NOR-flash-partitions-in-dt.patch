From f745b211ae2b1eae52b857c807b2baa51c5cc0b7 Mon Sep 17 00:00:00 2001
From: Jiang Bin <bin.jiang@windriver.com>
Date: Wed, 31 Oct 2012 17:15:42 +0800
Subject: [PATCH 4/6] powerpc/mpc8548cds: Adjust NOR flash partitions in dts

Modify first partition as a user area partition.
Expand kernel-nor partition to 5M.
Adjust partition address.

Signed-off-by: Jiang Bin <bin.jiang@windriver.com>
---
 arch/powerpc/boot/dts/mpc8548cds.dtsi |   14 +++++++-------
 1 file changed, 7 insertions(+), 7 deletions(-)

diff --git a/arch/powerpc/boot/dts/mpc8548cds.dtsi b/arch/powerpc/boot/dts/mpc8548cds.dtsi
index 9f7be4f..97136db 100644
--- a/arch/powerpc/boot/dts/mpc8548cds.dtsi
+++ b/arch/powerpc/boot/dts/mpc8548cds.dtsi
@@ -42,27 +42,27 @@
 		device-width = <2>;
 
 		partition@0 {
-			reg = <0x0 0x0b00000>;
-			label = "ramdisk-nor";
+			reg = <0x0 0x0a00000>;
+			label = "user-nor";
 		};
 
-		partition@300000 {
-			reg = <0x0b00000 0x0400000>;
+		partition@a00000 {
+			reg = <0x0a00000 0x0500000>;
 			label = "kernel-nor";
 		};
 
-		partition@700000 {
+		partition@f00000 {
 			reg = <0x0f00000 0x060000>;
 			label = "dtb-nor";
 		};
 
-		partition@760000 {
+		partition@f60000 {
 			reg = <0x0f60000 0x020000>;
 			label = "env-nor";
 			read-only;
 		};
 
-		partition@780000 {
+		partition@f80000 {
 			reg = <0x0f80000 0x080000>;
 			label = "u-boot-nor";
 			read-only;
-- 
