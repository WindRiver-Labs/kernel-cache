+++ b/drivers/net/ethernet/freescale/fman/Peripherals/FM/fm.h
@@ -543,6 +543,7 @@ typedef struct
     uint8_t                     fmId;
     e_FmPortType                portsTypes[FM_MAX_NUM_OF_HW_PORT_IDS];
