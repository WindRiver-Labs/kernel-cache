+++ b/drivers/net/ethernet/freescale/fman/inc/Peripherals/fm_ext.h
@@ -483,6 +483,9 @@ typedef struct t_FmParams {
                                                          to be used by the FM. */
     uint16_t                fmClkFreq;              /**< In Mhz;
                                                          Relevant when FM not runs in "guest-mode". */
