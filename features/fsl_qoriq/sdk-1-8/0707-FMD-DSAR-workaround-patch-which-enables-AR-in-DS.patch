-				p_Fm->p_FmFpmRegs->fmfp_brkc) | FPM_BRKC_SLP);
-		else
-			WRITE_UINT32(p_Fm->p_FmFpmRegs->fmfp_brkc, GET_UINT32(
-				p_Fm->p_FmFpmRegs->fmfp_brkc) & ~FPM_BRKC_SLP);
