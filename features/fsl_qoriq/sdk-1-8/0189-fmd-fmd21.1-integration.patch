 	/* Setting up MII Management Control Register with data */
 	iowrite32be((uint32_t)data, &regs->miimcon);
 	/* Dummy read to make sure MIIMCON is written */
 	tmp = ioread32be(&regs->miimcon);
+	wmb();
 
