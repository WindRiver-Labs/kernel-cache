 	layout[RX].hash_results = true;
 #if defined(CONFIG_FSL_DPAA_1588) || defined(CONFIG_FSL_DPAA_TS)
@@ -697,7 +697,7 @@ dpa_bp_alloc(struct dpa_bp *dpa_bp)
 		return -ENODEV;
 	}
 
