 	} else {
 		fq->flags = QMAN_FQ_FLAG_NO_MODIFY;
 	}
@@ -1065,7 +1065,7 @@ void dpa_fq_setup(struct dpa_priv_s *priv, const struct dpa_fq_cbs_t *fq_cbs,
