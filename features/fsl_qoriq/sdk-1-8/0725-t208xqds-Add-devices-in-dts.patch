From 1d7cd98285a1d177b7635370b3b47b64924f8506 Mon Sep 17 00:00:00 2001
From: Jiang Lu <lu.jiang@windriver.com>
Date: Thu, 17 Jul 2014 16:53:54 +0800
Subject: [PATCH 725/987] t208xqds:Add devices in dts

Add following devices to device-tree:

1.Partition info for 128M NOR flash.
2.Temperature sensor on i2c bus.

Signed-off-by: Jiang Lu <lu.jiang@windriver.com>
[Delete redundant part for upstream 3b6b17900bfa]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 arch/powerpc/boot/dts/t208xqds.dtsi | 65 +++++++++++++++++++++++++++++++++++++
 1 file changed, 65 insertions(+)

diff --git a/arch/powerpc/boot/dts/t208xqds.dtsi b/arch/powerpc/boot/dts/t208xqds.dtsi
index 0c1cc2d7..b1169fc 100644
--- a/arch/powerpc/boot/dts/t208xqds.dtsi
+++ b/arch/powerpc/boot/dts/t208xqds.dtsi
@@ -63,6 +63,71 @@
 			reg = <0x0 0x0 0x8000000>;
 			bank-width = <2>;
 			device-width = <1>;
+
+			partition@0 {
+				label = "rcw";
+				reg = <0x0 0x20000>;
+				read-only;
+			};
+			partition@20000 {
+				label = "kernel1";
+				reg = <0x20000 0x7e0000>;
+			};
+			partition@800000 {
+				label = "dtb1";
+				reg = <0x800000 0x100000>;
+			};
+			partition@900000 {
+				label = "fs1";
+				reg = <0x900000 0x3600000>;
+			};
+			partition@3f00000 {
+				label = "alt-fman-firmware";
+				reg = <0x3f00000 0x20000>;
+				read-only;
+			};
+			partition@3f20000 {
+				label = "alt-uboot-env";
+				reg = <0x3f20000 0x20000>;
+				read-only;
+			};
+			partition@3f40000 {
+				label = "alt-u-boot";
+				reg = <0x3f40000 0xc0000>;
+				read-only;
+			};
+			partition@4000000 {
+				label = "alt-rcw";
+				reg = <0x4000000 0x20000>;
+				read-only;
+			};
+			partition@4020000 {
+				label = "kernel2";
+				reg = <0x4020000 0x7e0000>;
+			};
+			partition@4800000 {
+				label = "dtb2";
+				reg = <0x4800000 0x100000>;
+			};
+			partition@4900000 {
+				label = "fs2";
+				reg = <0x4900000 0x3600000>;
+			};
+			partition@7f00000 {
+				label = "fman-firmware";
+				reg = <0x7f00000 0x20000>;
+				read-only;
+			};
+			partition@7f20000 {
+				label = "u-boot env";
+				reg = <0x7f20000  0x20000>;
+				read-only;
+			};
+			partition@7f40000 {
+				label = "u-boot";
+				reg = <0x7f40000 0xc0000>;
+				read-only;
+			};
 		};
 
 		nand@2,0 {
