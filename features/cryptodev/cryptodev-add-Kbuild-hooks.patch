From 8a9ed4b669ae373925cfd61afa39617b185e7a30 Mon Sep 17 00:00:00 2001
From: Bruce Ashfield <bruce.ashfield@windriver.com>
Date: Tue, 23 Sep 2014 15:54:01 -0400
Subject: [PATCH 292/292] cryptodev: add Kbuild hooks

Hook cryptodev-linux into the kernel build as a tristate option.

Signed-off-by: Bruce Ashfield <bruce.ashfield@windriver.com>
---
 crypto/Makefile | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/crypto/Makefile b/crypto/Makefile
index d1a02f817f9a..6e2098ea9518 100644
--- a/crypto/Makefile
+++ b/crypto/Makefile
@@ -104,7 +104,7 @@ obj-$(CONFIG_CRYPTO_USER_API_SKCIPHER) += algif_skcipher.o
 obj-$(CONFIG_XOR_BLOCKS) += xor.o
 obj-$(CONFIG_ASYNC_CORE) += async_tx/
 obj-$(CONFIG_ASYMMETRIC_KEY_TYPE) += asymmetric_keys/
-obj-$(CONFIG_CRYPTODEV) += cryptodev/
+obj-$(CONFIG_CRYPTODEV) += ../drivers/staging/crypto/cryptodev/
 
 obj-$(CONFIG_CRYPTO_HASH_INFO) += hash_info.o
 obj-$(CONFIG_CRYPTO_ABLK_HELPER) += ablk_helper.o
