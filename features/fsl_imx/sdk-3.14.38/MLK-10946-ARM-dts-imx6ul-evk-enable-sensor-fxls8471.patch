From 0255e4f97efd0e2cde7f1d7dbecad531ba7e1a40 Mon Sep 17 00:00:00 2001
From: gaopan <b54642@freescale.com>
Date: Thu, 21 May 2015 14:29:31 +0800
Subject: [PATCH 1115/1594] MLK-10946 ARM: dts: imx6ul-evk: enable sensor
 fxls8471

commit cb75ac1d83e3030e889c4b6f4861d8f2cd666dd6 from
git://git.freescale.com/imx/linux-2.6-imx.git

Enable sensor fxls8471.

Signed-off-by: Gao Pan <b54642@freescale.com>
---
 arch/arm/boot/dts/imx6ul-evk.dts |    9 +++++++++
 1 files changed, 9 insertions(+), 0 deletions(-)

diff --git a/arch/arm/boot/dts/imx6ul-evk.dts b/arch/arm/boot/dts/imx6ul-evk.dts
index a7bd870c..fc8fa31 100755
--- a/arch/arm/boot/dts/imx6ul-evk.dts
+++ b/arch/arm/boot/dts/imx6ul-evk.dts
@@ -310,6 +310,14 @@
 		reg = <0x0e>;
 		position = <2>;
 	};
+
+	fxls8471@1e {
+		compatible = "fsl,fxls8471";
+		reg = <0x1e>;
+		position = <0>;
+		interrupt-parent = <&gpio5>;
+		interrupts = <0 8>;
+	};
 };
 
 &i2c2 {
@@ -380,6 +388,7 @@
 				MX6UL_PAD_ENET2_TX_DATA0__ENET2_TDATA00	0x1b0b0
 				MX6UL_PAD_ENET2_TX_DATA1__ENET2_TDATA01	0x1b0b0
 				MX6UL_PAD_ENET2_TX_CLK__ENET2_REF_CLK2	0x4001b031
+				MX6UL_PAD_SNVS_TAMPER0__GPIO5_IO00	0x80000000
 			>;
 		};
 
