From 02706367a143c08a94bab97a8ce2e2767d8a05aa Mon Sep 17 00:00:00 2001
From: Sandor Yu <R01008@freescale.com>
Date: Fri, 20 Mar 2015 11:32:49 +0800
Subject: [PATCH 0547/1594] MLK-10447-02: dts: Add rest pin to imx7d sdb fec1

commit 3ec9acb723f510f3e29c28f2dc6f6dcb6525dc5b from
git://git.freescale.com/imx/linux-2.6-imx.git

-Add spi gpio pin ENET_RST_B to fec1.

Signed-off-by: Sandor Yu <R01008@freescale.com>
---
 arch/arm/boot/dts/imx7d-sdb.dts |    1 +
 1 files changed, 1 insertions(+), 0 deletions(-)

diff --git a/arch/arm/boot/dts/imx7d-sdb.dts b/arch/arm/boot/dts/imx7d-sdb.dts
index 875b4c1..445daa0 100644
--- a/arch/arm/boot/dts/imx7d-sdb.dts
+++ b/arch/arm/boot/dts/imx7d-sdb.dts
@@ -90,6 +90,7 @@
 &fec1 {
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_enet1>;
+	pinctrl-assert-gpios = <&gpio_spi 5 GPIO_ACTIVE_HIGH>;
 	phy-mode = "rgmii";
 	phy-handle = <&ethphy0>;
 	fsl,magic-packet;
