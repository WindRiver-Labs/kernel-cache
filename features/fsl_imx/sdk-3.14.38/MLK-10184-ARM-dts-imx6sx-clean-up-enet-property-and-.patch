From 3f2f54f9f65b37209a158323b567aa0d9909e45a Mon Sep 17 00:00:00 2001
From: Fugang Duan <b38611@freescale.com>
Date: Fri, 30 Jan 2015 13:43:03 +0800
Subject: [PATCH 0823/1594] MLK-10184 ARM: dts: imx6sx: clean up enet property
 and enable enet2 multi-queue

commit f36f39489e7e3ec1bc625d9a260d797693adeb44 from
git://git.freescale.com/imx/linux-2.6-imx.git

Clean up enet property and enebale enet2 multi-queue.

Signed-off-by: Fugang Duan <B38611@freescale.com>
---
 arch/arm/boot/dts/imx6sx-17x17-arm2.dts |    4 ----
 arch/arm/boot/dts/imx6sx-19x19-arm2.dts |    4 ----
 arch/arm/boot/dts/imx6sx-sabreauto.dts  |    4 ----
 arch/arm/boot/dts/imx6sx.dtsi           |    2 ++
 4 files changed, 2 insertions(+), 12 deletions(-)

diff --git a/arch/arm/boot/dts/imx6sx-17x17-arm2.dts b/arch/arm/boot/dts/imx6sx-17x17-arm2.dts
index b989ba7..936c7a6 100644
--- a/arch/arm/boot/dts/imx6sx-17x17-arm2.dts
+++ b/arch/arm/boot/dts/imx6sx-17x17-arm2.dts
@@ -146,8 +146,6 @@
 	pinctrl-0 = <&pinctrl_enet1_1>;
 	phy-mode = "rgmii";
 	phy-handle = <&ethphy1>;
-	fsl,num_tx_queues=<3>;
-	fsl,num_rx_queues=<3>;
 	pinctrl-assert-gpios = <&max7322_1 0 GPIO_ACTIVE_HIGH>;
 	fsl,magic-packet;
 	status = "okay";
@@ -173,8 +171,6 @@
 	pinctrl-0 = <&pinctrl_enet2_1>;
 	phy-mode = "rgmii";
 	phy-handle = <&ethphy0>;
-	fsl,num_tx_queues=<3>;
-	fsl,num_rx_queues=<3>;
 	pinctrl-assert-gpios = <&max7322_2 0 GPIO_ACTIVE_HIGH>;
 	fsl,magic-packet;
 	status = "okay";
diff --git a/arch/arm/boot/dts/imx6sx-19x19-arm2.dts b/arch/arm/boot/dts/imx6sx-19x19-arm2.dts
index 9555c8c..f1092fe 100644
--- a/arch/arm/boot/dts/imx6sx-19x19-arm2.dts
+++ b/arch/arm/boot/dts/imx6sx-19x19-arm2.dts
@@ -119,8 +119,6 @@
 	pinctrl-0 = <&pinctrl_enet1_1>;
 	phy-mode = "rgmii";
 	phy-handle = <&ethphy1>;
-	fsl,num_tx_queues=<3>;
-	fsl,num_rx_queues=<3>;
 	pinctrl-assert-gpios = <&max7322_1 0 GPIO_ACTIVE_HIGH>;
 	fsl,magic-packet;
 	status = "okay";
@@ -146,8 +144,6 @@
 	pinctrl-0 = <&pinctrl_enet2_1>;
 	phy-mode = "rgmii";
 	phy-handle = <&ethphy0>;
-	fsl,num_tx_queues=<3>;
-	fsl,num_rx_queues=<3>;
 	pinctrl-assert-gpios = <&max7322_2 0 GPIO_ACTIVE_HIGH>;
 	fsl,magic-packet;
 	status = "okay";
diff --git a/arch/arm/boot/dts/imx6sx-sabreauto.dts b/arch/arm/boot/dts/imx6sx-sabreauto.dts
index 3f03b25..8454788 100644
--- a/arch/arm/boot/dts/imx6sx-sabreauto.dts
+++ b/arch/arm/boot/dts/imx6sx-sabreauto.dts
@@ -553,8 +553,6 @@
 	pinctrl-0 = <&pinctrl_enet1_1>;
 	phy-mode = "rgmii";
 	phy-handle = <&ethphy1>;
-	fsl,num_tx_queues=<3>;
-	fsl,num_rx_queues=<3>;
 	pinctrl-assert-gpios = <&max7322 0 GPIO_ACTIVE_HIGH>;
 	fsl,magic-packet;
 	status = "okay";
@@ -580,8 +578,6 @@
 	pinctrl-0 = <&pinctrl_enet2_1>;
 	phy-mode = "rgmii";
 	phy-handle = <&ethphy0>;
-	fsl,num_tx_queues=<3>;
-	fsl,num_rx_queues=<3>;
 	fsl,magic-packet;
 	status = "okay";
 };
diff --git a/arch/arm/boot/dts/imx6sx.dtsi b/arch/arm/boot/dts/imx6sx.dtsi
index 20bdbaf..23508af 100644
--- a/arch/arm/boot/dts/imx6sx.dtsi
+++ b/arch/arm/boot/dts/imx6sx.dtsi
@@ -1079,6 +1079,8 @@
 					 <&clks IMX6SX_CLK_ENET_PTP>;
 				clock-names = "ipg", "ahb", "ptp",
 					      "enet_clk_ref", "enet_out";
+				fsl,num-tx-queues=<3>;
+				fsl,num-rx-queues=<3>;
 				status = "disabled";
 			};
 
