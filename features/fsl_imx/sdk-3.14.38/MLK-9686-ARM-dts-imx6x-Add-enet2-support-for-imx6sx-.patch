From 1179fce371fbcbac9083e5e53ad62d8c980c1574 Mon Sep 17 00:00:00 2001
From: Fugang Duan <b38611@freescale.com>
Date: Tue, 23 Sep 2014 17:20:50 +0800
Subject: [PATCH 0282/1594] MLK-9686 ARM: dts: imx6x: Add enet2 support for
 imx6sx-sdb board

commit af8d543c69d3a172f73c2ab8efa84a289e73a260 from
git://git.freescale.com/imx/linux-2.6-imx.git

Add enet2 support for imx6sx-sdb board, and add the "fsl,imx6q-fec"
compatible for fec2 node to be compatible with the old version.

Signed-off-by: Fugang Duan <B38611@freescale.com>
---
 arch/arm/boot/dts/imx6sx-sdb.dtsi |    7 +++++++
 1 files changed, 7 insertions(+), 0 deletions(-)

diff --git a/arch/arm/boot/dts/imx6sx-sdb.dtsi b/arch/arm/boot/dts/imx6sx-sdb.dtsi
index 69b6626..508a750 100644
--- a/arch/arm/boot/dts/imx6sx-sdb.dtsi
+++ b/arch/arm/boot/dts/imx6sx-sdb.dtsi
@@ -206,8 +206,11 @@
 &fec1 {
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_enet1>;
+	pinctrl-assert-gpios = <&gpio4 16 GPIO_ACTIVE_HIGH>, <&gpio2 6 GPIO_ACTIVE_LOW>;
 	phy-supply = <&reg_enet_3v3>;
 	phy-mode = "rgmii";
+	fsl,num_tx_queues=<3>;
+	fsl,num_rx_queues=<3>;
 	phy-handle = <&ethphy1>;
 	status = "okay";
 
@@ -230,6 +233,8 @@
 	pinctrl-0 = <&pinctrl_enet2>;
 	phy-mode = "rgmii";
 	phy-handle = <&ethphy2>;
+	fsl,num_tx_queues=<3>;
+	fsl,num_rx_queues=<3>;
 	status = "okay";
 };
 
@@ -531,6 +536,8 @@
 				MX6SX_PAD_RGMII1_RD2__ENET1_RX_DATA_2	0x3081
 				MX6SX_PAD_RGMII1_RD3__ENET1_RX_DATA_3	0x3081
 				MX6SX_PAD_RGMII1_RX_CTL__ENET1_RX_EN	0x3081
+				MX6SX_PAD_QSPI1A_DATA0__GPIO4_IO_16	0x80000000
+				MX6SX_PAD_QSPI1A_DATA3__GPIO4_IO_19	0x80000000
 				MX6SX_PAD_ENET2_RX_CLK__ENET2_REF_CLK_25M	0x91
 			>;
 		};
