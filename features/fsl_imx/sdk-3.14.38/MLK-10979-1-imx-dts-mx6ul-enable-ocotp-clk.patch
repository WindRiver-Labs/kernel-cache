From 2ae1c7a4ca55a87e33da464ec545f3ac2e17222d Mon Sep 17 00:00:00 2001
From: Peng Fan <Peng.Fan@freescale.com>
Date: Fri, 29 May 2015 13:55:37 +0800
Subject: [PATCH 1143/1594] MLK-10979-1 imx: dts: mx6ul enable ocotp clk

commit e800dd533a81a7e7a04d8e86765fd45725a1dfc9 from
git://git.freescale.com/imx/linux-2.6-imx.git

Change ocotp clock from dummy to IMX6UL_CLK_OCOTP

Signed-off-by: Peng Fan <Peng.Fan@freescale.com>
---
 arch/arm/boot/dts/imx6ul.dtsi |    4 ++--
 1 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/imx6ul.dtsi b/arch/arm/boot/dts/imx6ul.dtsi
index bf6aaa3..803ca3d 100644
--- a/arch/arm/boot/dts/imx6ul.dtsi
+++ b/arch/arm/boot/dts/imx6ul.dtsi
@@ -879,13 +879,13 @@
 			ocotp: ocotp-ctrl@021bc000 {
 				compatible = "syscon";
 				reg = <0x021bc000 0x4000>;
-				clocks = <&clks IMX6UL_CLK_DUMMY>;
+				clocks = <&clks IMX6UL_CLK_OCOTP>;
 			};
 
 			ocotp-fuse@021bc000 {
 				compatible = "fsl,imx6ul-ocotp", "fsl,imx6q-ocotp";
 				reg = <0x021bc000 0x4000>;
-				clocks = <&clks IMX6UL_CLK_DUMMY>;
+				clocks = <&clks IMX6UL_CLK_OCOTP>;
 			};
 
 			csu: csu@021c0000 {
-- 
