From 0c49bfab1655edccf595bba3f4c0d3c24fdd8088 Mon Sep 17 00:00:00 2001
From: Robby Cai <r63905@freescale.com>
Date: Thu, 23 Oct 2014 20:51:33 +0800
Subject: [PATCH 0232/1594] MLK-9719-4 ARM: dts: imx6qdl-sabreauto: Add V4L2
 output support

commit 381a9d5ea8dda54f72e9af6d4ba1493d08616d44 from
git://git.freescale.com/imx/linux-2.6-imx.git

Add v4l2 output support for imx6qdl sabreauto board

Signed-off-by: Robby Cai <r63905@freescale.com>
---
 arch/arm/boot/dts/imx6qdl-sabreauto.dtsi |    5 +++++
 1 files changed, 5 insertions(+), 0 deletions(-)

diff --git a/arch/arm/boot/dts/imx6qdl-sabreauto.dtsi b/arch/arm/boot/dts/imx6qdl-sabreauto.dtsi
index a97960f..9b67a31 100644
--- a/arch/arm/boot/dts/imx6qdl-sabreauto.dtsi
+++ b/arch/arm/boot/dts/imx6qdl-sabreauto.dtsi
@@ -181,6 +181,11 @@
 		mux-ext-port = <5>;
 	};
 
+	v4l2_out {
+		compatible = "fsl,mxc_v4l2_output";
+		status = "okay";
+	};
+
 	backlight {
 		compatible = "pwm-backlight";
 		pwms = <&pwm3 0 5000000>;
