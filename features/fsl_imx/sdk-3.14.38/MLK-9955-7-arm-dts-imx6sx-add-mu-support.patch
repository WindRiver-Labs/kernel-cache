From c83df9815961cdfe424aaf7920f1421d0b2103a9 Mon Sep 17 00:00:00 2001
From: Anson Huang <b20788@freescale.com>
Date: Thu, 4 Dec 2014 10:06:52 +0800
Subject: [PATCH 0353/1594] MLK-9955-7 arm: dts: imx6sx: add mu support

commit 1b6b90e424d72b72d5d2d4a99d4b3fd82a5a40e4 from
git://git.freescale.com/imx/linux-2.6-imx.git

add MU support for i.MX6SX.

Signed-off-by: Anson Huang <b20788@freescale.com>
---
 arch/arm/boot/dts/imx6sx.dtsi |    7 +++++++
 1 files changed, 7 insertions(+), 0 deletions(-)

diff --git a/arch/arm/boot/dts/imx6sx.dtsi b/arch/arm/boot/dts/imx6sx.dtsi
index a77ab8b..429a068 100644
--- a/arch/arm/boot/dts/imx6sx.dtsi
+++ b/arch/arm/boot/dts/imx6sx.dtsi
@@ -1339,6 +1339,13 @@
 				status = "okay";
 			};
 
+			mu: mu@02294000 { /* mu */
+				compatible = "fsl,imx6sx-mu";
+				reg = <0x02294000 0x4000>;
+				interrupts = <0 90 0x04>;
+				status = "okay";
+			};
+
 			uart6: serial@022a0000 {
 				compatible = "fsl,imx6sx-uart", "fsl,imx21-uart";
 				reg = <0x022a0000 0x4000>;
-- 
