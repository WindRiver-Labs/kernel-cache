+#define IMX6SX_GPR1_FEC_CLOCK_MUX_SEL_EXT       (0x3 << 13)
+
+#define IMX6SX_GPR4_FEC_ENET1_STOP_REQ		(0x1 << 3)
+#define IMX6SX_GPR4_FEC_ENET2_STOP_REQ		(0x1 << 4)
+
