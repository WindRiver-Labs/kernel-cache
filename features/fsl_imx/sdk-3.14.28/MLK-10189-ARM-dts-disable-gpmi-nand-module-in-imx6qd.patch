From 50630fb2131b73d660a08ac820a52b236df19d0f Mon Sep 17 00:00:00 2001
From: Allen Xu <b45815@freescale.com>
Date: Fri, 30 Jan 2015 01:57:44 +0800
Subject: [PATCH 1468/1543] MLK-10189: ARM:dts: disable gpmi nand module in
 imx6qdl sabreauto dts

commit 61f5ff47358e4c1c31fbed3ccbce722d216ee429 from
git://git.freescale.com/imx/linux-2.6-imx.git

disable gpmi nand module in imx6qdl default dts since it conflicts with
uart3

Signed-off-by: Allen Xu <b45815@freescale.com>
---
 arch/arm/boot/dts/imx6qdl-sabreauto.dtsi |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/arch/arm/boot/dts/imx6qdl-sabreauto.dtsi b/arch/arm/boot/dts/imx6qdl-sabreauto.dtsi
index 0987f86..ea296c9 100644
--- a/arch/arm/boot/dts/imx6qdl-sabreauto.dtsi
+++ b/arch/arm/boot/dts/imx6qdl-sabreauto.dtsi
@@ -325,7 +325,7 @@
 &gpmi {
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_gpmi_nand>;
-	status = "okay";
+	status = "disabled"; /* pin conflict with uart3 */
 	nand-on-flash-bbt;
 };
 
