From: Ralf Baechle <ralf@linux-mips.org>
Date: Mon, 12 Oct 2009 22:30:06 +0200
Subject: [PATCH 095/117] MIPS: Kconfig: Fix duplicate default value for MIPS_L1_CACHE_SHIFT.

commit cfef8b5f1018c25ae90cd60aee1b68ab47e22413 from linux-mips

Signed-off-by: Ralf Baechle <ralf@linux-mips.org>
(cherry picked from commit cf167f4464860123617126311872b964957528a4)
---
 arch/mips/Kconfig |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/arch/mips/Kconfig b/arch/mips/Kconfig
index befb073..26dd6ad 100644
--- a/arch/mips/Kconfig
+++ b/arch/mips/Kconfig
@@ -912,8 +912,8 @@ config BOOT_ELF32
 config MIPS_L1_CACHE_SHIFT
 	int
 	default "4" if MACH_DECSTATION || MIKROTIK_RB532
+	default "4" if MACH_DECSTATION || MIKROTIK_RB532 || PMC_MSP4200_EVAL
 	default "7" if SGI_IP22 || SGI_IP27 || SGI_IP28 || SNI_RM
-	default "4" if PMC_MSP4200_EVAL
 	default "5"
 
 config HAVE_STD_PC_SERIAL_PORT
