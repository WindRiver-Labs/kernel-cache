+++ b/drivers/staging/lttng/instrumentation/syscalls/headers/syscalls_pointers.h
@@ -1,4 +1,3 @@
-#if 0
-//#ifndef CONFIG_X64_64
+#ifdef CONFIG_X86_64
 #include "x86-64-syscalls-3.0.4_pointers.h"
 #endif
