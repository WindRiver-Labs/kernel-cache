+		sc_in(ctf_integer(size_t, len, len))
+		sc_in(ctf_integer(int, prot, prot))
+		sc_in(ctf_integer(int, flags, flags))
+		sc_in(ctf_integer(int, fd, fd))
+		sc_in(ctf_integer(off_t, offset, off))
