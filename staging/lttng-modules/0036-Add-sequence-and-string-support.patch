 #define tp_strcpy(dest, src)		__assign_str(dest, src)
 
 /*
@@ -40,59 +43,71 @@
 			     PARAMS(print));		       \
 	DEFINE_EVENT(name, name, PARAMS(proto), PARAMS(args));
 
