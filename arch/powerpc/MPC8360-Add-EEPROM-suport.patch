From 9ac46b454a3f79581ed8b0f86f4db948874cdded Mon Sep 17 00:00:00 2001
From: Jack Tan <jack.tan@windriver.com>
Date: Wed, 26 Jan 2011 16:20:09 +0800
Subject: [PATCH 2/4] MPC8360: Add EEPROM suport.

Adding EEPROM nodes in device tree.

Signed-off-by: Jack Tan <jack.tan@windriver.com>
---
 arch/powerpc/boot/dts/mpc836x_mds.dts |   10 ++++++++++
 1 files changed, 10 insertions(+), 0 deletions(-)

diff --git a/arch/powerpc/boot/dts/mpc836x_mds.dts b/arch/powerpc/boot/dts/mpc836x_mds.dts
index 9fb9536..605ff88 100644
--- a/arch/powerpc/boot/dts/mpc836x_mds.dts
+++ b/arch/powerpc/boot/dts/mpc836x_mds.dts
@@ -152,6 +152,11 @@
 				compatible = "dallas,ds1374";
 				reg = <0x68>;
 			};
+
+			eeprom@50 {
+				compatible = "at,24c256";
+				reg = <0x50>;
+			};
 		};
 
 		i2c@3100 {
@@ -163,6 +168,11 @@
 			interrupts = <15 0x8>;
 			interrupt-parent = <&ipic>;
 			dfsrr;
+
+			eeprom@50 {
+				compatible = "at,24c01";
+				reg = <0x50>;
+			};
 		};
 
 		serial0: serial@4500 {
