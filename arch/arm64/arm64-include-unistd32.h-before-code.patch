From 9286616219bd23f1b8324d0c18896665591b461c Mon Sep 17 00:00:00 2001
From: Yanjiang Jin <yanjiang.jin@windriver.com>
Date: Fri, 19 Jun 2015 14:14:10 +0800
Subject: [PATCH 039/132] arm64: include unistd32.h before code

This change is to avoid the compile error shown:
arch/arm64/kernel/built-in.o: In function `compat_sys_call_table':
arch/arm64/kernel/sys32.S:104: undefined reference to `sys_renameat2'
arch/arm64/kernel/sys32.S:104: undefined reference to `sys_getrandom'
arch/arm64/kernel/sys32.S:104: undefined reference to `sys_memfd_create'
Makefile:833: recipe for target 'vmlinux' failed

Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 arch/arm64/kernel/sys32.S |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/arch/arm64/kernel/sys32.S b/arch/arm64/kernel/sys32.S
index 423a5b3..0c6f812 100644
--- a/arch/arm64/kernel/sys32.S
+++ b/arch/arm64/kernel/sys32.S
@@ -22,6 +22,7 @@
 
 #include <asm/assembler.h>
 #include <asm/asm-offsets.h>
+#include <asm/unistd32.h>
 
 /*
  * System call wrappers for the AArch32 compatibility layer.
@@ -112,4 +113,3 @@ ENDPROC(compat_sys_fallocate_wrapper)
  */
 	.align	12
 ENTRY(compat_sys_call_table)
-#include <asm/unistd32.h>
-- 
1.7.5.4

