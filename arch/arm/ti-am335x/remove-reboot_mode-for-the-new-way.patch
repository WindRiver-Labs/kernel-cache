---
 arch/arm/mach-omap2/am33xx-restart.c |    2 +-
 arch/arm/mach-omap2/common.h         |    4 ++--
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/arm/mach-omap2/am33xx-restart.c b/arch/arm/mach-omap2/am33xx-restart.c
index 3c230cd..e5ea2c2 100644
--- a/arch/arm/mach-omap2/am33xx-restart.c
+++ b/arch/arm/mach-omap2/am33xx-restart.c
@@ -42,7 +42,7 @@ void am33xx_restart(char mode, const char *cmd)
  * Resets the SoC.  For @cmd, see the 'reboot' syscall in
  * kernel/sys.c.  No return value.
  */
-void am43xx_restart(enum reboot_mode mode, const char *cmd)
+void am43xx_restart(char mode, const char *cmd)
 {
 	/* TODO: Handle mode and cmd if necessary */
 
diff --git a/arch/arm/mach-omap2/common.h b/arch/arm/mach-omap2/common.h
index 9724908..604484e 100644
--- a/arch/arm/mach-omap2/common.h
+++ b/arch/arm/mach-omap2/common.h
@@ -152,9 +152,9 @@ static inline void am33xx_restart(char mode, const char *cmd)
 #endif
 
 #ifdef CONFIG_SOC_AM43XX
-void am43xx_restart(enum reboot_mode mode, const char *cmd);
+void am43xx_restart(char mode, const char *cmd);
 #else
-static inline void am43xx_restart(enum reboot_mode mode, const char *cmd)
+static inline void am43xx_restart(char mode, const char *cmd)
 {
 }
 #endif
