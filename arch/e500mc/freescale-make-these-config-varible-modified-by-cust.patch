From 62b6ddf4518cfa539df7f77f4e12e1ebcdb7831e Mon Sep 17 00:00:00 2001
From: Li Wang <li.wang@windriver.com>
Date: Thu, 7 Feb 2013 17:41:31 +0800
Subject: [PATCH] freescale: make these config varible modified by customer layer

Some freescale parameter of kernel config miss their description,
So it causes not to modify them by customer's layer.
Add a simple description such as "" will be OK.

Signed-off-by: Li Wang <li.wang@windriver.com>
---
 drivers/staging/fsl_qbman/Kconfig |   14 +++++++-------
 1 files changed, 7 insertions(+), 7 deletions(-)

diff --git a/drivers/staging/fsl_qbman/Kconfig b/drivers/staging/fsl_qbman/Kconfig
index f9c3e9d..96e06d9 100644
--- a/drivers/staging/fsl_qbman/Kconfig
+++ b/drivers/staging/fsl_qbman/Kconfig
@@ -267,31 +267,31 @@ config FSL_QMAN_FQD_SZ
 # Corenet initiator settings. Stash request queues are 4-deep to match cores'
 # ability to snart. Stash priority is 3, other priorities are 2.
 config FSL_QMAN_CI_SCHED_CFG_SRCCIV
-	int
+	int ""
 	depends on FSL_QMAN_CONFIG
 	default 4
 config FSL_QMAN_CI_SCHED_CFG_SRQ_W
-	int
+	int ""
 	depends on FSL_QMAN_CONFIG
 	default 3
 config FSL_QMAN_CI_SCHED_CFG_RW_W
-	int
+	int ""
 	depends on FSL_QMAN_CONFIG
 	default 2
 config FSL_QMAN_CI_SCHED_CFG_BMAN_W
-	int
+	int ""
 	depends on FSL_QMAN_CONFIG
 	default 2
 
 # portal interrupt settings
 config FSL_QMAN_PIRQ_DQRR_ITHRESH
-	int
+	int ""
 	default 12
 config FSL_QMAN_PIRQ_MR_ITHRESH
-	int
+	int ""
 	default 4
 config FSL_QMAN_PIRQ_IPERIOD
-	int
+	int ""
 	default 100
 
 # 64 bit kernel support
-- 
