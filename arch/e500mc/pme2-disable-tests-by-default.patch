From b01d28a0953de057a83d20367bf30b4797c453df Mon Sep 17 00:00:00 2001
From: Geoff Thorpe <Geoff.Thorpe@freescale.com>
Date: Thu, 17 Sep 2009 20:47:37 -0400
Subject: [PATCH 032/252] pme2: disable tests (by default).

This is required by subsequent patches for runnig on silicon. There is
a model bug that means we can't use matching test vectors.

Signed-off-by: Geoff Thorpe <Geoff.Thorpe@freescale.com>
[Cleanly applied the FSL SDK 2.0.3 patch:
"kernel-2.6.30-pme2-disable-tests-by-default.patch"]
Integrated-by: Yuri Nedel <Yuri.Nedel@windriver.com>
---
 drivers/match/Kconfig |    6 +++---
 1 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/drivers/match/Kconfig b/drivers/match/Kconfig
index c68c428..477427c 100644
--- a/drivers/match/Kconfig
+++ b/drivers/match/Kconfig
@@ -156,7 +156,7 @@ config FSL_PME2_TEST_LOW
 	tristate "PME2 low-level self-test"
 	depends on FSL_PME2 && FSL_QMAN_FQALLOCATOR && \
 		FSL_QMAN_PORTAL_DISABLEAUTO
-	default y
+	default n
 	---help---
 	  This takes an unused Qman portal and performs low-level PME2 API
 	  testing with it.
@@ -164,7 +164,7 @@ config FSL_PME2_TEST_LOW
 config FSL_PME2_TEST_HIGH
 	tristate "PME2 high-level self-test"
 	depends on FSL_PME2_HIGH
-	default y
+	default n
 	---help---
 	  This uses the high-level Qman driver (and the cpu-affine portals it
 	  manages) to perform high-level PME2 API testing with it.
@@ -172,7 +172,7 @@ config FSL_PME2_TEST_HIGH
 config FSL_PME2_TEST_SCAN
 	tristate "PME2 scan self-test"
 	depends on FSL_PME2_SAMPLE_DB
-	default y
+	default n
 	---help---
 	  This uses the high-level Qman driver (and the cpu-affine portals it
 	  manages) to perform scan PME2 API testing with it.
-- 
