From c9e0089a4dcdcfeb2beaa88d36c6195f15cc2958 Mon Sep 17 00:00:00 2001
From: "SengKai,Tan" <seng.kai.tan@intel.com>
Date: Tue, 19 Jul 2016 18:13:11 +0800
Subject: [PATCH 2232/2508] soc: Intel: Skylake: Add SND_SOC_COMPRESS
 configuration

commit 1b3751270701d077720e17bc84948719f68aeea9 from
https://github.com/01org/linux-apollolake-i

This commit is to add SND_SOC_COMPRESS dependency for
SND_SOC_INTEL_SKYLAKE soc driver

Signed-off-by: Tan, Seng Kai <seng.kai.tan@intel.com>
---
 sound/soc/intel/Kconfig |    1 +
 1 files changed, 1 insertions(+), 0 deletions(-)

diff --git a/sound/soc/intel/Kconfig b/sound/soc/intel/Kconfig
index abbbc52..b86e0d3 100644
--- a/sound/soc/intel/Kconfig
+++ b/sound/soc/intel/Kconfig
@@ -132,6 +132,7 @@ config SND_SOC_INTEL_SKYLAKE
 	select SND_SOC_TOPOLOGY
 	select SND_SOC_INTEL_SST
 	select WANT_DEV_COREDUMP
+	select SND_SOC_COMPRESS
 
 config SND_SOC_INTEL_SKL_RT286_MACH
 	tristate "ASoC Audio driver for SKL with RT286 I2S mode"
