From d22902091ec7a3b9e970c16a3cbc95c69e02f295 Mon Sep 17 00:00:00 2001
From: Weixing Shi <Weixing.Shi@windriver.com>
Date: Wed, 16 Sep 2009 04:17:47 +0800
Subject: [PATCH] fix yosemite dts error cause serial output garbage

remove the dts uart current_speed domain, Instead we should
allow u-boot to handling the baud rate setting.

Signed-off-by: Weixing Shi <Weixing.Shi@windriver.com>
---
 arch/powerpc/boot/dts/yosemite.dts |    4 ----
 1 files changed, 0 insertions(+), 4 deletions(-)

diff --git a/arch/powerpc/boot/dts/yosemite.dts b/arch/powerpc/boot/dts/yosemite.dts
index 00613fa..3f40d97 100644
--- a/arch/powerpc/boot/dts/yosemite.dts
+++ b/arch/powerpc/boot/dts/yosemite.dts
@@ -167,7 +167,6 @@
 				reg = <0xef600300 0x00000008>;
 				virtual-reg = <0xef600300>;
 				clock-frequency = <0>; /* Filled in by zImage */
-				current-speed = <9600>;
 				interrupt-parent = <&UIC0>;
 				interrupts = <0x0 0x4>;
 			};
@@ -178,7 +177,6 @@
 				reg = <0xef600400 0x00000008>;
 				virtual-reg = <0xef600400>;
 				clock-frequency = <0>;
-				current-speed = <0>;
 				interrupt-parent = <&UIC0>;
 				interrupts = <0x1 0x4>;
 			};
@@ -189,7 +187,6 @@
 				reg = <0xef600500 0x00000008>;
 				virtual-reg = <0xef600500>;
 				clock-frequency = <0>;
-				current-speed = <0>;
 				interrupt-parent = <&UIC0>;
 				interrupts = <0x3 0x4>;
 				status = "disabled";
@@ -201,7 +198,6 @@
 				reg = <0xef600600 0x00000008>;
 				virtual-reg = <0xef600600>;
 				clock-frequency = <0>;
-				current-speed = <0>;
 				interrupt-parent = <&UIC0>;
 				interrupts = <0x4 0x4>;
 				status = "disabled";
-- 
1.6.3.3

