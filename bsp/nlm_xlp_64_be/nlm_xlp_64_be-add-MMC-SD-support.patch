+	printk ("MMC_POWER_CTL = 0x%x\n", hc_rd16((host->base), HC_PC_HC, 0));
+	printk ("MMC_CLOK_CTL = 0x%x\n", hc_rd16((host->base), HC_CLOCK_CTRL, 0));
+	printk ("MMC_CAP0 = 0x%x\n", hc_rd32((host->base), 0x40, 0));
+#endif
+}
