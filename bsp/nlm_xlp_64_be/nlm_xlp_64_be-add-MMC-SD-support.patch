+static void xlpmmc_set_power(struct xlpmmc_host *host, int state)
+{	
+	if(state)
+		hc_wr16(host->base, HC_PC_HC, 0x1f00, 0);
+	else
+		hc_wr16(host->base, HC_PC_HC, 0x1e00, 0);
+}		
