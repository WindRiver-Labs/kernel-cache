From 96df1a9dd98bb8eb9add149c52e03f685fe14602 Mon Sep 17 00:00:00 2001
From: Nicolin Chen <b42378@freescale.com>
Date: Tue, 24 Sep 2013 11:05:50 +0800
Subject: [PATCH 0995/1072] ENGR00280852-2 arm: dtsi: imx6qdl: Set sdma
 priority of ASRC to medium

git://git.freescale.com/imx/linux-2.6-imx.git imx_3.10.17_1.0.0_beta
commit de542967bcbff22b3939bb412ac948dd840334a2

Set the priority as what kernel 3.0.35 does to keep it safe.

Signed-off-by: Nicolin Chen <b42378@freescale.com>
Signed-off-by: Hongbo Zhong <hongbo.zhong@windriver.com>
---
 arch/arm/boot/dts/imx6qdl.dtsi |    4 ++--
 1 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/imx6qdl.dtsi b/arch/arm/boot/dts/imx6qdl.dtsi
index a74d415..6de80a2 100644
--- a/arch/arm/boot/dts/imx6qdl.dtsi
+++ b/arch/arm/boot/dts/imx6qdl.dtsi
@@ -294,8 +294,8 @@
 					interrupts = <0 50 0x04>;
 					clocks = <&clks 107>, <&clks 156>;
 					clock-names = "core", "dma";
-					dmas = <&sdma 17 20 0>, <&sdma 18 20 0>, <&sdma 19 20 0>,
-					     <&sdma 20 20 0>, <&sdma 21 20 0>, <&sdma 22 20 0>;
+					dmas = <&sdma 17 20 1>, <&sdma 18 20 1>, <&sdma 19 20 1>,
+					     <&sdma 20 20 1>, <&sdma 21 20 1>, <&sdma 22 20 1>;
 					dma-names = "rxa", "rxb", "rxc",
 						"txa", "txb", "txc";
 					status = "okay";
-- 
