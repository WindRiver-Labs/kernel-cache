-#define IMX6Q_GPR12_PCIE_DEV_TYPE_MASK		(0xF << 12)
 
 #define IMX6Q_GPR13_SDMA_STOP_REQ		BIT(30)
 #define IMX6Q_GPR13_CAN2_STOP_REQ		BIT(29)
