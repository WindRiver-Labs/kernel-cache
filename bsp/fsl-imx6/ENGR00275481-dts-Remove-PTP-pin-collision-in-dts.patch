From 126e47b728fa962f99fba181a8133e1c313712af Mon Sep 17 00:00:00 2001
From: Luwei Zhou <b45643@freescale.com>
Date: Fri, 16 Aug 2013 10:53:26 +0800
Subject: [PATCH 0123/1072] ENGR00275481 dts: Remove PTP pin collision in dts

git://git.freescale.com/imx/linux-2.6-imx.git imx_3.10.17_1.0.0_beta
commit 11b2d67a4fcdbf6b3d17da274ab3106405bf3b21

This reverts commit 944b782d88b1ecece76942345f5e149c63a1d6d8.
We will use dts to deal with pin collision.

Signed-off-by: Luwei Zhou  <B45643@freescale.com>
Signed-off-by: Hongbo Zhong <hongbo.zhong@windriver.com>
---
 arch/arm/boot/dts/imx6qdl-sabreauto.dtsi |    1 -
 arch/arm/boot/dts/imx6qdl-sabresd.dtsi   |    1 -
 arch/arm/boot/dts/imx6qdl.dtsi           |    1 -
 3 files changed, 0 insertions(+), 3 deletions(-)

diff --git a/arch/arm/boot/dts/imx6qdl-sabreauto.dtsi b/arch/arm/boot/dts/imx6qdl-sabreauto.dtsi
index c163dc7..512e0cb 100644
--- a/arch/arm/boot/dts/imx6qdl-sabreauto.dtsi
+++ b/arch/arm/boot/dts/imx6qdl-sabreauto.dtsi
@@ -129,7 +129,6 @@
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_enet_2>;
 	phy-mode = "rgmii";
-	ptp-enable = <1>;
 	status = "okay";
 };
 
diff --git a/arch/arm/boot/dts/imx6qdl-sabresd.dtsi b/arch/arm/boot/dts/imx6qdl-sabresd.dtsi
index 5abed90..1589fb6 100644
--- a/arch/arm/boot/dts/imx6qdl-sabresd.dtsi
+++ b/arch/arm/boot/dts/imx6qdl-sabresd.dtsi
@@ -175,7 +175,6 @@
 	pinctrl-names = "default";
 	pinctrl-0 = <&pinctrl_enet_1>;
 	phy-mode = "rgmii";
-	ptp-enable = <1>;
 	status = "okay";
 };
 
diff --git a/arch/arm/boot/dts/imx6qdl.dtsi b/arch/arm/boot/dts/imx6qdl.dtsi
index 18f969b..e483c88 100644
--- a/arch/arm/boot/dts/imx6qdl.dtsi
+++ b/arch/arm/boot/dts/imx6qdl.dtsi
@@ -694,7 +694,6 @@
 							MX6QDL_PAD_RGMII_RD2__RGMII_RD2       0x1b0b0
 							MX6QDL_PAD_RGMII_RD3__RGMII_RD3       0x1b0b0
 							MX6QDL_PAD_RGMII_RX_CTL__RGMII_RX_CTL 0x1b0b0
-							MX6QDL_PAD_GPIO_16__ENET_REF_CLK      0x4001b0a8
 						>;
 					};
 
