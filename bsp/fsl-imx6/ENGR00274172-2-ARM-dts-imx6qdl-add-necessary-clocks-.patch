From b8bb35479dd9e85d5550ce321ae1bdc45b7cb886 Mon Sep 17 00:00:00 2001
From: Liu Ying <Ying.Liu@freescale.com>
Date: Tue, 20 Aug 2013 15:05:33 +0800
Subject: [PATCH 0146/1072] ENGR00274172-2 ARM: dts: imx6qdl: add necessary
 clocks for ldb

git://git.freescale.com/imx/linux-2.6-imx.git imx_3.10.17_1.0.0_beta
commit b7ce5d6d72af202d39bd996155cf65078cb3bbbd

This patch adds di[0|1]_div_3_5, di[0|1]_div_7 and di[0|1]_div_sel
clocks to the ldb nodes so that the ldb driver may use them to
setup the display clock tree.

Signed-off-by: Liu Ying <Ying.Liu@freescale.com>
Signed-off-by: Hongbo Zhong <hongbo.zhong@windriver.com>
---
 arch/arm/boot/dts/imx6qdl.dtsi |   10 ++++++++--
 1 files changed, 8 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/imx6qdl.dtsi b/arch/arm/boot/dts/imx6qdl.dtsi
index 2d827cd..be95327 100644
--- a/arch/arm/boot/dts/imx6qdl.dtsi
+++ b/arch/arm/boot/dts/imx6qdl.dtsi
@@ -1273,10 +1273,16 @@
 				reg = <0x020e0000 0x4000>;
 				clocks = <&clks 135>, <&clks 136>,
 					 <&clks 39>, <&clks 40>,
-					 <&clks 41>, <&clks 42>;
+					 <&clks 41>, <&clks 42>,
+					 <&clks 184>, <&clks 185>,
+					 <&clks 205>, <&clks 206>,
+					 <&clks 207>, <&clks 208>;
 				clock-names = "ldb_di0", "ldb_di1",
 					      "ipu1_di0_sel", "ipu1_di1_sel",
-					      "ipu2_di0_sel", "ipu2_di1_sel";
+					      "ipu2_di0_sel", "ipu2_di1_sel",
+					      "di0_div_3_5", "di1_div_3_5",
+					      "di0_div_7", "di1_div_7",
+					      "di0_div_sel", "di1_div_sel";
 				status = "disabled";
 			};
 
