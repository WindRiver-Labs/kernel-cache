+ * License.  See the file "COPYING" in the main directory of this archive
+ * for more details.
+ *
+ * Copyright (C) 2004-2008 Cavium Networks
+ */
 #include "linux/irq.h"
 #include "linux/hardirq.h"
 #include "linux/kernel_stat.h"
