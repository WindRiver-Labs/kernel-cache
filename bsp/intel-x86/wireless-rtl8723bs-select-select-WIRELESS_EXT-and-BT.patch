From d080f07ee0c367e06dfc9bba3c8e441b86ed08cc Mon Sep 17 00:00:00 2001
From: fli <fupan.li@windriver.com>
Date: Thu, 17 Mar 2016 00:24:43 -0700
Subject: [PATCH 7/7] wireless/rtl8723bs: select select WIRELESS_EXT and
 BT_HCIUART

Since this driver depends on wireless extention,
bluetooth hci uart and H5 protocol, select them
automatically!

Signed-off-by: fli <fupan.li@windriver.com>
---
 drivers/net/wireless/rtl8723bs/Kconfig |    3 +++
 1 files changed, 3 insertions(+), 0 deletions(-)

diff --git a/drivers/net/wireless/rtl8723bs/Kconfig b/drivers/net/wireless/rtl8723bs/Kconfig
index 92d50a4f..a22f9fa 100644
--- a/drivers/net/wireless/rtl8723bs/Kconfig
+++ b/drivers/net/wireless/rtl8723bs/Kconfig
@@ -1,5 +1,8 @@
 config RTL8723BS
 	tristate "Realtek 8723B SDIO or SPI WiFi"
+	select WIRELESS_EXT
+	select BT_HCIUART
+	select BT_HCIUART_3WIRE
 	---help---
 	  Help message of RTL8723BS
 
