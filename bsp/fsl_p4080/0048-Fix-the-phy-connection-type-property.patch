From 4868facaf06cfd4d4f4f289947b5f00b92901ac9 Mon Sep 17 00:00:00 2001
From: Greg Moffatt <greg.moffatt@windriver.com>
Date: Wed, 23 Sep 2009 09:34:05 -0400
Subject: [PATCH 48/52] Fix the phy connection type property

Extracted from P4080_WRlinux-rc1(/2)_20090904.iso vendor drop

Signed-off-by: Emil Medve <Emilian.Medve@Freescale.com>
[ Only formatting changes were made. ]
Signed-off-by: Greg Moffatt <greg.moffatt@windriver.com>
---
 arch/powerpc/boot/dts/p4080ds.dts |    4 ++--
 1 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/powerpc/boot/dts/p4080ds.dts b/arch/powerpc/boot/dts/p4080ds.dts
index faee2fb..fa60c88 100644
--- a/arch/powerpc/boot/dts/p4080ds.dts
+++ b/arch/powerpc/boot/dts/p4080ds.dts
@@ -1320,7 +1320,7 @@
 				reg = <0xe4000 0x1000>;
 				fsl,port-handles = <&fman0_rx2 &fman0_tx2>;
 				phy-handle = <&phy2>;
-				phy-connection-type = "rgmii-id";
+				phy-connection-type = "sgmii";
 			};
 
 			mdio@e5000 {
@@ -1337,7 +1337,7 @@
 				reg = <0xe6000 0x1000>;
 				fsl,port-handles = <&fman0_rx3 &fman0_tx3>;
 				phy-handle = <&phy3>;
-				phy-connection-type = "rgmii-id";
+				phy-connection-type = "sgmii";
 			};
 
 			mdio@e7000 {
