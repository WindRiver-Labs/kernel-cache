From 566fada29b774a55f009ba08c3aae8b28ea0e9bc Mon Sep 17 00:00:00 2001
From: auto commit <unknown@unknown>
Date: Mon, 25 Oct 2010 13:51:25 +0800
Subject: [PATCH 22/26] Make the sRIO physical address in DTS match the address set

by u-boot.

Signed-off-by: Kevin Hao <kexin.hao@windriver.com>
---
 arch/powerpc/boot/dts/p4080ds_36b.dts |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/arch/powerpc/boot/dts/p4080ds_36b.dts b/arch/powerpc/boot/dts/p4080ds_36b.dts
index 5cebd70..973a316 100644
--- a/arch/powerpc/boot/dts/p4080ds_36b.dts
+++ b/arch/powerpc/boot/dts/p4080ds_36b.dts
@@ -1735,7 +1735,7 @@
 		#size-cells = <2>;
 		compatible = "fsl,rapidio-delta";
 		reg = <0xf 0xfe0c0000 0 0x20000>;
-		ranges = <0 0 0xf 0xf5000000 0 0x01000000>;
+		ranges = <0 0 0xc 0x20000000 0 0x01000000>;
 		interrupt-parent = <&mpic>;
 		/* err_irq bell_outb_irq bell_inb_irq
 			msg1_tx_irq msg1_rx_irq	msg2_tx_irq msg2_rx_irq */
-- 
