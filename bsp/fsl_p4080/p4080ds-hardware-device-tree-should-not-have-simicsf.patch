From 8a7448e108da1dc07af3396938ec68a2c1fc5105 Mon Sep 17 00:00:00 2001
From: Scott Wood <scottwood@freescale.com>
Date: Tue, 1 Sep 2009 16:47:00 -0500
Subject: [PATCH 035/148] p4080ds: hardware device tree should not have simicsfs

Signed-off-by: Scott Wood <scottwood@freescale.com>
[Cleanly applied the FSL SDK 2.0.3 patch:
"kernel-2.6.30-p4080ds-hardware-device-tree-should-not-have.patch"]
Integrated-by: Yuri Nedel <Yuri.Nedel@windriver.com>
---
 arch/powerpc/boot/dts/p4080ds.dts |    7 -------
 1 files changed, 0 insertions(+), 7 deletions(-)

diff --git a/arch/powerpc/boot/dts/p4080ds.dts b/arch/powerpc/boot/dts/p4080ds.dts
index faee2fb..39c9ff3 100644
--- a/arch/powerpc/boot/dts/p4080ds.dts
+++ b/arch/powerpc/boot/dts/p4080ds.dts
@@ -90,7 +90,6 @@
 		fman1_rx3 = &fman1_rx3;
 		fman1_rx4 = &fman1_rx4;
 		rio0 = &rapidio0;
-		simicsfs = &simicsfs;
 	};
 
 	cpus {
@@ -167,12 +166,6 @@
 		device_type = "memory";
 	};
 
-	simicsfs: simicsfs@fdfffff0 {
-		compatible = "fsl,simicsfs";
-		reg = <0 0xfdfffff0 0 0x10>;
-	};
-
-
 	bman-portals@f4000000 {
 		#address-cells = <0x1>;
 		#size-cells = <0x1>;
-- 
1.6.5.2

