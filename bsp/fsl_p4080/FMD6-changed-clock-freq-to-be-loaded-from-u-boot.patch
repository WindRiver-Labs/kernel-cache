From 37957d14c2f16fff827857db66c97e0d3476cac9 Mon Sep 17 00:00:00 2001
From: Shlomi Gridish <gridish@freescale.com>
Date: Wed, 14 Oct 2009 22:21:27 +0200
Subject: [PATCH 07/26] FMD6 - changed clock-freq to be loaded from u-boot

Signed-off-by: Shlomi Gridish <gridish@freescale.com>
[Applied FSL SDK 2.0.3 patch
"0001-FMD6-changed-clock-freq-to-be-loaded-from-u-boot.patch"]
Integrated-by: Yuri Nedel <Yuri.Nedel@windriver.com>
---
 arch/powerpc/boot/dts/p4080ds.dts |    4 ++--
 1 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/powerpc/boot/dts/p4080ds.dts b/arch/powerpc/boot/dts/p4080ds.dts
index 04a6cea..9fe598d 100644
--- a/arch/powerpc/boot/dts/p4080ds.dts
+++ b/arch/powerpc/boot/dts/p4080ds.dts
@@ -1133,7 +1133,7 @@
 			compatible = "fsl,p4080-fman", "fsl,fman", "simple-bus";
 			ranges = <0 0x400000 0x100000>;
 			reg = <0x400000 0x100000>;
-			clock-frequency = <666>;
+			clock-frequency = <0>;
 			interrupts = <96 2>;
 			interrupt-parent = <&mpic>;
 
@@ -1487,7 +1487,7 @@
 			compatible = "fsl,p4080-fman", "fsl,fman", "simple-bus";
 			ranges = <0 0x500000 0x100000>;
 			reg = <0x500000 0x100000>;
-			clock-frequency = <666>;
+			clock-frequency = <0>;
 			interrupts = <97 2>;
 			interrupt-parent = <&mpic>;
 
-- 
