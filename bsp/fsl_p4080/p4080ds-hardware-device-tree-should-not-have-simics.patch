From f4ad7621d823bdeba7c72a90c57f1c12c42d8710 Mon Sep 17 00:00:00 2001
From: Scott Wood <scottwood@freescale.com>
Date: Tue, 1 Sep 2009 16:47:00 -0500
Subject: [PATCH] p4080ds: hardware device tree should not have simicsfs

Signed-off-by: Scott Wood <scottwood@freescale.com>
[Kevin: Original patch taken from Freescale p4080 SDK 2.0 ISO image.
Apply cleanly to kernel 2.6.27]
Integrated-by: Kevin Hao <kexin.hao@windriver.com>
---
 arch/powerpc/boot/dts/p4080ds.dts |    7 -------
 1 files changed, 0 insertions(+), 7 deletions(-)

diff --git a/arch/powerpc/boot/dts/p4080ds.dts b/arch/powerpc/boot/dts/p4080ds.dts
index 9a60164..4e0efaa 100644
--- a/arch/powerpc/boot/dts/p4080ds.dts
+++ b/arch/powerpc/boot/dts/p4080ds.dts
@@ -90,7 +90,6 @@
 		fman1_rx3 = &fman1_rx3;
 		fman1_rx4 = &fman1_rx4;
 		rio0 = &rapidio0;
-		simicsfs = &simicsfs;
 	};
 
 	cpus {
@@ -167,12 +166,6 @@
 		device_type = "memory";
 	};
 
-	simicsfs: simicsfs@fdfffff0 {
-		compatible = "fsl,simicsfs";
-		reg = <0 0xfdfffff0 0 0x10>;
-	};
-
-
 	bman-portals@f4000000 {
 		#address-cells = <0x1>;
 		#size-cells = <0x1>;
-- 
1.6.0.4

