From 8576822aca1c6ca7293d7a0b244db3e71611df3c Mon Sep 17 00:00:00 2001
From: Emil Medve <Emilian.Medve@freescale.com>
Date: Thu, 10 Sep 2009 16:12:57 -0500
Subject: [PATCH 037/148] P4080: Fix the phy-connection-type property for dTSEC2/3 in the DS board device tree(s)

Signed-off-by: Emil Medve <Emilian.Medve@Freescale.com>
[Cleanly applied the FSL SDK 2.0.3 patch:
"kernel-2.6.30-P4080-Fix-the-phy-connection-type-property-f.patch"]
Integrated-by: Yuri Nedel <Yuri.Nedel@windriver.com>
---
 arch/powerpc/boot/dts/p4080ds.dts     |    4 ++--
 arch/powerpc/boot/dts/p4080ds_36b.dts |    4 ++--
 2 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/powerpc/boot/dts/p4080ds.dts b/arch/powerpc/boot/dts/p4080ds.dts
index 39c9ff3..be672dc 100644
--- a/arch/powerpc/boot/dts/p4080ds.dts
+++ b/arch/powerpc/boot/dts/p4080ds.dts
@@ -1313,7 +1313,7 @@
 				reg = <0xe4000 0x1000>;
 				fsl,port-handles = <&fman0_rx2 &fman0_tx2>;
 				phy-handle = <&phy2>;
-				phy-connection-type = "rgmii-id";
+				phy-connection-type = "sgmii";
 			};
 
 			mdio@e5000 {
@@ -1330,7 +1330,7 @@
 				reg = <0xe6000 0x1000>;
 				fsl,port-handles = <&fman0_rx3 &fman0_tx3>;
 				phy-handle = <&phy3>;
-				phy-connection-type = "rgmii-id";
+				phy-connection-type = "sgmii";
 			};
 
 			mdio@e7000 {
diff --git a/arch/powerpc/boot/dts/p4080ds_36b.dts b/arch/powerpc/boot/dts/p4080ds_36b.dts
index 1babdbc..c9d4ded 100644
--- a/arch/powerpc/boot/dts/p4080ds_36b.dts
+++ b/arch/powerpc/boot/dts/p4080ds_36b.dts
@@ -1314,7 +1314,7 @@
 				reg = <0xe4000 0x1000>;
 				fsl,port-handles = <&fman0_rx2 &fman0_tx2>;
 				phy-handle = <&phy2>;
-				phy-connection-type = "rgmii-id";
+				phy-connection-type = "sgmii";
 			};
 
 			mdio@e5000 {
@@ -1331,7 +1331,7 @@
 				reg = <0xe6000 0x1000>;
 				fsl,port-handles = <&fman0_rx3 &fman0_tx3>;
 				phy-handle = <&phy3>;
-				phy-connection-type = "rgmii-id";
+				phy-connection-type = "sgmii";
 			};
 
 			mdio@e7000 {
