From 1efd310b6d4755a08f8fbfb5f02f4fab5b9eaaae Mon Sep 17 00:00:00 2001
From: Kevin Hao <kexin.hao@windriver.com>
Date: Thu, 5 Jul 2012 13:28:26 +0800
Subject: [PATCH] powerpc/p5040ds: add NOR flash

Add NOR flash partition.

Signed-off-by: Kevin Hao <kexin.hao@windriver.com>
---
 arch/powerpc/boot/dts/p5040ds.dts |    5 +++++
 1 file changed, 5 insertions(+)

diff --git a/arch/powerpc/boot/dts/p5040ds.dts b/arch/powerpc/boot/dts/p5040ds.dts
index 2769ee7..94427f9 100644
--- a/arch/powerpc/boot/dts/p5040ds.dts
+++ b/arch/powerpc/boot/dts/p5040ds.dts
@@ -507,6 +507,11 @@
 				label = "fs3";
 				reg = <0x7020000  0xf60000>;
 			};
+			partition@7f60000 {
+				label = "u-boot env";
+				reg = <0x7f60000  0x20000>;
+				read-only;
+			};
 			partition@7f80000 {
 				label = "u-boot";
 				reg = <0x7f80000 0x80000>;
-- 
