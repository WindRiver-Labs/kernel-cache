From e5cb9698360c2eb4a07bc16a497b83aef72cab37 Mon Sep 17 00:00:00 2001
From: Shaohui Xie <Shaohui.Xie@freescale.com>
Date: Sat, 13 Jun 2015 04:42:34 +0800
Subject: [PATCH 195/451] dts/ls1043aqds: fix mdio mux

Signed-off-by: Mingkai Hu <Mingkai.Hu@freescale.com>
[Xulin: Original patch taken from
Linux-LS1043A-SDK-V0.4-SOURCE-20150826-yocto.iso]
Signed-off-by: Xulin Sun <xulin.sun@windriver.com>
---
 arch/arm64/boot/dts/freescale/fsl-ls1043a-qds.dts |   10 +++++-----
 1 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-ls1043a-qds.dts b/arch/arm64/boot/dts/freescale/fsl-ls1043a-qds.dts
index 2cc522b..91b97d2 100644
--- a/arch/arm64/boot/dts/freescale/fsl-ls1043a-qds.dts
+++ b/arch/arm64/boot/dts/freescale/fsl-ls1043a-qds.dts
@@ -292,7 +292,7 @@
 
 		/* On-board RGMII2 PHY */
 		ls1043mdio1: mdio@1 {
-			reg = <0x1>;
+			reg = <0x20>;
 			#address-cells = <1>;
 			#size-cells = <0>;
 
@@ -303,7 +303,7 @@
 
 		/* Slot 1 */
 		ls1043mdio_s1: mdio@2 {
-			reg = <0x2>;
+			reg = <0x40>;
 			#address-cells = <1>;
 			#size-cells = <0>;
 			status = "disabled";
@@ -328,7 +328,7 @@
 
 		/* Slot 2 */
 		ls1043mdio_s2: mdio@3 {
-			reg = <0x3>;
+			reg = <0x60>;
 			#address-cells = <1>;
 			#size-cells = <0>;
 			status = "disabled";
@@ -353,7 +353,7 @@
 
 		/* Slot 3 */
 		ls1043mdio_s3: mdio@4 {
-			reg = <0x4>;
+			reg = <0x80>;
 			#address-cells = <1>;
 			#size-cells = <0>;
 			status = "disabled";
@@ -365,7 +365,7 @@
 
 		/* Slot 4 */
 		ls1043mdio_s4: mdio@5 {
-			reg = <0x5>;
+			reg = <0xa0>;
 			#address-cells = <1>;
 			#size-cells = <0>;
 			status = "disabled";
-- 
1.7.5.4

