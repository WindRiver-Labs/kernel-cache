From ea1bb349ea7ebff866eff76dbb0c72f554b375d0 Mon Sep 17 00:00:00 2001
From: Mingkai Hu <Mingkai.Hu@freescale.com>
Date: Thu, 7 May 2015 19:04:11 +0800
Subject: [PATCH 107/451] arm64/ls1043a: move MSI interrupt property out of
 PCIe controller

Signed-off-by: Mingkai Hu <Mingkai.Hu@freescale.com>
[Xulin: Original patch taken from
Linux-LS1043A-SDK-V0.4-SOURCE-20150826-yocto.iso]
Signed-off-by: Xulin Sun <xulin.sun@windriver.com>
---
 arch/arm64/boot/dts/freescale/fsl-ls1043a.dtsi |    6 +++---
 1 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-ls1043a.dtsi b/arch/arm64/boot/dts/freescale/fsl-ls1043a.dtsi
index 489f473..e5b2bc1 100644
--- a/arch/arm64/boot/dts/freescale/fsl-ls1043a.dtsi
+++ b/arch/arm64/boot/dts/freescale/fsl-ls1043a.dtsi
@@ -627,7 +627,7 @@
 		reg = <0x00 0x03400000 0x0 0x00010000   /* controller registers */
 		       0x40 0x00000000 0x0 0x00002000>; /* configuration space */
 		reg-names = "regs", "config";
-		interrupts = <0 110 0x4>, /* controller interrupt */
+		interrupts = <0 118 0x4>, /* controller interrupt */
 			     <0 116 0x4>, /* MSI interrupt */
 			     <0 117 0x4>; /* PME interrupt */
 		interrupt-names = "intr", "msi", "pme";
@@ -653,7 +653,7 @@
 		reg = <0x00 0x03500000 0x0 0x00010000   /* controller registers */
 		       0x48 0x00000000 0x0 0x00002000>; /* configuration space */
 		reg-names = "regs", "config";
-		interrupts = <0 120 0x4>,
+		interrupts = <0 128 0x4>,
 			     <0 126 0x4>,
 			     <0 127 0x4>;
 		interrupt-names = "intr", "msi", "pme";
@@ -679,7 +679,7 @@
 		reg = <0x00 0x03600000 0x0 0x00010000   /* controller registers */
 		       0x50 0x00000000 0x0 0x00002000>; /* configuration space */
 		reg-names = "regs", "config";
-		interrupts = <0 154 0x4>,
+		interrupts = <0 162 0x4>,
 			     <0 160 0x4>,
 			     <0 161 0x4>;
 		interrupt-names = "intr", "msi", "pme";
-- 
