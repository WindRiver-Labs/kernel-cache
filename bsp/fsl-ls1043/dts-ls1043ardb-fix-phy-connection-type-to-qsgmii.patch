From a0f61eb9d311160b9d3b085c3d1f3053fb944234 Mon Sep 17 00:00:00 2001
From: Shaohui Xie <Shaohui.Xie@freescale.com>
Date: Wed, 8 Jul 2015 17:20:41 +0800
Subject: [PATCH 271/451] dts/ls1043ardb: fix phy-connection-type to qsgmii

Signed-off-by: Mingkai Hu <Mingkai.Hu@freescale.com>
[Xulin: Original patch taken from
Linux-LS1043A-SDK-V0.4-SOURCE-20150826-yocto.iso]
Signed-off-by: Xulin Sun <xulin.sun@windriver.com>
---
 arch/arm64/boot/dts/freescale/fsl-ls1043a-rdb.dts |    8 ++++----
 1 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm64/boot/dts/freescale/fsl-ls1043a-rdb.dts b/arch/arm64/boot/dts/freescale/fsl-ls1043a-rdb.dts
index c4d89ba..900064a 100644
--- a/arch/arm64/boot/dts/freescale/fsl-ls1043a-rdb.dts
+++ b/arch/arm64/boot/dts/freescale/fsl-ls1043a-rdb.dts
@@ -224,12 +224,12 @@
 &fman0 {
 	ethernet@e0000 {
 		phy-handle = <&qsgmii_phy1>;
-		phy-connection-type = "sgmii";
+		phy-connection-type = "qsgmii";
 	};
 
 	ethernet@e2000 {
 		phy-handle = <&qsgmii_phy2>;
-		phy-connection-type = "sgmii";
+		phy-connection-type = "qsgmii";
 	};
 
 	ethernet@e4000 {
@@ -244,12 +244,12 @@
 
 	ethernet@e8000 {
 		phy-handle = <&qsgmii_phy3>;
-		phy-connection-type = "sgmii";
+		phy-connection-type = "qsgmii";
 	};
 
 	ethernet@ea000 {
 		phy-handle = <&qsgmii_phy4>;
-		phy-connection-type = "sgmii";
+		phy-connection-type = "qsgmii";
 	};
 
 	ethernet@f0000 { /* 10GEC1 */
-- 
