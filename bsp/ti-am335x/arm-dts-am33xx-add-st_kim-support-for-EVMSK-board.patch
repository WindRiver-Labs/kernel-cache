From f515ba7111e4e48579ebcba11cba71978ead9e77 Mon Sep 17 00:00:00 2001
From: Zumeng Chen <zumeng.chen@windriver.com>
Date: Sat, 8 Mar 2014 13:59:16 +0800
Subject: [PATCH 186/188] arm: dts: am33xx: add st_kim support for EVMSK board

Enable TI_ST kim by adding kim data in dts file.

Signed-off-by: Zumeng Chen <zumeng.chen@windriver.com>
---
 arch/arm/boot/dts/am335x-evmsk.dts |   12 ++++++++++++
 1 files changed, 12 insertions(+), 0 deletions(-)

diff --git a/arch/arm/boot/dts/am335x-evmsk.dts b/arch/arm/boot/dts/am335x-evmsk.dts
index 71a24e3..c3e3b30 100644
--- a/arch/arm/boot/dts/am335x-evmsk.dts
+++ b/arch/arm/boot/dts/am335x-evmsk.dts
@@ -590,6 +590,18 @@
 		gpio = <175>; /* Bank0, pin31 */
 		board-ref-clock = <4>;
 	};
+
+	kim {
+		compatible = "kim";
+		nshutdown_gpio = <117>; /* Bank3, pin21 */
+		dev_name = "/dev/ttyO1";
+		flow_cntrl = <1>;
+		baud_rate = <3000000>;
+	};
+
+	btwilink {
+		compatible = "btwilink";
+	};
 };
 
 &wkup_m3 {
