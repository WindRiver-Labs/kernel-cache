From fe00315399bfe8c2e0175b97666190e21fc506f0 Mon Sep 17 00:00:00 2001
From: Bin Jiang <bin.jiang@windriver.com>
Date: Wed, 30 Apr 2014 15:52:59 +0800
Subject: [PATCH 11/13] ARM: iProc: kernel decompression is not supported

iProc only supports to boot an uncompressed kernel image.

Signed-off-by: Bin Jiang <bin.jiang@windriver.com>
---
 arch/arm/boot/Makefile |    4 ++++
 1 files changed, 4 insertions(+), 0 deletions(-)

diff --git a/arch/arm/boot/Makefile b/arch/arm/boot/Makefile
index 84aa2ca..99dbe66 100644
--- a/arch/arm/boot/Makefile
+++ b/arch/arm/boot/Makefile
@@ -75,7 +75,11 @@ if [ $(words $(UIMAGE_LOADADDR)) -ne 1 ]; then \
 	false; \
 fi
 
+ifeq ($(CONFIG_ARCH_IPROC),y)
+$(obj)/uImage:  $(obj)/Image FORCE
+else
 $(obj)/uImage:	$(obj)/zImage FORCE
+endif
 	@$(check_for_multiple_loadaddr)
 	$(call if_changed,uimage)
 	@$(kecho) '  Image $@ is ready'
