Change-Id: Id8e8b9a617b0886037c16e3279a9f18c47205d03
Reviewed-on: http://vgitil04.il.marvell.com:8080/926
Reviewed-by: Tawfik Bayouk <tawfik@marvell.com>
Tested-by: Tawfik Bayouk <tawfik@marvell.com>
Signed-off-by: Zhong Hongbo <hongbo.zhong@windriver.com>
---
 release/build_release.sh                       |    8 ++++----
 release/configs/armada_xp_v7smp_lpae_defconfig |    3 ---
 2 files changed, 4 insertions(+), 7 deletions(-)

diff --git a/release/build_release.sh b/release/build_release.sh
index b749fed..663ec26 100644
--- a/release/build_release.sh
+++ b/release/build_release.sh
@@ -5,12 +5,12 @@
 
 echo "DSMP LSP - Build STD release $1 based on kernel $2 - Starting..."
 mkdir ./${1}
-echo "DSMP LSP - Prepare kernel and kernel+LSP tar balls" 
+echo "DSMP LSP - Prepare kernel and kernel+LSP tar balls"
 cd ../
 git archive --format=tar --prefix=linux-${2}-${1}/ HEAD | gzip > linux-${2}-${1}.tgz
 git archive --format=tar --prefix=linux-${2}/ v${2} | gzip > linux-${2}.tgz
 cd release
-echo "DSMP LSP - Extract and copy kernel files" 
+echo "DSMP LSP - Extract and copy kernel files"
 tar -xzf ../linux-${2}-${1}.tgz -C ./${1}
 tar -xzf ../linux-${2}.tgz -C ./${1}
 rm ../linux-${2}-${1}.tgz
@@ -31,10 +31,10 @@ cd openswan-2.6.37
 patch -p1 < ../0001-mv_openswan_2_6_37.patch
 cd ../../../../../
 echo "DSMP LSP - Compile external binaries"
-source ./make_release ${1} $2 binaries_external 370_v7up 370_v7up_nat 370_v7up_nas 370_v7up_stat xp_v7smp xp_v7smp_nat xp_v7smp_lpae xp_v7smp_stat xp_v7smp_nas xp_v7smp_be8 xp_amp_g1 xp_amp_g0
+source ./make_release ${1} $2 binaries_external 370_v7up 370_v7up_nat 370_v7up_nas 370_v7up_stat xp_v7smp xp_v7smp_nat xp_v7smp_lpae xp_v7smp_lpae_stat xp_v7smp_stat xp_v7smp_nas xp_v7smp_be8 xp_v7amp_g1 xp_v7amp_g0
 echo "DSMP LSP - Remove third party SLIC source code"
 source ./make_export ${1} $2 binaries_external
-echo "DSMP LSP - Cleanup" 
+echo "DSMP LSP - Cleanup"
 rm ./${1}/linux-${2}-${1}_release.zip
 rm -fr ./${1}/linux-${2}-${1}_release
 rm -fr ./${1}/linux-${2}-${1}
diff --git a/release/configs/armada_xp_v7smp_lpae_defconfig b/release/configs/armada_xp_v7smp_lpae_defconfig
index e15b46b..e4d2881 100644
--- a/release/configs/armada_xp_v7smp_lpae_defconfig
+++ b/release/configs/armada_xp_v7smp_lpae_defconfig
@@ -192,9 +192,6 @@ CONFIG_NEW_LEDS=y
 CONFIG_RTC_CLASS=y
 CONFIG_RTC_DRV_MV=y
 CONFIG_DMADEVICES=y
-CONFIG_MV_XOR=y
-CONFIG_NET_DMA=y
-CONFIG_ASYNC_TX_DMA=y
 CONFIG_EXT2_FS=y
 CONFIG_EXT3_FS=y
 # CONFIG_EXT3_DEFAULTS_TO_ORDERED is not set
