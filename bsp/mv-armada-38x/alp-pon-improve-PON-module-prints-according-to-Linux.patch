+	printk(KERN_INFO "XPON GPON Module loaded.\n");
 out:
 	return err;
 }
diff --git a/arch/arm/plat-armada/mv_drivers_lsp/mv_pon/perf/ponOnuLnxKsMI.c b/arch/arm/plat-armada/mv_drivers_lsp/mv_pon/perf/ponOnuLnxKsMI.c
index e9abd4f..7a02892 100644
--- a/arch/arm/plat-armada/mv_drivers_lsp/mv_pon/perf/ponOnuLnxKsMI.c
+++ b/arch/arm/plat-armada/mv_drivers_lsp/mv_pon/perf/ponOnuLnxKsMI.c
@@ -151,44 +151,44 @@ int mvPonMiInit(void)
 
 	status = ponDev.func.ponOnuSetup();
 	if (status != MV_OK) {
-		printk(KERN_INFO "= PON Module setup failed =\n");
+		printk(KERN_INFO "XPON Module setup failed.\n");
 		goto ponInitError;
 	}
 
 	status = ponDev.func.ponOnuSwitchOn();
 	if (status != MV_OK) {
-		printk(KERN_INFO "= PON Module switch ON failed =\n");
+		printk(KERN_INFO "XPON Module switch ON failed.\n");
 		goto ponInitError;
 	}
 
 	status = ponDev.func.ponOnuOperate();
 	if (status != MV_OK) {
-		printk(KERN_INFO "= PON Module operate failed =\n");
+		printk(KERN_INFO "XPON Module operate failed.\n");
 		goto ponInitError;
 	}
 
 	/* create user interface */
 	status = ponDev.func.userInterfaceCreate();
 	if (status != MV_OK) {
-		printk(KERN_INFO "= PON Module UI creation failed =\n");
+		printk(KERN_INFO "XPON Module UI creation failed.\n");
 		goto ponInitError;
 	}
 
 	/* register mng notification callback */
 	status = ponDev.func.apiStatusNotifyRegister(onuPonMiNotifyCallback);
 	if (status != MV_OK) {
-		printk(KERN_INFO "= PON Module API status notification callback registration failed =\n");
+		printk(KERN_INFO "XPON Module API status notification callback registration failed.\n");
 		goto ponInitError;
 	}
 
 	/* register link status callback */
 	status = ponDev.func.apiLinkStatusCallbackRegister();
 	if (status != MV_OK) {
-		printk(KERN_INFO "= PON Module link status notification callback registration failed =\n");
+		printk(KERN_INFO "XPON Module link status notification callback registration failed.\n");
 		goto ponInitError;
 	}
 
-	printk(KERN_INFO "= PON Module Init ended successfully =\n");
+	printk(KERN_INFO "XPON module initialized successfully\n");
 
 ponInitError:
 	return status;
@@ -881,22 +881,20 @@ int __init mvOnuPonModule_init(void)
 {
 	MV_STATUS status;
 
-	printk(KERN_INFO "= PON module init =\n");
-
 	memset(&ponDev, 0, sizeof(ponDev));
 	status = onuPonMngInterfaceCreate();
 	if (status != MV_OK) {
-		printk(KERN_ERR "= PON module management interface init failed =\n");
+		printk(KERN_ERR "XPON module failed to init management interface.\n");
 		return status;
 	}
 
 	status = mvPonSysfsInit();
 	if (status != MV_OK) {
-		printk(KERN_ERR "= PON module sysfs init failed =\n");
+		printk(KERN_ERR "XPON module failed to initialize sysfs.\n");
 		return status;
 	}
 
-	printk(KERN_INFO "= PON module init ended successfully =\n");
+	printk(KERN_INFO "XPON module basic init sysfs initialized.\n");
 	return 0;
 }
 
