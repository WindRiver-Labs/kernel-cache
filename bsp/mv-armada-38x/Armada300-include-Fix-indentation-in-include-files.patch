diff --git a/include/linux/fs.h b/include/linux/fs.h
index f2f1b82..b8a42a3 100644
--- a/include/linux/fs.h
+++ b/include/linux/fs.h
@@ -1638,7 +1638,7 @@ struct file_operations {
 	ssize_t (*splice_write)(struct pipe_inode_info *, struct file *, loff_t *, size_t, unsigned int);
 	ssize_t (*splice_read)(struct file *, loff_t *, struct pipe_inode_info *, size_t, unsigned int);
 	ssize_t (*splice_from_socket)(struct file *, struct socket *,
-				     loff_t __user *ppos, size_t count);
+				      loff_t __user *ppos, size_t count);
 	int (*setlease)(struct file *, long, struct file_lock **);
 	long (*fallocate)(struct file *file, int mode, loff_t offset,
 			  loff_t len);
@@ -2404,7 +2404,7 @@ extern ssize_t generic_splice_sendpage(struct pipe_inode_info *pipe,
 extern long do_splice_direct(struct file *in, loff_t *ppos, struct file *out,
 		size_t len, unsigned int flags);
 extern ssize_t generic_splice_from_socket(struct file *file, struct socket *sock,
-				     loff_t __user *ppos, size_t count);
+					  loff_t __user *ppos, size_t count);
 
 extern void
 file_ra_state_init(struct file_ra_state *ra, struct address_space *mapping);
