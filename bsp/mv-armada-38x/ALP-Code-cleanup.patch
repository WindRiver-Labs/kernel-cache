-		ethSataComplexOptions |= MV_ETH_COMPLEX_SW_P0_QUAD_PHY_P0;
+	if ( (ethComplexOptions & MV_ETHCOMP_GE_MAC0_2_GE_PHY_P0) == MV_FALSE )
+		ethComplexOptions |= MV_ETHCOMP_SW_P0_2_GE_PHY_P0;
 
