From c25e09f1c850c1a86c11f519653d89dc9ea9fd2c Mon Sep 17 00:00:00 2001
From: Xulin Sun <xulin.sun@windriver.com>
Date: Wed, 24 Aug 2016 03:20:35 +0000
Subject: [PATCH 1/2] crypto: talitos: disable RAID5 XOR Calculation Offload

Talitos XOR offloading has been marked as known bugs and limitation
from Vendor's SDK since the known issues of DMA operation, this is also
the reason it was blocked into the upstream. So set CRYPTO_DEV_TALITOS_RAIDXOR
default as n to disable the Talitos XOR offloading.

Signed-off-by: Xulin Sun <xulin.sun@windriver.com>
---
 drivers/crypto/Kconfig |    1 -
 1 files changed, 0 insertions(+), 1 deletions(-)

diff --git a/drivers/crypto/Kconfig b/drivers/crypto/Kconfig
index d28d899..ba88092 100644
--- a/drivers/crypto/Kconfig
+++ b/drivers/crypto/Kconfig
@@ -224,7 +224,6 @@ config CRYPTO_DEV_TALITOS
 
 config CRYPTO_DEV_TALITOS_RAIDXOR
 	bool "Talitos RAID5 XOR Calculation Offload"
-	default y
 	select DMA_ENGINE
 	select ASYNC_TX_ENABLE_CHANNEL_SWITCH
 	depends on FSL_DMA && CRYPTO_DEV_TALITOS
-- 
