From f4729d19313c21729c3f3b9f0d7ea21708f16703 Mon Sep 17 00:00:00 2001
From: Tian Le <le.tian@windriver.com>
Date: Wed, 11 Jan 2012 16:34:48 +0800
Subject: [PATCH 11/11] FSL P1010: Add the CAN nodes into p1010rdb_36b.dts

CQID: WIND00327766

Add the missing CAN nodes into p1010rdb_36b.dts.

Signed-off-by: Tian Le <le.tian@windriver.com>
---
 arch/powerpc/boot/dts/p1010rdb_36b.dts |   18 ++++++++++++++++++
 1 files changed, 18 insertions(+), 0 deletions(-)

diff --git a/arch/powerpc/boot/dts/p1010rdb_36b.dts b/arch/powerpc/boot/dts/p1010rdb_36b.dts
index 2687e5d..ccf1e9f 100644
--- a/arch/powerpc/boot/dts/p1010rdb_36b.dts
+++ b/arch/powerpc/boot/dts/p1010rdb_36b.dts
@@ -299,6 +299,24 @@
 			interrupt-parent = <&mpic>;
 		};
 
+		can0@1c000 {
+			compatible = "fsl,flexcan-v1.0";
+			reg = <0x1c000 0x1000>;
+			interrupts = <48 0x2>;
+			interrupt-parent = <&mpic>;
+			fsl,flexcan-clock-source = <1>;
+			fsl,flexcan-clock-divider = <2>;
+		};
+
+		can1@1d000 {
+			compatible = "fsl,flexcan-v1.0";
+			reg = <0x1d000 0x1000>;
+			interrupts = <61 0x2>;
+			interrupt-parent = <&mpic>;
+			fsl,flexcan-clock-source = <1>;
+			fsl,flexcan-clock-divider = <2>;
+		};
+
 		L2: l2-cache-controller@20000 {
 			compatible = "fsl,p1020-l2-cache-controller";
 			reg = <0x20000 0x1000>;
-- 
