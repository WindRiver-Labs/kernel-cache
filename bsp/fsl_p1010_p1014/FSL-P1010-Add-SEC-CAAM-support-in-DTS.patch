From 02e718adf7bb6d931647a524b51a04dce0c68655 Mon Sep 17 00:00:00 2001
From: Tian Le <le.tian@windriver.com>
Date: Thu, 24 Nov 2011 15:15:11 +0800
Subject: [PATCH 07/11] FSL P1010: Add SEC CAAM support in DTS

Add SEC CAAM support in FSL P1010 DTS.

Signed-off-by: Tian Le <le.tian@windriver.com>
---
 arch/powerpc/boot/dts/p1010rdb.dts     |   12 ++++++------
 arch/powerpc/boot/dts/p1010rdb_36b.dts |   10 +++++-----
 2 files changed, 11 insertions(+), 11 deletions(-)

diff --git a/arch/powerpc/boot/dts/p1010rdb.dts b/arch/powerpc/boot/dts/p1010rdb.dts
index 288303e..a764f27 100644
--- a/arch/powerpc/boot/dts/p1010rdb.dts
+++ b/arch/powerpc/boot/dts/p1010rdb.dts
@@ -335,10 +335,10 @@
 			interrupts = <28 0x2>;
 			dr_mode = "host";
 			phy_type = "utmi";
-			};
+		};
 
 		crypto: crypto@30000 {
-			compatible = "fsl,p4080-sec4.0", "fsl,sec4.0";
+			compatible = "fsl,p1010rdb-sec-v4.0", "fsl,sec-v4.0";
 			#address-cells = <1>;
 			#size-cells = <1>;
 			reg		 = <0x30000 0x10000>;
@@ -346,28 +346,28 @@
 			interrupt-parent = <&mpic>;
 
 			sec_jr0: jr@1000 {
-				compatible = "fsl,p4080-sec4.0-job-ring", "fsl,sec4.0-job-ring";
+				compatible = "fsl,p1010-sec-v4.0-job-ring", "fsl,sec-v4.0-job-ring";
 				reg	   = <0x1000 0x1000>;
 				interrupts	 = <45 2>;
 				interrupt-parent = <&mpic>;
 			};
 
 			sec_jr1: jr@2000 {
-				compatible = "fsl,p4080-sec4.0-job-ring", "fsl,sec4.0-job-ring";
+				compatible = "fsl,p1010-sec-v4.0-job-ring", "fsl,sec-v4.0-job-ring";
 				reg	   = <0x2000 0x1000>;
 				interrupts	 = <45 2>;
 				interrupt-parent = <&mpic>;
 			};
 
 			sec_jr2: jr@3000 {
-				compatible = "fsl,p4080-sec4.0-job-ring", "fsl,sec4.0-job-ring";
+				compatible = "fsl,p1010-sec-v4.0-job-ring", "fsl,sec-v4.0-job-ring";
 				reg	   = <0x3000 0x1000>;
 				interrupts	 = <45 2>;
 				interrupt-parent = <&mpic>;
 			};
 
 			sec_jr3: jr@4000 {
-				compatible = "fsl,p4080-sec4.0-job-ring", "fsl,sec4.0-job-ring";
+				compatible = "fsl,p1010-sec-v4.0-job-ring", "fsl,sec-v4.0-job-ring";
 				reg	   = <0x4000 0x1000>;
 				interrupts	 = <45 2>;
 				interrupt-parent = <&mpic>;
diff --git a/arch/powerpc/boot/dts/p1010rdb_36b.dts b/arch/powerpc/boot/dts/p1010rdb_36b.dts
index 26d8c8e..8d98980 100644
--- a/arch/powerpc/boot/dts/p1010rdb_36b.dts
+++ b/arch/powerpc/boot/dts/p1010rdb_36b.dts
@@ -311,7 +311,7 @@
 			};
 
 		crypto: crypto@30000 {
-			compatible = "fsl,p4080-sec4.0", "fsl,sec4.0";
+			compatible = "fsl,p1010rdb-sec-v4.0", "fsl,sec-v4.0";
 			#address-cells = <1>;
 			#size-cells = <1>;
 			reg		 = <0x30000 0x10000>;
@@ -319,28 +319,28 @@
 			interrupt-parent = <&mpic>;
 
 			sec_jr0: jr@1000 {
-				compatible = "fsl,p4080-sec4.0-job-ring", "fsl,sec4.0-job-ring";
+				compatible = "fsl,p1010-sec-v4.0-job-ring", "fsl,sec-v4.0-job-ring";
 				reg	   = <0x1000 0x1000>;
 				interrupts	 = <45 2>;
 				interrupt-parent = <&mpic>;
 			};
 
 			sec_jr1: jr@2000 {
-				compatible = "fsl,p4080-sec4.0-job-ring", "fsl,sec4.0-job-ring";
+				compatible = "fsl,p1010-sec-v4.0-job-ring", "fsl,sec-v4.0-job-ring";
 				reg	   = <0x2000 0x1000>;
 				interrupts	 = <45 2>;
 				interrupt-parent = <&mpic>;
 			};
 
 			sec_jr2: jr@3000 {
-				compatible = "fsl,p4080-sec4.0-job-ring", "fsl,sec4.0-job-ring";
+				compatible = "fsl,p1010-sec-v4.0-job-ring", "fsl,sec-v4.0-job-ring";
 				reg	   = <0x3000 0x1000>;
 				interrupts	 = <45 2>;
 				interrupt-parent = <&mpic>;
 			};
 
 			sec_jr3: jr@4000 {
-				compatible = "fsl,p4080-sec4.0-job-ring", "fsl,sec4.0-job-ring";
+				compatible = "fsl,p1010-sec-v4.0-job-ring", "fsl,sec-v4.0-job-ring";
 				reg	   = <0x4000 0x1000>;
 				interrupts	 = <45 2>;
 				interrupt-parent = <&mpic>;
-- 
