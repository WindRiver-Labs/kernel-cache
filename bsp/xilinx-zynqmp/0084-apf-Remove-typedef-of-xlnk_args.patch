 		return -ENOMEM;
@@ -972,11 +971,11 @@ static int xlnk_mcdmaregister_ioctl(struct file *filp, unsigned int code,
 static int xlnk_devunregister_ioctl(struct file *filp, unsigned int code,
 					unsigned long args)
