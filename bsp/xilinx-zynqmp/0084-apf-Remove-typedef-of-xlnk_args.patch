+	union xlnk_args temp_args;
 	int status;
 	u32 handle;
 
-	status = copy_from_user(&temp_args, (xlnk_args *)args,
-				sizeof(xlnk_args));
+	status = copy_from_user(&temp_args, (void __user *)args,
