+			};
+			slave2 {
+				label		= "slave2";
+				link-interface	= <2>;
+			};
+			slave3 {
+				label		= "slave3";
+				link-interface	= <2>;
+			};
+		};
+	};
+
+	pa: pa@2000000 {
+		label = "keystone-pa";
+		checksum-offload	= <1>; /* 1 - HW offload */
+		txhook-order		= <10>;
+		txhook-softcsum		= <40>;
+		rxhook-order		= <10>;
+
+		tx_cmd_queue_depth	= <64>;
+		tx_data_queue_depth	= <1024>;
