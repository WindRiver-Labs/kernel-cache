From 0281e4c29d5274baf8eeee199ef83b09730aa21a Mon Sep 17 00:00:00 2001
From: WingMan Kwok <w-kwok2@ti.com>
Date: Tue, 3 May 2016 12:58:26 -0400
Subject: [PATCH 286/347] ARM: dts: keystone: add qos support

This patch comes from:
  git://git.ti.com/processor-sdk/processor-sdk-linux.git

This patch adds the dts bindings that enable the support
of QMSS QoS.

Signed-off-by: WingMan Kwok <w-kwok2@ti.com>
Signed-off-by: Jacob Stiffler <j-stiffler@ti.com>
(cherry picked from commit 7b68495fc9c34b7fd5a1e9c1232f7c40e4d7f456)
Signed-off-by: Zumeng Chen <zumeng.chen@windriver.com>
---
 arch/arm/boot/dts/keystone-k2e-netcp.dtsi  |  114 +++++++-
 arch/arm/boot/dts/keystone-k2hk-netcp.dtsi |  110 +++++++-
 arch/arm/boot/dts/keystone-k2l-netcp.dtsi  |  111 +++++++-
 arch/arm/boot/dts/keystone-qostree.dtsi    |  427 ++++++++++++++++++++++++++++
 4 files changed, 750 insertions(+), 12 deletions(-)
 create mode 100644 arch/arm/boot/dts/keystone-qostree.dtsi

diff --git a/arch/arm/boot/dts/keystone-k2e-netcp.dtsi b/arch/arm/boot/dts/keystone-k2e-netcp.dtsi
index 4081a49..3a18f6f 100644
--- a/arch/arm/boot/dts/keystone-k2e-netcp.dtsi
+++ b/arch/arm/boot/dts/keystone-k2e-netcp.dtsi
@@ -8,6 +8,16 @@
  * published by the Free Software Foundation.
  */
 
+qostree0: qos-tree-0 {
+	output-queue = <910>;		/* allowed only on root node */
+};
+
+qostree1: qos-tree-1 {
+	output-queue = <910>;		/* allowed only on root node */
+};
+
+/include/ "keystone-qostree.dtsi"
+
 pdsp_intd: intd@2a0c000 {
 	compatible	= "syscon";
 	reg		= <0x2a0c000 0x3c8>;
@@ -81,6 +91,48 @@ qmss: qmss@2a40000 {
 				qalloc-by-id;
 			};
 		};
+		qos {
+			qosinputs0: qos-inputs-0 {
+				qrange = <8000 192>;
+				pdsp-id			= <3>;
+				ticks-per-sec		= <10000>;
+				/* inputs_per_port
+				 * drop cfg int_num
+				 * drop cfg qos_ticks
+				 * drop cfg drop_ticks
+				 * drop cfg seed_0,1,2
+				 */
+				qos-cfg			= <4 50 1 1 0xf00 0 0>;
+				/* 4 types of shadows
+				 *     each with <start-idx cnt size>
+				 */
+				sched-port-configs	= <0 20 0x1c8>;
+				drop-out-profiles	= <0 36 0xc>;
+				drop-cfg-profiles	= <0 8 0x14>;
+				drop-queue-configs	= <0 80 0x4>;
+
+				drop-policies           = <&droppolicies>;
+				qos-tree                = <&qostree0>;
+				statistics-profiles	= <0 48>;
+				qalloc-by-id;
+			};
+			qosinputs1: qos-inputs-1 {
+				qrange = <6400 192>;
+				pdsp-id			= <7>;
+				ticks-per-sec		= <10000>;
+				qos-cfg			= <4 50 1 1 0xf00 0 0>;
+
+				sched-port-configs	= <0 20 0x1c8>;
+				drop-out-profiles	= <0 36 0xc>;
+				drop-cfg-profiles	= <0 8 0x14>;
+				drop-queue-configs	= <0 80 0x4>;
+
+				drop-policies           = <&droppolicies>;
+				qos-tree                = <&qostree1>;
+				statistics-profiles	= <0 48>;
+				qalloc-by-id;
+			};
+		};
 		accumulator {
 			acc-low-0 {
 				qrange = <480 32>;
@@ -114,6 +166,24 @@ qmss: qmss@2a40000 {
 			       0x2a20000 0x4000>;  /*cmd*/
 			id = <0>;
 		};
+		pdsp3@0x2a13000 {
+			firmware_type = "qos";
+			syscon-intd = <&pdsp_intd>;
+			reg = <0x2a13000 0x1000>,
+			      <0x2a0f300 0x100>,
+			      <0x2a2c000 0x4000>;
+			reg-names = "iram", "regs", "command";
+			id = <3>;
+		};
+		pdsp7@0x2a17000 {
+			firmware_type = "qos";
+			syscon-intd = <&pdsp_intd>;
+			reg = <0x2a17000 0x1000>,
+			      <0x2a0f700 0x100>,
+			      <0x2a3c000 0x4000>;
+			reg-names = "iram", "regs", "command";
+			id = <7>;
+		};
 	};
 }; /* qmss */
 
@@ -124,7 +194,7 @@ knav_dmas: knav_dmas@0 {
 	#size-cells = <1>;
 	ranges;
 	ti,navigator-cloud-address = <0x23a80000 0x23a90000
-				 0x23a80000 0x23a90000>;
+				      0x23a80000 0x23a90000>;
 
 	dma_gbe: dma_gbe@0 {
 		reg = <0x24186000 0x100>,
@@ -319,6 +389,7 @@ netcp: netcp@24000000 {
 		#address-cells = <1>;
 		#size-cells = <1>;
 		ranges;
+
 		gbe@200000 { /* ETHSS */
 			label = "netcp-gbe";
 			compatible = "ti,netcp-gbe-9";
@@ -445,34 +516,67 @@ netcp: netcp@24000000 {
 				};
 			};
 		};
+
+		qos@0 {
+			label = "netcp-qos";
+			compatible = "ti,netcp-qos";
+			tx-channel = "pa-cl6";
+
+			interfaces {
+				qos0: interface-0 {
+					tx-queues = <910 8072 8073 8074
+						     8075 8076 8077>;
+				};
+				qos1: interface-1 {
+					tx-queues = <910 6472 6473 6474
+						     6475 6476 6477>;
+				};
+			};
+		};
 	};
 
 	netcp-interfaces {
 		interface-0 {
 			rx-channel = "netrx0";
-			rx-pool = <1024 12>;
-			tx-pool = <1024 12>;
+			rx-pool = <1024 12>; /* num_desc region-id */
 			rx-queue-depth = <128 128 0 0>;
 			rx-buffer-size = <1518 4096 0 0>;
 			rx-queue = <528>;
+			/* 7 pools, hence 7 subqueues
+			 *   <#desc rgn-id tx-thresh rx-thresh>
+			 */
+			tx-pools = <1024 12 17 17
+				    64 12 17 17
+				    64 12 17 17
+				    64 12 17 17
+				    64 12 17 17
+				    64 12 17 17
+				    64 12 17 17>;
 			tx-completion-queue = <530>;
 			efuse-mac = <1>;
 			netcp-gbe = <&gbe0>;
 			netcp-pa2 = <&pa0>;
-
+			netcp-qos = <&qos0>;
 		};
 		interface-1 {
 			rx-channel = "netrx1";
 			rx-pool = <1024 12>;
-			tx-pool = <1024 12>;
 			rx-queue-depth = <128 128 0 0>;
 			rx-buffer-size = <1518 4096 0 0>;
 			rx-queue = <529>;
+			tx-pools = <1024 12 17 17
+				    64 12 17 17
+				    64 12 17 17
+				    64 12 17 17
+				    64 12 17 17
+				    64 12 17 17
+				    64 12 17 17>;
 			tx-completion-queue = <531>;
 			efuse-mac = <0>;
 			local-mac-address = [02 18 31 7e 3e 00];
 			netcp-gbe = <&gbe1>;
 			netcp-pa2 = <&pa1>;
+			netcp-qos = <&qos1>;
 		};
 	};
 };
