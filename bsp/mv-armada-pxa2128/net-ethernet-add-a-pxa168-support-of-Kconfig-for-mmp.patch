From 100121a5d0da8a8691ef936c5d20fc959d416c88 Mon Sep 17 00:00:00 2001
From: Zumeng Chen <zumeng.chen@windriver.com>
Date: Tue, 6 Aug 2013 11:11:20 +0800
Subject: [PATCH 30/60] net: ethernet: add a pxa168 support of Kconfig for
 mmp3

Original patches come from M5.3.12.5.4_sources.

Signed-off-by: Zumeng Chen <zumeng.chen@windriver.com>
---
 drivers/net/ethernet/marvell/Kconfig |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/drivers/net/ethernet/marvell/Kconfig b/drivers/net/ethernet/marvell/Kconfig
index 0029934..9a8b998 100644
--- a/drivers/net/ethernet/marvell/Kconfig
+++ b/drivers/net/ethernet/marvell/Kconfig
@@ -33,7 +33,7 @@ config MV643XX_ETH
 
 config PXA168_ETH
 	tristate "Marvell pxa168 ethernet support"
-	depends on CPU_PXA168
+	depends on CPU_PXA168 || CPU_MMP3
 	select PHYLIB
 	---help---
 	  This driver supports the pxa168 Ethernet ports.
