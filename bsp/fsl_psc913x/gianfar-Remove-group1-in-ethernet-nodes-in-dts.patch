From 8b6fbd976e9d1b2062351311a53aba70e3b3192f Mon Sep 17 00:00:00 2001
From: Akhil Goyal <akhil.goyal@freescale.com>
Date: Tue, 18 Oct 2011 17:28:09 +0000
Subject: [PATCH 88/92] gianfar: Remove group1 in ethernet nodes in dts

Extracted from 913x_WUSDK_REL_0.9.tar.gz vendor drop.

As psc9131 is single core, two groups cannot exist and the rx-bit-map,
tx-bit-map should be changed to 0xFF to cater to all the interrups.

Signed-off-by: Akhil Goyal <akhil.goyal@freescale.com>
Integrated-by: Jiang Bin <bin.jiang@windriver.com>
---
 arch/powerpc/boot/dts/psc9131rdb.dts |   26 ++++----------------------
 1 files changed, 4 insertions(+), 22 deletions(-)

diff --git a/arch/powerpc/boot/dts/psc9131rdb.dts b/arch/powerpc/boot/dts/psc9131rdb.dts
index 349ddc8..77d0cf8 100644
--- a/arch/powerpc/boot/dts/psc9131rdb.dts
+++ b/arch/powerpc/boot/dts/psc9131rdb.dts
@@ -515,19 +515,10 @@
 					#address-cells = <1>;
 					#size-cells = <1>;
 					reg = <0xb0000 0x1000>;
-					rx-bit-map = <0xAA>;
-					tx-bit-map = <0xAA>;
+					rx-bit-map = <0xFF>;
+					tx-bit-map = <0xFF>;
 					interrupts = <26 2 27 2 28 2>;
 				};
-
-				queue-group@1 {
-					#address-cells = <1>;
-					#size-cells = <1>;
-					reg = <0xb4000 0x1000>;
-					rx-bit-map = <0x55>;
-					tx-bit-map = <0x55>;
-					interrupts = <29 2 30 2 31 2>;
-				};
 			};
 
 			enet1: ethernet@b1000 {
@@ -548,19 +539,10 @@
 					#address-cells = <1>;
 					#size-cells = <1>;
 					reg = <0xb1000 0x1000>;
-					rx-bit-map = <0xAA>;
-					tx-bit-map = <0xAA>;
+					rx-bit-map = <0xFF>;
+					tx-bit-map = <0xFF>;
 					interrupts = <33 2 34 2 35 2>;
 				};
-
-				queue-group@1 {
-					#address-cells = <1>;
-					#size-cells = <1>;
-					reg = <0xb5000 0x1000>;
-					rx-bit-map = <0x55>;
-					tx-bit-map = <0x55>;
-					interrupts = <36 2 37 2 38 2>;
-				};
 			};
 
 			sdhci@2e000 {
