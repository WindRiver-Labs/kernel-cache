From 8a3ce5da2b09002171b252e1238e405c2a6cff8f Mon Sep 17 00:00:00 2001
From: Hai-Ying Wang <Haiying.Wang@freescale.com>
Date: Thu, 17 May 2012 20:13:15 +0000
Subject: [PATCH 12/57] Add PAMU and QBMAN support to T4240QDS in Kconfig

commit f0ed9382408cb4a0e275156d769727214daf446c from
https://git.freescale.com/git-private/cgit.cgi/ppc/t4240/linux.git

Signed-off-by: Haiying Wang <Haiying.Wang@freescale.com>
---
 arch/powerpc/platforms/85xx/Kconfig |    2 ++
 1 file changed, 2 insertions(+)

diff --git a/arch/powerpc/platforms/85xx/Kconfig b/arch/powerpc/platforms/85xx/Kconfig
index 8fb5fe0..2cc39b0 100644
--- a/arch/powerpc/platforms/85xx/Kconfig
+++ b/arch/powerpc/platforms/85xx/Kconfig
@@ -300,6 +300,8 @@ config T4240_QDS
 	select HAS_RAPIDIO
 	select PPC_EPAPR_HV_PIC
 	select WORKAROUND_E5500_MFCR
+	select HAS_FSL_PAMU
+	select HAS_FSL_QBMAN
 	help
 	  This option enables support for the T4240 QDS board
 
