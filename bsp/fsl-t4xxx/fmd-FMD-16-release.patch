+    err = FmVSPAlloc(p_FmPort->h_Fm,
+                             p_FmPort->portType,
+                             p_FmPort->portId,
+                             p_VSPParams->numOfProfiles);
+    if (err != E_OK)
+        RETURN_ERROR(MAJOR, err, NO_MSG);
+
+    /*get absolute VSP ID for dfltRelative*/
