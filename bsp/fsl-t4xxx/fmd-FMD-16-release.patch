+t_Error FmVSPFree(  t_Handle        h_Fm,
+                    e_FmPortType    portType,
+                    uint8_t         portId)
+{
+    t_Fm            *p_Fm = (t_Fm *)h_Fm;
+    uint8_t         swPortIndex = 0, hardwarePortId, first, numOfVSPs, i;
+    uint32_t        intFlags;
+
