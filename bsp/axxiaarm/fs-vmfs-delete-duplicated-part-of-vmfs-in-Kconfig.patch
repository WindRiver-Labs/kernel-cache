From 0800359cd10df104031ad84e2b0c2b6d176d3280 Mon Sep 17 00:00:00 2001
From: Quanyang Wang <quanyang.wang@windriver.com>
Date: Wed, 22 Jun 2016 02:58:57 +0000
Subject: [PATCH 2/2] fs: vmfs: delete duplicated part of vmfs in Kconfig

The same contents has been included in fs/vmfs/Kconfig. So delete
the redundant part.

Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 fs/Kconfig |   16 ----------------
 1 files changed, 0 insertions(+), 16 deletions(-)

diff --git a/fs/Kconfig b/fs/Kconfig
index dbdfb86..3b6bba2 100644
--- a/fs/Kconfig
+++ b/fs/Kconfig
@@ -281,20 +281,4 @@ source "fs/nls/Kconfig"
 source "fs/dlm/Kconfig"
 source "fs/vmfs/Kconfig"
 
-config VMFS_FS
-	tristate "VMFS file system support (to mount host directories etc.)"
-	select NLS
-	help
-	  Say Y here to enable support for accessing the host filesystem
-	  when running the kernel in a virtual platform built with the Fast
-	  Models product from ARM.
-
-config VMFS_DEV_BASE
-	hex "VMFS base address"
-	depends on VMFS_FS
-
-config VMFS_IRQ
-	int "VMFS IRQ"
-	depends on VMFS_FS
-
 endmenu
