From 9a5c79b06c73229459a11ecbd2ffd63526ca5c27 Mon Sep 17 00:00:00 2001
From: Quanyang Wang <quanyang.wang@windriver.com>
Date: Wed, 1 Jun 2016 08:48:45 +0000
Subject: [PATCH] fs/vmfs: Assign default value to VMFS_IRQ

Assign 0 as default value to VMFS_IRQ.

Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
---
 fs/vmfs/Kconfig |    1 +
 1 files changed, 1 insertions(+), 0 deletions(-)

diff --git a/fs/vmfs/Kconfig b/fs/vmfs/Kconfig
index 523fd92..d011fd2 100644
--- a/fs/vmfs/Kconfig
+++ b/fs/vmfs/Kconfig
@@ -15,4 +15,5 @@ config VMFS_DEV_BASE
 config VMFS_IRQ
 	int "VMFS IRQ"
 	depends on VMFS_FS
+	default 0
 
-- 
1.7.5.4

