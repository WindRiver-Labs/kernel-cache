+		};
+
+		i2c11: i2c@80806b0000 {
+			compatible = "lsi,api2c";
+			#address-cells = <1>;
+			#size-cells = <0>;
+			reg = <0x80 0x806b0000 0x1000>;
+			interrupts = <GIC_SPI 56 IRQ_TYPE_LEVEL_HIGH>;
+			clocks = <&clks AXXIA_CLK_PER>;
+			clock-names = "i2c";
