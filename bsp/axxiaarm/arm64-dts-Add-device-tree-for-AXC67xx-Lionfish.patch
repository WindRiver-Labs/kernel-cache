+		i2c9: i2c@8080690000 {
+			compatible = "lsi,api2c";
+			#address-cells = <1>;
+			#size-cells = <0>;
+			reg = <0x80 0x80690000 0x1000>;
+			interrupts = <GIC_SPI 54 IRQ_TYPE_LEVEL_HIGH>;
+			clocks = <&clks AXXIA_CLK_PER>;
+			clock-names = "i2c";
