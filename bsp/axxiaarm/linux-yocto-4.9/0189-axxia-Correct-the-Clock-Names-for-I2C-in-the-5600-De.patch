From 4e38dc5aec767bae7c7940d12c423262e3bb6d3b Mon Sep 17 00:00:00 2001
From: John Jacques <john.jacques@intel.com>
Date: Wed, 16 Nov 2016 18:57:01 +0200
Subject: [PATCH 189/213] axxia: Correct the Clock Names for I2C in the 5600
 Device Trees

Signed-off-by: John Jacques <john.jacques@intel.com>
Signed-off-by: Bruce Ashfield <bruce.ashfield@windriver.com>
---
 arch/arm64/boot/dts/intel/axm56xx.dtsi | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm64/boot/dts/intel/axm56xx.dtsi b/arch/arm64/boot/dts/intel/axm56xx.dtsi
index 54e2e04..a8e0ff3 100644
--- a/arch/arm64/boot/dts/intel/axm56xx.dtsi
+++ b/arch/arm64/boot/dts/intel/axm56xx.dtsi
@@ -177,7 +177,7 @@
 			reg = <0x80 0x80080000 0 0x1000>;
 			interrupts = <GIC_SPI 22 IRQ_TYPE_LEVEL_HIGH>;
 			clocks = <&clk_per 0>;
-			clock-names = "apb_pclk";
+			clock-names = "i2c";
 			status = "disabled";
 		};
 
@@ -210,7 +210,7 @@
 			reg = <0x80 0x800b0000 0 0x1000>;
 			interrupts = <GIC_SPI 25 IRQ_TYPE_LEVEL_HIGH>;
 			clocks = <&clk_per 0>;
-			clock-names = "apb_pclk";
+			clock-names = "i2c";
 			status = "disabled";
 		};
 
-- 
1.9.1

