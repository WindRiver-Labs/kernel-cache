From 8e60c591b438d439eccb4eaa4dc548199e178d70 Mon Sep 17 00:00:00 2001
From: John Jacques <john.jacques@intel.com>
Date: Tue, 12 May 2015 12:30:45 -0500
Subject: [PATCH 031/213] arch/arm64: Update the Axxia Device Tree for
 Emulation

Signed-off-by: John Jacques <john.jacques@intel.com>
---
 arch/arm64/boot/dts/intel/axm5606-cpus.dtsi | 20 ++++++++++----------
 1 file changed, 10 insertions(+), 10 deletions(-)

diff --git a/arch/arm64/boot/dts/intel/axm5606-cpus.dtsi b/arch/arm64/boot/dts/intel/axm5606-cpus.dtsi
index deb0d76..ac0c1cf 100644
--- a/arch/arm64/boot/dts/intel/axm5606-cpus.dtsi
+++ b/arch/arm64/boot/dts/intel/axm5606-cpus.dtsi
@@ -17,42 +17,42 @@
 		cpu@0 {
 			device_type = "cpu";
 			compatible = "arm,armv8";
-			reg = <0x00>;
+			reg = <0x0>;
 			enable-method = "psci";
 		};
 
 		cpu@1 {
 			device_type = "cpu";
 			compatible = "arm,armv8";
-			reg = <0x01>;
+			reg = <0x1>;
 			enable-method = "psci";
 		};
 
-		cpu@4 {
+		cpu@2 {
 			device_type = "cpu";
 			compatible = "arm,armv8";
-			reg = <0x04>;
+			reg = <0x100>;
 			enable-method = "psci";
 		};
 
-		cpu@5 {
+		cpu@3 {
 			device_type = "cpu";
 			compatible = "arm,armv8";
-			reg = <0x05>;
+			reg = <0x101>;
 			enable-method = "psci";
 		};
 
-		cpu@8 {
+		cpu@4 {
 			device_type = "cpu";
 			compatible = "arm,armv8";
-			reg = <0x08>;
+			reg = <0x200>;
 			enable-method = "psci";
 		};
 
-		cpu@9 {
+		cpu@5 {
 			device_type = "cpu";
 			compatible = "arm,armv8";
-			reg = <0x09>;
+			reg = <0x201>;
 			enable-method = "psci";
 		};
 	};
-- 
