From 1d28d4c699a99b71c31a97a731f388672849e585 Mon Sep 17 00:00:00 2001
From: Sreedevi Joshi <sreedevi.joshi@intel.com>
Date: Thu, 11 Dec 2014 08:23:33 +0100
Subject: [PATCH 003/213] arm64: dts: Correct interrupt numbers for AXM56xx

Signed-off-by: Sreedevi Joshi <sreedevi.joshi@intel.com>
Signed-off-by: Anders Berg <anders.berg@intel.com>
---
 arch/arm64/boot/dts/intel/axm5604-sim.dts | 16 ++++++++++++++++
 arch/arm64/boot/dts/intel/axm56xx.dtsi    | 21 ++++++++++++++-------
 2 files changed, 30 insertions(+), 7 deletions(-)

diff --git a/arch/arm64/boot/dts/intel/axm5604-sim.dts b/arch/arm64/boot/dts/intel/axm5604-sim.dts
index d95b610..29de8dc 100644
--- a/arch/arm64/boot/dts/intel/axm5604-sim.dts
+++ b/arch/arm64/boot/dts/intel/axm5604-sim.dts
@@ -86,3 +86,19 @@
 &gpio1 {
 	status = "okay";
 };
+
+&i2c0 {
+	status = "okay";
+};
+
+&i2c1 {
+	status = "okay";
+};
+
+&i2c2 {
+	status = "okay";
+};
+
+&i2c3 {
+	status = "okay";
+};
diff --git a/arch/arm64/boot/dts/intel/axm56xx.dtsi b/arch/arm64/boot/dts/intel/axm56xx.dtsi
index e9d1b00..0b71f6a 100644
--- a/arch/arm64/boot/dts/intel/axm56xx.dtsi
+++ b/arch/arm64/boot/dts/intel/axm56xx.dtsi
@@ -49,7 +49,7 @@
 		};
 	};
 
-	gic: interrupt-controller@2001001000 {
+	gic: interrupt-controller@8010010000 {
 		compatible = "arm,gic-v3";
 		#interrupt-cells = <3>;
 		#address-cells = <0>;
@@ -108,7 +108,7 @@
 		usb0: usb@9000000000 {
 			compatible = "synopsys,dwc3";
 			reg = <0x90 0x00000000 0x10000>;
-			interrupts = <GIC_SPI 55 IRQ_TYPE_LEVEL_HIGH>;
+			interrupts = <GIC_SPI 122 IRQ_TYPE_LEVEL_HIGH>;
 			dma-coherent;
 			status = "disabled";
 		};
@@ -142,7 +142,7 @@
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0x80 0x80080000 0x1000>;
-			interrupts = <GIC_SPI 19 IRQ_TYPE_LEVEL_HIGH>;
+			interrupts = <GIC_SPI 54 IRQ_TYPE_LEVEL_HIGH>;
 			clocks = <&clks AXXIA_CLK_PER>;
 			clock-names = "i2c";
 			status = "disabled";
@@ -153,7 +153,7 @@
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0x80 0x80090000 0x1000>;
-			interrupts = <GIC_SPI 20 IRQ_TYPE_LEVEL_HIGH>;
+			interrupts = <GIC_SPI 55 IRQ_TYPE_LEVEL_HIGH>;
 			clocks = <&clks AXXIA_CLK_PER>;
 			clock-names = "i2c";
 			status = "disabled";
@@ -164,7 +164,7 @@
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0x80 0x800a0000 0x1000>;
-			interrupts = <GIC_SPI 21 IRQ_TYPE_LEVEL_HIGH>;
+			interrupts = <GIC_SPI 56 IRQ_TYPE_LEVEL_HIGH>;
 			clocks = <&clks AXXIA_CLK_PER>;
 			clock-names = "i2c";
 			status = "disabled";
@@ -175,12 +175,19 @@
 			#address-cells = <1>;
 			#size-cells = <0>;
 			reg = <0x80 0x800b0000 0x1000>;
-			interrupts = <GIC_SPI 22 IRQ_TYPE_LEVEL_HIGH>;
+			interrupts = <GIC_SPI 57 IRQ_TYPE_LEVEL_HIGH>;
 			clocks = <&clks AXXIA_CLK_PER>;
 			clock-names = "i2c";
 			status = "disabled";
 		};
 
+		mtc: mtc@8080210000 {
+			compatible = "lsi,mtc";
+			reg = <0x80 0x80210000 0x10000>;
+			interrupts = <GIC_SPI 60 IRQ_TYPE_LEVEL_HIGH>;
+			status = "disabled";
+		};
+
 		amba {
 			compatible = "arm,amba-bus";
 			#address-cells = <2>;
@@ -272,7 +279,7 @@
 				#address-cells = <1>;
 				#size-cells = <0>;
 				reg = <0x80 0x80100000 0x1000>;
-				interrupts = <GIC_SPI 42 IRQ_TYPE_LEVEL_HIGH>;
+				interrupts = <GIC_SPI 59 IRQ_TYPE_LEVEL_HIGH>;
 				num-cs = <5>;
 				clocks = <&clks AXXIA_CLK_PER>, <&clks AXXIA_CLK_PER>;
 				clock-names = "spi", "apb_pclk";
-- 
1.9.1

