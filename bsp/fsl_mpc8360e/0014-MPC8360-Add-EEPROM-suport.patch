From 8b8705d4ebd0cccef25dc6aedbc5188664d5e96e Mon Sep 17 00:00:00 2001
From: Andrei Dolnikov <adolnikov@embeddedalley.com>
Date: Tue, 23 Dec 2008 20:58:26 +0300
Subject: [PATCH] MPC8360: Add EEPROM suport.

Adding EEPROM nodes in device tree.

Signed-off-by: Andrei Dolnikov <adolnikov@embeddedalley.com>
---
 arch/powerpc/boot/dts/mpc836x_mds.dts |   10 ++++++++++
 1 files changed, 10 insertions(+), 0 deletions(-)

diff --git a/arch/powerpc/boot/dts/mpc836x_mds.dts b/arch/powerpc/boot/dts/mpc836x_mds.dts
index 090ad90..526305e 100644
--- a/arch/powerpc/boot/dts/mpc836x_mds.dts
+++ b/arch/powerpc/boot/dts/mpc836x_mds.dts
@@ -154,6 +154,11 @@
 				compatible = "dallas,ds1374";
 				reg = <0x68>;
 			};
+
+			eeprom@50 {
+				compatible = "at,24c256";
+				reg = <0x50>;
+			};
 		};
 
 		i2c@3100 {
@@ -165,6 +170,11 @@
 			interrupts = <15 0x8>;
 			interrupt-parent = <&ipic>;
 			dfsrr;
+
+			eeprom@50 {
+				compatible = "at,24c01";
+				reg = <0x50>;
+			};
 		};
 
 		serial0: serial@4500 {
-- 
1.6.0.3

