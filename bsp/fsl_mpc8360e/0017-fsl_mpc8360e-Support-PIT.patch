From 7b490261d9a3fa32c85bd551608b8470a84f7641 Mon Sep 17 00:00:00 2001
From: Tiejun Chen <tiejun.chen@windriver.com>
Date: Tue, 12 May 2009 10:14:04 +0800
Subject: [PATCH] fsl_mpc8360e: Support PIT

Fix incorrect interrupt reference in the MPC8313 dts file.

Signed-off-by: Tiejun Chen <tiejun.chen@windriver.com>
---
 arch/powerpc/boot/dts/mpc836x_mds.dts |    3 ++-
 1 files changed, 2 insertions(+), 1 deletions(-)

diff --git a/arch/powerpc/boot/dts/mpc836x_mds.dts b/arch/powerpc/boot/dts/mpc836x_mds.dts
index 7180ae6..7c075bd 100644
--- a/arch/powerpc/boot/dts/mpc836x_mds.dts
+++ b/arch/powerpc/boot/dts/mpc836x_mds.dts
@@ -137,7 +137,8 @@
 			device_type = "pit";
 			compatible = "mpc_pit";
 			reg = <400 100>;
-			interrupts = <41 8>;
+			interrupts = <65 0x8>;
+			clock-frequency = <264000000>;
 			interrupt-parent = < &ipic >;
 		};
 
-- 
