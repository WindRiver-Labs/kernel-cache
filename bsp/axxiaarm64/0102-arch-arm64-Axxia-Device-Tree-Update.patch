From e399527e2206e778cb250b143058df67a6e78cf4 Mon Sep 17 00:00:00 2001
From: John Jacques <john.jacques@intel.com>
Date: Fri, 21 Aug 2015 12:01:57 -0500
Subject: [PATCH 102/131] arch/arm64: Axxia Device Tree Update

In the XLF emulation case, use all 12 cores.

Signed-off-by: John Jacques <john.jacques@intel.com>
[Original patch is from linux-yocto-4.1 axxia/base branch.]
Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 arch/arm64/boot/dts/intel/axc6712-cpus.dtsi | 29 +++++++++++++++++++++++++++++
 1 file changed, 29 insertions(+)

diff --git a/arch/arm64/boot/dts/intel/axc6712-cpus.dtsi b/arch/arm64/boot/dts/intel/axc6712-cpus.dtsi
index ed0c4f5..f351e2f 100644
--- a/arch/arm64/boot/dts/intel/axc6712-cpus.dtsi
+++ b/arch/arm64/boot/dts/intel/axc6712-cpus.dtsi
@@ -70,5 +70,34 @@
 			enable-method = "psci";
 
 		};
+
+		cpu@8 {
+			device_type = "cpu";
+			compatible = "arm,armv8";
+			reg = <0x400>;
+			enable-method = "psci";
+		};
+
+		cpu@9 {
+			device_type = "cpu";
+			compatible = "arm,armv8";
+			reg = <0x401>;
+			enable-method = "psci";
+		};
+
+		cpu@10 {
+			device_type = "cpu";
+			compatible = "arm,armv8";
+			reg = <0x402>;
+			enable-method = "psci";
+		};
+
+		cpu@11 {
+			device_type = "cpu";
+			compatible = "arm,armv8";
+			reg = <0x403>;
+			enable-method = "psci";
+
+		};
 	};
 };
-- 
