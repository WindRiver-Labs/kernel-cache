From 6c85a0990f66dd2c8b1b7c1ab621be3abd0d9abd Mon Sep 17 00:00:00 2001
From: John Jacques <john.jacques@intel.com>
Date: Tue, 12 May 2015 12:30:45 -0500
Subject: [PATCH 030/131] arch/arm64: Update the Axxia Device Tree for
 Emulation

Signed-off-by: John Jacques <john.jacques@intel.com>
[Original patch is from linux-yocto-4.1 axxia/base branch.]
Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 arch/arm64/boot/dts/intel/axm5606-cpus.dtsi | 60 ++++++++++++++---------------
 1 file changed, 30 insertions(+), 30 deletions(-)

diff --git a/arch/arm64/boot/dts/intel/axm5606-cpus.dtsi b/arch/arm64/boot/dts/intel/axm5606-cpus.dtsi
index deb0d76..189d06c 100644
--- a/arch/arm64/boot/dts/intel/axm5606-cpus.dtsi
+++ b/arch/arm64/boot/dts/intel/axm5606-cpus.dtsi
@@ -17,43 +17,43 @@
 		cpu@0 {
 			device_type = "cpu";
 			compatible = "arm,armv8";
-			reg = <0x00>;
+			reg = <0x0>;
 			enable-method = "psci";
 		};
 
 		cpu@1 {
-			device_type = "cpu";
-			compatible = "arm,armv8";
-			reg = <0x01>;
-			enable-method = "psci";
-		};
+                      device_type = "cpu";
+                      compatible = "arm,armv8";
+                      reg = <0x1>;
+                      enable-method = "psci";
+               };
 
-		cpu@4 {
-			device_type = "cpu";
-			compatible = "arm,armv8";
-			reg = <0x04>;
-			enable-method = "psci";
-		};
+		cpu@2 {
+                      device_type = "cpu";
+                      compatible = "arm,armv8";
+                      reg = <0x100>;
+                      enable-method = "psci";
+               };
 
-		cpu@5 {
-			device_type = "cpu";
-			compatible = "arm,armv8";
-			reg = <0x05>;
-			enable-method = "psci";
-		};
+		cpu@3 {
+                      device_type = "cpu";
+                      compatible = "arm,armv8";
+                      reg = <0x101>;
+                      enable-method = "psci";
+               };
 
-		cpu@8 {
-			device_type = "cpu";
-			compatible = "arm,armv8";
-			reg = <0x08>;
-			enable-method = "psci";
-		};
+		cpu@4 {
+                      device_type = "cpu";
+                      compatible = "arm,armv8";
+                      reg = <0x200>;
+                      enable-method = "psci";
+               };
 
-		cpu@9 {
-			device_type = "cpu";
-			compatible = "arm,armv8";
-			reg = <0x09>;
-			enable-method = "psci";
-		};
+		cpu@5 {
+                      device_type = "cpu";
+                      compatible = "arm,armv8";
+                      reg = <0x201>;
+                      enable-method = "psci";
+               };
 	};
 };
