diff --git a/arch/arm64/boot/dts/intel/axc6716-cpus.dtsi b/arch/arm64/boot/dts/intel/axc6716-cpus.dtsi
new file mode 100644
index 0000000..b4dfd5f
--- /dev/null
+++ b/arch/arm64/boot/dts/intel/axc6716-cpus.dtsi
@@ -0,0 +1,131 @@
