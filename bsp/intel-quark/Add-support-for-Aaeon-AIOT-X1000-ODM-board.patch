+/** intel_qrk_spi_devs_addon
+ *
+ * addon spi device when gpio support in place
+ */
+static int intel_qrk_spi_devs_addon(void)
+{
+	int ret = 0;
+
+	if (aiot_x1000_probe != 1) {
+		ret = intel_qrk_spi_add_onboard_devs();
+		if (ret)
+			return ret;
+		ret = intel_qrk_spi_add_bpeak_devs();
+		aiot_x1000_probe = 1;
+	}
+
+	return ret;
+}
