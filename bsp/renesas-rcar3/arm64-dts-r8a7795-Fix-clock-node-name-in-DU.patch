From f05320c2b0fcb00e84227a8bf04da4421259ec01 Mon Sep 17 00:00:00 2001
From: Takeshi Kihara <takeshi.kihara.df@renesas.com>
Date: Wed, 14 Dec 2016 16:54:30 +0900
Subject: [PATCH 1424/2066] arm64: dts: r8a7795: Fix clock node name in DU

The node name of clock which DU node defined within .dtsi refers to is
the pin names of Salvaro-X Board, not R8A7795 SoC.

This patch changes to the pin names of R8A7795 SoC.

Signed-off-by: Takeshi Kihara <takeshi.kihara.df@renesas.com>
[zou:Original patch taken from
git://git.kernel.org/pub/scm/linux/kernel/git/horms/renesas-bsp.git
 v4.9/rcar-3.5.1]
Signed-off-by: zou cao <cao.zou@windriver.com>
---
 arch/arm64/boot/dts/renesas/r8a7795.dtsi |   15 +++++++--------
 1 files changed, 7 insertions(+), 8 deletions(-)

diff --git a/arch/arm64/boot/dts/renesas/r8a7795.dtsi b/arch/arm64/boot/dts/renesas/r8a7795.dtsi
index 40700d7..2d89479 100644
--- a/arch/arm64/boot/dts/renesas/r8a7795.dtsi
+++ b/arch/arm64/boot/dts/renesas/r8a7795.dtsi
@@ -193,25 +193,26 @@
 		clock-frequency = <0>;
 	};
 
-	programmable_clk0: clock_out0 {
+	/* DU input dot clock - to be overridden by boards that provide it */
+	du_dotclkin0: dclkin-0 {
 		compatible = "fixed-clock";
 		#clock-cells = <0>;
 		clock-frequency = <0>;
 	};
 
-	x21_clk: x21-clock {
+	du_dotclkin1: dclkin-1 {
 		compatible = "fixed-clock";
 		#clock-cells = <0>;
 		clock-frequency = <0>;
 	};
 
-	x22_clk: x22-clock {
+	du_dotclkin2: dclkin-2 {
 		compatible = "fixed-clock";
 		#clock-cells = <0>;
 		clock-frequency = <0>;
 	};
 
-	programmable_clk1: clock_out1 {
+	du_dotclkin3: dclkin-3 {
 		compatible = "fixed-clock";
 		#clock-cells = <0>;
 		clock-frequency = <0>;
@@ -1782,10 +1783,8 @@
 				 <&cpg CPG_MOD 722>,
 				 <&cpg CPG_MOD 721>,
 				 <&cpg CPG_MOD 727>,
-				 <&programmable_clk0>,
-				 <&x21_clk>,
-				 <&x22_clk>,
-				 <&programmable_clk1>;
+				 <&du_dotclkin0>, <&du_dotclkin1>,
+				 <&du_dotclkin2>, <&du_dotclkin3>;
 			clock-names = "du.0", "du.1", "du.2", "du.3",
 				      "lvds.0", "dclkin.0", "dclkin.1",
 				      "dclkin.2", "dclkin.3";
-- 
