From 2b566cba10370c31003a32c7bef6378915c70ff9 Mon Sep 17 00:00:00 2001
From: Jingchang Lu <jingchang.lu@freescale.com>
Date: Thu, 8 May 2014 08:28:30 +0800
Subject: [PATCH 085/255] arm: dts: ls1021a: fixup etsec base address and space
 size

Signed-off-by: Jingchang Lu <jingchang.lu@freescale.com>
[Kevin: Original patch taken from
LS1021A-SDK-V1.1-ARM-SOURCE-20140815-yocto.iso]
Signed-off-by: Kevin Hao <kexin.hao@windriver.com>
---
 arch/arm/boot/dts/ls1021a.dtsi | 10 +++++-----
 1 file changed, 5 insertions(+), 5 deletions(-)

diff --git a/arch/arm/boot/dts/ls1021a.dtsi b/arch/arm/boot/dts/ls1021a.dtsi
index c244975..1e66312 100644
--- a/arch/arm/boot/dts/ls1021a.dtsi
+++ b/arch/arm/boot/dts/ls1021a.dtsi
@@ -519,7 +519,7 @@
 			#size-cells = <0>;
 			device_type = "mdio";
 			compatible = "gianfar";
-			reg = <0x2d24000 0x1000>;
+			reg = <0x2d24000 0x4000>;
 			tbi0: tbi-phy@8 {
 				reg = <0x8>;
 				device_type = "tbi-phy";
@@ -541,7 +541,7 @@
 			queue-group@0 {
 				#address-cells = <1>;
 				#size-cells = <1>;
-				reg = <0x2d10000 0x1000>;
+				reg = <0x2d10000 0x8000>;
 				fsl,rx-bit-map = <0xff>;
 				fsl,tx-bit-map = <0xff>;
 				interrupts = <0 144 0x04>,
@@ -566,7 +566,7 @@
 			queue-group@0 {
 				#address-cells = <1>;
 				#size-cells = <1>;
-				reg = <0x2d50000 0x1000>;
+				reg = <0x2d50000 0x8000>;
 				fsl,rx-bit-map = <0xff>;
 				fsl,tx-bit-map = <0xff>;
 				interrupts = <0 150 0x04>,
@@ -576,7 +576,7 @@
 
 		};
 
-		enet2: ethernet@2d70000 {
+		enet2: ethernet@2d90000 {
 			#address-cells = <1>;
 			#size-cells = <1>;
 			interrupt-parent = <&gic>;
@@ -591,7 +591,7 @@
 			queue-group@0 {
 				#address-cells = <1>;
 				#size-cells = <1>;
-				reg = <0x2d70000 0x1000>;
+				reg = <0x2d90000 0x8000>;
 				fsl,rx-bit-map = <0xff>;
 				fsl,tx-bit-map = <0xff>;
 				interrupts = <0 157 0x04>,
-- 
