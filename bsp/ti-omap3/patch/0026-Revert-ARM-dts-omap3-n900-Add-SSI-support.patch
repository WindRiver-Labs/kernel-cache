From 27c1302c8936fcc92a449870b51017924cc5f7f2 Mon Sep 17 00:00:00 2001
From: czou <cao.zou@windriver.com>
Date: Mon, 7 Sep 2015 12:35:01 +0800
Subject: [PATCH 26/29] Revert "ARM: dts: omap3-n900: Add SSI support"

This reverts commit b55a5b25f9e8cf1cf05d18ac9e7539cc7f85a065.

Signed-off-by: czou <cao.zou@windriver.com>
---
 arch/arm/boot/dts/omap3.dtsi    | 45 -----------------------------------------
 arch/arm/boot/dts/omap34xx.dtsi | 11 ----------
 2 files changed, 56 deletions(-)

diff --git a/arch/arm/boot/dts/omap3.dtsi b/arch/arm/boot/dts/omap3.dtsi
index c8024ed..3f4d49b 100644
--- a/arch/arm/boot/dts/omap3.dtsi
+++ b/arch/arm/boot/dts/omap3.dtsi
@@ -726,51 +726,6 @@
 				clock-names = "fck";
 			};
 		};
-
-		ssi: ssi-controller@48058000 {
-			compatible = "ti,omap3-ssi";
-			ti,hwmods = "ssi";
-
-			status = "disabled";
-
-			reg = <0x48058000 0x1000>,
-			      <0x48059000 0x1000>;
-			reg-names = "sys",
-				    "gdd";
-
-			interrupts = <71>;
-			interrupt-names = "gdd_mpu";
-
-			#address-cells = <1>;
-			#size-cells = <1>;
-			ranges;
-
-			ssi_port1: ssi-port@4805a000 {
-				compatible = "ti,omap3-ssi-port";
-
-				reg = <0x4805a000 0x800>,
-				      <0x4805a800 0x800>;
-				reg-names = "tx",
-					    "rx";
-
-				interrupt-parent = <&intc>;
-				interrupts = <67>,
-					     <68>;
-			};
-
-			ssi_port2: ssi-port@4805b000 {
-				compatible = "ti,omap3-ssi-port";
-
-				reg = <0x4805b000 0x800>,
-				      <0x4805b800 0x800>;
-				reg-names = "tx",
-					    "rx";
-
-				interrupt-parent = <&intc>;
-				interrupts = <69>,
-					     <70>;
-			};
-		};
 	};
 };
 
diff --git a/arch/arm/boot/dts/omap34xx.dtsi b/arch/arm/boot/dts/omap34xx.dtsi
index 2e19544..2e92360 100644
--- a/arch/arm/boot/dts/omap34xx.dtsi
+++ b/arch/arm/boot/dts/omap34xx.dtsi
@@ -40,17 +40,6 @@
 	};
 };
 
-&ssi {
-	status = "ok";
-
-	clocks = <&ssi_ssr_fck_3430es2>,
-		 <&ssi_sst_fck_3430es2>,
-		 <&ssi_ick_3430es2>;
-	clock-names = "ssi_ssr_fck_3430es2",
-		      "ssi_sst_fck",
-		      "ssi_ick";
-};
-
 /include/ "omap34xx-omap36xx-clocks.dtsi"
 /include/ "omap36xx-omap3430es2plus-clocks.dtsi"
 /include/ "omap36xx-am35xx-omap3430es2plus-clocks.dtsi"
-- 
