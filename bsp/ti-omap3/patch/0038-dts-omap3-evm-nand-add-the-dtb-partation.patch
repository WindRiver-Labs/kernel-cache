From ce8af23227b50fc8e6f6b4f07301d8e33c1365da Mon Sep 17 00:00:00 2001
From: czou <cao.zou@windriver.com>
Date: Thu, 17 Sep 2015 06:27:16 +0000
Subject: [PATCH 2/2] dts: omap3-evm: nand: add the dtb partation

add 0x20000 partation size in nand for dtb

Signed-off-by: czou <cao.zou@windriver.com>
---
 arch/arm/boot/dts/omap3-evm.dts | 13 +++++++++----
 1 file changed, 9 insertions(+), 4 deletions(-)

diff --git a/arch/arm/boot/dts/omap3-evm.dts b/arch/arm/boot/dts/omap3-evm.dts
index 0c97c61..e0d9dcd 100644
--- a/arch/arm/boot/dts/omap3-evm.dts
+++ b/arch/arm/boot/dts/omap3-evm.dts
@@ -172,14 +172,19 @@
 			reg = <0x260000 0x20000>;
 		};
 
-		kernel@280000 {
+		dtb_env@280000 {
+			label = "dtb";
+			reg = <0x280000 0x20000>;
+		};
+
+		kernel@300000 {
 			label = "Kernel";
-			reg = <0x280000 0x400000>;
+			reg = <0x300000 0x600000>;
 		};
 
-		filesystem@680000 {
+		filesystem@900000 {
 			label = "File System";
-			reg = <0x680000 0xf980000>;
+			reg = <0x900000 0xf700000>;
 		};
 	};
 };
