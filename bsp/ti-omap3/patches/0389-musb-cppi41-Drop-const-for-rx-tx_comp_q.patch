---
 drivers/usb/musb/am35x.c      |    4 ++--
 drivers/usb/musb/cppi41_dma.h |    4 ++--
 2 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/drivers/usb/musb/am35x.c b/drivers/usb/musb/am35x.c
index 5c1e929..b39e411 100644
--- a/drivers/usb/musb/am35x.c
+++ b/drivers/usb/musb/am35x.c
@@ -104,9 +104,9 @@
  * ---------------------------------
  */
 
-static const u16 tx_comp_q[] = {63, 63, 63, 63, 63, 63, 63, 63, 63, 63, 63, 63, 63,
+static u16 tx_comp_q[] = {63, 63, 63, 63, 63, 63, 63, 63, 63, 63, 63, 63, 63,
 				63, 63};
-static const u16 rx_comp_q[] = {65, 65, 65, 65, 65, 65, 65, 65, 65, 65, 65, 65, 65,
+static u16 rx_comp_q[] = {65, 65, 65, 65, 65, 65, 65, 65, 65, 65, 65, 65, 65,
 				65, 65};
 
 /* Fair scheduling */
diff --git a/drivers/usb/musb/cppi41_dma.h b/drivers/usb/musb/cppi41_dma.h
index b99de78..c9e833f 100644
--- a/drivers/usb/musb/cppi41_dma.h
+++ b/drivers/usb/musb/cppi41_dma.h
@@ -37,8 +37,8 @@ struct usb_cppi41_info {
 	u8 q_mgr;
 	u8 num_tx_comp_q;
 	u8 num_rx_comp_q;
-	const u16 *tx_comp_q;
-	const u16 *rx_comp_q;
+	u16 *tx_comp_q;
+	u16 *rx_comp_q;
 };
 
 extern struct usb_cppi41_info usb_cppi41_info[];
