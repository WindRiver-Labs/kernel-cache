+		#size-cells = <0x1>;
+		ranges;
+
+		i2c@41600000 {
+			compatible = "xlnx,axi-iic-1.02.a", "xlnx,xps-iic-2.00.a";
+			interrupt-parent = <&gic>;
+			interrupts = <0 58 4>;
+			reg = <0x41600000 0x10000>;
+			bus-id = <0>;
+			#address-cells = <1>;
+			#size-cells = <0>;
+
+			mux@70 {
+				compatible = "pca9548";
+				reg = <0x70>;
+				#address-cells = <1>;
+				#size-cells = <0>;
+
+				i2c@0 {
+					#size-cells = <0>;
+					#address-cells = <1>;
+					reg = <0>;
+
+					rtc@68 {
+						compatible = "dallas,ds1337";
+						reg = <0x68>;
+					};
+				};
+
+				i2c@1 {
+					#size-cells = <0>;
+					#address-cells = <1>;
+					reg = <1>;
+
+					adv7511: adv7511@39 {
+						compatible = "adi,adv7511";
+						reg = <0x39>;
