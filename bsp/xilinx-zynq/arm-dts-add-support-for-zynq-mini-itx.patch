+				i2c@5 {
+					#size-cells = <0>;
+					#address-cells = <1>;
+					reg = <5>;
+					adau1761: adau1761@3b {
+						compatible = "adi,adau1761";
+						reg = <0x3b>;
+					};
+				};
+			};
