From cc81c670a95a4af1e054d6d3fdfa4ff66db05f7d Mon Sep 17 00:00:00 2001
From: Laurent Pinchart <laurent.pinchart@ideasonboard.com>
Date: Thu, 25 Sep 2014 16:07:57 +0300
Subject: [PATCH 313/456] v4l: xilinx: tpg: Remove axi- prefix from DT
 properties

This patch comes from:
  https://github.com/Xilinx/linux-xlnx.git

The prefix has been removed from the compatible strings, remove it from
the video-width and video-format properties as well.

Signed-off-by: Laurent Pinchart <laurent.pinchart@ideasonboard.com>
(cherry picked from commit b0e9ce7b5de50935adc8c8fe8032fe42677bfe95)
Signed-off-by: Zumeng Chen <zumeng.chen@windriver.com>
---
 .../bindings/media/xilinx/xlnx,v-tpg.txt           |   14 +++++++-------
 1 files changed, 7 insertions(+), 7 deletions(-)

diff --git a/Documentation/devicetree/bindings/media/xilinx/xlnx,v-tpg.txt b/Documentation/devicetree/bindings/media/xilinx/xlnx,v-tpg.txt
index 4c567a1..c6de1e3 100644
--- a/Documentation/devicetree/bindings/media/xilinx/xlnx,v-tpg.txt
+++ b/Documentation/devicetree/bindings/media/xilinx/xlnx,v-tpg.txt
@@ -13,8 +13,8 @@ Required properties:
 
 - reg: Physical base address and length of the registers set for the device.
 
-- xlnx,axi-video-format, xlnx,axi-video-width: Video format and width, as
-  defined in video.txt.
+- xlnx,video-format, xlnx,video-width: Video format and width, as defined in
+  video.txt.
 
 - port: Video port, using the DT bindings defined in ../video-interfaces.txt.
   The TPG has a single output port numbered 0.
@@ -33,7 +33,7 @@ synthesized with two ports and forbidden when synthesized with one port.
 
 Example:
 
-	axi_tpg_0: axi_tpg@40050000 {
+	tpg_0: tpg@40050000 {
 		compatible = "xlnx,v-tpg-6.0", "xlnx,v-tpg-5.0";
 		reg = <0x40050000 0x10000>;
 
@@ -47,8 +47,8 @@ Example:
 			port@0 {
 				reg = <0>;
 
-				xlnx,axi-video-format = "yuv422";
-				xlnx,axi-video-width = <8>;
+				xlnx,video-format = "yuv422";
+				xlnx,video-width = <8>;
 
 				tpg_in: endpoint {
 					remote-endpoint = <&adv7611_out>;
@@ -57,8 +57,8 @@ Example:
 			port@1 {
 				reg = <1>;
 
-				xlnx,axi-video-format = "yuv422";
-				xlnx,axi-video-width = <8>;
+				xlnx,video-format = "yuv422";
+				xlnx,video-width = <8>;
 
 				tpg1_out: endpoint {
 					remote-endpoint = <&switch_in0>;
-- 
