From b23448a4aa89c3df42280e70aa4f957480652c98 Mon Sep 17 00:00:00 2001
From: Zumeng Chen <zumeng.chen@windriver.com>
Date: Thu, 29 Jan 2015 09:55:09 +0800
Subject: [PATCH 14/14] arm: dts: enable ethernet for new version zc706 board

This patch comes from branch xlnx_3.14 currently
  https://github.com/Xilinx/linux-xlnx.git

Signed-off-by: Zumeng Chen <zumeng.chen@windriver.com>
---
 arch/arm/boot/dts/zynq-zc706.dts |   12 ++++++------
 1 files changed, 6 insertions(+), 6 deletions(-)

diff --git a/arch/arm/boot/dts/zynq-zc706.dts b/arch/arm/boot/dts/zynq-zc706.dts
index c4922c5..cf75835 100644
--- a/arch/arm/boot/dts/zynq-zc706.dts
+++ b/arch/arm/boot/dts/zynq-zc706.dts
@@ -119,11 +119,11 @@
 		ps7_ethernet_0: ps7-ethernet@e000b000 {
 			#address-cells = <1>;
 			#size-cells = <0>;
-			clock-names = "ref_clk", "aper_clk";
-			clocks = <&clkc 13>, <&clkc 30>;
-			compatible = "xlnx,ps7-ethernet-1.00.a";
-			interrupt-parent = <&ps7_scugic_0>;
-			interrupts = <0 22 4>;
+			clock-names = "pclk", "hclk", "tx_clk", "ref_clk", "aper_clk";
+			clocks = <0x2 0x1e 0x2 0x1e 0x2 0xd 0x2 0xd 0x2 0x1e>;
+			compatible = "cdns,gem", "xlnx,ps7-ethernet-1.00.a";
+			interrupt-parent = <0x3>;
+			interrupts = <0x0 0x16 0x4>;
 			local-mac-address = [00 0a 35 00 00 00];
 			phy-handle = <&phy0>;
 			phy-mode = "rgmii-id";
@@ -131,7 +131,7 @@
 			xlnx,enet-reset = "MIO 47";
 			xlnx,eth-mode = <0x1>;
 			xlnx,has-mdio = <0x1>;
-			xlnx,ptp-enet-clock = <111111115>;
+			xlnx,ptp-enet-clock = <0x69f6bcb>;
 			mdio {
 				#address-cells = <1>;
 				#size-cells = <0>;
-- 
1.7.5.4

