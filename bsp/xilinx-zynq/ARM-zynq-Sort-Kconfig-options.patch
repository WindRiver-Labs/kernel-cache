From 0a7041f468ed7ee630c65c1d625419a08ccba890 Mon Sep 17 00:00:00 2001
From: Zhong Hongbo <hongbo.zhong@windriver.com>
Date: Wed, 16 Jul 2014 16:20:23 +0800
Subject: [PATCH 028/509] ARM: zynq: Sort Kconfig options

https://github.com/analogdevicesinc/linux.git xcomm_zynq
commit 0efab1574b708178c09fa02eaab224c0db11f0ad

Based on discussion on ARM mailing list options
should be sorted.

Signed-off-by: Michal Simek <michal.simek@xilinx.com>
Signed-off-by: Zhong Hongbo <hongbo.zhong@windriver.com>
---
 arch/arm/mach-zynq/Kconfig |   22 +++++++++++-----------
 1 files changed, 11 insertions(+), 11 deletions(-)

diff --git a/arch/arm/mach-zynq/Kconfig b/arch/arm/mach-zynq/Kconfig
index 5c4692f..9f57cc2 100644
--- a/arch/arm/mach-zynq/Kconfig
+++ b/arch/arm/mach-zynq/Kconfig
@@ -1,26 +1,26 @@
 config ARCH_ZYNQ
 	bool "Xilinx Zynq ARM Cortex A9 Platform" if ARCH_MULTI_V7
+	select ARCH_HAS_CPUFREQ
+	select ARCH_HAS_OPP
+	select ARCH_REQUIRE_GPIOLIB
 	select ARM_AMBA
 	select ARM_GIC
-	select COMMON_CLK
-	select CPU_V7
+	select CACHE_L2X0
+	select CADENCE_TTC_TIMER
+	select GENERIC_ALLOCATOR
 	select GENERIC_CLOCKEVENTS
 	select HAVE_ARM_SCU if SMP
 	select HAVE_ARM_TWD if LOCAL_TIMERS
+	select HAVE_SMP
 	select ICST
+	select MFD_SYSCON
 	select MIGHT_HAVE_CACHE_L2X0
+	select MIGHT_HAVE_PCI
+	select COMMON_CLK
+	select CPU_V7
 	select USE_OF
-	select HAVE_SMP
 	select SPARSE_IRQ
-	select CACHE_L2X0
-	select ARCH_REQUIRE_GPIOLIB
 	select COMMON_CLK
-	select ARCH_HAS_CPUFREQ
-	select ARCH_HAS_OPP
-	select MIGHT_HAVE_PCI
-	select CADENCE_TTC_TIMER
-	select GENERIC_ALLOCATOR
-	select MFD_SYSCON
 	help
 	  Support for Xilinx Zynq ARM Cortex A9 Platform
 
