From 5b16f8ff24c44194a216c7c89f4b59c6f12f50cd Mon Sep 17 00:00:00 2001
From: Liming Wang <liming.wang@windriver.com>
Date: Fri, 30 Nov 2012 14:58:00 +0800
Subject: [PATCH] arm/zynq: Update device tree to unify stdout-path with
 correct device

"linux,stdout-path" should be assigned with one node of uart device, which
should be "/amba@0/uart@e0001000" instead of ""/amba@0/uart@E0001000".

Signed-off-by: Liming Wang <liming.wang@windriver.com>
---
 arch/arm/boot/dts/zynq-zc702-qemu-sd.dts |    2 +-
 arch/arm/boot/dts/zynq-zc702-sd.dts      |    2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/arm/boot/dts/zynq-zc702-qemu-sd.dts b/arch/arm/boot/dts/zynq-zc702-qemu-sd.dts
index 5d4f0dd..1929197 100644
--- a/arch/arm/boot/dts/zynq-zc702-qemu-sd.dts
+++ b/arch/arm/boot/dts/zynq-zc702-qemu-sd.dts
@@ -10,7 +10,7 @@
 	/*root on SD partition*/
 	chosen {
 		bootargs = "console=ttyPS0,115200 earlyprintk root=/dev/mmcblk0 rw rootwait ip=:::::eth0:dhcp";
-		linux,stdout-path = "/amba@0/uart@E0001000";
+		linux,stdout-path = "/amba@0/uart@e0001000";
 	};
 
 /include/ "zc702-qemu.dtsi"
diff --git a/arch/arm/boot/dts/zynq-zc702-sd.dts b/arch/arm/boot/dts/zynq-zc702-sd.dts
index 8208852..2735029 100644
--- a/arch/arm/boot/dts/zynq-zc702-sd.dts
+++ b/arch/arm/boot/dts/zynq-zc702-sd.dts
@@ -10,7 +10,7 @@
 /*root on SD second partition*/
 	chosen {
 		bootargs = "console=ttyPS0,115200 earlyprintk root=/dev/mmcblk0p2 ro rootwait";
-		linux,stdout-path = "/amba@0/uart@E0001000";
+		linux,stdout-path = "/amba@0/uart@e0001000";
 	};
 
 /include/ "zc702.dtsi"
-- 
1.7.9.5

