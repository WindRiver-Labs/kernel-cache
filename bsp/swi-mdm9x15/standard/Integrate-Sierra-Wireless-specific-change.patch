+int param_set_ldo5(const char *val, const struct kernel_param *kp)
+{
+	int voltage = 0;
+	int rv = param_set_bool( val, kp );
+	if( !rv )
+	{
+		printk( KERN_INFO "Setting LDO5 to %d\n", ldo5 );
+
+		if( ldo5 )
+			voltage = 2850000;
+
+		rpm_vreg_set_voltage(RPM_VREG_ID_PM8018_L5, RPM_VREG_VOTER3,
+							 voltage, voltage, 1);
+	}
+	return rv;
+}
