+  BSWP7104,
+  BSMC7305,
+  BSEM7305,
+  BSAC342U,
+  BSAC341U,
+  BSEM7655,
+  BSMC8805,
+  BSEM8805,
+  BSAC771S,
+  BSYW7X55,
+  BSWP7100_INSIM,   /* this is kept to align with BS_QFPROM_PROD_ID */
+  BSWP7102_INSIM,   /* this is kept to align with BS_QFPROM_PROD_ID */
+  BSWP7104_INSIM,   /* this is kept to align with BS_QFPROM_PROD_ID */
+  BSMC7800,
+  BSMC7800LO,
+  BSMC7802,
+  BSMC7804,
+  BSWP7100_LARGER_MEMORY,
