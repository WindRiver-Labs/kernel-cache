From 01f9a8586e3cea67d6d829faa3cb3b35d2c8190d Mon Sep 17 00:00:00 2001
From: Zumeng Chen <zumeng.chen@windriver.com>
Date: Mon, 11 Aug 2014 15:48:14 +0800
Subject: [PATCH 3/4] arm: dts: dra7-evm: clean up for double definition.

These three parts are defined by twice, so remove one of them.

Signed-off-by: Zumeng Chen <zumeng.chen@windriver.com>
---
 arch/arm/boot/dts/dra7-evm.dts |   69 ----------------------------------------
 1 files changed, 0 insertions(+), 69 deletions(-)

diff --git a/arch/arm/boot/dts/dra7-evm.dts b/arch/arm/boot/dts/dra7-evm.dts
index 78c290c..4518e01 100644
--- a/arch/arm/boot/dts/dra7-evm.dts
+++ b/arch/arm/boot/dts/dra7-evm.dts
@@ -1113,75 +1113,6 @@
 	};
 };
 
-&vip1 {
-	pinctrl-names = "default";
-	/*
-		Pinmux conflict between vin1a:16-23 & vin2a:0-8
-		Either use full 24bit of vin1a or use 16bit vin1a and 8bitvin2a
-		TO enable cam2, uncomment this
-		pinctrl-0 = <&vin1a_pins &vin2a>;
-	*/
-	pinctrl-0 = <&vin1a_pins &vin1a_d16_d23_pins>;
-
-	vin1a: port@0A {
-		#address-cells = <1>;
-		#size-cells = <0>;
-		reg = <0>;
-	};
-
-	vin2a: port@1A {
-		#address-cells = <1>;
-		#size-cells = <0>;
-		reg = <1>;
-	};
-};
-
-&vip2 {
-	pinctrl-names = "default";
-	/*
-		Pinmux conflict between vin3a and emac1
-		To enable vin3a, uncomment this
-		and remove Slave 2 section from cpsw_default_pins
-		pinctrl-0 = <&vin3a_pins>;
-	*/
-	pinctrl-0 = <&vin3a_pins>;
-
-	vin3a: port@0A {
-		#address-cells = <1>;
-		#size-cells = <0>;
-		reg = <0>;
-	};
-
-	vin4a: port@1A {
-		#address-cells = <1>;
-		#size-cells = <0>;
-		reg = <1>;
-	};
-};
-
-&vip3 {
-	pinctrl-names = "default";
-	/*
-		Pinmux conflict between vin6a and atl,radio,mcasp3 pins
-		To enable vin6a, uncomment this and
-		remove atl_pins, mcasp3_pins, radio_pins from dra7_pmx_core
-		pinctrl-0 = <&vin5a_pins &vin6a_pins>;
-	*/
-	pinctrl-0 = <&vin5a_pins>;
-
-	vin5a: port@0A {
-		#address-cells = <1>;
-		#size-cells = <0>;
-		reg = <0>;
-	};
-
-	vin6a: port@1A {
-		#address-cells = <1>;
-		#size-cells = <0>;
-		reg = <1>;
-	};
-};
-
 &cpu0 {
 	cpu0-supply = <&avs_mpu>;
 };
-- 
