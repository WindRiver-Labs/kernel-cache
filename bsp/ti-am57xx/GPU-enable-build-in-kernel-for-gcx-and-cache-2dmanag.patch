From 321e6b689a49a0da032ed7b41a95a0c8b531ae1c Mon Sep 17 00:00:00 2001
From: Zumeng Chen <zumeng.chen@windriver.com>
Date: Fri, 8 Aug 2014 10:36:08 +0800
Subject: [PATCH 263/263] GPU: enable build in kernel for gcx and
 cache-2dmanager

This patch comes from the following git repo:

  git://git.omapzoom.org/kernel/omap.git

Add supports cache-2dmanager in Makefile and Kconfig

Signed-off-by: Zumeng Chen <zumeng.chen@windriver.com>
---
 drivers/misc/Kconfig  |    4 ++++
 drivers/misc/Makefile |    4 ++++
 2 files changed, 8 insertions(+), 0 deletions(-)

diff --git a/drivers/misc/Kconfig b/drivers/misc/Kconfig
index 3d5191d..71a0338 100644
--- a/drivers/misc/Kconfig
+++ b/drivers/misc/Kconfig
@@ -556,4 +556,8 @@ source "drivers/misc/carma/Kconfig"
 source "drivers/misc/altera-stapl/Kconfig"
 source "drivers/misc/mei/Kconfig"
 source "drivers/misc/vmw_vmci/Kconfig"
+source "drivers/misc/gcx/gccore/Kconfig"
+source "drivers/misc/gcx/gcioctl/Kconfig"
+source "drivers/misc/gcx/gcbv/Kconfig"
+source "drivers/misc/cache-2dmanager/Kconfig"
 endmenu
diff --git a/drivers/misc/Makefile b/drivers/misc/Makefile
index dd772ea..5e8e2bd 100644
--- a/drivers/misc/Makefile
+++ b/drivers/misc/Makefile
@@ -55,3 +55,7 @@ obj-$(CONFIG_SI570)		+= si570.o
 obj-$(CONFIG_VMWARE_VMCI)	+= vmw_vmci/
 obj-$(CONFIG_LATTICE_ECP3_CONFIG)	+= lattice-ecp3-config.o
 obj-$(CONFIG_SRAM)		+= sram.o
+obj-$(CONFIG_GCCORE)            += gcx/gccore/
+obj-$(CONFIG_GCBV)              += gcx/gcbv/
+obj-$(CONFIG_GCIOCTL)           += gcx/gcioctl/
+obj-$(CONFIG_CACHE2DMANAGER)    += cache-2dmanager/
-- 
