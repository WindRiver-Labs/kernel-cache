@@ -378,6 +378,8 @@ int nlm_hal_init_cs34x7(int hwport,int num_lanes, int lane_rate)
 	}
 #endif
 
+	if(nae_id == 1)
+		hwport += 8;
 	switch(hwport) {
 		case XLP_ILK_PORT_0:
 			cortina_cs34x7_init(XLP_ILK_PORT0_CS, (XLP_ILK_MAX_LANES - num_lanes), lane_rate);
