-				sd->descs[keyidx+2] = key[0];
-			}
-			else
-				memcpy((char *)(long)&sd->descs[keyidx],initp->cipher.key, 
+			memcpy((char *)(long)&sd->descs[keyidx],initp->cipher.key, 
 					initp->cipher.key_len);
 			spriv->cipher_key_len_in_dw = 	((cryptosoc_roundup(initp->cipher.key_len,
 							sizeof(_uint64_t))) / sizeof(_uint64_t)); 
