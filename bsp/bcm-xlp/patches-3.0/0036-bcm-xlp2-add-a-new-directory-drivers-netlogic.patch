From a8712f3f88340c7d34a174a3ce006805196bd62e Mon Sep 17 00:00:00 2001
From: Jack Tan <jack.tan@windriver.com>
Date: Fri, 14 Feb 2014 17:54:36 +0800
Subject: [PATCH 36/58] bcm-xlp2: add a new directory 'drivers/netlogic'

It's to store the core kernel modules from SDK_DIR/linux-userspace/kmod

Signed-off-by: Jack Tan <jack.tan@windriver.com>
---
 drivers/Kconfig           |    2 ++
 drivers/Makefile          |    1 +
 drivers/netlogic/Kconfig  |   47 +++++++++++++++++++++++++++++++++++++++++++++
 drivers/netlogic/Makefile |   13 +++++++++++++
 4 files changed, 63 insertions(+)

diff --git a/drivers/Kconfig b/drivers/Kconfig
index c57b60b..303b401 100644
--- a/drivers/Kconfig
+++ b/drivers/Kconfig
@@ -168,4 +168,6 @@ source "drivers/ipack/Kconfig"
 
 source "drivers/reset/Kconfig"
 
+source "drivers/netlogic/Kconfig"
+
 endmenu
diff --git a/drivers/Makefile b/drivers/Makefile
index 0dd8827..6f6e37b 100644
--- a/drivers/Makefile
+++ b/drivers/Makefile
@@ -153,3 +153,4 @@ obj-$(CONFIG_IIO)		+= iio/
 obj-$(CONFIG_VME_BUS)		+= vme/
 obj-$(CONFIG_IPACK_BUS)		+= ipack/
 obj-$(CONFIG_NTB)		+= ntb/
+obj-$(CONFIG_NLM_XLP_BOARD)	+= netlogic/
diff --git a/drivers/netlogic/Kconfig b/drivers/netlogic/Kconfig
new file mode 100644
index 0000000..df7c1ff
--- /dev/null
+++ b/drivers/netlogic/Kconfig
@@ -0,0 +1,47 @@
+config XLP_NAE_SUPPORT
+        bool "Enable XLP NAE support"
+        depends on NLM_XLP_BOARD
+        default y
+        help
+          This option enables XLP Network Acceleration engine support.
+
+config XLP_CDE
+        tristate "broadcom xlp CDE driver"
+        depends on NLM_XLP_BOARD
+        default y
+        help
+          This driver supports broadcom's compression/decompression Engine.
+          For more information on xlp CDE driver, please visit
+
+          <http://support.broadcom.com>
+
+          To compile this driver as a module, choose M here.
+
+config XLP_NETL7
+        tristate "Broadcom xlp netl7 driver"
+        depends on NLM_XLP_BOARD
+        default y
+        help
+          This driver supports Broadcom regex driver.
+          It is only valid on XLP3XX and XLP2XX.
+          The module will be called netl7driver.
+          For more information on xlp netl7 driver, please visit
+
+          <http://support.broadcom.com>
+
+          To compile this driver as a module, choose M here.
+
+config XLP_SAE
+        tristate "Broadcom xlp SAE driver"
+        depends on NLM_XLP_BOARD
+	select CRYPTO_AEAD
+	select CRYPTO_BLKCIPHER
+	select CRYPTO_HASH
+        default y
+        help
+          This driver supports broadcom's Security Acceleration Engine.
+          For more information on xlp SAE driver, please visit
+
+          <http://support.broadcom.com>
+
+          To compile this driver as a module, choose M here. The module will be called sae.
diff --git a/drivers/netlogic/Makefile b/drivers/netlogic/Makefile
new file mode 100644
index 0000000..01c8605
--- /dev/null
+++ b/drivers/netlogic/Makefile
@@ -0,0 +1,13 @@
+obj-$(CONFIG_NLM_XLP_BOARD)	+= soc_interface/
+obj-$(CONFIG_NLM_XLP_BOARD)	+= nlmcrypto/
+obj-$(CONFIG_NLM_XLP_BOARD)	+= nmiprof/
+obj-$(CONFIG_NLM_XLP_BOARD)	+= pkt_pool_mem/
+obj-$(CONFIG_NLM_XLP_BOARD)	+= fmn_dp/
+obj-$(CONFIG_NLM_XLP_BOARD)	+= cmem/
+
+obj-$(CONFIG_XLP_NAE_SUPPORT)	+= nae/
+obj-$(CONFIG_XLP_CDE)		+= nlm_cde/
+obj-$(CONFIG_XLP_SAE)		+= sae/
+obj-$(CONFIG_XLP_NETL7)		+= netl7driver/
+
+obj-$(CONFIG_NLM_ADMA)		+= dtre/
-- 
