-#define XLP9XX_SYS_CLKDEV_BASE(node)   (KSEG1 +0x18000000 + (XLP9XX_SYS_BUS << 20) + (((node * XLP_MAXDEV_PERNODE) + XLP9XX_SYS_DEVICE) << 15) + (XLP9XX_SYS_FUNC_CLKDEV << 12))
