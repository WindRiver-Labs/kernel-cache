Date: Fri, 22 Nov 2013 15:40:47 -0800
Subject: [PATCH 055/160] netlib: Removed fdt parsing for intf fifo size

Signed-off-by: Jack Tan <jack.tan@windriver.com>
Signed-off-by: Jack Tan <jiankemeng@gmail.com>
---
 arch/mips/netlogic/lib/netlib/src/netsoc_fdt.c |    4 ++++
 1 file changed, 4 insertions(+)

diff --git a/arch/mips/netlogic/lib/netlib/src/netsoc_fdt.c b/arch/mips/netlogic/lib/netlib/src/netsoc_fdt.c
index 4530f87..159c0db 100644
--- a/arch/mips/netlogic/lib/netlib/src/netsoc_fdt.c
+++ b/arch/mips/netlogic/lib/netlib/src/netsoc_fdt.c
@@ -787,9 +787,13 @@ static void extract_complex_params(void *fdt, int intf_type, char *nae_port_str,
 		update_default_config(intf_type, cmplx->free_desc_size, get_free_desc_size(intf_type));
 	}
 
+#ifdef INTF_FIFOSZ_FROM_FDT
         if (GET_PORT_PROP("iface-fifo-size", &cmplx->intf_fifo_size, sizeof(cmplx->intf_fifo_size)) < 0) {
+#endif
 		update_default_config(intf_type, cmplx->intf_fifo_size, get_intf_fifo_size(intf_type));
+#ifdef INTF_FIFOSZ_FROM_FDT
 	}
+#endif
 
         if (GET_PORT_PROP("parser-sequence-fifo-size", &cmplx->prsr_seq_fifo_size, sizeof(cmplx->prsr_seq_fifo_size)) < 0) {
 		update_default_config(intf_type, cmplx->prsr_seq_fifo_size, get_prsr_seq_fifo_size(intf_type));
