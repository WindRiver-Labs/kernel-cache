 drivers/mtd/nand/xlp_plat_nand.c |    4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/drivers/mtd/nand/xlp_plat_nand.c b/drivers/mtd/nand/xlp_plat_nand.c
index 10c3a1c..5f82967 100644
--- a/drivers/mtd/nand/xlp_plat_nand.c
+++ b/drivers/mtd/nand/xlp_plat_nand.c
@@ -37,8 +37,8 @@ THE POSSIBILITY OF SUCH DAMAGE.
 #endif
 #include "xlp_plat_nand.h"
 
-int debug = 0;
-int enable_hwecc = 1;
+static int debug = 0;
+static int enable_hwecc = 1;
 
 #undef	NAND_DEBUG
 #define BUF_SIZE 		(16*1024)
