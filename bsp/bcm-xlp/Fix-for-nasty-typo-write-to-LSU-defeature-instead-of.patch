From 9580cc0c7bd962b5d49373a90827d5c362b46969 Mon Sep 17 00:00:00 2001
From: Venu Vadapalli <vvadapalli@netlogicmicro.com>
Date: Thu, 28 Oct 2010 21:01:22 -0700
Subject: [PATCH 216/761] Fix for nasty typo write to LSU defeature instead of
 SCHED defeature!

Based on Broadcom SDK 2.3.

Signed-off-by: Venu Vadapalli <vvadapalli@netlogicmicro.com>
Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 arch/mips/netlogic/xlp/cpu_control_asm.S |    6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/arch/mips/netlogic/xlp/cpu_control_asm.S b/arch/mips/netlogic/xlp/cpu_control_asm.S
index 12fce72..8a5909d 100644
--- a/arch/mips/netlogic/xlp/cpu_control_asm.S
+++ b/arch/mips/netlogic/xlp/cpu_control_asm.S
@@ -74,11 +74,11 @@
 	and	t1, t1, t2
 
 	mtcr    t1, t0
-	
-	li      t0, LSU_DEFEATURE
+
+	li      t0, SCHED_DEFEATURE
 	lui     t1, 0x0100 # Experimental: Disable BRU accepting ALU ops
 	mtcr    t1, t0
-	
+
 	.set pop
 .endm
 
