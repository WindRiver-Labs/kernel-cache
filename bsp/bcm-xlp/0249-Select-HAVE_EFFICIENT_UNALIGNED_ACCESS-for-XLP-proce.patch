From 78f6ed73e352b7aaf3170589977440baa233dd53 Mon Sep 17 00:00:00 2001
From: Mehul <vmehul@netlogicmicro.com>
Date: Tue, 5 Apr 2011 13:52:02 +0530
Subject: [PATCH 249/762] Select HAVE_EFFICIENT_UNALIGNED_ACCESS for XLP processors.

Based on Broadcom SDK 2.3.

Signed-off-by: Mehul <vmehul@netlogicmicro.com>
Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 arch/mips/Kconfig |    1 +
 1 files changed, 1 insertions(+), 0 deletions(-)

diff --git a/arch/mips/Kconfig b/arch/mips/Kconfig
index 372ef4b..c0ce0cc 100644
--- a/arch/mips/Kconfig
+++ b/arch/mips/Kconfig
@@ -1624,6 +1624,7 @@ config CPU_XLP
 	select WEAK_ORDERING
 	select WEAK_REORDERING_BEYOND_LLSC
 	select CPU_HAS_PREFETCH
+	select HAVE_EFFICIENT_UNALIGNED_ACCESS
 	help
 	  Netlogic Microsystems XLP processors.
 endchoice
