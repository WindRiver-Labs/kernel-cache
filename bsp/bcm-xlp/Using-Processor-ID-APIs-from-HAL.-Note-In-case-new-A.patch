From 5087209e3146aa8a42fa398d690c07cefa718ba6 Mon Sep 17 00:00:00 2001
From: Vikas Gupta <vikasg@netlogicmicro.com>
Date: Mon, 14 Nov 2011 14:55:06 +0530
Subject: [PATCH 476/761] Using Processor ID APIs from HAL. Note: In case new
 API is required related to processor ID please add
 in HAL.

Based on Broadcom SDK 2.3.

Signed-off-by: Vikas Gupta <vikasg@netlogicmicro.com>
Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 arch/mips/netlogic/common/Makefile |    2 +-
 arch/mips/netlogic/msgcfg/Makefile |    1 +
 2 files changed, 2 insertions(+), 1 deletion(-)

diff --git a/arch/mips/netlogic/common/Makefile b/arch/mips/netlogic/common/Makefile
index 021cb97..e50eafe 100644
--- a/arch/mips/netlogic/common/Makefile
+++ b/arch/mips/netlogic/common/Makefile
@@ -25,7 +25,7 @@ obj-$(CONFIG_SMP)                 	+= smp.o
 
 EXTRA_AFLAGS := $(CFLAGS)
 
-clean-files += msgring.o msgring_xls.o msgring_shared.o nlm_hal_fmn_config.o
+clean-files += msgring.o msgring_xls.o msgring_shared.o nlm_hal_fmn_config.o nlm_hal_cpu_info.o
 clean-files += nlm_hal.o
 clean-files += srio.o dma.o smp.o cpu_proc.o
 clean-files += msgring.c msgring_xls.c msgring_shared.c nlm_hal.c
diff --git a/arch/mips/netlogic/msgcfg/Makefile b/arch/mips/netlogic/msgcfg/Makefile
index 795da4c..7bd0cad 100644
--- a/arch/mips/netlogic/msgcfg/Makefile
+++ b/arch/mips/netlogic/msgcfg/Makefile
@@ -29,6 +29,7 @@ ifneq ("$(HAL_DIR)", "")
 	$(Q)cp -fr $(HAL_DIR)/nlm_evp_cpld.c  ../common
 	$(Q)cp -fr $(HAL_DIR)/cortina_cs34x7  ../common
 	$(Q)cp -fr $(HAL_DIR)/nlm_hal_nae.c  ../common
+	$(Q)cp -fr $(HAL_DIR)/nlm_hal_cpu_info.c  ../common
 	$(Q)cp -fr $(FDT_DIR)/libfdt/contrib/fdt_helper.c  ../common
 	/bin/rm -rf $(LOCAL_HAL_DIR)/nlm_hal_pic.h
 endif
-- 
