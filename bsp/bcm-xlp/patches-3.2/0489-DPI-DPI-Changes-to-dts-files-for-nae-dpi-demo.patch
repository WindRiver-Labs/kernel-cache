From 143c2a7db59af5d9a5aa3a3154986964b09090ff Mon Sep 17 00:00:00 2001
From: Siva Krishna Gudivada <sivag@broadcom.com>
Date: Mon, 10 Mar 2014 12:15:24 +0530
Subject: DPI: DPI: Changes to dts files for nae-dpi demo

    Removed unnecessary dts files
    Increased linux memory in dts files
[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
Signed-off-by: Nam Ninh <nam.ninh@windriver.com>

diff --git a/arch/mips/boot/dts/xlp2xx-dpi-nae-mc.dts b/arch/mips/boot/dts/xlp2xx-dpi-nae-mc.dts
new file mode 100644
index 0000000..0b7229a
--- /dev/null
+++ b/arch/mips/boot/dts/xlp2xx-dpi-nae-mc.dts
@@ -0,0 +1,169 @@
+/* XLP2XX Device Tree Source
+ *
+ */
+
+/dts-v1/;
+/ {
+	model = "netlogic,XLP-FVP";
+        compatible = "netlogic,xlp";
+        #address-cells = <2>;
+        #size-cells = <2>;
+	/*memory range for linux. This will not appear in dom0*/
+        memory {
+                device_type = "memory";
+                reg =  <0 0x01000000 0 0x0B000000   
+                        0 0xe0000000 0 0x20000000>;
+        };
+
+	hypervisor {
+		hypervisor-name = "Xen";
+		alloc_dom0_memory = <0>;
+		bootargs = "ncores=4 dom0_loadaddr=0x72000000 dom0_size=0x1c000000 dom0_cpumask=0xffff -- ";
+		domain_heap = <0x80000000 0x20000000>; /* 3584MB -> 6656MB */
+	};
+	doms {
+		#address-cells = <1>;
+		#size-cells = <1>;
+		dom@0 {
+			device_type = "domain";
+			os = "linux";
+
+			#address-cells = <1>;
+			#size-cells = <1>;
+
+			cpu {
+				onlinemask = <0xff>;
+				//onlinemask = <0x1>;
+				nae-rx-vc = <2>;
+				nae-fb-vc = <2>;
+				napi-vc-mask = <0x4>;
+			};
+			uart {
+				id = <0>;
+				owner = <1>;
+				sharedcfg = <0x1f000000>;
+			};
+			/*memory {
+				reg = <0x01000000 0x0B000000
+					0x28000000 0x38000000>;  //1024M - 128M at 512M + 128M
+					//0x28000000 0x98000000>;  // 2560M - 128M at 512M + 128M
+
+			};
+			pic {
+			};*/
+			nae {
+				freein-fifo-mask = <0x0 0 0 0>; 
+				sh-domains = <1>;
+				/* node offsets and size */
+				vfbtbl-sw-offset = <0 0 0 0>;
+				vfbtbl-sw-nentries = <32 32 32 32>;
+				vfbtbl-hw-offset = <0 0 0 0>;
+				vfbtbl-hw-nentries = <0 0 0 0>;
+			};
+			owner-config {
+				nae = <0x1>;   // currently for node 0
+				sae = <0x1>;
+				rsa = <0x1>;
+				fmn = <0x1>;
+			};
+			fmn {
+				node_0_vc_mask = <0x44444444 0x44444444 0x44444444 0x44444444>;
+				node_1_vc_mask = <0x44444444 0x44444444 0x44444444 0x44444444>;
+				node_2_vc_mask = <0x44444444 0x44444444 0x44444444 0x44444444>;
+				node_3_vc_mask = <0x44444444 0x44444444 0x44444444 0x44444444>;
+			};
+
+
+		};
+		dom@1 {
+			device_type = "domain";
+			os = "uspace";
+
+
+			#address-cells = <1>;
+			#size-cells = <1>;
+
+			kernel {
+				/* Kernel Memory allocated from xen domain-heap */
+				size = <0x02000000>;  // 32M
+
+
+				/* Domain destroy currently doesn't work with hyperexec */
+				dom_destroy_hack = <0>;
+			};
+
+			cpu {
+				onlinemask = <0xff>;
+				//onlinemask = <0x1>;
+				nae-rx-vc = <0>;
+				nae-fb-vc = <3>;
+			};
+			uart {
+				id = <0>;
+				sharedcfg = <0x1f000000>;
+			};
+			nae {
+				freein-fifo-mask = <0xf0 0 0 0>;
+				sh-domains = <0>;
+				/* node offsets and size */
+				vfbtbl-sw-offset = <32 32 32 32>;
+				vfbtbl-sw-nentries = <32 32 32 32>;
+				vfbtbl-hw-offset = <64 64 64 64>;
+				vfbtbl-hw-nentries = <20 20 20 20>;
+				/* replenish info syntax */
+				freein-fifo-replenish-by-owner = <0>;
+				/* < node paddr-H32bits paddr-L32bits size-H32bits size-L32bits */
+				//freein-fifo-replenish-addr-info = <0x0 0x0 0x80000000 0x0 0x10000000>; /* 256M @2048M*/
+				//freein-fifo-replenish-addr-info = <0x0 0x0 0xbe000000 0x0 0x2000000>; /* 32M <--@3072M*/
+				/* < node fifomask descsize headroom ndescs > */
+				//freein-fifo-replenish-desc-info = <0 0xffff 2048 0 64>;
+				//freein-fifo-replenish-desc-info = <0 0xf0 2048 0 2064>;
+			};
+			owner-config {
+				nae = <0x0>;
+				sae = <0x0>;
+				rsa = <0x0>;
+				fmn = <0x0>;
+			};
+
+			app-param {
+				shm-id = <0>;
+			};
+		};
+	};
+
+	chosen {
+		bootargs = " console=ttyS0,115200 ";
+	};
+
+	/* These binaries are downloaded at the resp physical memory locations
+	 * by their corresponding bootstrapping loaders. For example,
+	 * u-boot is loaded by x-load at 193M, sysconfig is loaded by u-boot
+	 * at 1M, hypervisor by u-boot hypervisor at 136M.
+	 * On simulator all these binaries are pre-loaded by using '-F' option
+	 * for convenience. On silicon, they will be loaded by their corresponding
+	 * temporary download locations the stage-1/stage-2 firmware
+	 */
+	firmware-download {
+		u-boot	= <0x0c100000 0x3f00000>;  /* 63M @ 193M */
+		sysconfig     = <0x00100000 0x00100000>; /* 1M @ 1M */
+		hypervisor    = <0x08800000 0x00800000>; /* 8M @ 136M */
+		dom0	  = <0x09000000 0x03000000>; /* 48M @ 144M */
+		domU-ramdisk  = <0x60000000 0x60800000>; /* 8M @ 1536M */
+	};
+
+	soc {
+		nodes {
+			num-nodes = <1>;
+		};
+
+	};
+	frequency-config {
+		nae = <250>;
+		sae = <500>;
+		rsa = <500>;
+		dtre = <250>;
+		cde = <250>;
+	};
+};
+
