From 0f14cf76632a1696648dffcf34b2efeea4dfc07d Mon Sep 17 00:00:00 2001
From: Venu Vadapalli <vvadapalli@netlogicmicro.com>
Date: Sat, 11 Sep 2010 13:03:30 -0700
Subject: Use same cpumask for hypervisor and dom@0 NetOS

[Based on SDK 3.2]
Signed-off-by: Quanyang Wang <quanyang.wang@windriver.com>
Signed-off-by: Nam Ninh <nam.ninh@windriver.com>

diff --git a/arch/mips/boot/dts/netos.dts b/arch/mips/boot/dts/netos.dts
index 3ea4de2..851925d 100644
--- a/arch/mips/boot/dts/netos.dts
+++ b/arch/mips/boot/dts/netos.dts
@@ -14,7 +14,7 @@
 		hypervisor-name = "Xen";
 		cpu_possible_map = <0xff>;
 		is_dom0_hvm = <0>;
-		bootargs = "dom0_loadaddr=0xe0800000 dom0_cpumask=0x01 -- ";
+		bootargs = "dom0_loadaddr=0xe0800000 dom0_cpumask=0x0f -- ";
 		domain_heap = <0x80000000 0x20000000>;
 	};
 
-- 
1.7.1

