+        alloc_dom0_memory = <0>;
+        bootargs = "ncores=8 dom0_loadaddr=0x72000000 dom0_size=0x1c000000 dom0_cpumask=0xFFFFFFFF -- ";
+        domain_heap = <0x30000000 0x20000000>; /* 0.75 GB -> 1.25 GB */
+    };
+    doms {
+        #address-cells = <1>;
+        #size-cells = <1>;
+        dom@0 {
