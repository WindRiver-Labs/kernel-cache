+++ b/arch/mips/boot/dts/xlp9xx-linux-mn-kernel.dts
@@ -75,11 +75,6 @@
 				reg = <0x68>;
 			};
 
-			dtt@2c {
-                                compatible = "max6653";
-                                reg = <0x2c>;
-			};
-
 			eeprom@57 {
                                 compatible = "xlpge_eeprom";
                                 reg = <0x57>;
@@ -280,6 +275,7 @@
                                 compatible = "xlpge_eeprom";
                                 reg = <0x57>;
                         };
+
 			dtt@2c {
 				compatible = "max6653";
 				reg = <0x2c>;
