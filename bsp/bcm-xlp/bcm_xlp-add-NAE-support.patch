+static irqreturn_t nlm_xlp_nae_int_handler(int irq, void *dev_id)
+{
+        struct net_device *dev;
+        struct dev_data *priv;
+	int i;
+	int cpu = 0;
+
+	cpu = hard_smp_processor_id();
+	priv->cpu_stats[cpu].interrupts++;
+
+	if(!dev_id) {
+		printk("[%s]: NULL dev_id \n", __FUNCTION__ );
