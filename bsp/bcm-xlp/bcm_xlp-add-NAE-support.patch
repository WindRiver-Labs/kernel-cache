+int nlm_xlp_disable_napi(void)
+{
+	int node, i, coff;
+	for (i = 0; i < NR_CPUS; i++) {
+		if (!cpu_isset(i, phys_cpu_present_map))
+			continue;
+		node = i / 32;
+		coff = i * NLM_MAX_VC_PER_THREAD;
+		nlm_hal_disable_vc_intr(node, (coff + nae_rx_vc) & 0x7f);
+		nlm_hal_disable_vc_intr(node, (coff + nae_fb_vc) & 0x7f);
+	}
+	return 0;
+}
