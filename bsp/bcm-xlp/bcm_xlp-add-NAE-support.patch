+		nlm_spawn_kthread();
+	}
+
+#ifdef CONFIG_NLM_ENABLE_LOAD_BALANCING
+	if (perf_mode == NLM_TCP_MODE)
+		nlm_prepad_len = PREPAD_LEN;
+#endif
+
+#ifdef CONFIG_NLM_ENABLE_LOAD_BALANCING
+	if (perf_mode == NLM_TCP_MODE) {
+		entry = create_proc_entry(
+				"load_info",
+				0,		/* def mode */
+				nlm_root_proc	/* parent   */
+				);
+		if (entry)
+			entry->proc_fops = &nlm_load_balance_proc_fops;
+		nlm_init_load_balance();
+	}
+#endif
