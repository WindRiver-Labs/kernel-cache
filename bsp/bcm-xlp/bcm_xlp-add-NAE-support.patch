+void nlm_xlp_nae_remove(void)
+{
+	struct net_device *dev = NULL;
+	struct dev_data *priv = NULL;
+	int node, i;
+
+	for (node = 0; node < NLM_MAX_NODES; node++) {
+		for(i = 0; i < MAX_GMAC_PORT; i++) {
