From c91ddf7e1e5958ffc479fc61d58bb8ed63149e08 Mon Sep 17 00:00:00 2001
From: Yanjiang Jin <yanjiang.jin@windriver.com>
Date: Mon, 22 Apr 2013 17:13:32 +0800
Subject: [PATCH 104/761] nlm_hal_fmn_config.o only needed by CONFIG_NLM_XLP
 build, CONFIG_NLM_XLR doesn't need it

Based on Broadcom SDK 2.3.

Signed-off-by: henry shao <hshao@netlogicmicro.com>
Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 arch/mips/netlogic/common/Makefile |    2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/mips/netlogic/common/Makefile b/arch/mips/netlogic/common/Makefile
index e17e926..2f6292a 100644
--- a/arch/mips/netlogic/common/Makefile
+++ b/arch/mips/netlogic/common/Makefile
@@ -5,7 +5,7 @@ obj-y :=   platform.o cpu_proc.o memory.o
 obj-y 					+= msgring.o
 obj-y 					+= msgring_xls.o
 obj-y 					+= msgring_shared.o
-obj-y 					+= nlm_hal_fmn_config.o
+obj-$(CONFIG_NLM_XLP)			+= nlm_hal_fmn_config.o
 
 obj-$(CONFIG_NLM_XLP)			+= nlm_hal.o
 
