From 5421c8b5593bac11cd8880291aefad82c3ef05e0 Mon Sep 17 00:00:00 2001
From: Yanjiang Jin <yanjiang.jin@windriver.com>
Date: Mon, 22 Apr 2013 17:01:11 +0800
Subject: [PATCH 070/565] rename msgring_xlp to nlm_hal_fmn_config

Rename msgring_xlp to nlm_hal_fmn_config.

Based on Broadcom SDK 2.3.

Signed-off-by: henry shao <hshao@netlogicmicro.com>
Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 arch/mips/netlogic/common/Makefile | 3 ++-
 1 file changed, 2 insertions(+), 1 deletion(-)

diff --git a/arch/mips/netlogic/common/Makefile b/arch/mips/netlogic/common/Makefile
index 26e00b2..97c0df6 100644
--- a/arch/mips/netlogic/common/Makefile
+++ b/arch/mips/netlogic/common/Makefile
@@ -4,6 +4,7 @@ obj-y :=   platform.o cpu_proc.o memory.o
 obj-y 					+= msgring.o
 obj-y 					+= msgring_xls.o
 obj-y 					+= msgring_shared.o
+obj-y 					+= nlm_hal_fmn_config.o
 
 obj-$(CONFIG_NLM_XLP)			+= nlm_hal.o
 
@@ -13,7 +14,7 @@ obj-$(CONFIG_SMP)                 	+= smp.o
 
 EXTRA_AFLAGS := $(CFLAGS)
 
-clean-files += msgring.o msgring_xls.o msgring_shared.o
+clean-files += msgring.o msgring_xls.o msgring_shared.o nlm_hal_fmn_config.o
 clean-files += nlm_hal.o
 clean-files += srio.o dma.o smp.o platform.o cpu_proc.o
 clean-files += msgring.c msgring_xls.c msgring_shared.c nlm_hal.c
