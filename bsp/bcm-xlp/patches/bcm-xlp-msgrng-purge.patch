From 7b9cc80419138b24eaeb5bf7a515b98f05188961 Mon Sep 17 00:00:00 2001
From: Zi Shen Lim <zlim@netlogicmicro.com>
Date: Tue, 10 Jan 2012 13:42:27 -0800
Subject: [PATCH 326/565] bcm-xlp: msgrng purge

msgrng purge

Based on Broadcom SDK 2.3.

Signed-off-by: Zi Shen Lim <zlim@netlogicmicro.com>
Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 arch/mips/netlogic/common/Makefile | 8 ++------
 1 file changed, 2 insertions(+), 6 deletions(-)

diff --git a/arch/mips/netlogic/common/Makefile b/arch/mips/netlogic/common/Makefile
index 94364fc..6c0e5f6 100644
--- a/arch/mips/netlogic/common/Makefile
+++ b/arch/mips/netlogic/common/Makefile
@@ -1,10 +1,6 @@
 EXTRA_CFLAGS := $(CFLAGS) -DNLM_HAL_LINUX_KERNEL -Iarch/mips/include/asm/netlogic/hal
 obj-y :=   cpu_proc.o memory.o bootinfo.o
 
-obj-$(CONFIG_NLM_XLR)			+= msgring.o
-obj-$(CONFIG_NLM_XLR)			+= msgring_xls.o
-obj-$(CONFIG_NLM_XLR)			+= msgring_shared.o
-
 obj-$(CONFIG_NLM_XLP)			+= nlm_hal_sys.o nlm_evp_cpld.o nlm_hal_cpu_info.o
 obj-$(CONFIG_NLM_XLP)			+= nlm_hal.o nlm_eeprom.o fdt_helper.o
 obj-$(CONFIG_NLM_XLP)		+= nlm_hal_mdio.o
@@ -24,8 +20,8 @@ obj-$(CONFIG_SMP)                 	+= smp.o
 
 EXTRA_AFLAGS := $(CFLAGS)
 
-clean-files += msgring.o msgring_xls.o msgring_shared.o nlm_hal_fmn_config.o
+clean-files += nlm_hal_fmn_config.o nlm_hal_cpu_info.o
 clean-files += nlm_hal.o
 clean-files += srio.o dma.o smp.o cpu_proc.o
-clean-files += msgring.c msgring_xls.c msgring_shared.c nlm_hal.c
+clean-files += nlm_hal.c
 
