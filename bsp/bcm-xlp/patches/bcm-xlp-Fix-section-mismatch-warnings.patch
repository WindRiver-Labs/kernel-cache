From 53a4f96ac07e6e81200223952bcb1be4f2cbb656 Mon Sep 17 00:00:00 2001
From: Yanjiang Jin <yanjiang.jin@windriver.com>
Date: Mon, 22 Apr 2013 17:23:04 +0800
Subject: [PATCH 274/565] bcm-xlp: Fix section mismatch warnings

Fix section mismatch warnings

Based on Broadcom SDK 2.3.

Signed-off-by: Zi Shen Lim <zlim@netlogicmicro.com>
Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
---
 arch/mips/mm/c-phoenix.c       |  4 ++--
 arch/mips/netlogic/xlp/mmu.c   |  6 +++---
 arch/mips/netlogic/xlp/setup.c |  4 ++--
 arch/mips/netlogic/xlp/smp.c   | 10 +++-------
 drivers/spi/spi_xlp.c          |  2 +-
 5 files changed, 11 insertions(+), 15 deletions(-)

diff --git a/arch/mips/mm/c-phoenix.c b/arch/mips/mm/c-phoenix.c
index ec66332..6d214e1 100644
--- a/arch/mips/mm/c-phoenix.c
+++ b/arch/mips/mm/c-phoenix.c
@@ -290,7 +290,7 @@ static void nlm_common_flush_l1_caches(void)
 
 static void nlm_common_noflush(void) { /* do nothing */ }
 
-static __init void probe_l1_cache(void)
+static __cpuinit void probe_l1_cache(void)
 {
   struct cpuinfo_mips *c = &current_cpu_data;
   unsigned int config1 = read_c0_config1();
@@ -358,7 +358,7 @@ static void update_kseg0_coherency(void)
 
 }
 
-void ld_mmu_xlr(void)
+void __cpuinit ld_mmu_xlr(void)
 {
 	extern void build_clear_page(void);
 	extern void build_copy_page(void);
diff --git a/arch/mips/netlogic/xlp/mmu.c b/arch/mips/netlogic/xlp/mmu.c
index f49de00..8d130bf 100644
--- a/arch/mips/netlogic/xlp/mmu.c
+++ b/arch/mips/netlogic/xlp/mmu.c
@@ -37,7 +37,7 @@
 
 #define _PMD_T_LOG2 3
 
-static int __initdata tlb_config = (ENABLE_ETLB | ENABLE_128_TLB | ENABLE_PGWALKER);
+static int __cpuinitdata tlb_config = (ENABLE_ETLB | ENABLE_128_TLB | ENABLE_PGWALKER);
 
 int __init disable_etlb(char *str)
 {
@@ -74,7 +74,7 @@ static int pgtable_levels = PGD | PMD | PTE;
 static int pgtable_levels = PGD | PTE;
 #endif
 
-static void pgwalker_init(void)
+static void __cpuinit pgwalker_init(void)
 {
 	unsigned int value;
 	int i = 0;
@@ -276,7 +276,7 @@ static void pgwalker_workaround_setup(void)
 static void pgwalker_workaround_setup(void) {}
 #endif
 
-void mmu_init(void)
+void __cpuinit mmu_init(void)
 {
 	uint32_t config4_val = 0;
 	int is_soc_xlp2xx = (get_proc_id() == CHIP_PROCESSOR_ID_XLP_2XX);
diff --git a/arch/mips/netlogic/xlp/setup.c b/arch/mips/netlogic/xlp/setup.c
index 882f661..bbcb530 100644
--- a/arch/mips/netlogic/xlp/setup.c
+++ b/arch/mips/netlogic/xlp/setup.c
@@ -370,7 +370,7 @@ void __init nlm_nmi_setup (void)
 #ifdef CONFIG_SERIAL_8250
 #define UART_CLK 133333333
 
-static void nlm_early_serial_setup(int uart_id)
+static void __init nlm_early_serial_setup(int uart_id)
 {
 	struct uart_port s;
 	extern int __init early_serial_setup(struct uart_port *port);
@@ -429,7 +429,7 @@ int nlm_get_fdt_app_param(const char *param, void *data, int size)
 }
 EXPORT_SYMBOL(nlm_get_fdt_app_param);
 
-static int fdt_process(void)
+static int __init fdt_process(void)
 {
 	int  domain=0;
 	char domstr[32] = "";
diff --git a/arch/mips/netlogic/xlp/smp.c b/arch/mips/netlogic/xlp/smp.c
index e668509..eb91625 100644
--- a/arch/mips/netlogic/xlp/smp.c
+++ b/arch/mips/netlogic/xlp/smp.c
@@ -132,13 +132,9 @@ void nlm_cpus_done(void)
 {
 }
 
-/*
- * init/main.c : smp_init ==> cpu_up ==> _cpu_up => __cpu_up (arch/mips/kernel/
- * smp.c) ==> mp_ops->boot_secondary
- * The cpu argument is the bit number from cpu_present_mask (for_each_online_cpu
- * ) => physical cpu id, not logical
- */
-void nlm_boot_secondary(int cpu, struct task_struct *idle)
+/* Boot all other cpus in the system, initialize them, and
+   bring them into the boot fn */
+void __cpuinit nlm_boot_secondary(int logical_cpu, struct task_struct *idle)
 {
 	unsigned long gp = (unsigned long)task_thread_info(idle);
 	unsigned long sp = (unsigned long)__KSTK_TOS(idle);
diff --git a/drivers/spi/spi_xlp.c b/drivers/spi/spi_xlp.c
index 683cd33..8107533 100644
--- a/drivers/spi/spi_xlp.c
+++ b/drivers/spi/spi_xlp.c
@@ -351,7 +351,7 @@ static int spi_xlp_txrx_bufs(struct spi_device *spi, struct spi_transfer *t)
         return ret;
 }
 
-static int __init spi_xlp_probe(struct platform_device *dev)
+static int __devinit spi_xlp_probe(struct platform_device *dev)
 {
 	int ret = 0;
 	struct spi_master *master;
