+CONFIG_RTC_INTF_SYSFS=y
+CONFIG_RTC_INTF_PROC=y
+CONFIG_RTC_INTF_DEV=y
+CONFIG_RTC_DRV_DS1307=y
+CONFIG_COMMON_CLK_DEBUG=y
+CONFIG_FRAME_POINTER=y
+CONFIG_GENERIC_TRACER=y
+CONFIG_FUNCTION_TRACER=y
