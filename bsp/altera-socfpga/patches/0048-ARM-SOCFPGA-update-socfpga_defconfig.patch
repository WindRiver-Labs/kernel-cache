+CONFIG_INOTIFY_USER=y
 CONFIG_VFAT_FS=y
 CONFIG_NTFS_FS=y
 CONFIG_NTFS_RW=y
@@ -126,8 +132,39 @@ CONFIG_DETECT_HUNG_TASK=y
 CONFIG_ENABLE_DEFAULT_TRACERS=y
