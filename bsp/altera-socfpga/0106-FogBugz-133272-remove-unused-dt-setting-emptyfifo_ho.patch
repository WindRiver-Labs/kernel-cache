From 17b29e50883ecddd8fe1c790c9f2ed4522b6b35f Mon Sep 17 00:00:00 2001
From: Alan Tull <atull@altera.com>
Date: Tue, 23 Jul 2013 14:54:02 -0500
Subject: [PATCH 106/248] FogBugz #133272: remove unused dt setting
 emptyfifo_hold_master

emptyfifo_hold_master is no longer used anywhere in the kernel.

[Original patch taken from
git://git.rocketboards.org/linux-socfpga.git socfpga-3.14]

Signed-off-by: Yanjiang Jin <yanjiang.jin@windriver.com>
Signed-off-by: Alan Tull <atull@altera.com>
---
 arch/arm/boot/dts/socfpga.dtsi | 4 ----
 1 file changed, 4 deletions(-)

diff --git a/arch/arm/boot/dts/socfpga.dtsi b/arch/arm/boot/dts/socfpga.dtsi
index 3e258fe..1010df6 100644
--- a/arch/arm/boot/dts/socfpga.dtsi
+++ b/arch/arm/boot/dts/socfpga.dtsi
@@ -529,7 +529,6 @@
 			compatible = "snps,designware-i2c";
 			reg = <0xffc04000 0x1000>;
 			interrupts = <0 158 4>;
-			emptyfifo_hold_master = <1>;
 			clocks = <&per_base_clk>;
 			status = "disabled";
 		};
@@ -540,7 +539,6 @@
 			compatible = "snps,designware-i2c";
 			reg = <0xffc05000 0x1000>;
 			interrupts = <0 159 4>;
-			emptyfifo_hold_master = <1>;
 			clocks = <&per_base_clk>;
 			status = "disabled";
 		};
@@ -551,7 +549,6 @@
 			compatible = "snps,designware-i2c";
 			reg = <0xffc06000 0x1000>;
 			interrupts = <0 160 4>;
-			emptyfifo_hold_master = <1>;
 			clocks = <&per_base_clk>;
 			status = "disabled";
 		};
@@ -562,7 +559,6 @@
 			compatible = "snps,designware-i2c";
 			reg = <0xffc07000 0x1000>;
 			interrupts = <0 161 4>;
-			emptyfifo_hold_master = <1>;
 			clocks = <&per_base_clk>;
 			status = "disabled";
 		};
