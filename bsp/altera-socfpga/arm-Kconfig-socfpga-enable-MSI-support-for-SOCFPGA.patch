From 776f73ae2152ef7f6e113239468e2a6d94095136 Mon Sep 17 00:00:00 2001
From: Yang Wei <Wei.Yang@windriver.com>
Date: Thu, 29 May 2014 15:58:35 +0800
Subject: [PATCH 3/3] arm:Kconfig:socfpga: enable MSI support for SOCFPGA

SOCFPGA has already supported MSI, so enable it.

Signed-off-by: Yang Wei <Wei.Yang@windriver.com>
---
 arch/arm/Kconfig |    1 +
 1 files changed, 1 insertions(+), 0 deletions(-)

diff --git a/arch/arm/Kconfig b/arch/arm/Kconfig
index 4bf8088..f526a66 100644
--- a/arch/arm/Kconfig
+++ b/arch/arm/Kconfig
@@ -280,6 +280,7 @@ choice
 
 config ARCH_SOCFPGA
 	bool "Altera SOCFPGA family"
+	select ARCH_SUPPORTS_MSI
 	select ARCH_WANT_OPTIONAL_GPIOLIB
 	select ARM_AMBA
 	select ARM_GIC
-- 
1.7.5.4

