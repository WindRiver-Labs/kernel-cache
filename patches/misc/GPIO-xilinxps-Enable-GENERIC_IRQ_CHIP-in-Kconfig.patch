From 4d1ff4f1cf9ce91c7504407d0c5ceb8e1c175952 Mon Sep 17 00:00:00 2001
From: Michal Simek <michal.simek@xilinx.com>
Date: Fri, 19 Jul 2013 14:30:21 +0200
Subject: [PATCH 322/628] GPIO: xilinxps: Enable GENERIC_IRQ_CHIP in Kconfig

git://github.com/Xilinx/linux-xlnx.git xilinx-v14.7
commit 2f6abae809941ff24ee36c984024cbc0861bd85d

It is required by the driver.

Signed-off-by: Michal Simek <michal.simek@xilinx.com>
Signed-off-by: Liming Wang <liming.wang@windriver.com>
---
 drivers/gpio/Kconfig |    1 +
 1 files changed, 1 insertions(+), 0 deletions(-)

diff --git a/drivers/gpio/Kconfig b/drivers/gpio/Kconfig
index 2897bd1..7aba83d 100644
--- a/drivers/gpio/Kconfig
+++ b/drivers/gpio/Kconfig
@@ -241,6 +241,7 @@ config GPIO_XILINX
 config GPIO_XILINX_PS
 	tristate "Xilinx GPIO PS"
 	depends on ARCH_ZYNQ
+	select GENERIC_IRQ_CHIP
 	help
 	  Say yes here to support Xilinx GPIO PS controller
 
