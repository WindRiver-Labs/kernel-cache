From 3ca1111917fe83eefeb81358f67581735c97468c Mon Sep 17 00:00:00 2001
From: Michal Simek <michal.simek@xilinx.com>
Date: Fri, 4 Oct 2013 09:53:22 +0200
Subject: [PATCH 628/628] zynq: dts: Add register and interrupt name
 description

git://github.com/Xilinx/linux-xlnx.git xilinx-v14.7
commit f563137b8c19a94db91593732d5d4f1524c24af1

They are optional parameters which better identify
what regs/irqs entry mean.

Signed-off-by: Michal Simek <michal.simek@xilinx.com>
Signed-off-by: Liming Wang <liming.wang@windriver.com>
---
 arch/arm/boot/dts/zynq-zc706.dts |    4 ++++
 1 files changed, 4 insertions(+), 0 deletions(-)

diff --git a/arch/arm/boot/dts/zynq-zc706.dts b/arch/arm/boot/dts/zynq-zc706.dts
index bd6ea93..3ba0864 100644
--- a/arch/arm/boot/dts/zynq-zc706.dts
+++ b/arch/arm/boot/dts/zynq-zc706.dts
@@ -59,6 +59,7 @@
 		interrupt-parent = <&ps7_scugic_0>;
 		interrupts = <0 5 4>, <0 6 4>;
 		reg = <0xf8891000 0x1000>, <0xf8893000 0x1000>;
+		reg-names = "cpu0", "cpu1";
 	} ;
 	ps7_ddr_0: memory@0 {
 		device_type = "memory";
@@ -106,6 +107,8 @@
 			clock-names = "apb_pclk";
 			clocks = <&clkc 27>;
 			compatible = "xlnx,ps7-dma-1.00.a", "arm,primecell", "arm,pl330";
+			interrupt-names = "abort", "dma0", "dma1", "dma2", "dma3",
+				"dma4", "dma5", "dma6", "dma7";
 			interrupt-parent = <&ps7_scugic_0>;
 			interrupts = <0 13 4>, <0 14 4>, <0 15 4>, <0 16 4>, <0 17 4>, <0 40 4>, <0 41 4>, <0 42 4>, <0 43 4>;
 			reg = <0xf8003000 0x1000>;
@@ -363,6 +366,7 @@
 		ps7_ttc_1: ps7-ttc@f8002000 {
 			clocks = <&clkc 6>;
 			compatible = "xlnx,ps7-ttc-1.00.a", "cdns,ttc";
+			interrupt-names = "ttc0", "ttc1", "ttc2";
 			interrupt-parent = <&ps7_scugic_0>;
 			interrupts = <0 37 4>, <0 38 4>, <0 39 4>;
 			reg = <0xf8002000 0x1000>;
-- 
