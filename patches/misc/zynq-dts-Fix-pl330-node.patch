From abd9d6d78a962c8366e995f6f0a224f4eab24acd Mon Sep 17 00:00:00 2001
From: Michal Simek <michal.simek@xilinx.com>
Date: Fri, 4 Oct 2013 09:50:25 +0200
Subject: [PATCH 626/628] zynq: dts: Fix pl330 node

git://github.com/Xilinx/linux-xlnx.git xilinx-v14.7
commit 6fc67d87de6c6af6599661d52aa21da3c7099853

pl330 requires clock-name setup as apg_pclk.

Signed-off-by: Michal Simek <michal.simek@xilinx.com>
Signed-off-by: Liming Wang <liming.wang@windriver.com>
---
 arch/arm/boot/dts/zynq-zc706.dts |    1 +
 1 files changed, 1 insertions(+), 0 deletions(-)

diff --git a/arch/arm/boot/dts/zynq-zc706.dts b/arch/arm/boot/dts/zynq-zc706.dts
index e8d3d3f..f931f6f 100644
--- a/arch/arm/boot/dts/zynq-zc706.dts
+++ b/arch/arm/boot/dts/zynq-zc706.dts
@@ -103,6 +103,7 @@
 			#dma-channels = <8>;
 			#dma-requests = <4>;
 			arm,primecell-periphid = <0x41330>;
+			clock-names = "apb_pclk";
 			clocks = <&clkc 27>;
 			compatible = "xlnx,ps7-dma-1.00.a", "arm,primecell", "arm,pl330";
 			interrupt-parent = <&ps7_scugic_0>;
-- 
