From 044e5c87dc156ab8a04b006082f15b4afcefacd4 Mon Sep 17 00:00:00 2001
From: Bryce Kellogg <bryce.kellogg@xilinx.com>
Date: Tue, 14 Aug 2012 09:46:56 -0700
Subject: [PATCH 597/628] Xilinx: ARM: added twd timer to zc706 devicetree.

git://github.com/Xilinx/linux-xlnx.git xilinx-v14.7
commit e478c58dc3b8ffcbb16d3e756957f9fdaa2e7d22

Signed-off-by: Bryce Kellogg <bryce.kellogg@xilinx.com>
Signed-off-by: Liming Wang <liming.wang@windriver.com>
---
 arch/arm/boot/dts/zynq-zc706.dts |    8 ++++++++
 1 files changed, 8 insertions(+), 0 deletions(-)

diff --git a/arch/arm/boot/dts/zynq-zc706.dts b/arch/arm/boot/dts/zynq-zc706.dts
index 1606e6d..a9cebc0 100644
--- a/arch/arm/boot/dts/zynq-zc706.dts
+++ b/arch/arm/boot/dts/zynq-zc706.dts
@@ -1,4 +1,5 @@
 /dts-v1/;
+
 / {
 	model = "Xilinx Zynq ZC706";
 	compatible = "xlnx,zynq-zc706";
@@ -57,6 +58,13 @@
 			clock-frequency-timer2 = <111111111>;
 		};
 
+		timer@f8f00600 {
+			compatible = "arm,cortex-a9-twd-timer";
+			reg = <0xf8f00600 0x20>;
+			interrupts = <1 13 0x0100>;
+			interrupt-parent = <&gic>;
+		};
+
 		swdt@f8005000 {
 			device_type = "watchdog";
 			compatible = "xlnx,ps7-wdt-1.00.a";
-- 
