 	__u8			cqes;
 	__u8			rsvd514[2];
@@ -102,7 +110,12 @@ struct nvme_id_ns {
 	__u8			dps;
 	__u8			nmic;
 	__u8			rescap;
-	__u8			rsvd32[88];
+	__u8			fpi;
+	__u8			rsvd33;
+	__le16			nawun;
