From c12ee0905a457ee3626c13aca51b1b9ab0dbe210 Mon Sep 17 00:00:00 2001
From: fupan li <fupan.li@windriver.com>
Date: Wed, 13 May 2015 10:59:21 +0800
Subject: [PATCH] restore i2c-i801 registraton order

The commit a2cbc9f4029631956db3c38392c3db7a5133c8bd
	Author: fupan li <fupan.li@windriver.com>
	Date:   Mon Feb 10 10:43:45 2014 +0800

	i2c: i801: enable SMBUS driver for baytrail

changed the i2c-i801 registraton order, which is not compatible
to mainline code.

Signed-off-by: fupan li <fupan.li@windriver.com>
---
 drivers/i2c/busses/Makefile |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/drivers/i2c/busses/Makefile b/drivers/i2c/busses/Makefile
index 5bbf680..3f211f2 100644
--- a/drivers/i2c/busses/Makefile
+++ b/drivers/i2c/busses/Makefile
@@ -12,6 +12,7 @@ obj-$(CONFIG_I2C_ALI15X3)	+= i2c-ali15x3.o
 obj-$(CONFIG_I2C_AMD756)	+= i2c-amd756.o
 obj-$(CONFIG_I2C_AMD756_S4882)	+= i2c-amd756-s4882.o
 obj-$(CONFIG_I2C_AMD8111)	+= i2c-amd8111.o
+obj-$(CONFIG_I2C_I801)          += i2c-i801.o
 obj-$(CONFIG_I2C_ISCH)		+= i2c-isch.o
 obj-$(CONFIG_I2C_ISMT)		+= i2c-ismt.o
 obj-$(CONFIG_I2C_NFORCE2)	+= i2c-nforce2.o
@@ -40,7 +41,6 @@ obj-$(CONFIG_I2C_DESIGNWARE_PLATFORM)	+= i2c-designware-platform.o
 i2c-designware-platform-objs := i2c-designware-platdrv.o
 obj-$(CONFIG_I2C_DESIGNWARE_PCI)	+= i2c-designware-pci.o
 i2c-designware-pci-objs := i2c-designware-pcidrv.o
-obj-$(CONFIG_I2C_I801)		+= i2c-i801.o
 obj-$(CONFIG_I2C_EG20T)		+= i2c-eg20t.o
 obj-$(CONFIG_I2C_GPIO)		+= i2c-gpio.o
 obj-$(CONFIG_I2C_HIGHLANDER)	+= i2c-highlander.o
-- 
