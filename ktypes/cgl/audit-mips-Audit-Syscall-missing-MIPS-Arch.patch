From a9f771596b47a42258f28dbcbf4093ee24f1c60d Mon Sep 17 00:00:00 2001
From: Yongli He <yongli.he@windriver.com>
Date: Tue, 27 Jul 2010 12:11:47 -0700
Subject: [PATCH] audit/mips: Audit Syscall missing MIPS Arch

Add MIPS to the supported architecture list for syscall
auditing.

Signed-off-by: Yongli He <yongli.he@windriver.com>
---
 init/Kconfig |    2 +-
 1 files changed, 1 insertions(+), 1 deletions(-)

diff --git a/init/Kconfig b/init/Kconfig
index e4978cf..4d26d06 100644
--- a/init/Kconfig
+++ b/init/Kconfig
@@ -309,7 +309,7 @@ config AUDIT
 
 config AUDITSYSCALL
 	bool "Enable system-call auditing support"
-	depends on AUDIT && (X86 || PPC || S390 || IA64 || UML || SPARC64 || SUPERH || ARM)
+	depends on AUDIT && (X86 || PPC || S390 || IA64 || UML || SPARC64 || SUPERH || ARM || MIPS)
 	default y if SECURITY_SELINUX
 	help
 	  Enable low-overhead system-call auditing infrastructure that
-- 
1.6.5.2

