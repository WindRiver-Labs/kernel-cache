From 0d7151350c4cd9c9178e317a1a4254ba34e54bf8 Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Thu, 3 Dec 2009 13:49:03 -0500
Subject: [PATCH 33/43] VBI_MSG_INFO --> struct vbi_msg_info

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 include/vbi/private.h |    2 +-
 include/vbi/syscall.h |    4 ++--
 kernel/vbi/msg.c      |    2 +-
 3 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/include/vbi/private.h b/include/vbi/private.h
index bbfda38..c00f952 100644
--- a/include/vbi/private.h
+++ b/include/vbi/private.h
@@ -37,7 +37,7 @@ extern asmlinkage int32_t vbi_ns_op(uint32_t op, char* name, uint32_t rev,
 
 /* Message receive private operator */
 extern asmlinkage int32_t vbi_rx_op(void *rmsg, uint32_t rlen, 
-			VBI_MSG_INFO *info, VBI_MSG_CTL *ctl);                                   
+			struct vbi_msg_info *info, VBI_MSG_CTL *ctl);
 
 #endif
 
diff --git a/include/vbi/syscall.h b/include/vbi/syscall.h
index 599710a..76df5ff 100644
--- a/include/vbi/syscall.h
+++ b/include/vbi/syscall.h
@@ -100,10 +100,10 @@ extern asmlinkage int vbi_vtlb_op(unsigned int op, unsigned long arg1,
 
 /* Message send */
 extern asmlinkage int32_t vbi_send(int32_t id, void *smsg, size_t slen, 
-			    void *rmsg, size_t rlen, VBI_MSG_INFO *info, 
+			    void *rmsg, size_t rlen, struct vbi_msg_info *info,
 			    VBI_MSG_CTL *ctl);
 /* message receive */
-extern int32_t vbi_receive(void *rmsg, uint32_t rlen, VBI_MSG_INFO *info,
+extern int32_t vbi_receive(void *rmsg, uint32_t rlen, struct vbi_msg_info *info,
 				VBI_MSG_CTL *ctl);
 /* message reply */
 extern asmlinkage int32_t vbi_reply(int32_t id, void *smsg, size_t slen, 
diff --git a/kernel/vbi/msg.c b/kernel/vbi/msg.c
index 6763b31..95b35f3 100644
--- a/kernel/vbi/msg.c
+++ b/kernel/vbi/msg.c
@@ -32,7 +32,7 @@
  *
  */
 
-int32_t vbi_receive(void *rmsg, uint32_t rlen, VBI_MSG_INFO *info,
+int32_t vbi_receive(void *rmsg, uint32_t rlen, struct vbi_msg_info *info,
 			VBI_MSG_CTL *ctl)
 {
 	int32_t retval;
-- 
