From 8050bfc5d0055a7b67e873f435c0445374018228 Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Thu, 29 Oct 2009 23:29:36 -0400
Subject: [PATCH 15/26] vbi: bootCount --> boot_count

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 include/vbi/interface.h |    4 ++--
 include/vbi/vbi.h       |    2 +-
 2 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/include/vbi/interface.h b/include/vbi/interface.h
index 6ad4298..0c630d1 100644
--- a/include/vbi/interface.h
+++ b/include/vbi/interface.h
@@ -431,7 +431,7 @@ struct vb_status {
 	uint64_t tickCount;
 
 	/* number of times this core has booted */
-	uint32_t bootCount;	
+	uint32_t boot_count;
 
 	/* initial program load flag */
 
@@ -524,7 +524,7 @@ struct vb_config
 
 	char boardName[VB_NAMELEN];	/* the name of this board */
 	uint32_t boardType;	/* the type of this board */
-	uint32_t bootCount;	/* number of times this core has booted */
+	uint32_t boot_count;	/* number of times this core has booted */
 
 				/* entry point for this VB */
 	VB_ALIGN_FIELD_64 (void *resetPC, pad6);
diff --git a/include/vbi/vbi.h b/include/vbi/vbi.h
index 2f774b1..9b58a28 100644
--- a/include/vbi/vbi.h
+++ b/include/vbi/vbi.h
@@ -388,7 +388,7 @@ various data structures.
  *
  */
 #define VBI_BOOT_COUNT_GET()			    \
-        (VBI_CONFIG_ADDR_GET()->bootCount)
+        (VBI_CONFIG_ADDR_GET()->boot_count)
 
 /*
  *
