From eecf02113f70c9df9a1d5265e6eca46bf493b2ea Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Tue, 3 Nov 2009 23:05:52 -0500
Subject: [PATCH 07/80] vbi: tickCount -> tick_count

for i in `git grep -l tickCount` ; do echo $i ; mv $i $i~ ; cat $i~ |sed 's/tickCount/tick_count/g' > $i ; done

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 include/vbi/interface.h |    4 ++--
 include/vbi/vbi.h       |    2 +-
 kernel/vbi/idle.c       |    4 ++--
 kernel/vbi/wrhv.c       |    4 ++--
 4 files changed, 7 insertions(+), 7 deletions(-)

diff --git a/include/vbi/interface.h b/include/vbi/interface.h
index 3809a7f..a5b84aa 100644
--- a/include/vbi/interface.h
+++ b/include/vbi/interface.h
@@ -386,7 +386,7 @@ struct vb_control
  *       |		|<-------64bits - vmmu1; reserved for future use
  *       |		|
  *       |--------------|
- *       |		|<-------64bits - tickCount; elapsed virtual core ticks
+ *       |		|<-------64bits - tick_count; elapsed virtual core ticks
  *       |		|
  *       |--------------|
  *
@@ -423,7 +423,7 @@ struct vb_status {
 	VB_ALIGN_FIELD_64 (void *vmmu1, pad2);
 
 	/* Virtual Board clock tick count */
-	uint64_t tickCount;
+	uint64_t tick_count;
 
 	/* number of times this core has booted */
 	uint32_t boot_count;
diff --git a/include/vbi/vbi.h b/include/vbi/vbi.h
index baa92bb..f9269ca 100644
--- a/include/vbi/vbi.h
+++ b/include/vbi/vbi.h
@@ -262,7 +262,7 @@ various data structures.
  *
  */
 #define VBI_TICK_COUNT_GET()             \
-        (VBI_STATUS_ADDR_GET()->tickCount)
+        (VBI_STATUS_ADDR_GET()->tick_count)
 
 /*
  * VBI_CONFIG_ADDR_GET - Get virtual core configuration structure base address
diff --git a/kernel/vbi/idle.c b/kernel/vbi/idle.c
index adf2ae4..8053b68 100644
--- a/kernel/vbi/idle.c
+++ b/kernel/vbi/idle.c
@@ -36,9 +36,9 @@
 
 void vbiIdle(uint64_t timeStamp)
 {
-	uint32_t tickCount = timeStamp & 0xFFFFFFFF;
+	uint32_t tick_count = timeStamp & 0xFFFFFFFF;
 
 	/* use the hypervisor system call to go into the idle state */
 
-	vbiCtxctl(VBI_CTXCTL_IDLE, tickCount, 0);
+	vbiCtxctl(VBI_CTXCTL_IDLE, tick_count, 0);
 }
diff --git a/kernel/vbi/wrhv.c b/kernel/vbi/wrhv.c
index 73b32c6..13dfae8 100644
--- a/kernel/vbi/wrhv.c
+++ b/kernel/vbi/wrhv.c
@@ -82,10 +82,10 @@ irqreturn_t __weak wrhv_timer_interrupt(int irq, void *dev_id)
 	int lost_jiffies = 0;
 	struct pt_regs *regs = get_irq_regs();
 
-	ticks = wrhvVbStatus->tickCount;
+	ticks = wrhvVbStatus->tick_count;
 	ticks -= mark_offset;
 	lost_jiffies = ticks - 1;
-	mark_offset = wrhvVbStatus->tickCount;
+	mark_offset = wrhvVbStatus->tick_count;
 
 	do {
 		do_timer(1);
-- 
1.6.5.2

