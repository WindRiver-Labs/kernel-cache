From 506dd43f4b2891d859042084a37fca537a59723d Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 10:07:11 -0500
Subject: [PATCH 17/80] vbiIoapicIoctl --> vbi_io_apic_ioctl

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/powerpc/kernel/vbi/syscalls.S |    8 ++++----
 arch/x86/kernel/vbi/syscalls.S     |    6 +++---
 include/vbi/syscall.h              |    2 +-
 3 files changed, 8 insertions(+), 8 deletions(-)

diff --git a/arch/powerpc/kernel/vbi/syscalls.S b/arch/powerpc/kernel/vbi/syscalls.S
index 61f051c..40019b9 100644
--- a/arch/powerpc/kernel/vbi/syscalls.S
+++ b/arch/powerpc/kernel/vbi/syscalls.S
@@ -34,7 +34,7 @@ FUNC_EXPORT(vbi_vb_write_mem)
 FUNC_EXPORT(vbi_vb_read_reg)
 FUNC_EXPORT(vbi_vb_write_reg)
 FUNC_EXPORT(vbi_io_apic_op)
-FUNC_EXPORT(vbiIoapicIoctl)
+FUNC_EXPORT(vbi_io_apic_ioctl)
 FUNC_EXPORT(vbiHyIoctl)
 FUNC_EXPORT(vbiCtxctl)
 FUNC_EXPORT(vbiSend)
@@ -717,7 +717,7 @@ FUNC_LABEL(vbiMemAttrGet)
 FUNC_END(vbiMemAttrGet)
 
 /*
- * vbiIoapicIoctl - virtual IO APIC ioctl call
+ * vbi_io_apic_ioctl - virtual IO APIC ioctl call
  *
  * This system call interfaces to the virtual IO APIC ioctl
  * function. For PPC this is a nop routine for now.
@@ -728,12 +728,12 @@ FUNC_END(vbiMemAttrGet)
  *     VBI_IOAPICIOCTL_MASK
  *
  */
-FUNC_LABEL(vbiIoapicIoctl)
+FUNC_LABEL(vbi_io_apic_ioctl)
 	lis r0, HI(VBI_SYS_vIoapicIoctl)
 	ori r0, r0, LO(VBI_SYS_vIoapicIoctl)
 	HCALL
 	blr
-FUNC_END(vbiIoapicIoctl)
+FUNC_END(vbi_io_apic_ioctl)
 
 /*
  *
diff --git a/arch/x86/kernel/vbi/syscalls.S b/arch/x86/kernel/vbi/syscalls.S
index 5182550..7a0d401 100644
--- a/arch/x86/kernel/vbi/syscalls.S
+++ b/arch/x86/kernel/vbi/syscalls.S
@@ -29,7 +29,7 @@
 .globl vbi_vb_write_mem
 .globl vbi_vb_read_mem
 .globl vbi_io_apic_op
-.globl vbiIoapicIoctl
+.globl vbi_io_apic_ioctl
 .globl vbiHyIoctl
 .globl vbiCtxctl
 .globl vbiSend
@@ -407,7 +407,7 @@ vbiHyIoctl:
 	ret
 
 /*
- * vbiIoapicIoctl - virtual IO APIC ioctl call
+ * vbi_io_apic_ioctl - virtual IO APIC ioctl call
  *
  * This system call interfaces to the virtual IO APIC ioctl
  * function.
@@ -420,7 +420,7 @@ vbiHyIoctl:
  * Returns: ioctl specific value
  *
  */
-vbiIoapicIoctl:
+vbi_io_apic_ioctl:
 	movl $VBI_SYS_vIoapicIoctl,%eax
 	push $3
 	vmcall
diff --git a/include/vbi/syscall.h b/include/vbi/syscall.h
index a33f61e..088b29e 100644
--- a/include/vbi/syscall.h
+++ b/include/vbi/syscall.h
@@ -85,7 +85,7 @@ typedef struct vbiMemCtl
 
 extern asmlinkage int vbiHyIoctl(unsigned ioctl, void *arg1, void *arg2,
 				void *arg3, void *arg4);
-extern asmlinkage int vbiIoapicIoctl(unsigned ioctl, unsigned arg1,
+extern asmlinkage int vbi_io_apic_ioctl(unsigned ioctl, unsigned arg1,
 				unsigned arg2);
 extern asmlinkage int vbiCtxctl(unsigned operation, unsigned arg1,
 				unsigned arg2);
-- 
