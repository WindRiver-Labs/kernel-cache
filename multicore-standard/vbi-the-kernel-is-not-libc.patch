-#define	getchar()	__sgetc(stdin)
-#define	getc(p)		__sgetc(p)
-#define	putchar(c)	(__sputc(c, (stdout)))
-#define	putc(c,p)	__sputc(c, p)
-#define	feof(p)		__sfeof(p)
-#define	ferror(p)	__sferror(p)
-#define	clearerr(p)	__sclearerr(p)
-
