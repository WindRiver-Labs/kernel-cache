From a61ebfb5487cb8cd59ef0b9f26a6fad7034348af Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 10:07:33 -0500
Subject: [PATCH 44/80] vbiVbMgmt --> vbi_vb_mgmt

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/powerpc/kernel/vbi/syscalls.S |    6 +++---
 arch/x86/kernel/vbi/syscalls.S     |    6 +++---
 include/vbi/syscall.h              |    2 +-
 include/vbi/syscalls.h             |    2 +-
 4 files changed, 8 insertions(+), 8 deletions(-)

diff --git a/arch/powerpc/kernel/vbi/syscalls.S b/arch/powerpc/kernel/vbi/syscalls.S
index d09b6e9..347dc77 100644
--- a/arch/powerpc/kernel/vbi/syscalls.S
+++ b/arch/powerpc/kernel/vbi/syscalls.S
@@ -61,7 +61,7 @@ FUNC_EXPORT(vbi_enable_vmmu)
 FUNC_EXPORT(vbi_disable_vmmu)
 FUNC_EXPORT(vbi_tlb_load_vmmu)
 FUNC_EXPORT(vbi_tlb_flush_vmmu)
-FUNC_EXPORT(vbiVbMgmt)
+FUNC_EXPORT(vbi_vb_mgmt)
 FUNC_EXPORT(vbiNsOp)
 FUNC_EXPORT(vbi_vb_suspend)
 FUNC_EXPORT(vbi_vb_reset)
@@ -112,12 +112,12 @@ _WRS_TEXT_SEG_START
  *   VBI_VTLB_OP_INIT
  *
  */
-FUNC_LABEL(vbiVbMgmt)
+FUNC_LABEL(vbi_vb_mgmt)
         lis r0, HI(VBI_SYS_vbMgmt)
         ori r0, r0, LO(VBI_SYS_vbMgmt)
         HCALL
         blr
-FUNC_END(vbiVbMgmt)
+FUNC_END(vbi_vb_mgmt)
 
 
 /*
diff --git a/arch/x86/kernel/vbi/syscalls.S b/arch/x86/kernel/vbi/syscalls.S
index d7c09e0..046baf9 100644
--- a/arch/x86/kernel/vbi/syscalls.S
+++ b/arch/x86/kernel/vbi/syscalls.S
@@ -42,7 +42,7 @@
 .globl vbi_kputc
 .globl vbi_vcore_irq_unlock
 .globl vbi_vcore_irq_lock
-.globl vbiVbMgmt
+.globl vbi_vb_mgmt
 .globl vbi_set_mem_attr
 .globl vbi_get_mem_attr
 .globl vbiNsOp
@@ -205,7 +205,7 @@ vbi_vcore_irq_lock:
     ret
 
 /*
- * vbiVbMgmt - virtual board management
+ * vbi_vb_mgmt - virtual board management
  * 
  * This routine executes the specified command on a given virtual board. The
  * possible commands are:
@@ -244,7 +244,7 @@ vbi_vcore_irq_lock:
  *
  * RETURNS: OK or error in case of failure
  */
-vbiVbMgmt:
+vbi_vb_mgmt:
 	movl $VBI_SYS_vbMgmt,%eax
 	push $5
 	vmcall
diff --git a/include/vbi/syscall.h b/include/vbi/syscall.h
index 0538673..371ac79 100644
--- a/include/vbi/syscall.h
+++ b/include/vbi/syscall.h
@@ -89,7 +89,7 @@ extern asmlinkage int vbi_io_apic_ioctl(unsigned ioctl, unsigned arg1,
 				unsigned arg2);
 extern asmlinkage int vbi_ctx_ctl(unsigned operation, unsigned arg1,
 				unsigned arg2);
-extern asmlinkage int32_t vbiVbMgmt(uint32_t cmd, uint32_t boardId,
+extern asmlinkage int32_t vbi_vb_mgmt(uint32_t cmd, uint32_t boardId,
 				int32_t *outError, uint32_t flags, void * ctl);
 extern asmlinkage int vbiVtlbOp(unsigned int op, unsigned long arg1,
 			  	unsigned long arg2, unsigned long arg3);
diff --git a/include/vbi/syscalls.h b/include/vbi/syscalls.h
index fc06d20..7cfeff7 100644
--- a/include/vbi/syscalls.h
+++ b/include/vbi/syscalls.h
@@ -282,7 +282,7 @@ typedef struct vbi_vtlb_control
 } VBI_VTLB_CONTROL;
 
 /*
- * Control structure used by vbiVbMgmt for commands memory read, memory write, 
+ * Control structure used by vbi_vb_mgmt for commands memory read, memory write, 
  *  register read, and register write.
  */
 
-- 
