From cae2a034ade163628e1380ec8003e35e96552a67 Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 12:35:08 -0500
Subject: [PATCH 69/80] vbiStsRegsDisplay --> vbi_disp_status_regs

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/powerpc/kernel/vbi/show.c |    4 ++--
 include/vbi/vbi.h              |    2 +-
 kernel/vbi/show.c              |    2 +-
 3 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/powerpc/kernel/vbi/show.c b/arch/powerpc/kernel/vbi/show.c
index e4ef2f1..8d48d57 100644
--- a/arch/powerpc/kernel/vbi/show.c
+++ b/arch/powerpc/kernel/vbi/show.c
@@ -18,13 +18,13 @@
 #include <vbi/vbi.h>
 
 /*
- * vbiStsRegsDisplay - display registers from a EREG_SET
+ * vbi_disp_status_regs - display registers from a EREG_SET
  *
  * This routine display the contents of the PowerPC emulated status registers
  * structure on the console.
  *
  */
-void vbiStsRegsDisplay(void)
+void vbi_disp_status_regs(void)
 {
     struct vb_status *p = VBI_STATUS_ADDR_GET();
 
diff --git a/include/vbi/vbi.h b/include/vbi/vbi.h
index 7441446..987c5dd 100644
--- a/include/vbi/vbi.h
+++ b/include/vbi/vbi.h
@@ -544,7 +544,7 @@ extern void vbi_show_stat(struct vb_status *p);
 extern void vbi_show_ctrl(struct vb_control *p);
 extern void vbi_show_cfg(struct vb_config *p);
 extern void vbi_show_mem(struct vb_config *config);
-extern void vbiStsRegsDisplay(void);
+extern void vbi_disp_status_regs(void);
 extern void vbi_disp_ctrl_regs(void);
 
 #endif	/* _ASMLANGUAGE */
diff --git a/kernel/vbi/show.c b/kernel/vbi/show.c
index 8e8d0f4..0ec14f3 100644
--- a/kernel/vbi/show.c
+++ b/kernel/vbi/show.c
@@ -119,7 +119,7 @@ void vbi_show_stat(struct vb_status *p)
 	printk("  timestamp:          %lld\n", p->timeStamp);
 	printk("  old int disable:    0x%08x\n", p->prev_irq_disable);
 #ifdef _WRHV_ARCH_HAS_STATUS_REGS
-	vbiStsRegsDisplay();
+	vbi_disp_status_regs();
 #endif
 }
 
-- 
