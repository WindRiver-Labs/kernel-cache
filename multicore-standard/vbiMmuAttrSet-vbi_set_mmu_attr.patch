From 213481659facefdc1d2a0c1825ab7d99198c8a70 Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 12:35:03 -0500
Subject: [PATCH 61/80] vbiMmuAttrSet --> vbi_set_mmu_attr

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/x86/kernel/vbi/syscalls.S |    2 +-
 include/vbi/syscall.h          |    2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/arch/x86/kernel/vbi/syscalls.S b/arch/x86/kernel/vbi/syscalls.S
index 9677dd5..24988b6 100644
--- a/arch/x86/kernel/vbi/syscalls.S
+++ b/arch/x86/kernel/vbi/syscalls.S
@@ -482,7 +482,7 @@ vbi_ctx_ctl:
  * RETURNS: OK or error number in case of failure
  *
  */
-vbiMmuAttrSet:
+vbi_set_mmu_attr:
 	movl $VBI_SYS_mmu_attr_set,%eax
 	push $3
 	vmcall
diff --git a/include/vbi/syscall.h b/include/vbi/syscall.h
index f7e9480..85cfcdb 100644
--- a/include/vbi/syscall.h
+++ b/include/vbi/syscall.h
@@ -123,7 +123,7 @@ extern vbiStatus_t vbiNsLookup(char *name, uint32_t  rev, VBI_NS_HANDLE *pHandle
 extern vbiStatus_t vbi_tlb_flush_vmmu(struct vmmuConfig * config, void *addr, size_t len);
 
 
-/* Prior to vbi 2.0 these api were vbiMmuAttrSet/Get */
+/* Prior to vbi 2.0 these api were vbi_set_mmu_attr/Get */
 
 extern asmlinkage vbiStatus_t  vbi_set_mem_attr(void *vaddr, size_t len, vbiMemAttr_t attr);
 extern asmlinkage vbiStatus_t  vbi_get_mem_attr(void *vaddr, vbiMemAttr_t * attr);
