From 010b4e3765efd78de3fde05eda2c7eb3019bf68a Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Fri, 30 Oct 2009 00:08:33 -0400
Subject: [PATCH 23/26] vbi: numSm -> num_sm

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 include/vbi/interface.h |    2 +-
 kernel/vbi/shmem.c      |    2 +-
 kernel/vbi/show.c       |   12 ++++++------
 3 files changed, 8 insertions(+), 8 deletions(-)

diff --git a/include/vbi/interface.h b/include/vbi/interface.h
index aa616c5..596235a 100644
--- a/include/vbi/interface.h
+++ b/include/vbi/interface.h
@@ -553,7 +553,7 @@ struct vb_config
 	 */
 
 	/* number of shared memory regions */
-	uint32_t numSm;
+	uint32_t num_sm;
 
 	/* shared memory state information */
 	VB_ALIGN_FIELD_64 (void *sharedMemoryRegionsStateAddress, pad7);
diff --git a/kernel/vbi/shmem.c b/kernel/vbi/shmem.c
index a029703..5049120 100644
--- a/kernel/vbi/shmem.c
+++ b/kernel/vbi/shmem.c
@@ -55,7 +55,7 @@ vbiStatus_t vbiShmemRegionFind(int8_t *smName, void **addr,
 {
 	struct vb_config *config = VBI_CONFIG_ADDR_GET();
 	struct vb_sm_info *info = config->sharedMemoryRegionsConfigAddress;
-	int32_t num = config->numSm;
+	int32_t num = config->num_sm;
 	int32_t i;
 
 	for (i = 0; i < num; i++, info++)
diff --git a/kernel/vbi/show.c b/kernel/vbi/show.c
index 2210017..c4d61f9 100644
--- a/kernel/vbi/show.c
+++ b/kernel/vbi/show.c
@@ -38,14 +38,14 @@ control and configuration data structures.
 void vbiSharedMemoryShow(struct vb_config *config)
 {
 	struct vb_sm_info *p = config->sharedMemoryRegionsConfigAddress;
-	int numSm = config->numSm;
+	int num_sm = config->num_sm;
 	void *pState = config->sharedMemoryRegionsStateAddress;
 	int i;
 
-	printk("%d Shared memory regions, Config at 0x%x:\n", numSm, p);
+	printk("%d Shared memory regions, Config at 0x%x:\n", num_sm, p);
 
 	/* dump shared memory info if we have any attached */
-	for (i=0; i < numSm; i++,p++) {
+	for (i=0; i < num_sm; i++,p++) {
 		printk("%8s: 0x%08x -> 0x%08x  (attr: 0x%x)\n",
 		p->name, p->addr,
 		((unsigned int)p->addr + p->length) - 1, p->attr);
@@ -67,13 +67,13 @@ void vbiSharedMemoryShow(struct vb_config *config)
 void vbiMemoryShow(struct vb_config *config)
 {
 	struct vb_mem_info *p = config->memoryRegionsConfigAddress;
-	int numSm = config->num_mem;
+	int num_sm = config->num_mem;
 	int i;
 
-	printk("%d memory regions, Config at 0x%x:\n", numSm, p);
+	printk("%d memory regions, Config at 0x%x:\n", num_sm, p);
 
 	/* dump shared memory info if we have any attached */
-	for (i=0; i < numSm; i++,p++) {
+	for (i=0; i < num_sm; i++,p++) {
 		printk("%8s: 0x%08x -> 0x%08x  (attr: 0x%x) (type: %s)\n",
 			p->name, p->addr,
 			((unsigned int)p->addr + p->length) - 1,
-- 
