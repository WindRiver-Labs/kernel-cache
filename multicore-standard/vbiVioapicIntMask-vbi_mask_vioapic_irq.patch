From 47f6645d04e3a4a2e51c4b937c07f020a89a2c83 Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 12:35:11 -0500
Subject: [PATCH 73/80] vbiVioapicIntMask --> vbi_mask_vioapic_irq

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/x86/kernel/vbi/wrhv.c |    4 ++--
 include/vbi/vbi.h          |    4 ++--
 kernel/vbi/io_apic.c       |    4 ++--
 kernel/vbi/wrhv.c          |    6 +++---
 4 files changed, 9 insertions(+), 9 deletions(-)

diff --git a/arch/x86/kernel/vbi/wrhv.c b/arch/x86/kernel/vbi/wrhv.c
index e8b7f7e..7282994 100644
--- a/arch/x86/kernel/vbi/wrhv.c
+++ b/arch/x86/kernel/vbi/wrhv.c
@@ -769,7 +769,7 @@ static inline void wrhv_umask_timer_for_vcore(void)
 static inline void wrhv_mask_timer_for_vcore(void)
 {
        /* mask hypervisor-provided timer interrupt for vcore */
-       vbiVioapicIntMask(0);
+       vbi_mask_vioapic_irq(0);
 }
 
 #ifdef CONFIG_SMP
@@ -819,7 +819,7 @@ static void inline wrhv_umask_IPIs_for_vcore(void)
 
 static void x86_wrhv_mask_irq(void *irq)
 {
-	vbiVioapicIntMask((unsigned int)irq);
+	vbi_mask_vioapic_irq((unsigned int)irq);
 }
 
 static void x86_wrhv_unmask_irq(void *irq)
diff --git a/include/vbi/vbi.h b/include/vbi/vbi.h
index c9b9729..203b596 100644
--- a/include/vbi/vbi.h
+++ b/include/vbi/vbi.h
@@ -80,7 +80,7 @@ various data structures.
  * 
  * vbi_ack_vioapic_irq()
  * 
- * vbiVioapicIntMask()
+ * vbi_mask_vioapic_irq()
  * 
  * vbiVioapicIntUnmask()
  *
@@ -524,7 +524,7 @@ extern void *vbi_get_vioapic_addr(void);
 extern vbiStatus_t vbiVioapicVectorSet(vbiIrq_t irq, vbiVector_t vector); 
 extern vbiVector_t vbiVioapicVectorGet(vbiIrq_t irq);
 extern vbiStatus_t vbiVioapicIntUnmask(vbiIrq_t irq);
-extern vbiStatus_t vbiVioapicIntMask(vbiIrq_t irq);
+extern vbiStatus_t vbi_mask_vioapic_irq(vbiIrq_t irq);
 extern vbiStatus_t vbi_ack_vioapic_irq(vbiIrq_t irq);
 extern vbiStatus_t vbiVioapicIntSend(vbiIrq_t irq, uint32_t filter,
 				      vbiVb_t target);
diff --git a/kernel/vbi/io_apic.c b/kernel/vbi/io_apic.c
index a9e8aed..7cbb869 100644
--- a/kernel/vbi/io_apic.c
+++ b/kernel/vbi/io_apic.c
@@ -233,7 +233,7 @@ vbiStatus_t vbiVioapicIntUnmask(vbiIrq_t irq)
 EXPORT_SYMBOL(vbiVioapicIntUnmask);
 
 /*
- * vbiVioapicIntMask - Mask an irq
+ * vbi_mask_vioapic_irq - Mask an irq
  *
  * This routine disables the interrupt vector that matches the specified IRQ at
  * the VIOAPIC for the running core. The mask bit for the IRQ entry in the
@@ -242,7 +242,7 @@ EXPORT_SYMBOL(vbiVioapicIntUnmask);
  * vbiVioapicIntUnmask().
  *
  */
-vbiStatus_t vbiVioapicIntMask(vbiIrq_t irq)
+vbiStatus_t vbi_mask_vioapic_irq(vbiIrq_t irq)
 {
 	volatile VIOAPIC *pVioapic = (volatile VIOAPIC *)VBI_VIOAPIC_BASE_GET();
 
diff --git a/kernel/vbi/wrhv.c b/kernel/vbi/wrhv.c
index 9a195be..ea8e4b8 100644
--- a/kernel/vbi/wrhv.c
+++ b/kernel/vbi/wrhv.c
@@ -30,7 +30,7 @@ static void wrhv_enable_irq(unsigned int irq)
 
 static void wrhv_disable_irq(unsigned int irq)
 {
-	vbiVioapicIntMask(irq);
+	vbi_mask_vioapic_irq(irq);
 }
 
 static void wrhv_ack_irq(unsigned int irq)
@@ -41,14 +41,14 @@ static void wrhv_ack_irq(unsigned int irq)
 static void wrhv_maskack_irq(unsigned int irq)
 {
 	if (irq != 0)
-		vbiVioapicIntMask(irq);
+		vbi_mask_vioapic_irq(irq);
 	vbi_ack_vioapic_irq(irq);
 }
 
 static void wrhv_mask_irq(unsigned int irq)
 {
 	if (irq != 0)
-		vbiVioapicIntMask(irq);
+		vbi_mask_vioapic_irq(irq);
 }
 
 static void wrhv_unmask_irq(unsigned int irq)
-- 
