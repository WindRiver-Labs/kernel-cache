From b3ea155bd9e8ae5b15ffcca5799de782134b34a7 Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 2 Dec 2009 15:54:52 -0500
Subject: [PATCH 19/43] vbi: rename various other version related variables.

for i in `git grep -l  vbiCreationDate` ; do mv $i $i~ ; cat $i~|sed 's/vbiCreationDate/vbi_creation_date/' > $i ; done

for i in `git grep -l  vbiRuntimeVersion` ; do mv $i $i~ ; cat $i~|sed 's/vbiRuntimeVersion/vbi_runtime_version/' > $i ; done

for i in `git grep -l  vbiRuntimeName` ; do mv $i $i~ ; cat $i~|sed 's/vbiRuntimeName/vbi_runtime_name/' > $i ; done

for i in `git grep -l  vbiVersion` ; do mv $i $i~ ; cat $i~|sed 's/vbiVersion/vbi_version/' > $i ; done

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 include/vbi/compat.h  |    4 ++++
 include/vbi/version.h |    8 ++++----
 kernel/vbi/version.c  |   10 +++++-----
 3 files changed, 13 insertions(+), 9 deletions(-)

diff --git a/include/vbi/compat.h b/include/vbi/compat.h
index 1eb1fb1..99e88e1 100644
--- a/include/vbi/compat.h
+++ b/include/vbi/compat.h
@@ -156,6 +156,10 @@
 #define vbiVersionMajor		vbi_version_major
 #define vbiVersionMinor		vbi_version_minor
 #define vbiVersionMaint		vbi_version_maint
+#define vbiCreationDate		vbi_creation_date
+#define vbiRuntimeVersion	vbi_runtime_version
+#define vbiRuntimeName		vbi_runtime_name
+#define vbiVersion		vbi_version
 
 #define vbiExcStub		vbi_exc_stub
 #define vbiGuestDmaAddrGet	vbi_get_guest_dma_addr
diff --git a/include/vbi/version.h b/include/vbi/version.h
index d7c8906..1fcb2c8 100644
--- a/include/vbi/version.h
+++ b/include/vbi/version.h
@@ -31,10 +31,10 @@
 #define VBI_VERSION VBI_RUNTIME_NAME " " VBI_RUNTIME_VERSION
 
 #ifndef _ASMLANGUAGE
-extern const char *vbiCreationDate;
-extern const char *vbiRuntimeVersion;
-extern const char *vbiRuntimeName;
-extern const char *vbiVersion;
+extern const char *vbi_creation_date;
+extern const char *vbi_runtime_version;
+extern const char *vbi_runtime_name;
+extern const char *vbi_version;
 
 extern const uint32_t vbi_version_major;
 extern const uint32_t vbi_version_minor;
diff --git a/kernel/vbi/version.c b/kernel/vbi/version.c
index 8c08c78..058caac 100644
--- a/kernel/vbi/version.c
+++ b/kernel/vbi/version.c
@@ -36,12 +36,12 @@ const uint32_t vbi_version_minor = VBI_VERSION_MINOR;
 const uint32_t vbi_version_maint = VBI_VERSION_MAINT;
 
 /* string identifiers for vbi version */
-const char *vbiRuntimeName = VBI_RUNTIME_NAME;
-const char *vbiRuntimeVersion = VBI_RUNTIME_VERSION;
-const char *vbiVersion = VBI_VERSION;
+const char *vbi_runtime_name = VBI_RUNTIME_NAME;
+const char *vbi_runtime_version = VBI_RUNTIME_VERSION;
+const char *vbi_version = VBI_VERSION;
 
 #ifdef VBI_RUNTIME_CREATION_DATE
-const char *vbiCreationDate = VBI_RUNTIME_CREATION_DATE;
+const char *vbi_creation_date = VBI_RUNTIME_CREATION_DATE;
 #else
-const char *vbiCreationDate = __DATE__ ", " __TIME__;
+const char *vbi_creation_date = __DATE__ ", " __TIME__;
 #endif
-- 
