From 816b229271e80e8c7d794cfb4d89ae1adb45bdd5 Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 10:07:08 -0500
Subject: [PATCH 14/80] vbiVcoreIntRed_op --> vbi_vcore_irq_redirect

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/powerpc/kernel/vbi/syscalls.S |    8 ++++----
 arch/x86/kernel/vbi/syscalls.S     |    6 +++---
 include/vbi/private.h              |    2 +-
 kernel/vbi/io_apic.c               |    2 +-
 4 files changed, 9 insertions(+), 9 deletions(-)

diff --git a/arch/powerpc/kernel/vbi/syscalls.S b/arch/powerpc/kernel/vbi/syscalls.S
index daea4ac..a911cf0 100644
--- a/arch/powerpc/kernel/vbi/syscalls.S
+++ b/arch/powerpc/kernel/vbi/syscalls.S
@@ -27,7 +27,7 @@
 
 /* globals */
 
-FUNC_EXPORT(vbiVcoreIntRed_op)
+FUNC_EXPORT(vbi_vcore_irq_redirect)
 FUNC_EXPORT(vbiDebugShellStart)
 FUNC_EXPORT(vbi_vb_read_mem)
 FUNC_EXPORT(vbi_vb_write_mem)
@@ -853,11 +853,11 @@ FUNC_LABEL(vbi_vb_write_reg)
 FUNC_END(vbi_vb_write_reg)
 
 /*
- * vbiVcoreIntRed_op - redirect an irq to another vcore
+ * vbi_vcore_irq_redirect - redirect an irq to another vcore
  */
-FUNC_LABEL(vbiVcoreIntRed_op)
+FUNC_LABEL(vbi_vcore_irq_redirect)
 	lis r0, HI(VBI_SYS_intRedirect)
 	ori r0, r0, LO(VBI_SYS_intRedirect)
 	HCALL
 	blr
-FUNC_END(vbiVcoreIntRed_op)
+FUNC_END(vbi_vcore_irq_redirect)
diff --git a/arch/x86/kernel/vbi/syscalls.S b/arch/x86/kernel/vbi/syscalls.S
index 13c67a1..58be69e 100644
--- a/arch/x86/kernel/vbi/syscalls.S
+++ b/arch/x86/kernel/vbi/syscalls.S
@@ -50,7 +50,7 @@
 .globl vbi_vb_restart
 .globl vbi_vb_resume
 .globl vbi_vb_suspend
-.globl vbiVcoreIntRed_op 
+.globl vbi_vcore_irq_redirect 
 .globl vbi_vb_read_reg 
 .globl vbi_vb_write_reg 
 .globl vbiVtlbOp 
@@ -639,12 +639,12 @@ vbi_vb_write_reg:
 	ret
 
 /*
- * vbiVcoreIntRed_op - redirect an irq to another vcore
+ * vbi_vcore_irq_redirect - redirect an irq to another vcore
  *
  * RETURNS: returns OK or error number in case of failure
  *
  */
-vbiVcoreIntRed_op:
+vbi_vcore_irq_redirect:
 	movl $VBI_SYS_intRedirect, %eax
 	push $2
 	vmcall
diff --git a/include/vbi/private.h b/include/vbi/private.h
index 1abf297..3bd86af 100644
--- a/include/vbi/private.h
+++ b/include/vbi/private.h
@@ -29,7 +29,7 @@
 extern asmlinkage vbiStatus_t vbiIoapicOp (uint32_t ioctl, vbiIrq_t irq, uint32_t filter, vbiVb_t vbId);
 
 /* private interrupt redirection operator */
-extern asmlinkage vbiStatus_t vbiVcoreIntRed_op(vbiIrq_t irq, vbiCore_t CoreId);
+extern asmlinkage vbiStatus_t vbi_vcore_irq_redirect(vbiIrq_t irq, vbiCore_t CoreId);
 
 /* Name service private operator */
 extern asmlinkage vbiStatus_t vbiNsOp(uint32_t op, char* name, uint32_t rev, 
diff --git a/kernel/vbi/io_apic.c b/kernel/vbi/io_apic.c
index 2bebdd2..649ab40 100644
--- a/kernel/vbi/io_apic.c
+++ b/kernel/vbi/io_apic.c
@@ -354,7 +354,7 @@ vbiStatus_t vbiVioapicVcoreIntSend(vbiIrq_t irq, vbiCoreSet_t coreSet,
  */
 vbiStatus_t vbiVioapicIntRedirect(vbiIrq_t irq, vbiCore_t core)
 {
-	return vbiVcoreIntRed_op(irq, core);
+	return vbi_vcore_irq_redirect(irq, core);
 }
 
