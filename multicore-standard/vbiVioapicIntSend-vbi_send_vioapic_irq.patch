From 199212bb0d57b5270722323b0d2b4069893e50cb Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 12:35:12 -0500
Subject: [PATCH 75/80] vbiVioapicIntSend --> vbi_send_vioapic_irq

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 include/vbi/vbi.h    |    6 +++---
 kernel/vbi/io_apic.c |    6 +++---
 2 files changed, 6 insertions(+), 6 deletions(-)

diff --git a/include/vbi/vbi.h b/include/vbi/vbi.h
index cf19588..c0757bb 100644
--- a/include/vbi/vbi.h
+++ b/include/vbi/vbi.h
@@ -72,7 +72,7 @@ various data structures.
  * API's for controlling the VIOAPIC:
  *
  * 
- * vbiVioapicIntSend()
+ * vbi_send_vioapic_irq()
  * 
  * vbiVioapicVectorSet()
  * 
@@ -526,9 +526,9 @@ extern vbiVector_t vbiVioapicVectorGet(vbiIrq_t irq);
 extern vbiStatus_t vbiVioapicIntUnmask(vbiIrq_t irq);
 extern vbiStatus_t vbi_mask_vioapic_irq(vbiIrq_t irq);
 extern vbiStatus_t vbi_ack_vioapic_irq(vbiIrq_t irq);
-extern vbiStatus_t vbiVioapicIntSend(vbiIrq_t irq, uint32_t filter,
+extern vbiStatus_t vbi_send_vioapic_irq(vbiIrq_t irq, uint32_t filter,
 				      vbiVb_t target);
-extern vbiStatus_t vbiVioapicIntSend(vbiIrq_t irq, uint32_t filter, 
+extern vbiStatus_t vbi_send_vioapic_irq(vbiIrq_t irq, uint32_t filter, 
 				     vbiVb_t vb);
 extern vbiStatus_t vbi_redir_vioapic_irq(vbiIrq_t irq, vbiCore_t tCore);
 extern vbiStatus_t vbiVioapicVcoreIntSend (vbiIrq_t irq, vbiCoreSet_t vcoreSet, 
diff --git a/kernel/vbi/io_apic.c b/kernel/vbi/io_apic.c
index 1dfa2a9..aefef88 100644
--- a/kernel/vbi/io_apic.c
+++ b/kernel/vbi/io_apic.c
@@ -278,7 +278,7 @@ vbiStatus_t vbi_ack_vioapic_irq(vbiIrq_t irq)
 }
 
 /*
- * vbiVioapicIntSend - Send an interrupt
+ * vbi_send_vioapic_irq - Send an interrupt
  *
  * This routine makes a hypercall to trigger an IRQ to one or more virtual
  * board that are connected to the line. The first argument passed to this
@@ -318,11 +318,11 @@ vbiStatus_t vbi_ack_vioapic_irq(vbiIrq_t irq)
  * VIOAPIC_SEND_NONE	- Ignore this call.
  *
  */
-vbiStatus_t vbiVioapicIntSend(vbiIrq_t irq, uint32_t filter, vbiVb_t target)
+vbiStatus_t vbi_send_vioapic_irq(vbiIrq_t irq, uint32_t filter, vbiVb_t target)
 {
 	return vbi_io_apic_op(VBI_IOAPICIOCTL_SEND, irq, filter, target);
 }
-EXPORT_SYMBOL(vbiVioapicIntSend);
+EXPORT_SYMBOL(vbi_send_vioapic_irq);
 
 /*
  * vbiVioapicVcoreIntSend - Send an IRQ to a set of virtual cores within a VB
-- 
