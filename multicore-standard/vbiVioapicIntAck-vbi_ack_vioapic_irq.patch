From 9957e90001d7038144cbd0be27c55ad9962872ae Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 12:35:10 -0500
Subject: [PATCH 72/80] vbiVioapicIntAck --> vbi_ack_vioapic_irq

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 include/vbi/vbi.h    |    4 ++--
 kernel/vbi/io_apic.c |    4 ++--
 kernel/vbi/wrhv.c    |    4 ++--
 3 files changed, 6 insertions(+), 6 deletions(-)

diff --git a/include/vbi/vbi.h b/include/vbi/vbi.h
index 1113a64..c9b9729 100644
--- a/include/vbi/vbi.h
+++ b/include/vbi/vbi.h
@@ -78,7 +78,7 @@ various data structures.
  * 
  * vbiVioapicVectorGet()
  * 
- * vbiVioapicIntAck()
+ * vbi_ack_vioapic_irq()
  * 
  * vbiVioapicIntMask()
  * 
@@ -525,7 +525,7 @@ extern vbiStatus_t vbiVioapicVectorSet(vbiIrq_t irq, vbiVector_t vector);
 extern vbiVector_t vbiVioapicVectorGet(vbiIrq_t irq);
 extern vbiStatus_t vbiVioapicIntUnmask(vbiIrq_t irq);
 extern vbiStatus_t vbiVioapicIntMask(vbiIrq_t irq);
-extern vbiStatus_t vbiVioapicIntAck(vbiIrq_t irq);
+extern vbiStatus_t vbi_ack_vioapic_irq(vbiIrq_t irq);
 extern vbiStatus_t vbiVioapicIntSend(vbiIrq_t irq, uint32_t filter,
 				      vbiVb_t target);
 extern vbiStatus_t vbiVioapicIntSend(vbiIrq_t irq, uint32_t filter, 
diff --git a/kernel/vbi/io_apic.c b/kernel/vbi/io_apic.c
index 129b9a9..a9e8aed 100644
--- a/kernel/vbi/io_apic.c
+++ b/kernel/vbi/io_apic.c
@@ -258,7 +258,7 @@ vbiStatus_t vbiVioapicIntMask(vbiIrq_t irq)
 }
 
 /*
- * vbiVioapicIntAck - Acknowledge an irq
+ * vbi_ack_vioapic_irq - Acknowledge an irq
  *
  * This routine acknowledges the specified IRQ for the running core.
  * Calling this routine causes Hypervisor to purge any pending interrupt
@@ -268,7 +268,7 @@ vbiStatus_t vbiVioapicIntMask(vbiIrq_t irq)
  * required to be acknowledged.
  *
  */
-vbiStatus_t vbiVioapicIntAck(vbiIrq_t irq)
+vbiStatus_t vbi_ack_vioapic_irq(vbiIrq_t irq)
 {
 
 	if (irq > VIOAPIC_MAX_REDTABLE_ENTRIES-1)
diff --git a/kernel/vbi/wrhv.c b/kernel/vbi/wrhv.c
index 13dfae8..9a195be 100644
--- a/kernel/vbi/wrhv.c
+++ b/kernel/vbi/wrhv.c
@@ -35,14 +35,14 @@ static void wrhv_disable_irq(unsigned int irq)
 
 static void wrhv_ack_irq(unsigned int irq)
 {
-	vbiVioapicIntAck(irq);
+	vbi_ack_vioapic_irq(irq);
 }
 
 static void wrhv_maskack_irq(unsigned int irq)
 {
 	if (irq != 0)
 		vbiVioapicIntMask(irq);
-	vbiVioapicIntAck(irq);
+	vbi_ack_vioapic_irq(irq);
 }
 
 static void wrhv_mask_irq(unsigned int irq)
-- 
