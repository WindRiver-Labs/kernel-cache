From 29ece0b3aeefb1f0cd0285b92b2eb1b2cf828567 Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 10:07:28 -0500
Subject: [PATCH 38/80] vbiMemAttrGet --> vbi_get_mem_attr

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/powerpc/kernel/vbi/syscalls.S |    8 ++++----
 arch/x86/kernel/vbi/syscalls.S     |    4 ++--
 include/vbi/syscall.h              |    2 +-
 3 files changed, 7 insertions(+), 7 deletions(-)

diff --git a/arch/powerpc/kernel/vbi/syscalls.S b/arch/powerpc/kernel/vbi/syscalls.S
index 118a503..8250be8 100644
--- a/arch/powerpc/kernel/vbi/syscalls.S
+++ b/arch/powerpc/kernel/vbi/syscalls.S
@@ -55,7 +55,7 @@ FUNC_EXPORT(vbi_set_exc_base)
 FUNC_EXPORT(vbi_set_exc_offset)
 FUNC_EXPORT(vbi_get_exc_offset)
 FUNC_EXPORT(vbi_set_mem_attr)
-FUNC_EXPORT(vbiMemAttrGet)
+FUNC_EXPORT(vbi_get_mem_attr)
 FUNC_EXPORT(vbiVmmuConfig)
 FUNC_EXPORT(vbiVmmuEnable)
 FUNC_EXPORT(vbiVmmuDisable)
@@ -701,7 +701,7 @@ FUNC_LABEL(vbi_set_mem_attr)
 FUNC_END(vbi_set_mem_attr)
 
 /*
- * vbiMemAttrGet - Get MMU page protections
+ * vbi_get_mem_attr - Get MMU page protections
  *
  * This routine makes a hypercall to retrieve the MMU attributes associated for
  * the page where the specified address is mapped. This is not the VMMU
@@ -709,12 +709,12 @@ FUNC_END(vbi_set_mem_attr)
  * therefore a hypercall is not necessary.
  *
  */
-FUNC_LABEL(vbiMemAttrGet)
+FUNC_LABEL(vbi_get_mem_attr)
 	lis r0, HI(VBI_SYS_mmu_attr_get)
 	ori r0, r0, LO(VBI_SYS_mmu_attr_get)
 	HCALL
 	blr
-FUNC_END(vbiMemAttrGet)
+FUNC_END(vbi_get_mem_attr)
 
 /*
  * vbi_io_apic_ioctl - virtual IO APIC ioctl call
diff --git a/arch/x86/kernel/vbi/syscalls.S b/arch/x86/kernel/vbi/syscalls.S
index d410bdc..d7c09e0 100644
--- a/arch/x86/kernel/vbi/syscalls.S
+++ b/arch/x86/kernel/vbi/syscalls.S
@@ -44,7 +44,7 @@
 .globl vbi_vcore_irq_lock
 .globl vbiVbMgmt
 .globl vbi_set_mem_attr
-.globl vbiMemAttrGet
+.globl vbi_get_mem_attr
 .globl vbiNsOp
 .globl vbi_vb_reset
 .globl vbi_vb_restart
@@ -490,7 +490,7 @@ vbiMmuAttrSet:
 	ret
 
 /*
- * vbiMemAttrGet - Get MMU page protections
+ * vbi_get_mem_attr - Get MMU page protections
  *
  * This routine makes a hypercall to retrieve the MMU attributes associated for
  * the page where the specified address is mapped. This is not the VMMU
diff --git a/include/vbi/syscall.h b/include/vbi/syscall.h
index b3719ab..7dc935e 100644
--- a/include/vbi/syscall.h
+++ b/include/vbi/syscall.h
@@ -126,7 +126,7 @@ extern vbiStatus_t vbiVmmuTlbFlush(struct vmmuConfig * config, void *addr, size_
 /* Prior to vbi 2.0 these api were vbiMmuAttrSet/Get */
 
 extern asmlinkage vbiStatus_t  vbi_set_mem_attr(void *vaddr, size_t len, vbiMemAttr_t attr);
-extern asmlinkage vbiStatus_t  vbiMemAttrGet(void *vaddr, vbiMemAttr_t * attr);
+extern asmlinkage vbiStatus_t  vbi_get_mem_attr(void *vaddr, vbiMemAttr_t * attr);
 
 /*
  * START: New APIs introduced for vbi 2.0  
