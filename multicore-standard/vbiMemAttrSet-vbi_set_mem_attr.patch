From 97ba2f9df82dcc8f36b773a18de6a9886bb90b2e Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 10:07:27 -0500
Subject: [PATCH 37/80] vbiMemAttrSet --> vbi_set_mem_attr

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/powerpc/kernel/vbi/syscalls.S |    8 ++++----
 arch/x86/kernel/vbi/syscalls.S     |    4 ++--
 include/vbi/syscall.h              |    2 +-
 3 files changed, 7 insertions(+), 7 deletions(-)

diff --git a/arch/powerpc/kernel/vbi/syscalls.S b/arch/powerpc/kernel/vbi/syscalls.S
index 81a42d2..118a503 100644
--- a/arch/powerpc/kernel/vbi/syscalls.S
+++ b/arch/powerpc/kernel/vbi/syscalls.S
@@ -54,7 +54,7 @@ FUNC_EXPORT(vbi_vcore_irq_state)
 FUNC_EXPORT(vbi_set_exc_base)
 FUNC_EXPORT(vbi_set_exc_offset)
 FUNC_EXPORT(vbi_get_exc_offset)
-FUNC_EXPORT(vbiMemAttrSet)
+FUNC_EXPORT(vbi_set_mem_attr)
 FUNC_EXPORT(vbiMemAttrGet)
 FUNC_EXPORT(vbiVmmuConfig)
 FUNC_EXPORT(vbiVmmuEnable)
@@ -683,7 +683,7 @@ FUNC_END(vbi_ctx_ctl)
 
 /*
  *
- * vbiMemAttrSet - Set protection for a page of memory
+ * vbi_set_mem_attr - Set protection for a page of memory
  *
  * This routine makes a hypercall to set the MMU attribures for the specified
  * memory range. The possible attributes one or a combinarion of the following:
@@ -693,12 +693,12 @@ FUNC_END(vbi_ctx_ctl)
  * VBI_MMU_PROT_EXECUTE - allow supervisor and user execute
  *
  */
-FUNC_LABEL(vbiMemAttrSet)
+FUNC_LABEL(vbi_set_mem_attr)
 	lis r0, HI(VBI_SYS_mmu_attr_set)
 	ori r0, r0, LO(VBI_SYS_mmu_attr_set)
 	HCALL
 	blr
-FUNC_END(vbiMemAttrSet)
+FUNC_END(vbi_set_mem_attr)
 
 /*
  * vbiMemAttrGet - Get MMU page protections
diff --git a/arch/x86/kernel/vbi/syscalls.S b/arch/x86/kernel/vbi/syscalls.S
index dcd1f91..d410bdc 100644
--- a/arch/x86/kernel/vbi/syscalls.S
+++ b/arch/x86/kernel/vbi/syscalls.S
@@ -43,7 +43,7 @@
 .globl vbi_vcore_irq_unlock
 .globl vbi_vcore_irq_lock
 .globl vbiVbMgmt
-.globl vbiMemAttrSet
+.globl vbi_set_mem_attr
 .globl vbiMemAttrGet
 .globl vbiNsOp
 .globl vbi_vb_reset
@@ -470,7 +470,7 @@ vbi_ctx_ctl:
 	ret
 
 /*
- * vbiMemAttrSet - Set protection for a page of memory
+ * vbi_set_mem_attr - Set protection for a page of memory
  *
  * This routine makes a hypercall to set the MMU attribures for the specified
  * memory range. The possible attributes one or a combinarion of the following:
diff --git a/include/vbi/syscall.h b/include/vbi/syscall.h
index e32228b..b3719ab 100644
--- a/include/vbi/syscall.h
+++ b/include/vbi/syscall.h
@@ -125,7 +125,7 @@ extern vbiStatus_t vbiVmmuTlbFlush(struct vmmuConfig * config, void *addr, size_
 
 /* Prior to vbi 2.0 these api were vbiMmuAttrSet/Get */
 
-extern asmlinkage vbiStatus_t  vbiMemAttrSet(void *vaddr, size_t len, vbiMemAttr_t attr);
+extern asmlinkage vbiStatus_t  vbi_set_mem_attr(void *vaddr, size_t len, vbiMemAttr_t attr);
 extern asmlinkage vbiStatus_t  vbiMemAttrGet(void *vaddr, vbiMemAttr_t * attr);
 
 /*
