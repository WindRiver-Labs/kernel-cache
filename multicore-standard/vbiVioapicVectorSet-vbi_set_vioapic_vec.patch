From 674e069f9c72992ac2f10740b0b3eafaa7fd6c39 Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 12:35:15 -0500
Subject: [PATCH 79/80] vbiVioapicVectorSet --> vbi_set_vioapic_vec

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 include/vbi/vbi.h    |    4 ++--
 kernel/vbi/io_apic.c |    6 +++---
 2 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/include/vbi/vbi.h b/include/vbi/vbi.h
index 96c4bd5..ea2fbd7 100644
--- a/include/vbi/vbi.h
+++ b/include/vbi/vbi.h
@@ -74,7 +74,7 @@ various data structures.
  * 
  * vbi_send_vioapic_irq()
  * 
- * vbiVioapicVectorSet()
+ * vbi_set_vioapic_vec()
  * 
  * vbi_get_vioapic_vec()
  * 
@@ -521,7 +521,7 @@ extern void vbi_exc_stub(void);
 extern void ASSERT_FN(const char *, const char *, int);
 extern void vbi_idle(uint64_t);
 extern void *vbi_get_vioapic_addr(void);
-extern vbiStatus_t vbiVioapicVectorSet(vbiIrq_t irq, vbiVector_t vector); 
+extern vbiStatus_t vbi_set_vioapic_vec(vbiIrq_t irq, vbiVector_t vector); 
 extern vbiVector_t vbi_get_vioapic_vec(vbiIrq_t irq);
 extern vbiStatus_t vbi_unmask_vioapic_irq(vbiIrq_t irq);
 extern vbiStatus_t vbi_mask_vioapic_irq(vbiIrq_t irq);
diff --git a/kernel/vbi/io_apic.c b/kernel/vbi/io_apic.c
index c1c639b..31d9bc8 100644
--- a/kernel/vbi/io_apic.c
+++ b/kernel/vbi/io_apic.c
@@ -141,7 +141,7 @@ void *vbi_get_vioapic_addr(void)
 }
 
 /*
- * vbiVioapicVectorSet - Set a vector for the specified irq entry
+ * vbi_set_vioapic_vec - Set a vector for the specified irq entry
  *
  * This routine sets a vector for the specified entry in the VIOAPIC
  * redirection table. The previous entry in the table is trashed. Before
@@ -155,7 +155,7 @@ void *vbi_get_vioapic_addr(void)
  * This routine is currently not supported for PPC.
  *
  */
-vbiStatus_t vbiVioapicVectorSet(vbiIrq_t irq, vbiVector_t vector)
+vbiStatus_t vbi_set_vioapic_vec(vbiIrq_t irq, vbiVector_t vector)
 {
 	volatile VIOAPIC *pVioapic = (volatile VIOAPIC *)VBI_VIOAPIC_BASE_GET();
 
@@ -165,7 +165,7 @@ vbiStatus_t vbiVioapicVectorSet(vbiIrq_t irq, vbiVector_t vector)
 	if (irq > VIOAPIC_MAX_REDTABLE_ENTRIES-1)
 		return VBI_VIOAPIC_IRQ_OUTBOUND;
 
-	VB_DEBUG_MSG("vbiVioapicVectorSet: base @ 0x%x\n", vioapicBase);
+	VB_DEBUG_MSG("vbi_set_vioapic_vec: base @ 0x%x\n", vioapicBase);
 	VB_DEBUG_MSG("Set vector %d: @ 0x%x\n", vioapicBase,
 		 &(pVioapic->entry[irq].value));
 
-- 
