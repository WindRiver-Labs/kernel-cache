From 24d235e981e40dc147075e021eea6aada8c5077b Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 10:07:20 -0500
Subject: [PATCH 28/80] vbiPs --> vbi_ps

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/powerpc/kernel/vbi/syscalls.S |    2 +-
 arch/x86/kernel/vbi/syscalls.S     |    6 +++---
 2 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/powerpc/kernel/vbi/syscalls.S b/arch/powerpc/kernel/vbi/syscalls.S
index a11b07f..7184939 100644
--- a/arch/powerpc/kernel/vbi/syscalls.S
+++ b/arch/powerpc/kernel/vbi/syscalls.S
@@ -45,7 +45,7 @@ FUNC_EXPORT(vbi_flush_dcache)
 FUNC_EXPORT(vbi_update_text_cache)
 FUNC_EXPORT(vbi_flush_tlb)
 FUNC_EXPORT(vbi_panic)
-FUNC_EXPORT(vbiPs)
+FUNC_EXPORT(vbi_ps)
 FUNC_EXPORT(vbiKputs)
 FUNC_EXPORT(vbiKputc)
 FUNC_EXPORT(vbiIntVCoreUnlock)
diff --git a/arch/x86/kernel/vbi/syscalls.S b/arch/x86/kernel/vbi/syscalls.S
index 49cf420..09033f4 100644
--- a/arch/x86/kernel/vbi/syscalls.S
+++ b/arch/x86/kernel/vbi/syscalls.S
@@ -37,7 +37,7 @@
 .globl vbi_reply
 .globl vbi_flush_tlb
 .globl vbi_panic
-.globl vbiPs
+.globl vbi_ps
 .globl vbiKputs
 .globl vbiKputc
 .globl vbiIntVCoreUnlock
@@ -161,7 +161,7 @@ vbi_panic:
 
 
 /*
- * vbiPs - display the list of contexts on the console
+ * vbi_ps - display the list of contexts on the console
  *
  * This system call sends a "ps" like output of the hypervisor contexts to
  * the system console.
@@ -169,7 +169,7 @@ vbi_panic:
  * Returns: OK or ERROR
  *
  */
-vbiPs:
+vbi_ps:
 	movl $VBI_SYS_ps,%eax
 	push $0
 	vmcall
-- 
1.6.5.2

