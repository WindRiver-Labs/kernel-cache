From 6ae98e9c835c78bedcb8846a36c73e8cac8d85e2 Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Mon, 16 Nov 2009 23:44:10 -0500
Subject: [PATCH 09/80] vbi: intName --> irq_name

for i in `git grep -l intName` ; do echo $i ; mv $i $i~ ; cat $i~ |sed 's/intName/irq_name/g' > $i ; done

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 include/vbi/interface.h |    2 +-
 include/vbi/interrupt.h |    2 +-
 kernel/vbi/interrupt.c  |    4 ++--
 kernel/vbi/show.c       |    4 ++--
 4 files changed, 6 insertions(+), 6 deletions(-)

diff --git a/include/vbi/interface.h b/include/vbi/interface.h
index cd3eac0..69d3611 100644
--- a/include/vbi/interface.h
+++ b/include/vbi/interface.h
@@ -439,7 +439,7 @@ struct vb_status {
 /* Configuration information for interrupts */
 struct vb_int_info
 {
-	char intName[VB_NAMELEN];/* the name of this interrupt */
+	char irq_name[VB_NAMELEN];/* the name of this interrupt */
 	uint16_t intDirection;	/* interrupt direction: INPUT_INT, OUTPUT_INT */
 	uint16_t intNumber;	/* the local VB interrupt number */
 	uint32_t irq_core;	/* the receiving core for this incomming int */
diff --git a/include/vbi/interrupt.h b/include/vbi/interrupt.h
index 3280263..99cc745 100644
--- a/include/vbi/interrupt.h
+++ b/include/vbi/interrupt.h
@@ -26,7 +26,7 @@ interrupts configuration data.
 
 #ifndef	_ASMLANGUAGE
 
-extern vbiIrq_t vbiIntVecFind (char * intName, int32_t intDirection);
+extern vbiIrq_t vbiIntVecFind (char * irq_name, int32_t intDirection);
 
 #endif /* _ASMLANGUAGE */
 
diff --git a/kernel/vbi/interrupt.c b/kernel/vbi/interrupt.c
index 02540ed..62c811c 100644
--- a/kernel/vbi/interrupt.c
+++ b/kernel/vbi/interrupt.c
@@ -43,7 +43,7 @@
  *
  */
 
-vbiIrq_t vbiIntVecFind(char *intName, int32_t intDirection)
+vbiIrq_t vbiIntVecFind(char *irq_name, int32_t intDirection)
 {
 	struct vb_config *config = VBI_CONFIG_ADDR_GET();
 	struct vb_int_info *info = config->interruptConfiguration;
@@ -54,7 +54,7 @@ vbiIrq_t vbiIntVecFind(char *intName, int32_t intDirection)
 		if (intDirection != (int32_t)(info->intDirection))
 			continue;
 
-		if (!strncmp((char *)intName, (char *)info->intName,
+		if (!strncmp((char *)irq_name, (char *)info->irq_name,
 					VB_NAMELEN)) {
 			/* Found */
 			return (int32_t)info->intNumber;
diff --git a/kernel/vbi/show.c b/kernel/vbi/show.c
index c4265ed..9e5d8bc 100644
--- a/kernel/vbi/show.c
+++ b/kernel/vbi/show.c
@@ -98,10 +98,10 @@ static void vbiInterruptsShow(struct vb_config *config)
 	for (i = 0; i < num_int; i++, p++) {
 		if (p->intDirection == VB_INPUT_INT)
 			printk("%8s: Dir: In, Vector #: %d, Core: %d\n",
-		            p->intName, p->intNumber, p->irq_core);
+		            p->irq_name, p->intNumber, p->irq_core);
 		else
 			printk("%8s: Dir: Out, Vector #: %d\n",
-		            p->intName, p->intNumber);
+		            p->irq_name, p->intNumber);
 	}
 }
 
