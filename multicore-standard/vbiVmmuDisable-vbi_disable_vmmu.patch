From 48455292f0ff57d9bfa2c685150c3c1312903e2e Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 10:07:30 -0500
Subject: [PATCH 41/80] vbiVmmuDisable --> vbi_disable_vmmu

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/powerpc/kernel/vbi/syscalls.S |    8 ++++----
 include/vbi/syscall.h              |    2 +-
 2 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/arch/powerpc/kernel/vbi/syscalls.S b/arch/powerpc/kernel/vbi/syscalls.S
index b462592..6e1448d 100644
--- a/arch/powerpc/kernel/vbi/syscalls.S
+++ b/arch/powerpc/kernel/vbi/syscalls.S
@@ -58,7 +58,7 @@ FUNC_EXPORT(vbi_set_mem_attr)
 FUNC_EXPORT(vbi_get_mem_attr)
 FUNC_EXPORT(vbi_config_vmmu)
 FUNC_EXPORT(vbi_enable_vmmu)
-FUNC_EXPORT(vbiVmmuDisable)
+FUNC_EXPORT(vbi_disable_vmmu)
 FUNC_EXPORT(vbiVmmuTlbLoad)
 FUNC_EXPORT(vbiVmmuTlbFlush)
 FUNC_EXPORT(vbiVbMgmt)
@@ -597,17 +597,17 @@ FUNC_LABEL(vbi_enable_vmmu)
 FUNC_END(vbi_enable_vmmu)
 
 /*
- * vbiVmmuDisable - disable the virtual MMU
+ * vbi_disable_vmmu - disable the virtual MMU
  *
  * This system call disables a context's virtual MMU.
  *
  */
-FUNC_LABEL(vbiVmmuDisable)
+FUNC_LABEL(vbi_disable_vmmu)
 	lis r0, HI(VBI_SYS_vmmu_disable)
 	ori r0, r0, LO(VBI_SYS_vmmu_disable)
 	HCALL
 	blr
-FUNC_END(vbiVmmuDisable)
+FUNC_END(vbi_disable_vmmu)
 
 /*
  *
diff --git a/include/vbi/syscall.h b/include/vbi/syscall.h
index 3298e58..ccabe23 100644
--- a/include/vbi/syscall.h
+++ b/include/vbi/syscall.h
@@ -116,7 +116,7 @@ extern asmlinkage vbiStatus_t vbi_kputs(const char *s);
 extern asmlinkage vbiStatus_t vbi_kputc(int c);
 extern vbiStatus_t vbi_config_vmmu(struct vmmuConfig * config);
 extern vbiStatus_t vbi_enable_vmmu(uint32_t  vmmu_num);
-extern vbiStatus_t vbiVmmuDisable(uint32_t vmmu_num);
+extern vbiStatus_t vbi_disable_vmmu(uint32_t vmmu_num);
 extern vbiStatus_t vbiNsRegister(char  *name, uint32_t  revision);
 extern vbiStatus_t vbiNsUnRegister(char *name, uint32_t  revision);
 extern vbiStatus_t vbiNsLookup(char *name, uint32_t  rev, VBI_NS_HANDLE *pHandle);
-- 
