From e10dc97f9efacad91b8dabfed3281539ea9626b3 Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Fri, 30 Oct 2009 17:33:44 -0400
Subject: [PATCH 26/26] vbi: numInt --> num_int

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 include/vbi/interface.h |    2 +-
 kernel/vbi/interrupt.c  |    2 +-
 kernel/vbi/show.c       |    8 ++++----
 3 files changed, 6 insertions(+), 6 deletions(-)

diff --git a/include/vbi/interface.h b/include/vbi/interface.h
index e7d0b8d..dcb37c4 100644
--- a/include/vbi/interface.h
+++ b/include/vbi/interface.h
@@ -569,7 +569,7 @@ struct vb_config
 	uint32_t timeStampFrequency;	/* ticks per second */
 
 	/* number of interrupts connected to this board */
-	uint32_t numInts;
+	uint32_t num_ints;
 
 	/*
 	 *  misc settings
diff --git a/kernel/vbi/interrupt.c b/kernel/vbi/interrupt.c
index acef2a0..c36a75b 100644
--- a/kernel/vbi/interrupt.c
+++ b/kernel/vbi/interrupt.c
@@ -47,7 +47,7 @@ vbiIrq_t vbiIntVecFind(char *intName, int32_t intDirection)
 {
 	struct vb_config *config = VBI_CONFIG_ADDR_GET();
 	struct vb_int_info *info = config->interruptConfiguration;
-	int32_t num  = config->numInts;
+	int32_t num  = config->num_ints;
 	int32_t i;
 
 	for (i = 0; i < num; i++, info++)
diff --git a/kernel/vbi/show.c b/kernel/vbi/show.c
index 471ef44..6b02db3 100644
--- a/kernel/vbi/show.c
+++ b/kernel/vbi/show.c
@@ -90,12 +90,12 @@ void vbiMemoryShow(struct vb_config *config)
 static void vbiInterruptsShow(struct vb_config *config)
 {
 	struct vb_int_info *p = config->interruptConfiguration;
-	int numInt = config->numInts;
+	int num_int = config->num_ints;
 	int i;
 
-	printk("%d interrupt configurations, Config at 0x%p\n", numInt, p);
+	printk("%d interrupt configurations, Config at 0x%p\n", num_int, p);
 
-	for (i = 0; i < numInt; i++, p++) {
+	for (i = 0; i < num_int; i++, p++) {
 		if (p->intDirection == VB_INPUT_INT)
 			printk("%8s: Dir: In, Vector #: %d, Core: %d\n",
 		            p->intName, p->intNumber, p->intCore);
@@ -181,7 +181,7 @@ void vbiConfigShow(struct vb_config *p)
 	printk("Timer Frequency:     %d/sec\n", p->tickTimerFrequency);
 	printk("TimeStamp Frequency: %dULL/sec\n", p->timeStampFrequency);
 
-	printk("numInts:             %d\n", p->numInts);
+	printk("num_ints:             %d\n", p->num_ints);
 
 	vbiMemoryShow(p);
 	vbiSharedMemoryShow(p);
-- 
