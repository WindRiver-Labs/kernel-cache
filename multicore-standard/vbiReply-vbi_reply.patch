From 95abb25120ee35baf9ed98cf44db1235850a634c Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 10:07:15 -0500
Subject: [PATCH 22/80] vbiReply --> vbi_reply

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/powerpc/kernel/vbi/syscalls.S |    8 ++++----
 arch/x86/kernel/vbi/syscalls.S     |    6 +++---
 include/vbi/syscall.h              |    2 +-
 3 files changed, 8 insertions(+), 8 deletions(-)

diff --git a/arch/powerpc/kernel/vbi/syscalls.S b/arch/powerpc/kernel/vbi/syscalls.S
index a3a989a..64b727c 100644
--- a/arch/powerpc/kernel/vbi/syscalls.S
+++ b/arch/powerpc/kernel/vbi/syscalls.S
@@ -39,7 +39,7 @@ FUNC_EXPORT(vbi_hy_ioctl)
 FUNC_EXPORT(vbi_ctx_ctl)
 FUNC_EXPORT(vbi_send)
 FUNC_EXPORT(vbi_rx_op)
-FUNC_EXPORT(vbiReply)
+FUNC_EXPORT(vbi_reply)
 FUNC_EXPORT(vbiIcacheFlush)
 FUNC_EXPORT(vbiDcacheFlush)
 FUNC_EXPORT(vbiCacheTextUpdate)
@@ -270,7 +270,7 @@ FUNC_LABEL(vbi_rx_op)
 FUNC_END(vbi_rx_op)
 
 /*
- * vbiReply - Reply to message received from another context
+ * vbi_reply - Reply to message received from another context
  *
  * This routine makes a hypercall in order to reply to a message received from
  * another context. A message is received from remote context by calling
@@ -278,12 +278,12 @@ FUNC_END(vbi_rx_op)
  * the caller.
  *
  */
-FUNC_LABEL(vbiReply)
+FUNC_LABEL(vbi_reply)
 	lis r0, HI(VBI_SYS_reply)
 	ori r0, r0, LO(VBI_SYS_reply)
 	HCALL
 	blr
-FUNC_END(vbiReply)
+FUNC_END(vbi_reply)
 
 /*
  *
diff --git a/arch/x86/kernel/vbi/syscalls.S b/arch/x86/kernel/vbi/syscalls.S
index a65a32c..59c8658 100644
--- a/arch/x86/kernel/vbi/syscalls.S
+++ b/arch/x86/kernel/vbi/syscalls.S
@@ -34,7 +34,7 @@
 .globl vbi_ctx_ctl
 .globl vbi_send
 .globl vbi_rx_op
-.globl vbiReply
+.globl vbi_reply
 .globl vbiTlbFlush
 .globl vbiPanic
 .globl vbiPs
@@ -92,7 +92,7 @@ vbi_rx_op:
 	ret
 
 /*
- * vbiReply - Reply to message received from another context
+ * vbi_reply - Reply to message received from another context
  *
  * This routine makes a hypercall in order to reply to a message received from
  * another context. A message is received from remote context by calling
@@ -102,7 +102,7 @@ vbi_rx_op:
  * RETURNS: OK or an error number in case of failure
  *
  */
-vbiReply:
+vbi_reply:
 	movl $VBI_SYS_reply,%eax
 	push $4
 	vmcall
diff --git a/include/vbi/syscall.h b/include/vbi/syscall.h
index 2b28bd0..4150277 100644
--- a/include/vbi/syscall.h
+++ b/include/vbi/syscall.h
@@ -106,7 +106,7 @@ extern asmlinkage vbiStatus_t vbi_send(vbiCtx_t id, void *smsg, size_t slen,
 extern vbiStatus_t vbiReceive(void *rmsg, uint32_t rlen, VBI_MSG_INFO *info,
 				VBI_MSG_CTL *ctl);
 /* message reply */
-extern asmlinkage int32_t vbiReply(vbiCtx_t id, void *smsg, size_t slen, 
+extern asmlinkage int32_t vbi_reply(vbiCtx_t id, void *smsg, size_t slen, 
 				VBI_MSG_CTL *ctl);
 
 extern asmlinkage vbiStatus_t vbiPanic(const char *msg);
-- 
