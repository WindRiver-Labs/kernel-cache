From 7c184a40f964a2f4527e718884a886af9ba5164f Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 10:07:21 -0500
Subject: [PATCH 29/80] vbiKputs --> vbi_kputs

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/powerpc/kernel/vbi/syscalls.S |    8 ++++----
 arch/x86/kernel/vbi/syscalls.S     |    6 +++---
 include/vbi/syscall.h              |    2 +-
 3 files changed, 8 insertions(+), 8 deletions(-)

diff --git a/arch/powerpc/kernel/vbi/syscalls.S b/arch/powerpc/kernel/vbi/syscalls.S
index 7184939..26119b7 100644
--- a/arch/powerpc/kernel/vbi/syscalls.S
+++ b/arch/powerpc/kernel/vbi/syscalls.S
@@ -46,7 +46,7 @@ FUNC_EXPORT(vbi_update_text_cache)
 FUNC_EXPORT(vbi_flush_tlb)
 FUNC_EXPORT(vbi_panic)
 FUNC_EXPORT(vbi_ps)
-FUNC_EXPORT(vbiKputs)
+FUNC_EXPORT(vbi_kputs)
 FUNC_EXPORT(vbiKputc)
 FUNC_EXPORT(vbiIntVCoreUnlock)
 FUNC_EXPORT(vbiIntVCoreLock)
@@ -287,18 +287,18 @@ FUNC_END(vbi_reply)
 
 /*
  *
- * vbiKputs - print a string on the hypervisor kernel console
+ * vbi_kputs - print a string on the hypervisor kernel console
  *
  * This routine makes a hypercall and prints a string of characters to to
  * hypervisor console.
  *
  */
-FUNC_LABEL(vbiKputs)
+FUNC_LABEL(vbi_kputs)
 	lis r0, HI(VBI_SYS_kputs)
 	ori r0, r0, LO(VBI_SYS_kputs)
 	HCALL
 	blr
-FUNC_END(vbiKputs)
+FUNC_END(vbi_kputs)
 
 /*
  *
diff --git a/arch/x86/kernel/vbi/syscalls.S b/arch/x86/kernel/vbi/syscalls.S
index 09033f4..b095f7f 100644
--- a/arch/x86/kernel/vbi/syscalls.S
+++ b/arch/x86/kernel/vbi/syscalls.S
@@ -38,7 +38,7 @@
 .globl vbi_flush_tlb
 .globl vbi_panic
 .globl vbi_ps
-.globl vbiKputs
+.globl vbi_kputs
 .globl vbiKputc
 .globl vbiIntVCoreUnlock
 .globl vbiIntVCoreLock
@@ -111,14 +111,14 @@ vbi_reply:
 
 
 /*
- * vbiKputs - print a string on the kernel console
+ * vbi_kputs - print a string on the kernel console
  *
  * This system call sends the specified string to the system console.
  *
  * Returns: OK or ERROR
  *
  */
-vbiKputs:
+vbi_kputs:
 	movl $VBI_SYS_kputs,%eax
 	push $1
 	vmcall
diff --git a/include/vbi/syscall.h b/include/vbi/syscall.h
index 4bee829..5d09d39 100644
--- a/include/vbi/syscall.h
+++ b/include/vbi/syscall.h
@@ -112,7 +112,7 @@ extern asmlinkage int32_t vbi_reply(vbiCtx_t id, void *smsg, size_t slen,
 extern asmlinkage vbiStatus_t vbi_panic(const char *msg);
 extern vbiStatus_t vbi_flush_dcache(void *saddr, size_t size);
 extern vbiStatus_t vbi_flush_icache(void *saddr, size_t size);
-extern asmlinkage vbiStatus_t vbiKputs(const char *s);
+extern asmlinkage vbiStatus_t vbi_kputs(const char *s);
 extern asmlinkage vbiStatus_t vbiKputc(int c);
 extern vbiStatus_t vbiVmmuConfig(struct vmmuConfig * config);
 extern vbiStatus_t vbiVmmuEnable(uint32_t  vmmu_num);
-- 
1.6.5.2

