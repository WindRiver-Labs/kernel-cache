From 369c5c8c942dc4c8a3c8622d2689f9e20b68ad2a Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Tue, 3 Nov 2009 22:12:03 -0500
Subject: [PATCH 03/80] vbi: timeStampFrequency -> stamp_freq

for i in `git grep -l timeStampFrequency` ; do echo $i ; mv $i $i~ ; cat $i~ |sed 's/timeStampFrequency/stamp_freq/g' > $i ; done

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 include/vbi/interface.h |    2 +-
 include/vbi/vbi.h       |    2 +-
 kernel/vbi/show.c       |    2 +-
 kernel/vbi/wrhv.c       |    4 ++--
 4 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/include/vbi/interface.h b/include/vbi/interface.h
index 2fd67c1..1c62c17 100644
--- a/include/vbi/interface.h
+++ b/include/vbi/interface.h
@@ -559,7 +559,7 @@ struct vb_config
 	uint32_t tick_freq;	/* ticks per second */
 
 	/* the frequency of the timestamp */
-	uint32_t timeStampFrequency;	/* ticks per second */
+	uint32_t stamp_freq;	/* ticks per second */
 
 	/* number of interrupts connected to this board */
 	uint32_t num_ints;
diff --git a/include/vbi/vbi.h b/include/vbi/vbi.h
index 28454d9..38ff3e4 100644
--- a/include/vbi/vbi.h
+++ b/include/vbi/vbi.h
@@ -479,7 +479,7 @@ various data structures.
         (VBI_CONFIG_ADDR_GET()->tick_freq)
 
 #define VBI_TIMESTAMP_FREQ_GET()			\
-        (VBI_CONFIG_ADDR_GET()->timeStampFrequency)
+        (VBI_CONFIG_ADDR_GET()->stamp_freq)
 
 /*
  * VBI_BOOTLINE_ADDR_GET - Get a virtual board bootline parameters address
diff --git a/kernel/vbi/show.c b/kernel/vbi/show.c
index a54b509..74764ef 100644
--- a/kernel/vbi/show.c
+++ b/kernel/vbi/show.c
@@ -179,7 +179,7 @@ void vbiConfigShow(struct vb_config *p)
 	printk("Reset PC:           0x%p\n", p->reset_pc);
 
 	printk("Timer Frequency:     %d/sec\n", p->tick_freq);
-	printk("TimeStamp Frequency: %dULL/sec\n", p->timeStampFrequency);
+	printk("TimeStamp Frequency: %dULL/sec\n", p->stamp_freq);
 
 	printk("num_ints:             %d\n", p->num_ints);
 
diff --git a/kernel/vbi/wrhv.c b/kernel/vbi/wrhv.c
index c95f732..73b32c6 100644
--- a/kernel/vbi/wrhv.c
+++ b/kernel/vbi/wrhv.c
@@ -71,8 +71,8 @@ struct irq_chip wrhv_irq_chip = {
 unsigned long wrhv_calculate_cpu_khz(void)
 {
 	printk(KERN_DEBUG "WRHV: Timestamp Frequency %u Hz\n",
-		wrhvVbConfig->timeStampFrequency);
-	return wrhvVbConfig->timeStampFrequency / 1000;
+		wrhvVbConfig->stamp_freq);
+	return wrhvVbConfig->stamp_freq / 1000;
 }
 
 irqreturn_t __weak wrhv_timer_interrupt(int irq, void *dev_id)
-- 
