From 3643e3463e4f9bf2e8eed65080a594d847ea8b86 Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 10:07:21 -0500
Subject: [PATCH 30/80] vbiKputc --> vbi_kputc

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/powerpc/kernel/vbi/syscalls.S |    8 ++++----
 arch/x86/kernel/vbi/syscalls.S     |    6 +++---
 include/vbi/syscall.h              |    2 +-
 3 files changed, 8 insertions(+), 8 deletions(-)

diff --git a/arch/powerpc/kernel/vbi/syscalls.S b/arch/powerpc/kernel/vbi/syscalls.S
index 26119b7..baf3c6a 100644
--- a/arch/powerpc/kernel/vbi/syscalls.S
+++ b/arch/powerpc/kernel/vbi/syscalls.S
@@ -47,7 +47,7 @@ FUNC_EXPORT(vbi_flush_tlb)
 FUNC_EXPORT(vbi_panic)
 FUNC_EXPORT(vbi_ps)
 FUNC_EXPORT(vbi_kputs)
-FUNC_EXPORT(vbiKputc)
+FUNC_EXPORT(vbi_kputc)
 FUNC_EXPORT(vbiIntVCoreUnlock)
 FUNC_EXPORT(vbiIntVCoreLock)
 FUNC_EXPORT(vbiIntVCoreStateGet)
@@ -302,18 +302,18 @@ FUNC_END(vbi_kputs)
 
 /*
  *
- * vbiKputc - print a character on the hypervisor kernel console
+ * vbi_kputc - print a character on the hypervisor kernel console
  *
  * This routine makes a hypercall in order to print the specified character to
  * hypervisor console.
  *
  */
-FUNC_LABEL(vbiKputc)
+FUNC_LABEL(vbi_kputc)
 	lis r0, HI(VBI_SYS_kputc)
 	ori r0, r0, LO(VBI_SYS_kputc)
 	HCALL
 	blr
-FUNC_END(vbiKputc)
+FUNC_END(vbi_kputc)
 
 /*
  * vbi_panic - halt the system and dump debug info
diff --git a/arch/x86/kernel/vbi/syscalls.S b/arch/x86/kernel/vbi/syscalls.S
index b095f7f..d60adab 100644
--- a/arch/x86/kernel/vbi/syscalls.S
+++ b/arch/x86/kernel/vbi/syscalls.S
@@ -39,7 +39,7 @@
 .globl vbi_panic
 .globl vbi_ps
 .globl vbi_kputs
-.globl vbiKputc
+.globl vbi_kputc
 .globl vbiIntVCoreUnlock
 .globl vbiIntVCoreLock
 .globl vbiVbMgmt
@@ -127,14 +127,14 @@ vbi_kputs:
 
 
 /*
- * vbiKputc - print a character on the kernel console
+ * vbi_kputc - print a character on the kernel console
  *
  * This system call sends the specified character to the system console.
  *
  * Returns: OK or ERROR
  *
  */
-vbiKputc:
+vbi_kputc:
 	movl $VBI_SYS_kputc,%eax
 	push $1
 	vmcall
diff --git a/include/vbi/syscall.h b/include/vbi/syscall.h
index 5d09d39..f736a39 100644
--- a/include/vbi/syscall.h
+++ b/include/vbi/syscall.h
@@ -113,7 +113,7 @@ extern asmlinkage vbiStatus_t vbi_panic(const char *msg);
 extern vbiStatus_t vbi_flush_dcache(void *saddr, size_t size);
 extern vbiStatus_t vbi_flush_icache(void *saddr, size_t size);
 extern asmlinkage vbiStatus_t vbi_kputs(const char *s);
-extern asmlinkage vbiStatus_t vbiKputc(int c);
+extern asmlinkage vbiStatus_t vbi_kputc(int c);
 extern vbiStatus_t vbiVmmuConfig(struct vmmuConfig * config);
 extern vbiStatus_t vbiVmmuEnable(uint32_t  vmmu_num);
 extern vbiStatus_t vbiVmmuDisable(uint32_t vmmu_num);
-- 
