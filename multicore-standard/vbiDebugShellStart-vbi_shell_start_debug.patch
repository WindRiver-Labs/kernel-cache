From 05f17a4a4b9b60cf09945d04b9a6dfbc2a6ce6c2 Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 10:07:09 -0500
Subject: [PATCH 15/80] vbiDebugShellStart --> vbi_shell_start_debug

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/powerpc/kernel/vbi/syscalls.S |    8 ++++----
 arch/x86/kernel/vbi/syscalls.S     |    6 +++---
 include/vbi/syscall.h              |    2 +-
 3 files changed, 8 insertions(+), 8 deletions(-)

diff --git a/arch/powerpc/kernel/vbi/syscalls.S b/arch/powerpc/kernel/vbi/syscalls.S
index a911cf0..3b11ab3 100644
--- a/arch/powerpc/kernel/vbi/syscalls.S
+++ b/arch/powerpc/kernel/vbi/syscalls.S
@@ -28,7 +28,7 @@
 /* globals */
 
 FUNC_EXPORT(vbi_vcore_irq_redirect)
-FUNC_EXPORT(vbiDebugShellStart)
+FUNC_EXPORT(vbi_shell_start_debug)
 FUNC_EXPORT(vbi_vb_read_mem)
 FUNC_EXPORT(vbi_vb_write_mem)
 FUNC_EXPORT(vbi_vb_read_reg)
@@ -335,7 +335,7 @@ FUNC_LABEL(vbiPanic)
 FUNC_END(vbiPanic)
 
 /*
- * vbiDebugShellStart - start the hypervisor debug shell
+ * vbi_shell_start_debug - start the hypervisor debug shell
  *
  * This routine sends a message to the hypervisor debug shell manager in order
  * to start the WRHV shell program. The shell program spins therefore does not
@@ -347,7 +347,7 @@ FUNC_END(vbiPanic)
  *
  */
 
-FUNC_LABEL(vbiDebugShellStart)
+FUNC_LABEL(vbi_shell_start_debug)
 
 	/* r3 - flags for attachement behaviour */
 
@@ -355,7 +355,7 @@ FUNC_LABEL(vbiDebugShellStart)
 	ori r0, r0, LO(VBI_SYS_dbgShStart)
 	HCALL
 	blr
-FUNC_END(vbiDebugShellStart)
+FUNC_END(vbi_shell_start_debug)
 
 /*
  *
diff --git a/arch/x86/kernel/vbi/syscalls.S b/arch/x86/kernel/vbi/syscalls.S
index 58be69e..f8fed24 100644
--- a/arch/x86/kernel/vbi/syscalls.S
+++ b/arch/x86/kernel/vbi/syscalls.S
@@ -25,7 +25,7 @@
 #include <vbi/syscall.h>
 
 /* globals */
-.globl vbiDebugShellStart
+.globl vbi_shell_start_debug
 .globl vbi_vb_write_mem
 .globl vbi_vb_read_mem
 .globl vbiIoapicOp
@@ -582,7 +582,7 @@ vbi_vb_read_mem:
 	ret
 
 /*
- * vbiDebugShellStart - start the hypervisor debug shell
+ * vbi_shell_start_debug - start the hypervisor debug shell
  *
  * This routine sends a message to the hypervisor debug shell manager in
  * order to start the WRHV shell program. The shell program spins
@@ -594,7 +594,7 @@ vbi_vb_read_mem:
  * shell program is running.
  *
  */
-vbiDebugShellStart:
+vbi_shell_start_debug:
 	movl $VBI_SYS_dbgShStart, %eax
 	push	$1
 	vmcall
diff --git a/include/vbi/syscall.h b/include/vbi/syscall.h
index 12c713f..a33f61e 100644
--- a/include/vbi/syscall.h
+++ b/include/vbi/syscall.h
@@ -146,7 +146,7 @@ extern asmlinkage vbiStatus_t vbi_vb_resume(vbiVb_t id, vbiCore_t core);
 /* read remote vb's memory */
 extern asmlinkage vbiStatus_t vbi_vb_read_mem(VBI_MEM_CTL *memCtl, vbiVb_t targetBoard);
 extern asmlinkage vbiStatus_t vbi_vb_write_mem(VBI_MEM_CTL *memCtl, vbiVb_t targetBoard);
-extern asmlinkage void vbiDebugShellStart(uint32_t  flags);
+extern asmlinkage void vbi_shell_start_debug(uint32_t  flags);
 
 /* read remote vb's registers */
 extern asmlinkage vbiStatus_t vbi_vb_read_reg(VBI_HREG_SET *regCtl, 
