From a15df240c6f94744f62fdd0f7085c66f12d3787d Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Wed, 18 Nov 2009 10:07:18 -0500
Subject: [PATCH 26/80] vbiTlbFlush --> vbi_flush_tlb

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 arch/powerpc/kernel/vbi/syscalls.S |    8 ++++----
 arch/x86/kernel/vbi/syscalls.S     |    2 +-
 2 files changed, 5 insertions(+), 5 deletions(-)

diff --git a/arch/powerpc/kernel/vbi/syscalls.S b/arch/powerpc/kernel/vbi/syscalls.S
index 16b5b00..51bdacd 100644
--- a/arch/powerpc/kernel/vbi/syscalls.S
+++ b/arch/powerpc/kernel/vbi/syscalls.S
@@ -43,7 +43,7 @@ FUNC_EXPORT(vbi_reply)
 FUNC_EXPORT(vbi_flush_icache)
 FUNC_EXPORT(vbi_flush_dcache)
 FUNC_EXPORT(vbi_update_text_cache)
-FUNC_EXPORT(vbiTlbFlush)
+FUNC_EXPORT(vbi_flush_tlb)
 FUNC_EXPORT(vbiPanic)
 FUNC_EXPORT(vbiPs)
 FUNC_EXPORT(vbiKputs)
@@ -388,17 +388,17 @@ FUNC_END(vbi_flush_dcache)
 
 /*
  *
- * vbiTlbFlush - flush an MMU TLB entry
+ * vbi_flush_tlb - flush an MMU TLB entry
  *
  * This system call flushes the TLB associated with the specified context id
  *
  */
-FUNC_LABEL(vbiTlbFlush)
+FUNC_LABEL(vbi_flush_tlb)
 	lis r0, HI(VBI_SYS_tlb_flush)
 	ori r0, r0, LO(VBI_SYS_tlb_flush)
 	HCALL
 	blr
-FUNC_END(vbiTlbFlush)
+FUNC_END(vbi_flush_tlb)
 
 /*
  * vbiIntVCoreUnlock - unlock interrupts interrupts for running core
diff --git a/arch/x86/kernel/vbi/syscalls.S b/arch/x86/kernel/vbi/syscalls.S
index 59c8658..e4a3403 100644
--- a/arch/x86/kernel/vbi/syscalls.S
+++ b/arch/x86/kernel/vbi/syscalls.S
@@ -35,7 +35,7 @@
 .globl vbi_send
 .globl vbi_rx_op
 .globl vbi_reply
-.globl vbiTlbFlush
+.globl vbi_flush_tlb
 .globl vbiPanic
 .globl vbiPs
 .globl vbiKputs
-- 
