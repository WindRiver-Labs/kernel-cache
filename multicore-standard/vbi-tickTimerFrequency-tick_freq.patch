From 411090b43e718942e2fb78183f28f1ad4898210e Mon Sep 17 00:00:00 2001
From: Paul Gortmaker <paul.gortmaker@windriver.com>
Date: Tue, 3 Nov 2009 22:08:32 -0500
Subject: [PATCH 02/80] vbi: tickTimerFrequency -> tick_freq

for i in `git grep -l tickTimerFrequency` ; do echo $i ; mv $i $i~ ; cat $i~ |sed 's/tickTimerFrequency/tick_freq/g' > $i ; done

Signed-off-by: Paul Gortmaker <paul.gortmaker@windriver.com>
---
 include/vbi/interface.h |    2 +-
 include/vbi/vbi.h       |    2 +-
 kernel/vbi/show.c       |    2 +-
 3 files changed, 3 insertions(+), 3 deletions(-)

diff --git a/include/vbi/interface.h b/include/vbi/interface.h
index c95c5f2..2fd67c1 100644
--- a/include/vbi/interface.h
+++ b/include/vbi/interface.h
@@ -556,7 +556,7 @@ struct vb_config
 	 */
 
 	/* the frequency of the periodic tick interrupt */
-	uint32_t tickTimerFrequency;	/* ticks per second */
+	uint32_t tick_freq;	/* ticks per second */
 
 	/* the frequency of the timestamp */
 	uint32_t timeStampFrequency;	/* ticks per second */
diff --git a/include/vbi/vbi.h b/include/vbi/vbi.h
index cdec452..28454d9 100644
--- a/include/vbi/vbi.h
+++ b/include/vbi/vbi.h
@@ -476,7 +476,7 @@ various data structures.
  *
  */
 #define VBI_TICK_TIMER_FREQ_GET()		        \
-        (VBI_CONFIG_ADDR_GET()->tickTimerFrequency)
+        (VBI_CONFIG_ADDR_GET()->tick_freq)
 
 #define VBI_TIMESTAMP_FREQ_GET()			\
         (VBI_CONFIG_ADDR_GET()->timeStampFrequency)
diff --git a/kernel/vbi/show.c b/kernel/vbi/show.c
index 6cce44b..a54b509 100644
--- a/kernel/vbi/show.c
+++ b/kernel/vbi/show.c
@@ -178,7 +178,7 @@ void vbiConfigShow(struct vb_config *p)
 	printk("Memory Alias Size:  %u\n", p->mem_alias_size);
 	printk("Reset PC:           0x%p\n", p->reset_pc);
 
-	printk("Timer Frequency:     %d/sec\n", p->tickTimerFrequency);
+	printk("Timer Frequency:     %d/sec\n", p->tick_freq);
 	printk("TimeStamp Frequency: %dULL/sec\n", p->timeStampFrequency);
 
 	printk("num_ints:             %d\n", p->num_ints);
-- 
